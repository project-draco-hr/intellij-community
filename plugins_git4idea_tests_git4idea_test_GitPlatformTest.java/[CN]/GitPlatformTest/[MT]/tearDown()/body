{
  try {
    if (myDialogManager != null) {
      myDialogManager.cleanup();
    }
    if (myVcsNotifier != null) {
      myVcsNotifier.cleanup();
    }
  }
  finally {
    try {
      super.tearDown();
    }
  finally {
      if (myAssertionsInTestDetected) {
        TestLoggerFactory.dumpLogToStdout(myTestStartedIndicator);
      }
    }
  }
}
