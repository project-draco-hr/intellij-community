{
  try {
    super.defaultRunBare();
  }
 catch (  Throwable throwable) {
    try {
      if (myTestStartedIndicator != null) {
        TestLoggerFactory.dumpLogToStdout(myTestStartedIndicator);
      }
      throw throwable;
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
