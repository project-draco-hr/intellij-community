{
  File[] files=testRoot.listFiles();
  if (files != null && files.length > 0) {
    LOG.warn("Test root was not cleaned up during some previous test run. " + "testRoot: " + testRoot + ", files: "+ Arrays.toString(files));
    for (    File file : files) {
      LOG.assertTrue(FileUtil.delete(file));
    }
  }
}
