{
  TextAttributes attributes=getTextAttributes(type,editor,ignored,true);
  RangeHighlighter highlighter=editor.getMarkupModel().addRangeHighlighter(start,end,HighlighterLayer.SELECTION - 3,start != end ? attributes : null,area);
  highlighter.setThinErrorStripeMark(true);
  installGutterRenderer(highlighter,type,ignored);
  return highlighter;
}
