{
  if (line1 == line2) {
    if (line1 == 0)     return Collections.emptyList();
    return createLineMarker(editor,line1 - 1,type,SeparatorPlacement.BOTTOM,true,applied);
  }
 else {
    return ContainerUtil.concat(createLineMarker(editor,line1,type,SeparatorPlacement.TOP,false,applied),createLineMarker(editor,line2 - 1,type,SeparatorPlacement.BOTTOM,false,applied));
  }
}
