{
  TextAttributes attributes=getTextAttributes(type,editor,false,false);
  RangeHighlighter highlighter=editor.getMarkupModel().addRangeHighlighter(start,end,HighlighterLayer.SELECTION - 2,attributes,HighlighterTargetArea.EXACT_RANGE);
  if (start == end)   installEmptyRangeRenderer(highlighter,type);
  return highlighter;
}
