{
  PsiElement elt=myFile.findElementAt(problemStart);
  int startOffset=problemStart - elt.getTextRange().getStartOffset();
  int endOffset=startOffset + problemLength;
  myProblems.add(myManager.createProblemDescriptor(elt,new TextRange(startOffset,endOffset),descriptionTemplate,ProblemHighlightType.GENERIC_ERROR_OR_WARNING,myOnTheFly,new ConvertIndentsFix(false),new ConvertIndentsFix(true)));
}
