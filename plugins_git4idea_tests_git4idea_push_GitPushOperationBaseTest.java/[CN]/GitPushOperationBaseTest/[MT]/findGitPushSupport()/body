{
  return (GitPushSupport)ObjectUtils.assertNotNull(ContainerUtil.find(Extensions.getExtensions(PushSupport.PUSH_SUPPORT_EP,myProject),new Condition<PushSupport<?,?,?>>(){
    @Override public boolean value(    PushSupport<?,?,?> support){
      return support instanceof GitPushSupport;
    }
  }
));
}
