{
  List<TextAttributesKey> keys=RainbowHighlighter.getRainbowKeys();
  if (!keys.isEmpty()) {
    List<HighlightData> newData=new ArrayList<HighlightData>();
    for (    HighlightData d : data) {
      if (!keys.contains(d.getHighlightKey())) {
        newData.add(d);
      }
    }
    data.clear();
    data.addAll(newData);
  }
}
