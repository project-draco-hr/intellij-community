{
  String url=StringUtil.nullize(urlTextField.getText());
  boolean enabled;
  try {
    enabled=url != null && url.length() > 1 && icsManager.getRepositoryService().checkUrl(url,null);
  }
 catch (  Exception e) {
    enabled=false;
  }
  for (  Action syncAction : syncActions) {
    syncAction.setEnabled(enabled);
  }
}
