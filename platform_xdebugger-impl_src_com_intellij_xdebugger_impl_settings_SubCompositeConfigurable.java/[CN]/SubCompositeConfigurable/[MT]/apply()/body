{
  if (root != null) {
    root.apply(getSettings());
    for (    DebuggerSupport support : DebuggerSupport.getDebuggerSupports()) {
      support.getSettingsPanelProvider().generalApplied(getCategory());
    }
  }
  if (isChildrenMerged()) {
    for (    Configurable child : children) {
      if (child.isModified()) {
        child.apply();
      }
    }
  }
}
