{
  boolean result;
  try {
    myFactory.createHistoryClient().doLog(SvnTarget.fromURL(url),SVNRevision.HEAD,SVNRevision.create(1),false,false,true,1,null,null);
    result=true;
  }
 catch (  SvnBindException e) {
    if (e.contains(SVNErrorCode.UNSUPPORTED_FEATURE) && e.getMessage().contains("mergeinfo")) {
      result=false;
    }
 else {
      throw e;
    }
  }
  return result;
}
