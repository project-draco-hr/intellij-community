{
  try {
    Result result;
    if (count == 1) {
      result=startRunnerWithArgs(args,listeners,name,sendTree);
      if (result == null) {
        return -1;
      }
    }
 else {
      if (count > 0) {
        boolean success=true;
        int i=0;
        while (i++ < count) {
          result=startRunnerWithArgs(args,listeners,name,sendTree);
          if (result == null) {
            return -1;
          }
          success&=result.wasSuccessful();
        }
        return success ? 0 : -1;
      }
 else {
        boolean success=true;
        while (true) {
          result=startRunnerWithArgs(args,listeners,name,sendTree);
          if (result == null) {
            return -1;
          }
          success&=result.wasSuccessful();
          if (count == -2 && !success) {
            return -1;
          }
        }
      }
    }
    if (!result.wasSuccessful()) {
      return -1;
    }
    return 0;
  }
 catch (  Exception e) {
    System.err.println("Internal Error occured.");
    e.printStackTrace(System.err);
    return -2;
  }
}
