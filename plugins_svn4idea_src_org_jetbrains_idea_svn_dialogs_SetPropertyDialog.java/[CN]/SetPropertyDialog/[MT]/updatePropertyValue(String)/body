{
  if (myFiles.length == 0 || myFiles.length > 1) {
    return;
  }
  File file=myFiles[0];
  PropertyValue property=!StringUtil.isEmpty(name) ? getProperty(file,name) : null;
  if (property != null) {
    myValueText.setText(property.toString());
    myValueText.selectAll();
  }
 else {
    myValueText.setText("");
  }
}
