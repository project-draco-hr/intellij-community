{
  if (myFiles.length == 0 || myFiles.length > 1) {
    return;
  }
  File file=myFiles[0];
  PropertyData property=!StringUtil.isEmpty(name) ? getProperty(file,name) : null;
  if (property != null) {
    myValueText.setText(PropertyValue.toString(property.getValue()));
    myValueText.selectAll();
  }
 else {
    myValueText.setText("");
  }
}
