{
  final InstructionImpl popped=myProcessingStack.pop();
  if (!instruction.equals(popped)) {
    String description="popped: " + popped.toString() + " : "+ popped.hashCode()+ "   ,  expected: "+ instruction.toString()+ " : "+ instruction.hashCode()+ "same objects:"+ (popped == instruction);
    error(description);
  }
}
