{
  connectIfNeeded(new ConnectionCallbackBase<D>(){
    @Override public void connected(    @NotNull ServerRuntimeInstance<D> instance){
      myDeploymentInfos.put(task.getSource(),new DeploymentInformation(DeploymentStatus.UNDEPLOYING));
      instance.undeploy(task,new UpdateDeploymentStatusCallback(task.getSource(),DeploymentStatus.NOT_DEPLOYED,DeploymentStatus.DEPLOYED));
    }
  }
);
}
