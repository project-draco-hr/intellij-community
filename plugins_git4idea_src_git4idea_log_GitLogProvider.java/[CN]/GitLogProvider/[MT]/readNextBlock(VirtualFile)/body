{
  List<GitCommit> history=GitHistoryUtils.history(myProject,root);
  return ContainerUtil.map(history,new Function<GitCommit,CommitParents>(){
    @Override public CommitParents fun(    GitCommit gitCommit){
      return new SimpleCommitParents(Hash.build(gitCommit.getHash().getValue()),ContainerUtil.map(gitCommit.getParentsHashes(),new Function<String,Hash>(){
        @Override public Hash fun(        String s){
          return Hash.build(s);
        }
      }
));
    }
  }
);
}
