{
  MyTimer timer=new MyTimer("Git Log ALL");
  List<GitCommit> history=GitHistoryUtils.history(myProject,root,"HEAD","--branches","--remotes","--tags","--date-order","--encoding=UTF-8","--full-history","--sparse","--max-count=" + VcsLogProvider.COMMIT_BLOCK_SIZE);
  List<CommitParents> map=ContainerUtil.map(history,new Function<GitCommit,CommitParents>(){
    @Override public CommitParents fun(    GitCommit gitCommit){
      return new SimpleCommitParents(Hash.build(gitCommit.getHash().toString()),gitCommit.getParents());
    }
  }
);
  timer.print();
  return map;
}
