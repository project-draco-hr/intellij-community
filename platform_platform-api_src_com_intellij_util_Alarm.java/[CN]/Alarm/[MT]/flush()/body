{
  List<Request> requests;
synchronized (LOCK) {
    if (myRequests.isEmpty()) {
      return;
    }
    requests=new SmartList<Request>();
    for (    Request request : myRequests) {
      if (request.cancel()) {
        requests.add(request);
      }
    }
    myRequests.clear();
  }
  for (  Request request : requests) {
    request.run();
  }
}
