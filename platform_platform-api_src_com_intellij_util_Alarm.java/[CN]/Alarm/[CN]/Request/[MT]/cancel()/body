{
  boolean result;
synchronized (LOCK) {
    if (myFuture != null) {
      result=myFuture.cancel(false);
      ((ScheduledThreadPoolExecutor)JobScheduler.getScheduler()).remove((Runnable)myFuture);
      myFuture=null;
    }
 else {
      result=false;
    }
    myTask=null;
  }
  return result;
}
