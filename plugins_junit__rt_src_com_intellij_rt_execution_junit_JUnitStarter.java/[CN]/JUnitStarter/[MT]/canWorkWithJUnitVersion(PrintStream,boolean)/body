{
  try {
    junitVersionChecks(isJUnit4);
  }
 catch (  Throwable e) {
    printStream.println("!!! JUnit version 3.8 or later expected:");
    printStream.println();
    e.printStackTrace(printStream);
    printStream.flush();
    return false;
  }
 finally {
    printStream.flush();
  }
  return true;
}
