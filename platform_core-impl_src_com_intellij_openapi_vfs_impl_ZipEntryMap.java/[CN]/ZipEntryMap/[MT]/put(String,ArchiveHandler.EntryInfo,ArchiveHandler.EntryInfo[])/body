{
  int index=index(relativePath,entries);
  ArchiveHandler.EntryInfo entry;
  int i=index;
  while (true) {
    entry=entries[i];
    if (entry == null || entry == REMOVED || isTheOne(entry,relativePath)) {
      entries[i]=value;
      break;
    }
    if (++i == entries.length) {
      i=0;
    }
  }
  return entry == REMOVED ? null : entry;
}
