{
  myVcs=vcs;
  myProject=vcs.getProject();
  myManager=manager;
  myLocation=location;
  myDataLoader=new WcInfoLoader(myVcs,myLocation);
  myMergePanels=new HashMap<>();
  myHolders=new HashMap<>();
