{
  return new ChangeListsMergerFactory(checker.getSelectedLists()){
    @Override public IMerger createMerger(    final SvnVcs vcs,    final File target,    final UpdateEventHandler handler,    final SVNURL currentBranchUrl,    String branchName){
      return new Merger(vcs,myChangeListsList,target,handler,currentBranchUrl,branchName,false,!myDirect);
    }
  }
;
}
