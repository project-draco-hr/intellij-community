{
  try {
    SVNURL result=encoded ? SVNURL.parseURIEncoded(url) : SVNURL.parseURIDecoded(url);
    if (result.hasPort() && hasDefaultPort(result)) {
      result=SVNURL.create(result.getProtocol(),result.getUserInfo(),result.getHost(),DEFAULT_PORT_INDICATOR,result.getURIEncodedPath(),true);
    }
    return result;
  }
 catch (  SVNException e) {
    throw new SvnBindException(e);
  }
}
