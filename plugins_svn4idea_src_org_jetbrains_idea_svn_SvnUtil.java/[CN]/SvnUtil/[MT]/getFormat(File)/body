{
  WorkingCopyFormat result=null;
  File dbFile=resolveDatabase(path);
  if (dbFile != null) {
    result=FileUtilRt.doIOOperation(new WorkingCopyFormatOperation(dbFile));
    if (result == null) {
      notifyDatabaseError();
    }
  }
  return result != null ? result : WorkingCopyFormat.UNKNOWN;
}
