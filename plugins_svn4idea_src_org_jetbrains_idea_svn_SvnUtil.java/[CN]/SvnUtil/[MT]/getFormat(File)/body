{
  int format=0;
  File dbFile=resolveDatabase(path);
  if (dbFile != null) {
    SqlJetDb db=null;
    try {
      db=SqlJetDb.open(dbFile,false);
      format=db.getOptions().getUserVersion();
    }
 catch (    SqlJetException e) {
      LOG.error(e);
    }
 finally {
      if (db != null) {
        try {
          db.close();
        }
 catch (        SqlJetException e) {
          LOG.error(e);
        }
      }
    }
  }
  return WorkingCopyFormat.getInstance(format);
}
