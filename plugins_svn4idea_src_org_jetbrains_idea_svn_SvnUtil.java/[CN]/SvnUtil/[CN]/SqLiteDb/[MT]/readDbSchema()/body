{
  StringBuilder result=new StringBuilder();
  ISqlJetBtreeSchemaTable table=new SqlJetBtreeSchemaTable(btree,false);
  try {
    table.lock();
    try {
      for (table.first(); !table.eof(); table.next()) {
        String sql=table.getSqlField();
        if (sql != null) {
          result.append(sql);
          result.append("\n");
        }
      }
    }
  finally {
      table.unlock();
    }
  }
  finally {
    table.close();
  }
  return result.toString();
}
