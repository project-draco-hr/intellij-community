{
  SqlJetDb db=null;
  WorkingCopyFormat result=null;
  try {
    db=SqlJetDb.open(myDbFile,true);
    result=WorkingCopyFormat.getInstance(db.getOptions().getUserVersion());
  }
 catch (  NonWritableChannelException e) {
    LOG.info(e);
  }
catch (  SqlJetException e) {
    LOG.info(e);
  }
 finally {
    close(db);
  }
  return result;
}
