{
  int first=0;
  int count=mappings.size();
  while (count > 1) {
    int step=count >> 1;
    int middle=first + step;
    MappingEntry mapping=mappings.get(middle);
    if (generatedLine < mapping.getGeneratedLine() || (generatedLine == mapping.getGeneratedLine() && generatedColumn < mapping.getGeneratedColumn())) {
      count=step;
    }
 else {
      first=middle;
      count-=step;
    }
  }
  return first;
}
