{
  final String text=field.getText();
  if (text.length() > 0) {
    final String s=ScriptSupport.checkValidScript(text);
    if (s != null) {
      Messages.showErrorDialog(SSRBundle.message("invalid.groovy.script"),SSRBundle.message("invalid.groovy.script"));
      field.requestFocus();
      return false;
    }
  }
  return true;
}
