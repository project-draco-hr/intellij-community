{
  myDialogManager.registerDialogHandler(GithubShareAction.GithubUntrackedFilesDialog.class,new TestDialogHandler<GithubShareAction.GithubUntrackedFilesDialog>(){
    @Override public int handleDialog(    GithubShareAction.GithubUntrackedFilesDialog dialog){
      for (      GitRepository repository : GitUtil.getRepositoryManager(myProject).getRepositories()) {
        setGitIdentity(repository.getRoot());
      }
      return DialogWrapper.OK_EXIT_CODE;
    }
  }
);
}
