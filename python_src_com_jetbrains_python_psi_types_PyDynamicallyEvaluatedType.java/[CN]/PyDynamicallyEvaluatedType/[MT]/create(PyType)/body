{
  final List<PyType> members=new ArrayList<>();
  if (type instanceof PyUnionType) {
    final PyUnionType unionType=(PyUnionType)type;
    members.addAll(unionType.getMembers());
    if (!unionType.isWeak()) {
      members.add(null);
    }
  }
 else {
    members.add(type);
    members.add(null);
  }
  return new PyDynamicallyEvaluatedType(members);
}
