{
  StringBuilder errorStr=new StringBuilder();
  try {
    String line;
    while ((line=errorReader.readLine()) != null) {
      errorStr.append(line).append("\n");
    }
    writeErrorMessage(errorStr.toString());
  }
 catch (  IOException e) {
    writeErrorMessage(e.getMessage() + " | " + errorStr.toString());
  }
}
