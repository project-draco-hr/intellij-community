{
  if (classToDelegate == null)   return true;
  if (state.get(ClassHint.RESOLVE_CONTEXT) == null) {
    state=state.put(ClassHint.RESOLVE_CONTEXT,this);
  }
  return ResolveUtil.processAllDeclarationsSeparately(classToDelegate,processor,nonCodeProcessor,state.put(ClassHint.RESOLVE_CONTEXT,this),place);
}
