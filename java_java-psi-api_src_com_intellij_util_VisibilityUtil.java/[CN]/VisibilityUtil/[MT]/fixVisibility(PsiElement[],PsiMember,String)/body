{
  if (newVisibility == null)   return;
  if (ESCALATE_VISIBILITY.equals(newVisibility)) {
    for (    PsiElement element : elements) {
      if (element != null) {
        escalateVisibility(member,element);
      }
    }
  }
 else {
    setVisibility(member.getModifierList(),newVisibility);
  }
}
