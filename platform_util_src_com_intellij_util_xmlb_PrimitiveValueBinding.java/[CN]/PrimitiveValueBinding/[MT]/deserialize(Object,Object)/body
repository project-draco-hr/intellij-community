{
  if (nodes.length == 0) {
    return convertString("");
  }
  String value;
  if (nodes.length > 1) {
    value=JDOMUtil.concatTextNodesValues(nodes);
  }
 else {
    Object node=nodes[0];
    value=node instanceof Attribute ? ((Attribute)node).getValue() : ((Content)node).getValue();
  }
  return convertString(value);
}
