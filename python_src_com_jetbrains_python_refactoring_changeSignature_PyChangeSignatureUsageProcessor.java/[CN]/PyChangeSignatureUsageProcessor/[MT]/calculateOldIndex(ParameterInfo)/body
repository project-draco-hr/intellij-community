{
  if (info.getName().equals(PyNames.CANONICAL_SELF)) {
    isMethod=true;
  }
  if (info.getName().equals("*")) {
    isAfterStar=true;
    useKeywords=true;
  }
  int oldIndex=info.getOldIndex();
  oldIndex=isMethod ? oldIndex - 1 : oldIndex;
  oldIndex=isAfterStar ? oldIndex - 1 : oldIndex;
  return oldIndex;
}
