{
  super.writeSettings(node);
  for (  Element child : new ArrayList<Element>(node.getChildren())) {
    String name=child.getAttributeValue("name");
    String value=child.getAttributeValue("value");
    if ("IGNORE_EXTERNAL_SUPER_NOTNULL".equals(name) && "false".equals(value) || "REPORT_NOTNULL_PARAMETERS_OVERRIDES_NOT_ANNOTATED".equals(name) && "false".equals(value)) {
      node.removeContent(child);
    }
  }
}
