{
  if (size.equals("-1")) {
    return IdeBundle.message("plugin.info.unknown");
  }
  if (size.length() >= 7) {
    size=String.format("%.1f",(float)Integer.parseInt(size) / MB) + " M";
  }
 else   if (size.length() >= 4) {
    size=String.format("%.1f",(float)Integer.parseInt(size) / KB) + " K";
  }
  return size;
}
