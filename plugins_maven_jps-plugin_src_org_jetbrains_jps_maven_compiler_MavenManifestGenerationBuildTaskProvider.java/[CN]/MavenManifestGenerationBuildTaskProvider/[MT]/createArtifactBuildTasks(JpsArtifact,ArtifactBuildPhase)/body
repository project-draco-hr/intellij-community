{
  String artifactName=artifact.getName();
  if (buildPhase == ArtifactBuildPhase.PRE_PROCESSING && (artifactName.endsWith(" exploded") || artifactName.endsWith("ejb-client")) && artifact.getRootElement() instanceof JpsArtifactRootElement) {
    return Collections.singletonList(new MavenManifestGenerationBuildTask(artifact));
  }
  return Collections.emptyList();
}
