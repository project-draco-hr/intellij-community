{
  registrar.registerReferenceProvider(psiElement(PyStringLiteralExpression.class).withParent(psiElement(PySequenceExpression.class).with(IN_OPTIONAL_PARENTHESIS_INSIDE_ASSIGNMENT).inside(true,psiElement(PyAssignmentStatement.class).withFirstChild(psiElement(PyTargetExpression.class).withName(name)))),provider);
