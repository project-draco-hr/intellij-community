{
  runPythonTest(new PyDebuggerTask("/debug","Test_Resume.py"){
    @Override public void before() throws Exception {
      toggleBreakpoint(getScriptPath(),2);
    }
    @Override public void testing() throws Exception {
      waitForPause();
      eval("x").hasValue("1");
      resume();
      waitForPause();
      eval("x").hasValue("2");
      resume();
    }
  }
);
}
