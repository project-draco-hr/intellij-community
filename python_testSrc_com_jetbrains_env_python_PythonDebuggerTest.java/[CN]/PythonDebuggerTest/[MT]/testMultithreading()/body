{
  runPythonTest(new PyDebuggerTask("/debug","test_multithread.py"){
    @Override public void before() throws Exception {
      toggleBreakpoint(getScriptPath(),9);
      toggleBreakpoint(getScriptPath(),13);
    }
    @Override public void testing() throws Exception {
      waitForPause();
      eval("y").hasValue("2");
      resume();
      waitForPause();
      eval("z").hasValue("102");
      resume();
    }
    @Override public Set<String> getTags(){
      return ImmutableSet.of("-pypy");
    }
  }
);
}
