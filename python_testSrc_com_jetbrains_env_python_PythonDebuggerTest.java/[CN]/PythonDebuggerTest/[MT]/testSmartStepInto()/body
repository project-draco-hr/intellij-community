{
  runPythonTest(new PyDebuggerTask("/debug","test3.py"){
    @Override public void before() throws Exception {
      toggleBreakpoint(getScriptPath(),14);
    }
    @Override public void testing() throws Exception {
      waitForPause();
      smartStepInto("foo");
      waitForPause();
      stepOver();
      waitForPause();
      eval("y").hasValue("4");
    }
  }
);
}
