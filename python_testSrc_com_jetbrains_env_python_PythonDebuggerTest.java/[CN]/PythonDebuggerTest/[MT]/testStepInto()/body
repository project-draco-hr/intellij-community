{
  runPythonTest(new PyDebuggerTask("/debug","test2.py"){
    @Override public void before() throws Exception {
      toggleBreakpoint(getFilePath(getScriptName()),5);
    }
    @Override public void testing() throws Exception {
      waitForPause();
      stepInto();
      waitForPause();
      eval("x").hasValue("1");
      stepOver();
      waitForPause();
      eval("y").hasValue("3");
      stepOver();
      waitForPause();
      eval("z").hasValue("1");
    }
  }
);
}
