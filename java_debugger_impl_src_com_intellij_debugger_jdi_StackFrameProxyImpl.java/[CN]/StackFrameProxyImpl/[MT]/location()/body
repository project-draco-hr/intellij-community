{
  InvalidStackFrameException error=null;
  for (int attempt=0; attempt < 2; attempt++) {
    try {
      return getStackFrame().location();
    }
 catch (    InvalidStackFrameException e) {
      error=e;
      clearCaches();
    }
  }
  if (error != null) {
    throw new EvaluateException(error.getMessage(),error);
  }
  return null;
}
