{
  final PsiElementRole role=nodeSpecificHasher.getDuplicatesProfile().getRole(element);
  if (role != null) {
switch (role) {
case VARIABLE_NAME:
      if (!mySettings.DISTINGUISH_VARIABLES) {
        return true;
      }
    break;
case FIELD_NAME:
  if (!mySettings.DISTINGUISH_FIELDS) {
    return true;
  }
break;
case FUNCTION_NAME:
if (!mySettings.DISTINGUISH_METHODS) {
return true;
}
break;
}
}
return false;
}
