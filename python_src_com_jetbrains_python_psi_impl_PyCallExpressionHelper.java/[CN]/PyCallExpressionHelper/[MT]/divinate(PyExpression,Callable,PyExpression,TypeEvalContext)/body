{
  PyFunction method=resolved.asMethod();
  if (method != null) {
    if (lastQualifier == null)     return true;
    PyType qtype=context.getType(lastQualifier);
    if (qtype != null) {
      if (qtype instanceof PyClassType) {
        return !((PyClassType)qtype).isDefinition();
      }
 else       return true;
    }
 else     return true;
  }
  return false;
}
