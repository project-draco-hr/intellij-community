{
  super.checkConfiguration();
  Sdk sdkPath=PythonSdkType.findSdkByPath(getInterpreterPath());
  if (sdkPath != null && !VFSTestFrameworkListener.getInstance().isNoseTestInstalled(sdkPath))   throw new RuntimeConfigurationWarning(PyBundle.message("runcfg.testing.no.test.framework","nosetest"));
}
