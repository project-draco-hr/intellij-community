{
  if (myButton > 3 && myButton != MouseShortcut.BUTTON_WHEEL_UP && myButton != MouseShortcut.BUTTON_WHEEL_DOWN && getClickCount() == 2) {
    return null;
  }
  if (myButton != -1 && myModifiers != -1) {
    return new MouseShortcut(myButton,myModifiers,getClickCount());
  }
 else {
    return null;
  }
}
