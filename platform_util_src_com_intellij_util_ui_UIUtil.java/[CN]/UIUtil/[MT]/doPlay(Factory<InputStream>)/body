{
  new Thread(new Runnable(){
    public void run(){
      try {
        Clip clip=AudioSystem.getClip();
        AudioInputStream inputStream=AudioSystem.getAudioInputStream(streamProducer.create());
        clip.open(inputStream);
        clip.start();
      }
 catch (      Exception ignore) {
        LOG.info(ignore);
      }
    }
  }
).start();
}
