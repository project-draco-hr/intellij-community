{
  if (state != State.PENDING) {
    return false;
  }
  result=error;
  state=State.REJECTED;
  Consumer<? super Throwable> rejected=this.rejected;
  clearHandlers();
  if (rejected == null) {
    Promise.logError(LOG,error);
  }
 else   if (!isObsolete(rejected)) {
    rejected.consume(error);
  }
  return true;
}
