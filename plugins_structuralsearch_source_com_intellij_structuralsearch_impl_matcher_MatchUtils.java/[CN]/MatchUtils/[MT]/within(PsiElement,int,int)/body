{
  if (element == null)   return false;
  int elementOffset=element.getTextOffset();
  if (element instanceof PsiNamedElement) {
    PsiElement el;
    for (el=element.getFirstChild(); el != null && !(el instanceof PsiModifierList); el=el.getNextSibling())     ;
    if (el != null)     elementOffset=el.getTextOffset();
  }
  return (elementOffset >= start) && (end >= elementOffset + element.getTextLength());
}
