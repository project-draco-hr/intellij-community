{
  assertPointer(id);
  try {
    if (id >= pointers.size) {
      return ArrayUtil.EMPTY_INT_ARRAY;
    }
    int arrayBase=pointers.get(id);
    IntArray array=arrayBase == 0 ? EMPTY : new IntArray(data,arrayBase);
    return array.toArray();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
