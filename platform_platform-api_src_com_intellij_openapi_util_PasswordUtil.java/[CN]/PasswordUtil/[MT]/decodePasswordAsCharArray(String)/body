{
  if (password == null) {
    return ArrayUtil.EMPTY_CHAR_ARRAY;
  }
  char[] result=new char[password.length()];
  for (int i=0; i < password.length(); i+=4) {
    int c=Integer.parseInt(password.substring(i,i + 4),16);
    c^=0xdfaa;
    result[i]=new Character((char)c).charValue();
  }
  return result;
}
