{
  if (myIndicator != null) {
    if (paths != null && (!paths.isEmpty())) {
      for (      FilePath path : paths) {
        checkName(path.getPath());
      }
      myIndicator.setFraction(myCnt / myTotal);
      myIndicator.setText2(VcsBundle.message("rolling.back.file",paths.get(0).getPath()));
    }
  }
}
