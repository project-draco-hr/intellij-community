{
  List<VcsException> exceptions=new ArrayList<VcsException>();
  List<File> files=ChangesUtil.filePathsToFiles(filePaths);
  for (  File file : files) {
    try {
      mySvnVcs.getFactory(file).createDeleteClient().delete(file,true,false);
    }
 catch (    VcsException e) {
      exceptions.add(e);
    }
  }
  return exceptions;
}
