{
  String path="/authorizations";
  GithubAuthorizationRequest request=new GithubAuthorizationRequest(new ArrayList<String>(scopes),note,null);
  GithubAuthorization response=GithubAuthorization.create(fromJson(postRequest(auth,path,gson.toJson(request)),GithubAuthorizationRaw.class));
  return response.getToken();
}
