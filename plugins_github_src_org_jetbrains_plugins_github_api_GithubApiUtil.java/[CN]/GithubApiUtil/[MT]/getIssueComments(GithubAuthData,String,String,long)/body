{
  String path="/repos/" + user + "/"+ repo+ "/issues/"+ id+ "/comments?per_page=100";
  PagedRequest<GithubIssueComment,GithubIssueCommentRaw> request=new PagedRequest<GithubIssueComment,GithubIssueCommentRaw>(auth,path,GithubIssueComment.class,GithubIssueCommentRaw[].class);
  return request.getAll();
}
