{
  String message=null;
  try {
    String resp=method.getResponseBodyAsString();
    if (resp != null) {
      GithubErrorMessage error=fromJson(parseResponse(resp),GithubErrorMessage.class);
      message=error.getMessage();
    }
  }
 catch (  IOException e) {
    message=null;
  }
  if (message != null) {
    return message;
  }
 else {
    return method.getStatusText();
  }
}
