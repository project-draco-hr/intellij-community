{
  String message=null;
  try {
    InputStream resp=method.getResponseBodyAsStream();
    if (resp != null) {
      GithubErrorMessageRaw error=fromJson(parseResponse(resp),GithubErrorMessageRaw.class);
      message=error.getMessage();
    }
  }
 catch (  IOException e) {
    message=null;
  }
  if (message != null) {
    return message;
  }
 else {
    return method.getStatusText();
  }
}
