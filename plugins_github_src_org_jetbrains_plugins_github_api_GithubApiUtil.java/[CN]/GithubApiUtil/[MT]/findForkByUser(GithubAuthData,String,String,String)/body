{
  String path="/repos/" + user + "/"+ repo+ "/forks?per_page=100";
  PagedRequest<GithubRepo,GithubRepoRaw> request=new PagedRequest<GithubRepo,GithubRepoRaw>(auth,path,GithubRepo.class,GithubRepoRaw[].class);
  while (request.hasNext()) {
    for (    GithubRepo fork : request.next()) {
      if (StringUtil.equalsIgnoreCase(fork.getUserName(),forkUser)) {
        return fork;
      }
    }
  }
  return null;
}
