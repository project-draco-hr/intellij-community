{
  final String name=node.getName();
  if (name == null)   return;
  if (!MIXEDCASE_REGEX.matcher(name).matches()) {
    final ASTNode nameNode=node.getNameNode();
    if (nameNode != null) {
      registerAndAddRenameQuickFix(nameNode.getPsi(),"Class names should use CamelCase convention");
    }
  }
}
