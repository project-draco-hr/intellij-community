{
  List<LocalChangeList> changeLists=myChangeListManager.getChangeListsCopy();
  assertTrue("Didn't find changelist with name '" + name + "' among :"+ changeLists,ContainerUtil.exists(changeLists,new Condition<LocalChangeList>(){
    @Override public boolean value(    LocalChangeList list){
      return list.getName().equals(virtualCommits.replaceVirtualHashes(name));
    }
  }
));
}
