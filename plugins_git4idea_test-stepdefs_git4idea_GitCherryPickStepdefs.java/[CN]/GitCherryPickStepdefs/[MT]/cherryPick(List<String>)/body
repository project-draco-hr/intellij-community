{
  List<VcsFullCommitDetails> commits=ContainerUtil.newArrayList();
  for (  String virtualHash : virtualHashes) {
    commits.add(createMockCommit(virtualHash));
  }
  new GitCherryPicker(myProject,myGit,myPlatformFacade,mySettings.isAutoCommitOnCherryPick()).cherryPick(Collections.singletonMap(myRepository,commits));
}
