{
  return ContainerUtil.mapNotNull(myLogDataHolder.getTopCommitDetails(),new Function<VcsFullCommitDetails,Pair<VcsFullCommitDetails,VirtualFile>>(){
    @Override public Pair<VcsFullCommitDetails,VirtualFile> fun(    final VcsFullCommitDetails details){
      boolean allFilterMatch=!ContainerUtil.exists(detailsFilters,new Condition<VcsLogDetailsFilter>(){
        @Override public boolean value(        VcsLogDetailsFilter filter){
          return !filter.matches(details);
        }
      }
);
      Node node=graphModel.getNodeIfVisible(details.getHash());
      if (node == null || !allFilterMatch) {
        return null;
      }
      return Pair.create(details,node.getBranch().getRepositoryRoot());
    }
  }
);
}
