{
  if (bounds.x == -bounds.width || bounds.x == getWidth()) {
    if (bounds.y + bounds.height + bounds.y == getHeight()) {
      return true;
    }
  }
  if (bounds.y == -bounds.height || bounds.y == getHeight()) {
    if (bounds.x + bounds.width + bounds.x == getWidth()) {
      return true;
    }
  }
  return false;
}
