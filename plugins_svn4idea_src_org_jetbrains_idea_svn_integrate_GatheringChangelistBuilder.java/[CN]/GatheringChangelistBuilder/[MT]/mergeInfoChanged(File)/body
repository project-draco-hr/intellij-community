{
  SvnTarget target=SvnTarget.fromFile(file);
  try {
    SVNPropertyData current=myVcs.getFactory(target).createPropertyClient().getProperty(target,SvnPropertyKeys.MERGE_INFO,false,SVNRevision.WORKING);
    SVNPropertyData base=myVcs.getFactory(target).createPropertyClient().getProperty(target,SvnPropertyKeys.MERGE_INFO,false,SVNRevision.BASE);
    if (current != null) {
      return base == null || !Comparing.equal(current.getValue(),base.getValue());
    }
  }
 catch (  VcsException e) {
    LOG.info(e);
  }
  return false;
}
