{
  String result=null;
  for (int i=0; i < endElement; i++) {
    String arg=evaluate(args[i],containingFilePath,visited);
    if (arg == null) {
      return null;
    }
    if (result == null) {
      result=arg;
    }
 else {
      result=new File(result,arg).getPath();
    }
  }
  return result;
}
