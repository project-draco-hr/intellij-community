{
  Element element;
  if (state instanceof Element) {
    element=(Element)state;
  }
 else {
    try {
      element=unarchiveState((byte[])state);
    }
 catch (    IOException e) {
      LOG.error(e);
      return "internal error";
    }
catch (    JDOMException e) {
      LOG.error(e);
      return "internal error";
    }
  }
  return JDOMUtil.writeParent(element,"\n");
}
