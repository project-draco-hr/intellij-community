{
  try {
switch (Integer.parseInt(state.getId())) {
case 4:
      return "{\"transition\": \"4\"}";
case 5:
    return "{\"transition\": \"5\", \"resolution\": \"Fixed\"}";
case 3:
  return "{\"transition\": \"3\"}";
}
}
 catch (NumberFormatException ignored) {
}
LOG.error("Unknown ID of predefined issue state: " + state.getId());
return null;
}
