{
  final TaskState taskState=state.asPredefined();
  if (taskState != null) {
switch (taskState) {
case IN_PROGRESS:
      return "{\"transition\": \"4\"}";
case RESOLVED:
    return "{\"transition\": \"5\", \"resolution\": \"Fixed\"}";
case REOPENED:
  return "{\"transition\": \"3\"}";
default :
return null;
}
}
return null;
}
