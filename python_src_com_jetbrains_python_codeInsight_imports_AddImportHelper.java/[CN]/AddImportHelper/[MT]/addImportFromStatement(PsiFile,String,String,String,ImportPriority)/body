{
  String as_clause;
  if (asName == null) {
    as_clause="";
  }
 else {
    as_clause=" as " + asName;
  }
  final PyFromImportStatement importNodeToInsert=PyElementGenerator.getInstance(file.getProject()).createFromText(LanguageLevel.getDefault(),PyFromImportStatement.class,"from " + from + " import "+ name+ as_clause);
  try {
    file.addBefore(importNodeToInsert,getInsertPosition(file,from,priority));
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
