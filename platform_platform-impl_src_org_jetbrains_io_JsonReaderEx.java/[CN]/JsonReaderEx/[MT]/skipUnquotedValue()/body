{
  int i=position;
  for (; i < limit; i++) {
switch (in.charAt(i)) {
case '/':
case '\\':
case ';':
case '#':
case '=':
      checkLenient();
case '{':
case '}':
case '[':
case ']':
case ':':
case ',':
case ' ':
case '\t':
case '\f':
case '\r':
case '\n':
    position=i;
  return;
}
}
position=i;
}
