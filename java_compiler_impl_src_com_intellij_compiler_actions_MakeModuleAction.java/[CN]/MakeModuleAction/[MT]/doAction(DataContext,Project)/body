{
  Module[] modules=LangDataKeys.MODULE_CONTEXT_ARRAY.getData(dataContext);
  Module module;
  if (modules == null) {
    module=LangDataKeys.MODULE.getData(dataContext);
    if (module == null) {
      return;
    }
    modules=new Module[]{module};
  }
  try {
    ActivityManager.getInstance(project).buildDirty(modules);
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
