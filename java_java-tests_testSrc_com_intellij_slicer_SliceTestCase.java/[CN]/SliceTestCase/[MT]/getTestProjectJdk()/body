{
  Sdk sdk=IdeaTestUtil.getMockJdk17();
  SdkModificator sdkModificator=sdk.getSdkModificator();
  VirtualFile root=LocalFileSystem.getInstance().findFileByPath(FileUtil.toSystemIndependentName(PlatformTestUtil.getCommunityPath()) + "/java/jdkAnnotations");
  assertNotNull(root);
  sdkModificator.addRoot(root,AnnotationOrderRootType.getInstance());
  sdkModificator.commitChanges();
  return sdk;
}
