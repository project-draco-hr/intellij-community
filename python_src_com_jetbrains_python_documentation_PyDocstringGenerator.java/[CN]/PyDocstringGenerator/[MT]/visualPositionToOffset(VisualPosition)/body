{
  final String text=myDocStringExpression.getText();
  int offset=offsetOfLineFeed(text,pos.getLine());
  if (offset < 0) {
    return -1;
  }
  offset++;
  while (offset < text.length() && text.charAt(offset) == ' ') {
    offset++;
  }
  if (offset == text.length()) {
    return -1;
  }
  return offset + pos.getColumn();
}
