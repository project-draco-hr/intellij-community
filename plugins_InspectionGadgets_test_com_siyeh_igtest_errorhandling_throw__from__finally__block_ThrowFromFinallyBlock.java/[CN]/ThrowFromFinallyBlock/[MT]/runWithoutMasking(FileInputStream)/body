{
  IOException myException=null;
  try {
    a.read();
  }
 catch (  IOException e) {
    myException=e;
    throw e;
  }
 finally {
    if (myException != null) {
      try {
        a.close();
      }
 catch (      Throwable t) {
        myException.addSuppressed(t);
      }
    }
 else {
      a.close();
    }
  }
}
