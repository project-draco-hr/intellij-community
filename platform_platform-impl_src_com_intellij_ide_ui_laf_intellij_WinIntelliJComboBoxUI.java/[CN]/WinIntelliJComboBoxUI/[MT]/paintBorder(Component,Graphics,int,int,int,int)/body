{
  if (comboBox == null || arrowButton == null) {
    return;
  }
  hasFocus=false;
  checkFocus();
  if (hasFocus) {
    g2.setColor(UIManager.getColor("ComboBox.activeBorderColor"));
  }
 else {
    g2.setColor(UIManager.getColor("ComboBox.borderColor"));
  }
  g2.drawRect(x + 1,y + 1,width - 2,height - 2);
}
