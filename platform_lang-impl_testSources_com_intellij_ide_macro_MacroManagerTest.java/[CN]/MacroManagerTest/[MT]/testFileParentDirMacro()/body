{
  doTest("foo/bar/baz/test.txt","ans: $FileParentDir(bar)$ ","ans: " + myFixture.getTempDirPath() + File.separator+ "foo ");
  doTest("foo/bar/baz/test2.txt","ans: $FileDirPathFromParent(bar)$ ","ans: baz" + File.separator + " ");
  doTest("foo/bar/baz/test3.txt","ans: $FileDirPathFromParent(foo/bar)$ ","ans: baz" + File.separator + " ");
  doTest("foo/bar/baz/test4.txt","ans: $FileDirPathFromParent(foo/bar/baz/)$ ","ans:  ");
}
