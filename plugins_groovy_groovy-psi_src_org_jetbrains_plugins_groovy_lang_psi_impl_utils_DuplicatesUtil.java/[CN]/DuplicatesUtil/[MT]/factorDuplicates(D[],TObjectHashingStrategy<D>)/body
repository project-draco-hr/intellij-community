{
  if (elements == null || elements.length == 0)   return Collections.emptyMap();
  THashMap<D,List<D>> map=new THashMap<>(strategy);
  for (  D element : elements) {
    List<D> list=map.get(element);
    if (list == null) {
      list=new ArrayList<>();
    }
    list.add(element);
    map.put(element,list);
  }
  return map;
}
