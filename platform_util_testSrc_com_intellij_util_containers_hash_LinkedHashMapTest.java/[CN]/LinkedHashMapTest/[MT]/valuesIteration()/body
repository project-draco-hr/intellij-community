{
  Map<Integer,String> map=new LinkedHashMap<>();
  map.put(1,"a");
  map.put(2,"b");
  map.put(3,"c");
  Iterator<String> iterator=map.values().iterator();
  assertEquals("a",iterator.next());
  assertEquals("b",iterator.next());
  assertEquals("c",iterator.next());
  assertFalse(iterator.hasNext());
}
