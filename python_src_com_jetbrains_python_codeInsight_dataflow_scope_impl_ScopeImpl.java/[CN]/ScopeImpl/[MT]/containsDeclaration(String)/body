{
  if (myNamedElements == null || myNameDefiners == null) {
    collectDeclarations();
  }
  if (isNonlocal(name)) {
    return false;
  }
  if (getNamedElement(name) != null) {
    return true;
  }
  for (  NameDefiner definer : getNameDefiners()) {
    if (definer.getElementNamed(name) != null) {
      return true;
    }
  }
  return false;
}
