{
  SVNAuthentication authentication=myAuthCallback.requestCredentials(myExecutor.getCommand().getRepositoryUrl(),ISVNAuthenticationManager.SSH);
  if (authentication != null && authentication instanceof SVNSSHAuthentication) {
    try {
      myExecutor.write(String.format("%s\n",((SVNSSHAuthentication)authentication).getPassphrase()));
      return true;
    }
 catch (    SvnBindException e) {
      LOG.info(e);
    }
  }
  return false;
}
