{
  final SVNURL repositoryUrl=myExecutor.getCommand().getRepositoryUrl();
  String auth=myRuntime.getAuthenticationService().requestSshCredentials(repositoryUrl != null ? repositoryUrl.toDecodedString() : "",mode,key);
  if (!StringUtil.isEmpty(auth)) {
    sendData(auth);
  }
 else {
    cancelAuthentication();
  }
  return !StringUtil.isEmpty(auth);
}
