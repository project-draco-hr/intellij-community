{
  HttpClientBuilder builder=HttpClients.custom();
  builder.setDefaultRequestConfig(createRequestConfig()).setDefaultConnectionConfig(createConnectionConfig()).setDefaultHeaders(createHeaders()).setSslcontext(CertificateManager.getInstance().getSslContext()).setHostnameVerifier((X509HostnameVerifier)CertificateManager.HOSTNAME_VERIFIER);
  setupCredentialsProvider(builder);
  return builder.build();
}
