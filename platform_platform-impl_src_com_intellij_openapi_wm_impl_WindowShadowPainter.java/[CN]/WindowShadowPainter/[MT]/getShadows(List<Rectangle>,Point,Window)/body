{
  if (windows != null) {
    for (    Window window : windows) {
      Rectangle bounds=getShadowBounds(point,window);
      if (bounds != null) {
        if (list == null)         list=newArrayList();
        list.add(bounds);
      }
      list=getShadows(list,point,window.getOwnedWindows());
    }
  }
  return list;
}
