{
  final PsiMethod method=(PsiMethod)infos[0];
  final int booleanParameterCount=((Integer)infos[1]).intValue();
  if (booleanParameterCount == 1) {
    return method.isConstructor() ? InspectionGadgetsBundle.message("boolean.parameter.constructor.problem.descriptor") : InspectionGadgetsBundle.message("boolean.parameter.problem.descriptor");
  }
 else {
    return method.isConstructor() ? InspectionGadgetsBundle.message("boolean.parameters.constructor.problem.descriptor") : InspectionGadgetsBundle.message("boolean.parameters.problem.descriptor");
  }
}
