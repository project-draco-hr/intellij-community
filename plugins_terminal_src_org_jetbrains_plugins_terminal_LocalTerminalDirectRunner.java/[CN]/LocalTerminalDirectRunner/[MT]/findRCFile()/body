{
  try {
    final String folder=PtyUtil.getPtyLibFolderPath();
    if (folder != null) {
      File rcFile=new File(folder,"jediterm.in");
      if (rcFile.exists()) {
        return rcFile;
      }
    }
  }
 catch (  Exception e) {
    LOG.warn("Unable to get jar folder",e);
  }
  return null;
}
