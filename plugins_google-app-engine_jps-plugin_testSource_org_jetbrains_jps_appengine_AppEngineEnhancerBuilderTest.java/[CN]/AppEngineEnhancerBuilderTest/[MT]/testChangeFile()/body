{
  String file=createFile("src/A.java","class A{}");
  addAppEngineModule("a",true,PathUtil.getParentPath(file));
  buildAllModules();
  assertEnhanced("out/production/a/A.class");
  buildAllModules();
  assertEnhanced();
  change(file);
  buildAllModules();
  assertEnhanced("out/production/a/A.class");
}
