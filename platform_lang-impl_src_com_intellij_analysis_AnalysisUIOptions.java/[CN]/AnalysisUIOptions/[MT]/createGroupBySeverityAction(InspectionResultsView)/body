{
  return new InspectionResultsViewToggleAction(view,InspectionsBundle.message("inspection.action.group.by.severity"),InspectionsBundle.message("inspection.action.group.by.severity.description"),AllIcons.Nodes.SortBySeverity){
    @Override public boolean isSelected(    AnActionEvent e){
      return GROUP_BY_SEVERITY;
    }
    @Override protected void setSelected(    boolean state){
      GROUP_BY_SEVERITY=state;
    }
  }
;
}
