{
  if (AnnotationUtil.isAnnotated(aClass,annotations,false)) {
    return true;
  }
  PsiModifierList modifierList=aClass.getModifierList();
  if (modifierList != null) {
    for (    PsiAnnotation psiAnnotation : modifierList.getApplicableAnnotations()) {
      PsiJavaCodeReferenceElement nameReferenceElement=psiAnnotation.getNameReferenceElement();
      if (nameReferenceElement != null) {
        PsiElement resolve=nameReferenceElement.resolve();
        if (resolve instanceof PsiClass && visited.add((PsiClass)resolve) && ((PsiClass)resolve).isAnnotationType()&& isTestAnnotatedAnnotation((PsiClass)resolve,visited,annotations)) {
          return true;
        }
      }
    }
  }
  return false;
}
