{
  XValue container=node.getValueContainer();
  if (container instanceof JavaValue) {
    JavaValue javaValue=((JavaValue)container);
    XDebuggerTree tree=XDebuggerTree.getTree(e.getDataContext());
    XInspectDialog dialog=new XInspectDialog(tree.getProject(),tree.getEditorsProvider(),tree.getSourcePosition(),nodeName,new ReferringObjectsValue(javaValue,false),tree.getValueMarkers());
    dialog.setTitle(DebuggerBundle.message("showReferring.dialog.title",nodeName));
    dialog.show();
  }
}
