{
  if (node.getValueContainer() instanceof JavaValue) {
    JavaValue javaValue=((JavaValue)node.getValueContainer());
    XDebuggerTree tree=XDebuggerTree.getTree(e.getDataContext());
    XInspectDialog dialog=new XInspectDialog(tree.getProject(),tree.getEditorsProvider(),tree.getSourcePosition(),nodeName,new ReferringObjectsValue(javaValue),tree.getValueMarkers());
    dialog.setTitle("Referring objects for " + nodeName);
    dialog.show();
  }
}
