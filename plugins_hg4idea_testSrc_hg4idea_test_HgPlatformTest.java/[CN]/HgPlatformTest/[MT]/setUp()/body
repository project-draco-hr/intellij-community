{
  super.setUp();
  cd(myProjectRoot);
  hg("version");
  createRepository(myProjectRoot);
  myVcs=HgVcs.getInstance(myProject);
  assertNotNull(myVcs);
  myVcs.getGlobalSettings().setHgExecutable(HgExecutor.getHgExecutable());
  myVcs.checkVersion();
  myRepository=myProjectRoot;
  setUpHgrc(myRepository);
}
