{
  AddWildcardBreakpointDialog dialog=new AddWildcardBreakpointDialog(project);
  dialog.show();
  if (!dialog.isOK()) {
    return null;
  }
  AccessToken token=WriteAction.start();
  try {
    return XDebuggerManager.getInstance(project).getBreakpointManager().addBreakpoint(this,new JavaMethodBreakpointProperties(dialog.getClassPattern(),dialog.getMethodName()));
  }
  finally {
    token.finish();
  }
}
