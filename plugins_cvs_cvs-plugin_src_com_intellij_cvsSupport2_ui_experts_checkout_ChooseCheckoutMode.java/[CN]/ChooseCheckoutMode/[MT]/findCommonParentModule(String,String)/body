{
  int diff=indexOfFirstDifference(module1,module2);
  if (diff == 0) {
    return null;
  }
  if (diff == module1.length()) {
    return module1;
  }
  if (module1.charAt(diff - 1) == '/') {
    return module1.substring(0,diff);
  }
 else {
    int index=module1.lastIndexOf('/',diff);
    if (index < 0) {
      return null;
    }
    return module1.substring(0,index);
  }
}
