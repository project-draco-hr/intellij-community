{
  return new RelatedItemLineMarkerInfo<PsiElement>(element,element.getTextRange(),icon,Pass.UPDATE_OVERRIDEN_MARKERS,new ConstantFunction<PsiElement,String>(tooltip),new DefaultGutterIconNavigationHandler<PsiElement>(targets,popupTitle),GutterIconRenderer.Alignment.RIGHT,GotoRelatedItem.createItems(targets,"DevKit"));
}
