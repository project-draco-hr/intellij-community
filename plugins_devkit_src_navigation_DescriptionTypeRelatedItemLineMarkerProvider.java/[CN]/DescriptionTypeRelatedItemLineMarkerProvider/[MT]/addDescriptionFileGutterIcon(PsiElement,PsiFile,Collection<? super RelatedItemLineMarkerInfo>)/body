{
  final RelatedItemLineMarkerInfo<PsiElement> info=NavigationGutterIconBuilder.create(AllIcons.FileTypes.Html,CONVERTER,RELATED_ITEM_PROVIDER).setTarget(descriptionFile).setTooltipText("Description").setAlignment(GutterIconRenderer.Alignment.RIGHT).createLineMarkerInfo(highlightingElement);
  result.add(info);
}
