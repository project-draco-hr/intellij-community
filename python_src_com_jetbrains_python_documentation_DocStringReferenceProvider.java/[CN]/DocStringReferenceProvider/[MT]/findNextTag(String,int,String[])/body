{
  int result=Integer.MAX_VALUE;
  String foundTag=null;
  for (  String paramTag : paramTags) {
    int tagPos=docString.indexOf(paramTag + " ",pos);
    if (tagPos >= 0 && tagPos < result) {
      foundTag=paramTag;
      result=tagPos;
    }
  }
  return foundTag == null ? -1 : result + foundTag.length();
}
