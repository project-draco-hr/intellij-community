{
  if (from != null) {
    for (    final VirtualFile vf : from) {
      ApplicationManager.getApplication().runReadAction(new Runnable(){
        @Override public void run(){
          final AbstractVcs vcs=myGuess.getVcsForDirty(vf);
          if (vcs != null) {
            to.add(new VcsRoot(vcs,vf));
          }
        }
      }
);
    }
  }
}
