{
  if (from != null) {
    for (    final FilePath fp : from) {
      ApplicationManager.getApplication().runReadAction(new Runnable(){
        @Override public void run(){
          final AbstractVcs vcs=myGuess.getVcsForDirty(fp);
          if (vcs != null) {
            to.add(new FilePathUnderVcs(fp,vcs));
          }
        }
      }
);
    }
  }
}
