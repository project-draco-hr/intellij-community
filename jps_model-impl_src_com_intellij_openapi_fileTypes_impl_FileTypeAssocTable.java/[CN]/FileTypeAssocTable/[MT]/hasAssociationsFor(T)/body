{
  if (myExtensionMappings.values().contains(fileType) || myExactFileNameMappings.values().contains(fileType) || myExactFileNameAnyCaseMappings.values().contains(fileType)) {
    return true;
  }
  for (  Pair<FileNameMatcher,T> mapping : myMatchingMappings) {
    if (mapping.getSecond() == fileType) {
      return true;
    }
  }
  return false;
}
