{
  doTest("","X",new TextRange(0,1));
  doTest("a B c","a X c",new TextRange(2,3));
  doTest("while()","while ()",new TextRange(5,6));
  doTest("else {","else\n    {",new TextRange(4,8));
  doTest("while ()","while()",new TextRange(5,5));
  doTest("if\n  then","if\n\n  then",new TextRange(3,4));
}
