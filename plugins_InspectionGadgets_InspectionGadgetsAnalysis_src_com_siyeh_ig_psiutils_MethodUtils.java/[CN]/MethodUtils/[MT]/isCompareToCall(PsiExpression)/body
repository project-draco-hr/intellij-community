{
  if (!(expression instanceof PsiMethodCallExpression)) {
    return false;
  }
  final PsiMethodCallExpression methodCallExpression=(PsiMethodCallExpression)expression;
  if (methodCallExpression.getMethodExpression().getQualifierExpression() == null || !HardcodedMethodConstants.COMPARE_TO.equals(methodCallExpression.getMethodExpression().getReferenceName())) {
    return false;
  }
  final PsiMethod psiMethod=methodCallExpression.resolveMethod();
  return isCompareTo(psiMethod);
}
