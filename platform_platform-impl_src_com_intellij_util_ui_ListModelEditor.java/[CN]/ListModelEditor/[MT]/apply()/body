{
  final List<T> items=getItems();
  if (!helper.hasModifiedItems()) {
    return items;
  }
  helper.process(new TObjectObjectProcedure<T,T>(){
    @Override public boolean execute(    T newItem,    T oldItem){
      XmlSerializerUtil.copyBean(newItem,oldItem);
      model.setElementAt(oldItem,ContainerUtil.indexOfIdentity(items,newItem));
      return true;
    }
  }
);
  helper.clear();
  return getItems();
}
