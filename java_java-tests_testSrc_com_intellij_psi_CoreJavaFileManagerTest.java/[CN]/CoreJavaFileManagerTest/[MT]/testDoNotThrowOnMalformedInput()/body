{
  CoreJavaFileManager fileWithEmptyName=configureManager("package foo;\n\n public class Top$Level {}","");
  assertCannotFind(fileWithEmptyName,"foo.");
  assertCannotFind(fileWithEmptyName,".");
  assertCannotFind(fileWithEmptyName,"..");
  assertCannotFind(fileWithEmptyName,"");
  assertCannotFind(fileWithEmptyName,".foo");
}
