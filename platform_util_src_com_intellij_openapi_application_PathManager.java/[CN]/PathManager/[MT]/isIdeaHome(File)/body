{
  return new File(root,FileUtil.toSystemDependentName("bin/" + PROPERTIES_FILE_NAME)).exists() || new File(root,FileUtil.toSystemDependentName("bin/" + getOSSpecificBinSubdir() + "/"+ PROPERTIES_FILE_NAME)).exists() || new File(root,FileUtil.toSystemDependentName("community/bin/" + PROPERTIES_FILE_NAME)).exists();
}
