{
  if (!SCHEME_NODE_NAME.equals(element.getName())) {
    throw new InvalidDataException();
  }
  final EditorColorsSchemeImpl scheme=isEditable ? new EditorColorsSchemeImpl(null,DefaultColorSchemesManager.getInstance()) : new ReadOnlyColorsSchemeImpl(null,DefaultColorSchemesManager.getInstance());
  scheme.readExternal(element);
  return scheme;
}
