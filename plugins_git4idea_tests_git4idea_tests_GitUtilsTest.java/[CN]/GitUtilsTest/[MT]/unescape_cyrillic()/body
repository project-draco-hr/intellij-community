{
  final String CHARSET=" Default charset: " + GitConfigUtil.getFileNameEncoding();
  assertEquals("Cyrillic folder was unescaped incorrectly." + CHARSET,"??????????/file.txt",unescapePath("\\320\\277\\320\\260\\320\\277\\320\\272\\320\\260/file.txt"));
  assertEquals("Cyrillic folder with file name were unescaped incorrectly." + CHARSET,"??????????/????????????????",unescapePath("\\320\\277\\320\\260\\320\\277\\320\\272\\320\\260/\\320\\264\\320\\276\\320\\272\\321\\203\\320\\274\\320\\265\\320\\275\\321\\202"));
}
