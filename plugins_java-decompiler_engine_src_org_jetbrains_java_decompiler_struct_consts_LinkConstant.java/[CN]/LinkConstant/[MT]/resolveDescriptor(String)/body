{
  String[] arr=descr.split("[()]");
  String par=arr[1];
  int index=0, counter=0;
  int len=par.length();
  while (index < len) {
    char c=par.charAt(index);
    if (c == 'L') {
      index=par.indexOf(";",index);
    }
 else     if (c == '[') {
      index++;
      continue;
    }
    counter++;
    index++;
  }
  paramCount=counter;
  isVoid="V".equals(arr[2]);
  returnCategory2=("D".equals(arr[2]) || "J".equals(arr[2]));
}
