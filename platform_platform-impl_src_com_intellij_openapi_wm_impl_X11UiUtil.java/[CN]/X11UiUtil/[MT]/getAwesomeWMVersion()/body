{
  try {
    String version=ExecUtil.execAndReadLine("awesome","--version");
    if (version != null) {
      Matcher m=Pattern.compile("awesome v([0-9.]+)").matcher(version);
      if (m.find()) {
        return m.group(1);
      }
    }
  }
 catch (  Throwable t) {
    LOG.warn(t);
  }
  return null;
}
