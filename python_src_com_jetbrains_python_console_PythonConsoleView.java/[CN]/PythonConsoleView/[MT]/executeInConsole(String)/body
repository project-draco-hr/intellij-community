{
  UIUtil.invokeLaterIfNeeded(new Runnable(){
    @Override public void run(){
      String text=getConsoleEditor().getDocument().getText();
      setInputText(code);
      myExecuteActionHandler.runExecuteAction(PythonConsoleView.this);
      if (!StringUtil.isEmpty(text)) {
        setInputText(text);
      }
    }
  }
);
}
