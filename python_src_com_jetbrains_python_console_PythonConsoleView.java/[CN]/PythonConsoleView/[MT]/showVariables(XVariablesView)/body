{
  removeAll();
  view.getPanel().setPreferredSize(new Dimension(150,500));
  JSplitPane p=new JSplitPane(JSplitPane.HORIZONTAL_SPLIT);
  p.add(myLanguageConsoleView.getComponent(),JSplitPane.LEFT);
  p.add(view.getPanel(),JSplitPane.RIGHT);
