{
  final List<String> ownSlots=cls.getOwnSlots();
  if (ownSlots == null || ownSlots.contains(PyNames.DICT)) {
    return true;
  }
  if (!cls.equals(declaringClass) || !ownSlots.contains(targetName)) {
    return false;
  }
  return LanguageLevel.forElement(declaringClass).isAtLeast(LanguageLevel.PYTHON30) || declaringClass.findClassAttribute(targetName,false,myTypeEvalContext) == null;
}
