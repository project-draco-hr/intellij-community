{
  final Set<String> signatures=new LinkedHashSet<String>();
  signatures.add(formatMethodSignature(method,true,java8FormatFirst));
  signatures.add(formatMethodSignature(method,false,java8FormatFirst));
  signatures.add(formatMethodSignature(method,true,!java8FormatFirst));
  signatures.add(formatMethodSignature(method,false,!java8FormatFirst));
  return signatures;
}
