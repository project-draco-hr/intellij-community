{
  if (!myCourses.isEmpty()) {
    return myCourses;
  }
  if (myCoursesDir.exists()) {
    File cacheFile=new File(myCoursesDir,CACHE_NAME);
    if (cacheFile.exists()) {
      myCourses=getCoursesFromCache(cacheFile);
      if (!myCourses.isEmpty()) {
        return myCourses;
      }
    }
    myCourses=loadCourses();
    if (!myCourses.isEmpty()) {
      return myCourses;
    }
  }
  downloadAndUnzip(false);
  myCourses=loadCourses();
  flushCache();
  return myCourses;
}
