{
  final String name=stub.getName();
  if (name != null) {
    sink.occurrence(PyClassNameIndex.KEY,name);
  }
  for (  PyQualifiedName s : stub.getSuperClasses()) {
    String className=s.getLastComponent();
    if (className != null) {
      sink.occurrence(PySuperClassIndex.KEY,className);
    }
  }
}
