{
  if (virtualFile != null && virtualFile.isValid() && project.isInitialized() && myProjectFileIndex.isContentSourceFile(virtualFile) && (virtualFile.isDirectory() || virtualFile.getFileType() == StdFileTypes.JAVA)) {
    return true;
  }
  if (virtualFile instanceof VirtualFileWithId) {
    int id=getAbsId(virtualFile);
    fileIsResolved.set(id);
  }
  return false;
}
