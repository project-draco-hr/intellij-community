{
  int indentBefore=myCurrentIndentSize;
  if (text.isEmpty()) {
    processOneLine(text);
  }
 else {
    Scanner s=new Scanner(text);
    while (s.hasNextLine()) {
      String line=s.nextLine();
      processOneLine(line);
    }
  }
  if (execAnyway && myCurrentIndentSize > 0 && indentBefore == 0) {
    finishExecution();
  }
}
