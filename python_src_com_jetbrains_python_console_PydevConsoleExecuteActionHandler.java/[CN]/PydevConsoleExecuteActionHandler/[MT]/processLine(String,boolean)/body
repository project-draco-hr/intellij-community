{
  int indentBefore=myCurrentIndentSize;
  if (text.isEmpty()) {
    processOneLine(text);
  }
 else {
    if (StringUtil.countNewLines(text.trim()) > 0) {
      executeMultiLine(text);
    }
 else {
      processOneLine(text);
    }
  }
  if (execAnyway && myCurrentIndentSize > 0 && indentBefore == 0) {
    finishExecution();
  }
}
