{
  Scanner s=new Scanner(text);
  while (s.hasNextLine()) {
    String line=s.nextLine();
    int indentSize=myIndentHelper.getIndent(line,false);
    if (indentSize < myCurrentIndentSize && !shouldIndent(line)) {
      doProcessLine("\n");
      doProcessLine(line);
    }
 else {
      doProcessLine(line);
    }
  }
}
