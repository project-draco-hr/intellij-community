{
  visited.add(aClass);
  if (myClasses == null) {
    myClasses=new HashSet<PyClass>();
  }
  for (  PyClass clazz : aClass.getSuperClasses()) {
    getSubclasses(clazz).add(aClass);
    myClasses.add(clazz);
    if (!visited.contains(clazz)) {
      buildSubClassesMapImpl(clazz,visited);
    }
  }
}
