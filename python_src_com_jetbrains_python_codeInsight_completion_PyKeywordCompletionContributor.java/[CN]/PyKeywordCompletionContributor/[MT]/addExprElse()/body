{
  extend(CompletionType.BASIC,psiElement().withLanguage(PythonLanguage.getInstance()).afterLeafSkipping(psiElement().whitespace(),psiElement().inside(psiElement(PyConditionalExpression.class)).and(psiElement().afterLeaf(PyKeywords.IF))),new PyKeywordCompletionProvider(TailType.SPACE,PyKeywords.ELSE));
}
