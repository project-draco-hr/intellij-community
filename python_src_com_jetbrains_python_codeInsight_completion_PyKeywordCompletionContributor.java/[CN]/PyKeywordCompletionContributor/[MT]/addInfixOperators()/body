{
  extend(CompletionType.BASIC,psiElement().withLanguage(PythonLanguage.getInstance()).andNot(IN_COMMENT).andNot(BEFORE_COND).andNot(IN_IMPORT_STMT).andNot(IN_PARAM_LIST).andNot(AFTER_QUALIFIER).andNot(IN_STRING_LITERAL).and(IN_BEGIN_STMT),new PyKeywordCompletionProvider(PyNames.AND,PyNames.OR,PyNames.IS,PyNames.IN));
}
