{
  extend(CompletionType.BASIC,psiElement().withLanguage(PythonLanguage.getInstance()).and(FIRST_ON_LINE).andOr(IN_COND_STMT,IN_EXCEPT_BODY,AFTER_COND_STMT_NO_ELSE,AFTER_LOOP_NO_ELSE,AFTER_EXCEPT).andNot(AFTER_QUALIFIER).andNot(IN_STRING_LITERAL),new PyKeywordCompletionProvider(TailType.CASE_COLON,PyUnindentingInsertHandler.INSTANCE,PyNames.ELSE));
}
