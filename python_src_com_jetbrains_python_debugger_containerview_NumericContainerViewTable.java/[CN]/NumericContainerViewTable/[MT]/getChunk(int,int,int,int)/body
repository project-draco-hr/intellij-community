{
  final PyDebugValue slicedValue=new PyDebugValue(getSliceText(),myValue.getType(),myValue.getTypeQualifier(),myValue.getValue(),myValue.isContainer(),myValue.isErrorOnEval(),myValue.isReturnedVal(),myValue.getParent(),myValue.getFrameAccessor());
  final String format=getFormat().isEmpty() ? "%" : getFormat();
  return myValue.getFrameAccessor().getArrayItems(slicedValue,rowOffset,colOffset,rows,cols,format);
}
