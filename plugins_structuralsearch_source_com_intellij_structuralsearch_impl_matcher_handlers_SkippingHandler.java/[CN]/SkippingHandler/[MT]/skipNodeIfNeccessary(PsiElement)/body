{
  if (element == null) {
    return null;
  }
  if (!canSkip(element) && getOnlyNonWhitespaceChild(element) == null) {
    return element;
  }
  FilteringNodeIterator it=new FilteringNodeIterator(new SiblingNodeIterator(element.getFirstChild()));
  PsiElement child=it.current();
  if (child != null) {
    it.advance();
    if (!it.hasNext()) {
      return child;
    }
  }
  return element;
}
