{
  FilteringNodeIterator it=filter != null ? new FilteringNodeIterator(new SiblingNodeIterator(element.getFirstChild()),filter) : new FilteringNodeIterator(element.getFirstChild());
  PsiElement child=it.current();
  if (child != null) {
    it.advance();
    if (!it.hasNext()) {
      return child;
    }
  }
  return element;
}
