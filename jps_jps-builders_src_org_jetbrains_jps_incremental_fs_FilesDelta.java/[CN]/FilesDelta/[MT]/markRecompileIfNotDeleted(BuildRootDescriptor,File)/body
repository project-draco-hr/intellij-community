{
  lockData();
  try {
    final boolean isMarkedDeleted=!myDeletedPaths.isEmpty() && myDeletedPaths.contains(FileUtil.toCanonicalPath(file.getPath()));
    if (!isMarkedDeleted) {
      _addToRecompiled(root,file);
      return true;
    }
    return false;
  }
  finally {
    unlockData();
  }
}
