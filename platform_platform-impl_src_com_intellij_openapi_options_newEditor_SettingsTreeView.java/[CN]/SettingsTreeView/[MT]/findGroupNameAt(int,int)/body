{
  TreePath path=myTree.getClosestPathForLocation(x - myTree.getX(),y - myTree.getY());
  while (path != null) {
    MyNode node=extractNode(path);
    if (node == null) {
      return null;
    }
    if (myRoot == node.getParent()) {
      return node.myDisplayName;
    }
    path=path.getParentPath();
  }
  return null;
}
