{
  for (  Class cls : ANNOTATOR_CLASSES) {
    PyAnnotator annotator;
    try {
      annotator=(PyAnnotator)cls.newInstance();
    }
 catch (    InstantiationException e) {
      LOGGER.error(e);
      continue;
    }
catch (    IllegalAccessException e) {
      LOGGER.error(e);
      continue;
    }
    myAnnotators.add(annotator);
  }
}
