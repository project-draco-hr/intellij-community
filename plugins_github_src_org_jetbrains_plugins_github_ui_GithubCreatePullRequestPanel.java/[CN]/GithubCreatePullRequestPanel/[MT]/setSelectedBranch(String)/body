{
  if (branch != null) {
    for (    BranchInfo info : myBranchModel.getItems()) {
      if (branch.equals(info.getRemoteName())) {
        myBranchModel.setSelectedItem(info);
        return;
      }
    }
  }
  if (myBranchModel.getSize() > 0)   myBranchModel.setSelectedItem(myBranchModel.get(0));
}
