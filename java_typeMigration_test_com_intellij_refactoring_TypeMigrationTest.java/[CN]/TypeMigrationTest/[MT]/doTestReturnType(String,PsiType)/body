{
  start(new RulesProvider(){
    @Override public PsiType migrationType() throws Exception {
      return migrationType;
    }
    @Override public PsiElement victims(    PsiClass aClass){
      for (      PsiMethod method : PsiTreeUtil.findChildrenOfType(aClass,PsiMethod.class)) {
        if (methodName.equals(method.getName())) {
          return method;
        }
      }
      throw new AssertionError();
    }
  }
);
}
