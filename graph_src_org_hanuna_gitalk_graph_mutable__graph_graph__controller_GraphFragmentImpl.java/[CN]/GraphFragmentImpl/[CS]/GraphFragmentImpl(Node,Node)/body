{
  if (upNode.getDownEdges().size() != 1 || downNode.getUpEdges().size() != 1) {
    throw new IllegalStateException("bad GraphFragment up: " + upNode.getDownEdges().size() + " down: "+ downNode.getUpEdges().size());
  }
  if (downNode.getRowIndex() < upNode.getRowIndex()) {
    throw new IllegalArgumentException("up index: " + upNode.getRowIndex() + " down index:"+ downNode.getRowIndex());
  }
  this.downNode=downNode;
  this.upNode=upNode;
}
