{
  waitForReady();
  myCommandSemaphore.acquire(1);
  UIUtil.invokeAndWaitIfNeeded(new Runnable(){
    @Override public void run(){
      myConsoleView.executeInConsole(command);
    }
  }
);
  Assert.assertTrue(String.format("Command execution wasn't finished: `%s` \n" + "Output: %s",command,output()),waitFor(myCommandSemaphore));
  myCommandSemaphore.release();
}
