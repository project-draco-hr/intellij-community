{
  int count=0;
  while (true) {
    String out=output();
    boolean flag=true;
    for (    String s : string) {
      if (!out.contains(s)) {
        flag=false;
      }
    }
    if (flag) {
      break;
    }
    if (count > 10) {
      Assert.fail("'" + string + "'"+ " is not present in output.\n"+ output());
    }
    Thread.sleep(2000);
    count++;
  }
}
