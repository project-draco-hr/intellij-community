{
  int count=0;
  while (true) {
    List<String> missing=Lists.newArrayList();
    String out=output();
    boolean flag=true;
    for (    String s : string) {
      if (!out.contains(s)) {
        flag=false;
        missing.add(s);
      }
    }
    if (flag) {
      break;
    }
    if (count > 10) {
      Assert.fail("Strings: <--\n" + StringUtil.join(missing,"\n---\n") + "-->"+ "are not present in output.\n"+ output());
    }
    Thread.sleep(2000);
    count++;
  }
}
