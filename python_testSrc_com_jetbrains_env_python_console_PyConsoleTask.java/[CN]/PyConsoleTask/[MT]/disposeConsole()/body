{
  if (myCommunication != null) {
    try {
      myCommunication.close();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    myCommunication=null;
  }
  disposeConsoleProcess();
  if (myConsoleView != null) {
    new WriteAction(){
      protected void run(      Result result) throws Throwable {
        Disposer.dispose(myConsoleView);
        myConsoleView=null;
      }
    }
.execute();
  }
}
