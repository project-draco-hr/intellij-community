{
  super.visitParameter(parameter);
  if (!(parameter.getDeclarationScope() instanceof PsiMethod)) {
    return;
  }
  final PsiTypeElement typeElement=parameter.getTypeElement();
  if (typeElement == null) {
    return;
  }
  if (!ConcreteClassUtil.typeIsConcreteClass(typeElement,ignoreAbstractClasses)) {
    return;
  }
  final String variableName=parameter.getName();
  registerError(typeElement,variableName);
}
