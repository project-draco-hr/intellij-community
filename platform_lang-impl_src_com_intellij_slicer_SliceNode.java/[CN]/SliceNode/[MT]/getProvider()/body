{
  AbstractTreeNode<SliceUsage> element=getElement();
  if (element == null) {
    return null;
  }
  SliceUsage usage=element.getValue();
  if (usage == null) {
    return null;
  }
  PsiElement psiElement=usage.getElement();
  if (psiElement == null) {
    return null;
  }
  return LanguageSlicing.getProvider(psiElement);
}
