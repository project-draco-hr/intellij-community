{
  myDoCompare=false;
  try {
    doTest("Super");
    fail("Conflict was not detected");
  }
 catch (  BaseRefactoringProcessor.ConflictsInTestsException e) {
    String message=e.getMessage();
    assertTrue(message,message.equals("method <b><code>Super.foo()</code></b> has 1 usage that is not safe to delete.\n" + "Of those 0 usages are in strings, comments, or non-code files."));
  }
}
