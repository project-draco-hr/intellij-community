{
  final InferenceVariable variable=new InferenceVariable(myParameter);
  for (  InferenceBound bound : InferenceBound.values()) {
    for (    PsiType type : getBounds(bound)) {
      variable.addBound(type,bound);
    }
  }
  if (myThrownBound) {
    variable.setThrownBound();
  }
  return variable;
}
