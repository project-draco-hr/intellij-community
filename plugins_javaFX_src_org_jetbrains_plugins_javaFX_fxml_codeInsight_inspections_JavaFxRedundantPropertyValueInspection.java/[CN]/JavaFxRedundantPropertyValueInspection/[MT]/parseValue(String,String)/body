{
  try {
switch (type) {
case "Boolean":
      return Boolean.valueOf(text);
case "Integer":
    return Integer.valueOf(text);
case "Double":
  return Double.valueOf(text);
case "String":
case "Enum":
return text;
default :
LOG.warn("Unsupported value type " + type + " for '"+ text+ "'");
return null;
}
}
 catch (NumberFormatException ignored) {
LOG.warn("Invalid format of " + type + ": '"+ text+ "'");
return null;
}
}
