{
  if (ourInstance == null) {
    IdeFrame frame=null;
    for (    WelcomeFrameProvider provider : EP.getExtensions()) {
      frame=provider.createFrame();
      if (frame != null)       break;
    }
    if (frame == null) {
      frame=new WelcomeFrame();
    }
    IdeMenuBar.installAppMenuIfNeeded((JFrame)frame);
    ((JFrame)frame).setVisible(true);
    ourInstance=frame;
  }
}
