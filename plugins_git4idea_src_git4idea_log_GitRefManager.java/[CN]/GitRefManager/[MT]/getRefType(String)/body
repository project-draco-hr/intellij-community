{
  if (refName.startsWith(GitBranch.REFS_HEADS_PREFIX)) {
    return LOCAL_BRANCH;
  }
  if (refName.startsWith(GitBranch.REFS_REMOTES_PREFIX)) {
    return REMOTE_BRANCH;
  }
  if (refName.startsWith(GitTag.REFS_TAGS_PREFIX)) {
    return TAG;
  }
  if (refName.startsWith("HEAD")) {
    return HEAD;
  }
  return OTHER;
}
