{
  out.append("read");
  if (isNullable()) {
    out.append("Nullable");
  }
  out.append(readPostfix).append('(');
  addReaderParameter(subtyping,out);
  if (!isNullable()) {
    out.comma();
    if (subtyping) {
      out.append("null");
    }
 else     if (fieldName == null) {
      out.append("name");
    }
 else {
      out.quoute(fieldName);
    }
  }
}
