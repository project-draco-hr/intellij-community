  doTest("  Test test = new Test().invoke();\n" + "      int i = test.getI();\n" + "      int j = test.getJ();","public class Test {\n" + "        private int i;\n" + "        private int j;\n"+ "\n"+ "        public int getI() {\n"+ "            return i;\n"+ "        }\n"+ "\n"+ "        public int getJ() {\n"+ "            return j;\n"+ "        }\n"+ "\n"+ "        public Test invoke() {\n"+ "            i = 0;\n"+ "            j = 0;\n"+ "            return this;\n"+ "        }\n"+ "    }");
