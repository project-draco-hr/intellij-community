{
  myLock=new Object();
  myProject=project;
  myDefinitions=new JsonSchemaExportedDefinitions(new Consumer<PairConsumer<VirtualFile,NullableLazyValue<JsonSchemaObject>>>(){
    @Override public void consume(    PairConsumer<VirtualFile,NullableLazyValue<JsonSchemaObject>> consumer){
      iterateSchemas(consumer);
    }
  }
);
  ApplicationManager.getApplication().getMessageBus().connect(project).subscribe(VirtualFileManager.VFS_CHANGES,new JsonSchemaVfsListener(project,this));
  ensureSchemaFiles(project);
}
