{
  final String text;
  try {
    text=FileUtil.loadFile(file);
  }
 catch (  IOException e) {
    errorConsumer.consume(e.getMessage());
    return false;
  }
  try {
    return JsonSchemaReader.isJsonSchema(getDefinitions(),Pair.create(SchemaType.userSchema,file),text,errorConsumer);
  }
 catch (  IOException e) {
    reset();
    errorConsumer.consume(e.getMessage());
    return false;
  }
}
