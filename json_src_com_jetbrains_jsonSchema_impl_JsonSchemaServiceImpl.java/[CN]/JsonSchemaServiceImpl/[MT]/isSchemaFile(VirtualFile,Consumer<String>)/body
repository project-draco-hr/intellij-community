{
  final String text;
  try {
    text=VfsUtil.loadText(file);
  }
 catch (  IOException e) {
    errorConsumer.consume(e.getMessage());
    return false;
  }
  try {
    return JsonSchemaReader.isJsonSchema(getDefinitions(),file,text,errorConsumer);
  }
 catch (  IOException e) {
    reset();
    errorConsumer.consume(e.getMessage());
    return false;
  }
}
