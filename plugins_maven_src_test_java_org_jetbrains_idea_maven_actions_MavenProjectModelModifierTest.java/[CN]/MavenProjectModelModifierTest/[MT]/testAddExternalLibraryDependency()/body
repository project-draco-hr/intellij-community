{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  Promise<Void> result=getExtension().addExternalLibraryDependency(Collections.singletonList(getModule("project")),new CommonsIoLibraryDescriptor(),DependencyScope.COMPILE);
  assertNotNull(result);
  String version=assertHasDependency(myProjectPom,"junit","junit");
  waitUntilImported(result);
  assertModuleLibDep("project","Maven: junit:junit:" + version);
}
