{
  Bean4 bean=new Bean4();
  bean.list.add("one");
  bean.list.add("two");
  bean.list.add("three");
  doSerializerTest("<b>\n" + "  <list>\n" + "    <item value=\"one\" />\n"+ "    <item value=\"two\" />\n"+ "    <item value=\"three\" />\n"+ "  </list>\n"+ "</b>",bean,new SkipDefaultsSerializationFilter());
}
