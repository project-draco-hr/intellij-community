{
  super(null,false,true);
  getPeer().setAppIcons();
  myText=text;
  String title;
  if (programName != null) {
    title=MessageFormat.format(myBundle.getString("license.agreement.title.for"),programName);
  }
 else {
    title=myBundle.getString("license.agreement.title");
  }
  setTitle(title);
  init();
  getOKAction().setEnabled(false);
}
