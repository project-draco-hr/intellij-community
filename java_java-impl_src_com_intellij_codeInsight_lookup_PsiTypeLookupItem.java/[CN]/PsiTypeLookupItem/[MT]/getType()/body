{
  Object object=getObject();
  PsiType type=object instanceof PsiType ? getSubstitutor().substitute((PsiType)object) : JavaPsiFacade.getElementFactory(((PsiClass)object).getProject()).createType((PsiClass)object,getSubstitutor());
  for (int i=0; i < getBracketsCount(); i++) {
    type=new PsiArrayType(type);
  }
  return type;
}
