{
  try {
    myActionRunnable.run(this);
  }
 catch (  ProcessCanceledException e) {
    throw e;
  }
catch (  Throwable t) {
    myThrowable=t;
    if (!myActionRunnable.isSilentExecution()) {
      ExceptionUtil.rethrowUnchecked(t);
      throw new RuntimeException(myThrowable);
    }
  }
 finally {
    myActionRunnable=null;
  }
  return this;
}
