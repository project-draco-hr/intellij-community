{
  super.visitVariable(variable);
  if (IGNORE_UNCHECKED_ASSIGNMENT)   return;
  PsiExpression initializer=variable.getInitializer();
  if (initializer == null || initializer instanceof PsiArrayInitializerExpression)   return;
  final PsiType initializerType=initializer.getType();
  checkRawToGenericsAssignment(initializer,initializer,variable.getType(),initializerType,true,myOnTheFly ? getChangeVariableTypeFixes(variable,initializerType,myGenerifyFixes) : LocalQuickFix.EMPTY_ARRAY);
}
