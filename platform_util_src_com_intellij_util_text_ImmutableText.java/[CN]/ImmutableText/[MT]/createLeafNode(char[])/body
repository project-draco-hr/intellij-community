{
  if (chars.length == 0) {
    return EMPTY_NODE;
  }
  byte[] packed=new byte[chars.length];
  boolean success=true;
  for (int i=0; i < chars.length; i++) {
    char c=chars[i];
    if (c >= 256) {
      success=false;
      break;
    }
    packed[i]=(byte)c;
  }
  if (success) {
    return new Leaf8BitNode(packed);
  }
  return new WideLeafNode(chars);
}
