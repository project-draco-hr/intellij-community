{
  if (DumbService.isDumb(file.getProject())) {
    CharSequence contents=file.getViewProvider().getContents();
    return CharArrayUtil.indexOf(contents,HTTP_JAVA_SUN_COM_DTD_PROPERTIES_DTD,0) != -1 && isAccepted(contents);
  }
  return !FileBasedIndex.getInstance().processValues(NAME,MARKER_KEY,file.getVirtualFile(),new FileBasedIndex.ValueProcessor<String>(){
    @Override public boolean process(    VirtualFile file,    String value){
      return false;
    }
  }
,new EverythingGlobalScope());
}
