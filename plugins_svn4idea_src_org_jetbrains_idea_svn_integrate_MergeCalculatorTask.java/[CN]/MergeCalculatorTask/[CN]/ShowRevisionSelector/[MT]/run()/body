{
  QuickMergeInteraction.SelectMergeItemsResult result=myInteraction.selectMergeItems(myChangeLists,myMergeContext.getTitle(),myMergeChecker);
switch (result.getResultCode()) {
case cancel:
    end();
  break;
case all:
next(getMergeAllTasks());
break;
default :
List<CommittedChangeList> lists=result.getSelectedLists();
if (!lists.isEmpty()) {
runChangeListsMerge(lists,myMergeContext.getTitle());
}
break;
}
}
