{
  ProgressManager.getInstance().getProgressIndicator().setText("Checking merge information...");
  String repositoryRelativeWorkingCopyRoot=SvnUtil.ensureStartSlash(SVNPathUtil.getRelativePath(myMergeContext.getWcInfo().getRepositoryRoot(),myMergeContext.getWcInfo().getRootUrl()));
  String repositoryRelativeSourceBranch=SvnUtil.ensureStartSlash(SVNPathUtil.getRelativePath(myMergeContext.getWcInfo().getRepositoryRoot(),myMergeContext.getSourceUrl()));
  return getNotMergedChangeLists(changeLists,repositoryRelativeWorkingCopyRoot,repositoryRelativeSourceBranch);
}
