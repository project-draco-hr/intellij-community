{
  ProgressManager.getInstance().getProgressIndicator().setText("Checking merge information...");
  String repositoryRelativeWorkingCopyRoot=ensureStartSlash(getRelativePath(myMergeContext.getWcInfo().getRepositoryRoot(),myMergeContext.getWcInfo().getRootUrl()));
  String repositoryRelativeSourceBranch=ensureStartSlash(getRelativePath(myMergeContext.getWcInfo().getRepositoryRoot(),myMergeContext.getSourceUrl()));
  return getNotMergedChangeLists(changeLists,repositoryRelativeWorkingCopyRoot,repositoryRelativeSourceBranch);
}
