{
  myModel=model;
  myWrapper=InitialInfoBuilder.prepareToBuildBlocksSequentially(root,model,affectedRanges,mySettings,myDefaultIndentOption,interestingOffset,myProgressCallback);
  myWrapper.setCollectAlignmentsInsideFormattingRange(myReformatContext);
