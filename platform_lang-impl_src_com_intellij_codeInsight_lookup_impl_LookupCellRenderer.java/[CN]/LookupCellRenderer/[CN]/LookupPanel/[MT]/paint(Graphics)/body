{
  super.paint(g);
  if (!myLookup.isFocused() && myLookup.isCompletion()) {
    g=g.create();
    try {
      g.setColor(ColorUtil.withAlpha(BACKGROUND_COLOR,.4));
      Rectangle r=new Rectangle(getSize());
      g.fillRect(r.x,r.y,r.width,r.height);
    }
  finally {
      g.dispose();
    }
  }
}
