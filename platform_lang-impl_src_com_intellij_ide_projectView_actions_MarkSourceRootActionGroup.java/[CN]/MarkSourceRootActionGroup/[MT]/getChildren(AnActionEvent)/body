{
  List<AnAction> actions=new ArrayList<>();
  for (  JpsModuleSourceRootType<?> type : Arrays.asList(JavaSourceRootType.SOURCE,JavaSourceRootType.TEST_SOURCE,JavaResourceRootType.RESOURCE,JavaResourceRootType.TEST_RESOURCE)) {
    actions.add(new MarkSourceRootAction(type));
  }
  return actions.toArray(new AnAction[actions.size()]);
}
