{
  myBuffer.append("\n");
  for (int i=text.getStartOffset(), limit=text.getEndOffset(); i < limit; i++) {
    char c=text.getCharAt(myRawText,i);
    if (c > 127) {
      myBuffer.append(String.format("\\u%04d?",(int)c));
      continue;
    }
switch (c) {
case '\t':
      myBuffer.append(TAB);
    continue;
case '\n':
  myBuffer.append(NEW_LINE);
continue;
case '\\':
case '{':
case '}':
myBuffer.append('\\');
}
myBuffer.append(c);
}
}
