{
  if (o instanceof PyExpression) {
    return getStringValue((PyExpression)o);
  }
 else   if (o instanceof LookupElement) {
    return ((LookupElement)o).getLookupString();
  }
 else   if (o instanceof PsiElement) {
    return ((PsiElement)o).getText();
  }
 else {
    return o.toString();
  }
}
