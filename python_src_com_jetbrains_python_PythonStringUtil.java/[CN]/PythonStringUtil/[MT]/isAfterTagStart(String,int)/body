{
  int i=offsetInElement - 1;
  while (i >= 0 && isIdentifier(text.substring(i,offsetInElement))) {
    i--;
  }
  while (i >= 0 && Character.isWhitespace(text.charAt(i))) {
    i--;
  }
  if (i > 0 && (i + 1) < text.length() && "{%".equals(text.substring(i - 1,i + 1))) {
    return true;
  }
  return false;
}
