{
  String text=buffer.toString();
  if (text.isEmpty()) {
    return null;
  }
  text=convertHtmlLinks(text);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Generated JavaDoc:");
    LOG.debug(text);
  }
  text=StringUtil.replaceIgnoreCase(text,"<p/>","<p></p>");
  return StringUtil.replace(text,"/>",">");
}
