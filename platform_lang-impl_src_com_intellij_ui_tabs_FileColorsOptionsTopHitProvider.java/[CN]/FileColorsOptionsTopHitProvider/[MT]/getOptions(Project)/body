{
  if (project != null) {
    FileColorManager manager=FileColorManager.getInstance(project);
    if (manager != null) {
      BooleanOptionDescription enabled=new Option(manager,"File Colors enabled","isEnabled","setEnabled");
      return !enabled.isOptionEnabled() ? Collections.singletonList(enabled) : Collections.unmodifiableCollection(Arrays.asList(enabled,new Option(manager,"Use File Colors in Editor Tabs","isEnabledForTabs","setEnabledForTabs"),new Option(manager,"Use File Colors in Project View","isEnabledForProjectView","setEnabledForProjectView")));
    }
  }
  return Collections.emptyList();
}
