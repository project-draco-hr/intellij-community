{
  String toInsert="(";
  PsiParameter[] parameters=method.getParameterList().getParameters();
  for (int i=0; i < parameters.length; i++) {
    PsiParameter parameter=parameters[i];
    if (i != 0)     toInsert+=", ";
    toInsert+=parameter.getType().getCanonicalText();
  }
  toInsert+=")";
  return toInsert;
}
