{
  Editor editor=e.getData(CommonDataKeys.EDITOR_EVEN_IF_INACTIVE);
  Project project=e.getProject();
  if (project != null && !project.isDisposed()) {
    e.getPresentation().setEnabled(editor != null && myEditorSearchComponent.hasMatches() && PsiDocumentManager.getInstance(project).getPsiFile(editor.getDocument()) != null);
  }
}
