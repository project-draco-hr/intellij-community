{
  GrSourceFilterScope filterScope=new GrSourceFilterScope(scope);
  return StubIndex.getInstance().processElements(GrMethodNameIndex.KEY,name,myProject,filterScope,filter,GrMethod.class,processor) && StubIndex.getInstance().processElements(GrAnnotationMethodNameIndex.KEY,name,myProject,filterScope,filter,GrAnnotationMethod.class,processor);
