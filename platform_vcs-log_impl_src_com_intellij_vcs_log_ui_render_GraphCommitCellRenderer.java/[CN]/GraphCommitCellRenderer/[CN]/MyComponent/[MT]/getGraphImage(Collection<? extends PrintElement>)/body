{
  double maxIndex=0;
  for (  PrintElement printElement : printElements) {
    maxIndex=Math.max(maxIndex,printElement.getPositionInCurrentRow());
    if (printElement instanceof EdgePrintElement) {
      maxIndex=Math.max(maxIndex,(printElement.getPositionInCurrentRow() + ((EdgePrintElement)printElement).getPositionInOtherRow()) / 2.0);
    }
  }
  maxIndex++;
  maxIndex=Math.max(maxIndex,Math.min(MAX_GRAPH_WIDTH,myGraphTable.getVisibleGraph().getRecommendedWidth()));
  BufferedImage image=UIUtil.createImage((int)(PaintParameters.getNodeWidth(myGraphTable.getRowHeight()) * (maxIndex + 2)),myGraphTable.getRowHeight(),BufferedImage.TYPE_INT_ARGB);
  Graphics2D g2=image.createGraphics();
  myPainter.draw(g2,printElements);
  int width=(int)(maxIndex * PaintParameters.getNodeWidth(myGraphTable.getRowHeight()));
  return new PaintInfo(image,width);
}
