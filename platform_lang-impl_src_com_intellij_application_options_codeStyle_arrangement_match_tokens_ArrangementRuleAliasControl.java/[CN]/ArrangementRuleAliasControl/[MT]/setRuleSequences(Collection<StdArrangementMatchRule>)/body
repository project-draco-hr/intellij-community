{
  myComponents.clear();
  getModel().clear();
  if (sequences == null) {
    return;
  }
  for (  StdArrangementMatchRule rule : sequences) {
    getModel().add(rule);
  }
  if (ArrangementConstants.LOG_ALIAS_RULE_MODIFICATION) {
    LOG.info("Arrangement rules sequences is refreshed. Given rules:");
    for (    StdArrangementMatchRule rule : sequences) {
      LOG.info("  " + rule.toString());
    }
  }
}
