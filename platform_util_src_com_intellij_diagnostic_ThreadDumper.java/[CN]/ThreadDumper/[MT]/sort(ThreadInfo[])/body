{
  int edtIndex=-1;
  for (int i=0; i < threads.length; i++) {
    if (threads[i].getThreadName().startsWith("AWT-EventQueue")) {
      edtIndex=i;
      break;
    }
  }
  if (edtIndex > 0) {
    ThreadInfo edt=threads[edtIndex];
    System.arraycopy(threads,0,threads,1,edtIndex);
    threads[0]=edt;
  }
  return threads;
}
