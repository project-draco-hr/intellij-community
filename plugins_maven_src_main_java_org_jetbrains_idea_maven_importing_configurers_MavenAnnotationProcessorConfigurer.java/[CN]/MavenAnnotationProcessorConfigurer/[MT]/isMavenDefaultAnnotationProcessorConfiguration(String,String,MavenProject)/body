{
  Map<String,String> options=mavenProject.getAnnotationProcessorOptions();
  List<String> processors=mavenProject.getDeclaredAnnotationProcessors();
  return ContainerUtil.isEmpty(processors) && options.isEmpty() && DEFAULT_ANNOTATION_PATH_OUTPUT.equals(annotationProcessorDirectory.replace('\\','/'))&& DEFAULT_TEST_ANNOTATION_OUTPUT.equals(testAnnotationProcessorDirectory.replace('\\','/'));
}
