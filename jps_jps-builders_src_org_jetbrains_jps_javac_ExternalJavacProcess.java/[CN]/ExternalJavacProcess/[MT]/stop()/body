{
  try {
    final ChannelFuture future=myConnectFuture;
    if (future != null) {
      future.channel().close().await();
    }
    myEventLoopGroup.shutdownGracefully(0,15,TimeUnit.SECONDS).await();
    System.exit(0);
  }
 catch (  Throwable e) {
    e.printStackTrace(System.err);
    System.exit(-1);
  }
}
