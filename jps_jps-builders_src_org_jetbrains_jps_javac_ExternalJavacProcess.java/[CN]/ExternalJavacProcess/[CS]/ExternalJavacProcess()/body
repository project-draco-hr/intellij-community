{
  final JavacRemoteProto.Message msgDefaultInstance=JavacRemoteProto.Message.getDefaultInstance();
  myEventLoopGroup=new NioEventLoopGroup(1,SharedThreadPool.getInstance());
  myChannelInitializer=new ChannelInitializer(){
    @Override protected void initChannel(    Channel channel) throws Exception {
      channel.pipeline().addLast(new ProtobufVarint32FrameDecoder(),new ProtobufDecoder(msgDefaultInstance),new ProtobufVarint32LengthFieldPrepender(),new ProtobufEncoder(),new CompilationRequestsHandler());
    }
  }
;
}
