{
  if (first.type == Exprent.EXPRENT_MONITOR) {
    MonitorExprent monexpr=(MonitorExprent)first;
    if (monexpr.getMonType() == MonitorExprent.MONITOR_EXIT && monexpr.getValue().type == Exprent.EXPRENT_VAR && !((VarExprent)monexpr.getValue()).isStack()) {
      return true;
    }
  }
  return false;
}
