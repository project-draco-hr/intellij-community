{
  try {
    return supplier.get();
  }
 catch (  ProcessCanceledException e) {
    if (!ignorePCE) {
      throw e;
    }
  }
catch (  VMDisconnectedException|ObjectCollectedException e) {
    throw e;
  }
catch (  InternalException e) {
    LOG.info(e);
  }
catch (  Exception|AssertionError e) {
    if (rethrow != null && rethrow.isInstance(e)) {
      throw e;
    }
 else {
      LOG.error(e);
    }
  }
  return defaultValue;
}
