{
  final Sdk sdk=StudyUtils.findSdk(task,project);
  if (sdk != null) {
    if (sdk.getVersionString() != null && sdk.getVersionString().startsWith("3")) {
      return "python3";
    }
 else {
      return "python2";
    }
  }
 else {
    StudyUtils.showNoSdkNotification(task,project);
  }
  return null;
}
