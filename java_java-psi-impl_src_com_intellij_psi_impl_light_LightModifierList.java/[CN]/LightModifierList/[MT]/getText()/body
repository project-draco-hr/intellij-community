{
  StringBuilder buffer=new StringBuilder();
  for (  String modifier : PsiModifier.MODIFIERS) {
    if (hasExplicitModifier(modifier)) {
      buffer.append(modifier);
      buffer.append(' ');
    }
  }
  if (buffer.length() > 0) {
    buffer.delete(buffer.length() - 1,buffer.length());
  }
  return buffer.toString();
}
