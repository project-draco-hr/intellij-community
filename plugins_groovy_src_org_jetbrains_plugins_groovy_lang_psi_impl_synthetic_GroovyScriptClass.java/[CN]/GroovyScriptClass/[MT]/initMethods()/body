{
  if (myInitialized)   return;
  myMainMethod=new LightMethodBuilder(getManager(),GroovyLanguage.INSTANCE,"main").setContainingClass(this).setMethodReturnType(PsiType.VOID).addParameter("args",new PsiArrayType(PsiType.getJavaLangString(getManager(),getResolveScope()))).addModifiers(PsiModifier.PUBLIC,PsiModifier.STATIC);
  myRunMethod=new LightMethodBuilder(getManager(),GroovyLanguage.INSTANCE,"run").setContainingClass(this).setMethodReturnType(TypesUtil.getJavaLangObject(this)).addModifier(PsiModifier.PUBLIC);
  myInitialized=true;
}
