{
  if (!super.match(matchNode,engine)) {
    return false;
  }
  RuleValue rule=matchNode.getRules().get(MatchProperties.EXPRENT_FIELD_NAME);
  if (rule != null) {
    if (rule.isVariable()) {
      if (!engine.checkAndSetVariableValue((String)rule.value,this.name)) {
        return false;
      }
    }
 else {
      if (!rule.value.equals(this.name)) {
        return false;
      }
    }
  }
  return true;
}
