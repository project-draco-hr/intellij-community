{
  try {
    myIpnbPanels.clear();
    mySelectedCell=null;
    myIpnbFile=IpnbParser.parseIpnbFile(myDocument,myVirtualFile.getPath());
    if (myIpnbFile.getCells().isEmpty()) {
      final IpnbCodeCell cell=new IpnbCodeCell("python",new String[]{""},null,new ArrayList<IpnbOutputCell>());
      myIpnbFile.addCell(cell,0);
      saveToFile();
    }
  }
 catch (  IOException e) {
    if (showError)     Messages.showErrorDialog(getProject(),e.getMessage(),"Can't open " + myVirtualFile.getPath());
 else     LOG.error(e.getMessage(),"Can't open " + myVirtualFile.getPath());
  }
  layoutFile();
}
