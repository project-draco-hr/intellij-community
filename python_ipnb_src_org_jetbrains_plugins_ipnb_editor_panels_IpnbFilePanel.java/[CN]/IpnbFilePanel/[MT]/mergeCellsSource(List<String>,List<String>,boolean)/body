{
  final ArrayList<String> source=new ArrayList<>();
  if (below) {
    source.addAll(currentCellSource);
    source.add("\n");
    source.addAll(cellToMergeSource);
  }
 else {
    source.addAll(cellToMergeSource);
    source.add("\n");
    source.addAll(currentCellSource);
  }
  return source;
}
