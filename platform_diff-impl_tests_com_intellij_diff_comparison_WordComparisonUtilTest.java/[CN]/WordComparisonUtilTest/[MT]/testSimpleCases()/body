{
  TestData.words("x z","y z")._______Def_("-  ","-  ").all();
  TestData.words(" x z","y z")._______Def_("--  ","-  ").______Trim_(" -  ","-  ").all();
  TestData.words("x z ","y z")._______Def_("-  -","-  ").______Trim_("-   ","-  ").all();
  TestData.words("x z ","y z")._______Def_("-  -","-  ").______Trim_("-   ","-  ").all();
  TestData.words("x z"," y z ")._______Def_("-  ","--  -").______Trim_("-  "," -   ").all();
  TestData.words("x y","x z ")._______Def_("  -","  --").______Trim_("  -","  - ").all();
  TestData.words("x,y","x")._______Def_(" --"," ").all();
  TestData.words("x,y","y")._______Def_("-- "," ").all();
  TestData.words(".x=",".!=")._______Def_(" - "," - ").all();
}
