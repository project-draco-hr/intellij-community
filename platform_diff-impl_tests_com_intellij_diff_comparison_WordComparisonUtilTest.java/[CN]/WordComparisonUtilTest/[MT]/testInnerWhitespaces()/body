{
  TestData.words("<< x >>","<.<>.>")._______Def_("  ---  "," -  - ").____Ignore_("   -   "," -  - ").all();
  TestData.words("<< x >>","y<<x>>y")._______Def_("  - -  ","-     -").____Ignore_("       ","-     -").all();
  TestData.words("x .. z","x y .. z")._______Def_("      "," --     ").____Ignore_("      ","  -     ").all();
  TestData.words("  x..z","x..y  ")._______Def_("--   -","   ---").______Trim_("     -","   -  ").all();
  TestData.words(" x y x _ x z x ","x x_x x")._______Def_("- --  - - --  -","       ").______Trim_("  --      --   ","       ").____Ignore_("   -       -   ","       ").all();
}
