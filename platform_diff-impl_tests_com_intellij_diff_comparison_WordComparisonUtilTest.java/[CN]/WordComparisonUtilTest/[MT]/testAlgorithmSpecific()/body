{
  TestData.words("...x","x...")._______Def_("--- "," ---").all();
  TestData.words("x x y","x y")._______Def_("--   ","   ").____Ignore_("-    ","   ").all();
  TestData.words("y x x","y x")._______Def_("   --","   ").____Ignore_("    -","   ").all();
  TestData.words("A X A B","A B")._______Def_("----   ","   ").____Ignore_("---    ","   ").all();
  TestData.words("A.X A.Z","A.X A.Y A.Z")._______Def_("       ","   ----    ").____Ignore_("       ","    ---    ").all();
  TestData.words("X.A Z.A","X.A Y.A Z.A")._______Def_("       ","   ----    ").____Ignore_("       ","    ---    ").all();
  TestData.words(".   ","   .")._______Def_(" ---","--- ").def();
}
