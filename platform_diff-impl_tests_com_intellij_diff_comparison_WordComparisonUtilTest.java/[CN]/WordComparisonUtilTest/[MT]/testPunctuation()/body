{
  TestData.words(" x.z.x ","x..x")._______Def_("-  -  -","    ").______Trim_("   -   ","    ").all();
  TestData.words("x..x"," x.z.x ")._______Def_("    ","-  -  -").______Trim_("    ","   -   ").all();
  TestData.words("x ... z","y ... z")._______Def_("-      ","-      ").all();
  TestData.words("x ... z","x ... y")._______Def_("      -","      -").all();
  TestData.words("x ,... z","x ... y")._______Def_("  -    -","      -").all();
  TestData.words("x . , .. z","x ... y")._______Def_("   ---   -","      -").____Ignore_("    -    -","      -").all();
  TestData.words("x==y==z","x====z")._______Def_("   -   ","      ").all();
  TestData.words("x====z","x==t==z")._______Def_("      ","   -   ").all();
}
