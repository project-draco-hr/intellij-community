{
  myDataLoaderQueue.run(new Task.Backgroundable(myProject,"Loading history...",false){
    public void run(    @NotNull final ProgressIndicator indicator){
      try {
        MyTimer timer=new MyTimer("Read all history");
        dataPack=DataPack.build(myLogProvider.readFirstBlock(myRoot),myLogProvider.readAllRefs(myRoot),indicator);
        timer.print();
      }
 catch (      VcsException e) {
        notifyError(e);
      }
      UIUtil.invokeAndWaitIfNeeded(new Runnable(){
        @Override public void run(){
        }
      }
);
    }
  }
);
}
