{
  StringBuilder stringBuilder=new StringBuilder("print (");
  PsiElement target=PsiTreeUtil.findChildOfType(expression,PyExpression.class);
  if (target != null)   stringBuilder.append(target.getText());
  stringBuilder.append(")");
  expression.replace(elementGenerator.createFromText(LanguageLevel.forElement(expression),PyExpression.class,stringBuilder.toString()));
}
