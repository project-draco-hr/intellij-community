{
  if (from < 0)   return null;
  int index=from;
  List<TabInfo> infos=getVisibleInfos();
  while (true) {
    index--;
    if (index == -1) {
      if (!cycle)       break;
      index=infos.size() - 1;
    }
    if (index == from)     break;
    final TabInfo each=infos.get(index);
    if (each.isEnabled())     return each;
  }
  return null;
}
