{
  checkIsNull(reader,null);
  reader.beginArray();
  if (!reader.hasNext()) {
    reader.endArray();
    return Collections.emptyList();
  }
  List<StringIntPair> result=new ArrayList<StringIntPair>();
  do {
    reader.beginArray();
    result.add(new StringIntPair(reader.nextInt(),reader.nextString()));
    reader.endArray();
  }
 while (reader.hasNext());
  reader.endArray();
  return result;
}
