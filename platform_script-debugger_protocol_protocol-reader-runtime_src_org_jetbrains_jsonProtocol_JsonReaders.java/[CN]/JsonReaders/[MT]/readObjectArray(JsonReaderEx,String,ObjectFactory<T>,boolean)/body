{
  if (reader.peek() == JsonToken.NULL) {
    if (nullable) {
      reader.skipValue();
      return null;
    }
 else {
      checkIsNull(reader,fieldName);
    }
  }
  reader.beginArray();
  if (!reader.hasNext()) {
    reader.endArray();
    if (nullable) {
      return null;
    }
 else {
      return Collections.emptyList();
    }
  }
  List<T> result=new ArrayList<T>();
  do {
    result.add(factory.read(reader));
  }
 while (reader.hasNext());
  reader.endArray();
  return result;
}
