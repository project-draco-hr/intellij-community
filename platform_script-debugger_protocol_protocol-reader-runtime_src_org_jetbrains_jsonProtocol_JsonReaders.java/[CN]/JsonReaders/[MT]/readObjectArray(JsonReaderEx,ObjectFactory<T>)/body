{
  if (reader.peek() == JsonToken.NULL) {
    reader.skipValue();
    return null;
  }
  reader.beginArray();
  if (!reader.hasNext()) {
    reader.endArray();
    return Collections.emptyList();
  }
  List<T> result=new ArrayList<T>();
  do {
    result.add(factory.read(reader));
  }
 while (reader.hasNext());
  reader.endArray();
  return result;
}
