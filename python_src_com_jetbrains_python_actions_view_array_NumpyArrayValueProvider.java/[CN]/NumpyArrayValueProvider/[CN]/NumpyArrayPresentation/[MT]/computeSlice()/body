{
  String presentation="";
  if (myBaseNode != null) {
    presentation+=((XValueNodeImpl)myBaseNode).getName();
    if (myShape != null) {
      presentation+=new String(new char[myShape.length - 2]).replace("\0","[0]");
      if (myShape[0] == 1) {
        presentation+="[0:" + myShape[1] + "]";
      }
 else {
        presentation+="[0:" + myShape[myShape.length - 2] + "]";
      }
    }
  }
  setSlice(presentation);
}
