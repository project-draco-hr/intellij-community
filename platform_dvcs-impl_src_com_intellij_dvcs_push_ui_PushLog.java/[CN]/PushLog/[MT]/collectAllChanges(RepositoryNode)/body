{
  ArrayList<Change> changes=new ArrayList<Change>();
  if (rootNode.getChildCount() <= 0)   return changes;
  for (DefaultMutableTreeNode childNode=(DefaultMutableTreeNode)rootNode.getFirstChild(); childNode != null; childNode=(DefaultMutableTreeNode)rootNode.getChildAfter(childNode)) {
    if (childNode instanceof VcsFullCommitDetailsNode) {
      changes.addAll(((VcsFullCommitDetailsNode)childNode).getUserObject().getChanges());
    }
  }
  return changes;
}
