{
  createProjectSubFile("src/main/resources/dir/file.txt");
  importProjectUsingSingeModulePerGradleProject("apply plugin: 'java'\n" + "\n" + "import org.apache.tools.ant.filters.*\n"+ "processResources {\n"+ "  rename 'file.txt', 'file001.txt'\n"+ "}");
  assertModules("project");
  compileModules("project");
  assertCopied("build/resources/main/dir/file001.txt");
}
