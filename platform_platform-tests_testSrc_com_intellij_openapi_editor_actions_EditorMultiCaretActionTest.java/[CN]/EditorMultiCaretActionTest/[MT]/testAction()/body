{
  new WriteCommandAction<Void>(null){
    @Override protected void run(    @NotNull Result<Void> result) throws Throwable {
      configureByFile(getBeforeFileName());
      EditorScrollingFixture.setVisibleSize(myEditor,120,20);
      executeAction(getActionName());
      checkResultByFile(getAfterFileName());
    }
  }
.execute();
}
