{
  PsiElement onlyChild=null;
  for (PsiElement child=element.getFirstChild(); child != null; child=child.getNextSibling()) {
    if (LexicalNodesFilter.getInstance().accepts(child)) {
      continue;
    }
    if (onlyChild != null) {
      return null;
    }
    onlyChild=child;
  }
  return onlyChild;
}
