{
  Activatable activatable=new Activatable(){
    public void showNotify(){
      runnable.run();
    }
    public void hideNotify(){
    }
  }
;
  new UiNotifyConnector(c,activatable){
    @Override protected void showNotify(){
      super.showNotify();
      Disposer.dispose(this);
    }
  }
;
}
