{
  if (!myGroupSequentialChangeLists) {
    List<CommittedChangeList> processed=myMergeChunk != null ? ContainerUtil.newArrayList(myMergeChunk.chunkAndBeforeLists()) : ContainerUtil.<CommittedChangeList>emptyList();
    myProject.getMessageBus().syncPublisher(COMMITTED_CHANGES_MERGED_STATE).event(processed);
  }
}
