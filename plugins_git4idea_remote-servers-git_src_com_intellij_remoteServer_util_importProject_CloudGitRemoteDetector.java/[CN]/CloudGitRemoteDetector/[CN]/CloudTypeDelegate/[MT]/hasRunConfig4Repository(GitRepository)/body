{
  List<RunConfiguration> runConfigurations=RunManagerEx.getInstanceEx(myProject).getConfigurationsList(DeployToServerConfigurationTypesRegistrar.getDeployConfigurationType(getCloudType()));
  VirtualFile repositoryRoot=repository.getRoot();
  for (  RunConfiguration runConfiguration : runConfigurations) {
    if (repositoryRoot.equals(getContentRoot(runConfiguration))) {
      return true;
    }
  }
  return false;
}
