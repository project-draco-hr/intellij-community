{
  if (element instanceof GraphEdge) {
    GraphEdge edge=(GraphEdge)element;
    if (edge.getType() == GraphEdgeType.DOTTED)     return edge;
  }
 else {
    int nodeIndex=((GraphNode)element).getNodeIndex();
    for (    int upNode : graphWithElementsInfo.getUpNodes(nodeIndex)) {
      if (graphWithElementsInfo.getEdgeType(upNode,nodeIndex) == GraphEdgeType.DOTTED)       return new GraphEdge(upNode,nodeIndex,GraphEdgeType.DOTTED);
    }
    for (    int downNode : graphWithElementsInfo.getDownNodes(nodeIndex)) {
      if (graphWithElementsInfo.getEdgeType(nodeIndex,downNode) == GraphEdgeType.DOTTED)       return new GraphEdge(nodeIndex,downNode,GraphEdgeType.DOTTED);
    }
  }
  return null;
}
