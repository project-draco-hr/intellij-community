{
  InputStream stream=null;
  try {
    stream=new FileInputStream(path);
    return (X509Certificate)ourFactory.generateCertificate(stream);
  }
 catch (  Exception e) {
    LOG.error("Can't add certificate for path: " + path,e);
    return null;
  }
 finally {
    StreamUtil.closeStream(stream);
  }
}
