{
  if (!GroovyConfigUtils.getInstance().isVersionAtLeast(context,GroovyConfigUtils.GROOVY1_8))   return null;
  if (!(InheritanceUtil.isInheritor(rType,GroovyCommonClassNames.JAVA_LANG_CHAR_SEQUENCE) || InheritanceUtil.isInheritor(rType,GroovyCommonClassNames.GROOVY_LANG_GSTRING))) {
    return null;
  }
  if (lType == PsiType.BOOLEAN || TypesUtil.resolvesTo(lType,CommonClassNames.JAVA_LANG_BOOLEAN))   return true;
  if (TypesUtil.resolvesTo(lType,CommonClassNames.JAVA_LANG_CLASS))   return true;
  if (isEnum(lType))   return true;
  return null;
}
