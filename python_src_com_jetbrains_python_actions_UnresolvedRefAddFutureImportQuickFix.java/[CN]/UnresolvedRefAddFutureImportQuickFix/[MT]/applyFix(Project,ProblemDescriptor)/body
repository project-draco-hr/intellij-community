{
  PyFile file=(PyFile)myElement.getContainingFile();
  if (!CodeInsightUtilBase.prepareFileForWrite(file))   return;
  PyElementGenerator elementGenerator=PyElementGenerator.getInstance(project);
  PyFromImportStatement statement=elementGenerator.createFromText(LanguageLevel.forElement(myElement),PyFromImportStatement.class,"from __future__ import with_statement");
  file.addBefore(statement,file.getStatements().get(0));
}
