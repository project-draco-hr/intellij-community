{
  apply();
  this.item=item;
  if (item == null) {
    return;
  }
  myDisplayName.setText(this.item.getName());
  myDescription.setText(item.getDescription());
  ((DefaultListModel)myActionsList.getModel()).clear();
  for (  String id : item.getActionIds()) {
    includeActionId(id);
  }
  myDisplayName.requestFocus();
}
