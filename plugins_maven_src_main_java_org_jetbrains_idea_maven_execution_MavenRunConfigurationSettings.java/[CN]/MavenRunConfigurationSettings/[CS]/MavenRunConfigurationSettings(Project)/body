{
  myCompositeConfigurable=new CompositeConfigurable(new MavenRunnerConfigurableWithUseProjectSettings(p){
    protected MavenRunnerSettings getState(){
      return configuration.getRunnerSettings();
    }
    @Override public void setState(    @Nullable MavenRunnerSettings state){
      configuration.setRunnerSettings(state);
    }
  }
);
}
