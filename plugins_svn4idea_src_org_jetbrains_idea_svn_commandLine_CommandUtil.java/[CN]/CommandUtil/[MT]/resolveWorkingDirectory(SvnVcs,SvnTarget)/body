{
  File workingDirectory=target.isFile() ? target.getFile() : null;
  workingDirectory=SvnBindUtil.correctUpToExistingParent(workingDirectory);
  if (workingDirectory == null) {
    workingDirectory=!vcs.getProject().isDefault() ? VfsUtilCore.virtualToIoFile(vcs.getProject().getBaseDir()) : new File(PathManager.getHomePath());
  }
  return workingDirectory;
}
