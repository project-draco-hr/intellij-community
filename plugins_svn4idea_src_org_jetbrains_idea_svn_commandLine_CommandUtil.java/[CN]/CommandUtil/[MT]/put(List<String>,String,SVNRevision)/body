{
  StringBuilder builder=new StringBuilder(path);
  builder.append("@");
  if (pegRevision != null && !SVNRevision.UNDEFINED.equals(pegRevision) && !SVNRevision.WORKING.equals(pegRevision) && pegRevision.isValid() && pegRevision.getNumber() != 0) {
    builder.append(pegRevision);
  }
  parameters.add(builder.toString());
}
