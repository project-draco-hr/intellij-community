{
  SVNURL repositoryUrl=resolveRepositoryUrl(vcs,name,target);
  CommandRuntime runtime=new CommandRuntime(new IdeaSvnkitBasedAuthenticationCallback(vcs));
  return runtime.runWithAuthenticationAttempt(workingDirectory,repositoryUrl,name,listener != null ? listener : new SvnCommitRunner.CommandListener(null),ArrayUtil.toStringArray(parameters));
}
