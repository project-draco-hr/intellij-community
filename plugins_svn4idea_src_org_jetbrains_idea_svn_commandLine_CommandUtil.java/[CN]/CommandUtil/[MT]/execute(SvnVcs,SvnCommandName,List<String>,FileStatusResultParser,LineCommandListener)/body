{
  String exe=resolveExePath();
  File base=resolveBaseDirectory(null,exe);
  SVNURL url=resolveRepositoryUrl(vcs,null);
  SvnLineCommand command=SvnLineCommand.runWithAuthenticationAttempt(base,url,name,listener != null ? listener : new SvnCommitRunner.CommandListener(null),new IdeaSvnkitBasedAuthenticationCallback(vcs),ArrayUtil.toStringArray(parameters));
  if (parser != null) {
    parser.parse(command.getOutput());
  }
  return command;
}
