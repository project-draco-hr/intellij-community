{
  String exe=resolveExePath();
  base=resolveBaseDirectory(base,exe);
  url=resolveRepositoryUrl(vcs,url);
  try {
    return SvnLineCommand.runWithAuthenticationAttempt(base,url,name,new SvnCommitRunner.CommandListener(null),new IdeaSvnkitBasedAuthenticationCallback(vcs),ArrayUtil.toStringArray(parameters));
  }
 catch (  SvnBindException e) {
    throw new SVNException(SVNErrorMessage.create(SVNErrorCode.IO_ERROR,e),e);
  }
}
