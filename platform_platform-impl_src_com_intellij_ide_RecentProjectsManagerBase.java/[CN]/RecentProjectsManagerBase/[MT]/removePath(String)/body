{
  if (path == null)   return;
synchronized (myStateLock) {
    if (SystemInfo.isFileSystemCaseSensitive) {
      myState.removePath(path);
    }
 else {
      for (      String p : ArrayUtil.toStringArray(myState.recentPaths)) {
        if (path.equalsIgnoreCase(p)) {
          myState.removePath(path);
        }
      }
    }
  }
}
