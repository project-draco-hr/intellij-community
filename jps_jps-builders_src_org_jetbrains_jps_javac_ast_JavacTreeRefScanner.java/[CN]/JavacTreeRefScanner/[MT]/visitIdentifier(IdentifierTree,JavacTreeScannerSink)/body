{
  final JCTree.JCIdent javacIdentifier=(JCTree.JCIdent)node;
  final Type type=javacIdentifier.type;
  if (type == null) {
    return null;
  }
  if (type.getKind() == TypeKind.PACKAGE) {
    return null;
  }
  final Symbol sym=javacIdentifier.sym;
  if (sym.getKind() == ElementKind.PARAMETER || sym.getKind() == ElementKind.LOCAL_VARIABLE || sym.getKind() == ElementKind.FIELD || sym.getKind() == ElementKind.EXCEPTION_PARAMETER || sym.getKind() == ElementKind.TYPE_PARAMETER) {
    return null;
  }
  sink.sinkReference(new JavacRefSymbol(sym,Tree.Kind.IDENTIFIER));
  return null;
}
