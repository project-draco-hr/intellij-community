{
  String testCount;
  if (myDefects > 0)   testCount=ExecutionBundle.message("junit.running.info.status.completed.from.total.failed",myCompleted + offset,myTotal,myDefects);
 else   testCount=ExecutionBundle.message("junit.running.info.status.completed.from.total",myCompleted + offset,myTotal);
  return testCount;
}
