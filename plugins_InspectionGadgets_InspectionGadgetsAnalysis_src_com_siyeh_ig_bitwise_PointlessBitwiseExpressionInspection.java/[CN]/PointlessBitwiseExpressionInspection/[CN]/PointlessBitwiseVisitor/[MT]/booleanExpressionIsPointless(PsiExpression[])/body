{
  PsiExpression previousExpression=null;
  for (  PsiExpression operand : operands) {
    if (isZero(operand) || isAllOnes(operand) || EquivalenceChecker.getCanonicalPsiEquivalence().expressionsAreEquivalent(previousExpression,operand)) {
      return true;
    }
    previousExpression=operand;
  }
  return false;
}
