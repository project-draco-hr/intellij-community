{
  LocalChangeList changeList=ChangeListManager.getInstance(myVcs.getProject()).getChangeList(myChange);
  ApplyPatchDifferentiatedDialog dialog=new ApplyPatchDifferentiatedDialog(myVcs.getProject(),new TreeConflictApplyTheirsPatchExecutor(myVcs,context,myBaseForPatch),singletonList(new ApplyPatchSaveToFileExecutor(myVcs.getProject(),myBaseForPatch)),ApplyPatchMode.APPLY_PATCH_IN_MEMORY,myTextPatches,changeList);
  context.suspend();
  dialog.show();
}
