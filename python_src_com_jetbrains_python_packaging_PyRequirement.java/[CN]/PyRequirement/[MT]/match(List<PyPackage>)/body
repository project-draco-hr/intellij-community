{
  for (  PyPackage pkg : packages) {
    if (myName.equalsIgnoreCase(pkg.getName())) {
      if (myVersion == null) {
        return true;
      }
      final int cmp=VERSION_COMPARATOR.compare(pkg.getVersion(),myVersion);
switch (myRelation) {
case LT:
        return cmp < 0;
case LTE:
      return cmp <= 0;
case GT:
    return cmp > 0;
case GTE:
  return cmp >= 0;
case EQ:
return cmp == 0;
case NE:
return cmp != 0;
}
}
}
return false;
}
