{
  final List<PyRequirement> result=new ArrayList<PyRequirement>();
  for (  String line : StringUtil.splitByLines(s)) {
    final String trimmed=line.trim();
    if (!trimmed.isEmpty()) {
      final PyRequirement req=fromString(line);
      if (req != null) {
        result.add(req);
      }
    }
  }
  return result;
}
