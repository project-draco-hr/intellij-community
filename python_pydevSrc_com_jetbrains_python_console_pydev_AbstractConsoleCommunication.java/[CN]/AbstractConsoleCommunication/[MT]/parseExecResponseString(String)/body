{
  Boolean more;
  String errorContents=null;
  String lower=str.toLowerCase();
  if (lower.equals("true") || lower.equals("1")) {
    more=true;
  }
 else   if (lower.equals("false") || lower.equals("0")) {
    more=false;
  }
 else {
    more=false;
    errorContents=str;
  }
  return new Pair<String,Boolean>(errorContents,more);
}
