{
  String onlyChunkSlice="[" + rOffset + ":"+ (rOffset + rows)+ ", "+ cOffset+ ":"+ (cOffset + columns)+ "]";
  if (isOneRow()) {
    onlyChunkSlice="[" + cOffset + ":"+ (cOffset + columns)+ "]";
  }
  if (baseSlice.endsWith(onlyChunkSlice)) {
    return baseSlice;
  }
  return baseSlice + onlyChunkSlice;
}
