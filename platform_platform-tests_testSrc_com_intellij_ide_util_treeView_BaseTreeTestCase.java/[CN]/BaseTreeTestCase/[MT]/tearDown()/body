{
  invokeLaterIfNeeded(new Runnable(){
    @Override public void run(){
      if (getBuilder() != null) {
        Disposer.dispose(getBuilder());
      }
    }
  }
);
  waitFor(6000,new Condition(){
    @Override public boolean value(    Object o){
      return getBuilder() == null || getBuilder().getUi() == null;
    }
  }
);
  super.tearDown();
}
