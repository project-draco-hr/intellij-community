{
  if (psiElement().inside(PsiDocTag.class).inside(or(psiElement(PsiJavaCodeReferenceElement.class),psiElement(PsiDocMethodOrFieldRef.class))).accepts(contextElement)) {
    return ThreeState.NO;
  }
  if (PlatformPatterns.psiElement(JavaDocTokenType.DOC_TAG_NAME).accepts(contextElement)) {
    return ThreeState.NO;
  }
  return super.shouldSkipAutopopup(contextElement,psiFile,offset);
}
