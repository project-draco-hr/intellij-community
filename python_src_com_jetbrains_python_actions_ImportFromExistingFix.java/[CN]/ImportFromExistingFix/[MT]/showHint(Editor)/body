{
  if (myNode == null || !myNode.isValid() || myNode.getName() == null || myImports.size() <= 0) {
    return false;
  }
  final String message=ShowAutoImportPass.getMessage(myImports.size() > 1,myImports.get(0).getFirst().getVisibleName() + "." + myNode.getName());
  final ImportFromExistingAction action=new ImportFromExistingAction(myNode,myImports,myName,editor);
  HintManager.getInstance().showQuestionHint(editor,message,myNode.getTextOffset(),myNode.getTextRange().getEndOffset(),action);
  return true;
}
