{
  if (!super.isValidSdkPath(file))   return false;
  File bin=file.getParentFile();
  if (bin != null) {
    File[] children=bin.listFiles();
    if (children != null) {
      for (      File f : children) {
        if (f.getName().equals("activate_this.py")) {
          return true;
        }
      }
    }
  }
  return false;
}
