{
  cat.append("@");
  name_wrapper.apply(PyUtil.getReadableRepr(deco.getCallee(),true),cat);
  if (deco.hasArgumentList()) {
    PyArgumentList arglist=deco.getArgumentList();
    if (arglist != null) {
      List<String> argnames=FP.map(FP.combine(LReadableRepr,arg_wrapper),Arrays.asList(arglist.getArguments()));
      cat.append("(");
      join(", ",argnames,cat);
      cat.append(")");
    }
  }
  return cat;
}
