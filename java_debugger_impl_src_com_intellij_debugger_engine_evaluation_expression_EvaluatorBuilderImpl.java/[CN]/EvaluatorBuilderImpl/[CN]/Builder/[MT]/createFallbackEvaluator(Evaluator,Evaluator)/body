{
  return new Evaluator(){
    @Override public Object evaluate(    EvaluationContextImpl context) throws EvaluateException {
      try {
        return primary.evaluate(context);
      }
 catch (      EvaluateException e) {
        try {
          return fallback.evaluate(context);
        }
 catch (        EvaluateException e1) {
          throw e;
        }
      }
    }
    @Override public Modifier getModifier(){
      return primary.getModifier();
    }
  }
;
}
