{
  if (SystemInfo.isMacOSElCapitan) {
    FontInfo fontInfo=FontInfo.get(".SF NS Text");
    if (fontInfo != null) {
      Font font=fontInfo.getFont();
      if (font != null) {
        return new FontUIResource(font.deriveFont(style,size));
      }
    }
  }
  return new FontUIResource(yosemite,style,size);
}
