{
  try {
    if (SystemInfo.isWindows || SystemInfo.isLinux) {
      return new IdeaLaf();
    }
 else {
      final String name=UIManager.getSystemLookAndFeelClassName();
      return (BasicLookAndFeel)Class.forName(name).newInstance();
    }
  }
 catch (  Exception e) {
    log(e);
  }
  return null;
}
