{
  try {
    if (canShowMacSheetPanel()) {
      return MacMessages.getInstance().showYesNoDialog(title,message,YES_BUTTON,NO_BUTTON,WindowManager.getInstance().suggestParentWindow(project),doNotAskOption);
    }
  }
 catch (  Exception exception) {
    LOG.error(exception);
  }
  int result=showYesNoDialog(project,message,title,YES_BUTTON,NO_BUTTON,icon,doNotAskOption);
  LOG.assertTrue(result == YES || result == NO,result);
  return result;
}
