{
  if (file != null) {
    execute(new GuiTask(){
      @Override protected void executeInEDT() throws Throwable {
        ApplicationManager.getApplication().runWriteAction(new Runnable(){
          @Override public void run(){
            try {
              file.delete(this);
            }
 catch (            IOException e) {
            }
          }
        }
);
      }
    }
);
  }
}
