{
  OutputStream os=null;
  try {
    os=new BufferedOutputStream(new FileOutputStream(targetFile));
    myVCS.getFactoryFromSettings().createDiffClient().unifiedDiff(SvnTarget.fromURL(sourceURL,SVNRevision.HEAD),SvnTarget.fromURL(targetURL,SVNRevision.HEAD),os);
  }
 catch (  IOException e) {
    LOG.info(e);
  }
catch (  VcsException e) {
    LOG.info(e);
  }
 finally {
    if (os != null) {
      try {
        os.close();
      }
 catch (      IOException e) {
        LOG.info(e);
      }
    }
  }
}
