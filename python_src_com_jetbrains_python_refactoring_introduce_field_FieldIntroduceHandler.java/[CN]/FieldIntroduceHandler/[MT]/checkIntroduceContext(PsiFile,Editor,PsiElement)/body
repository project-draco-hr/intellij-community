{
  if (element != null && isInStaticMethod(element)) {
    CommonRefactoringUtil.showErrorHint(file.getProject(),editor,"Introduce Field refactoring cannot be used in static methods",RefactoringBundle.message("introduce.field.title"),"refactoring.extractMethod");
    return false;
  }
  return super.checkIntroduceContext(file,editor,element);
}
