{
  return new ElementFilter(){
    @Override public boolean isAcceptable(    Object element,    PsiElement context){
      String name=((XmlTag)context).getName();
      if (element instanceof PsiElement && ((PsiElement)element).getParent() == context) {
        return true;
      }
      if (equalNames(name,HtmlUtil.STYLE_TAG_NAME) || equalNames(name,HtmlUtil.SCRIPT_TAG_NAME)) {
        return false;
      }
      if (isStyleAttributeContext((PsiElement)element))       return false;
      return true;
    }
    @Override public boolean isClassAcceptable(    Class hintClass){
      return true;
    }
  }
;
}
