{
  final VirtualFile file=getVirtualFile();
  if (file != null) {
    if (name.equals(file.getNameWithoutExtension())) {
      return this;
    }
 else     if (PyNames.INIT_DOT_PY.equals(file.getName())) {
      final VirtualFile dir=file.getParent();
      if ((dir != null) && name.equals(dir.getName())) {
        return this;
      }
    }
  }
  return null;
}
