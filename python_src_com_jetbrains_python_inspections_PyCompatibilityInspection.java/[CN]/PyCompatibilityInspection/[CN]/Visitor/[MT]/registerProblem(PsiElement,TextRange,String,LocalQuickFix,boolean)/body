{
  if (element.getTextLength() == 0) {
    return;
  }
  range=TextRange.create(range.getStartOffset() - element.getTextOffset(),range.getEndOffset() - element.getTextOffset());
  if (quickFix != null)   myHolder.registerProblem(element,range,message,quickFix);
 else   myHolder.registerProblem(element,range,message);
}
