{
  if (Arrays.asList(getArguments()).contains(element)) {
    ASTNode node=element.getNode();
    ASTNode next=getNextComma(node);
    if (next == null) {
      next=getPrevComma(node);
    }
    ASTNode me=getNode();
    if (next != null) {
      me.removeChild(next);
    }
    me.removeChild(node);
  }
}
