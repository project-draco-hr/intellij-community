{
  boolean res=false;
  if (stat.getExprents() == null) {
    for (; ; ) {
      boolean changed=false;
      for (      Statement st : stat.getStats()) {
        res|=mergeAllIfsRec(st,setReorderedIfs);
        if (changed=IfHelper.mergeIfs(st,setReorderedIfs)) {
          break;
        }
      }
      res|=changed;
      if (!changed) {
        break;
      }
    }
  }
  return res;
}
