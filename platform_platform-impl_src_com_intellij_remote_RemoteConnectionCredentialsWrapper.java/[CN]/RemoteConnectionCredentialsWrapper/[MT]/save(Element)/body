{
  switchType(new RemoteSdkConnectionAcceptor(){
    @Override public void ssh(    RemoteCredentialsHolder cred){
      cred.save(rootElement);
    }
    @Override public void vagrant(    VagrantBasedCredentialsHolder cred){
      cred.save(rootElement);
    }
    @Override public void deployment(    WebDeploymentCredentialsHolder cred){
      cred.save(rootElement);
    }
  }
);
}
