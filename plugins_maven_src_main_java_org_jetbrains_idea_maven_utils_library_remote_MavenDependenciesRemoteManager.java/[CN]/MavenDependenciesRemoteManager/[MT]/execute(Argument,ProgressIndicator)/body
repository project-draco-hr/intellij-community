{
  String coordinates=arg.libraryProperties.getGroupId() + ":" + arg.libraryProperties.getArtifactId()+ ":"+ RepositoryUtils.resolveEffectiveVersion(myProject,arg.libraryProperties);
  return RepositoryAttachHandler.resolveAndDownloadImpl(myProject,coordinates,arg.downloadJavaDocs,arg.downloadSources,arg.copyTo,RepositoryLibraryDescription.findDescription(arg.libraryProperties).getRemoteRepositories(),indicator);
}
