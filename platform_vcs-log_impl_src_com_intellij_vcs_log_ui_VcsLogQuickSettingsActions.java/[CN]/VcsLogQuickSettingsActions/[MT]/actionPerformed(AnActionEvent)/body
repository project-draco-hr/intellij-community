{
  Project project=e.getRequiredData(CommonDataKeys.PROJECT);
  VcsLogUi logUi=e.getRequiredData(VcsLogDataKeys.VCS_LOG_UI);
  VcsLogSettings settings=ServiceManager.getService(project,VcsLogSettings.class);
  JBPopupFactory.getInstance().createActionGroupPopup(null,new MySettingsActionGroup(settings,logUi),e.getDataContext(),JBPopupFactory.ActionSelectionAid.MNEMONICS,true,ToolWindowContentUi.POPUP_PLACE).showUnderneathOf(e.getInputEvent().getComponent());
}
