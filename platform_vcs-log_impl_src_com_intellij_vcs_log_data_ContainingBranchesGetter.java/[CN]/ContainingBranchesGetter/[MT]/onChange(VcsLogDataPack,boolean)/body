{
  if (refreshHappened) {
    Collection<VcsRef> currentBranches=dataPack.getRefs().getBranches();
    int checksum=currentBranches.hashCode();
    if (myCurrentBranchesChecksum != 0 && myCurrentBranchesChecksum != checksum) {
      clearCache();
    }
    myCurrentBranchesChecksum=checksum;
  }
}
