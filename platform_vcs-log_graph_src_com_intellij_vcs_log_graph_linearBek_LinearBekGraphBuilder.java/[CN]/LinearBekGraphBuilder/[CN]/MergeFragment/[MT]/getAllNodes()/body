{
  Set<Integer> nodes=ContainerUtil.newHashSet();
  nodes.add(myParent);
  nodes.add(myFirstChild);
  nodes.add(mySecondChild);
  TIntIterator it=myBlockBody.iterator();
  while (it.hasNext()) {
    nodes.add(it.next());
  }
  it=myTails.iterator();
  while (it.hasNext()) {
    nodes.add(it.next());
  }
  return nodes;
}
