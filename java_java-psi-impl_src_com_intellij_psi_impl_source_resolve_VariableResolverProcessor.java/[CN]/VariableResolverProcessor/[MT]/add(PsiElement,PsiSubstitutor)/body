{
  final boolean staticProblem=myStaticScopeFlag && !((PsiModifierListOwner)element).hasModifierProperty(PsiModifier.STATIC) && (element instanceof PsiField || !(element instanceof PsiVariable && PsiUtil.isCompileTimeConstant((PsiVariable)element)));
  add(new CandidateInfo(element,substitutor,myPlace,myAccessClass,staticProblem,myCurrentFileContext));
}
