{
  for (  StreamProvider streamProvider : myStreamProviders) {
    try {
      if (streamProvider.isEnabled() && streamProvider.isApplicable(fileSpec,roamingType)) {
        streamProvider.saveContent(fileSpec,content,size,roamingType,async);
      }
    }
 catch (    Exception e) {
      LOG.debug(e);
    }
  }
}
