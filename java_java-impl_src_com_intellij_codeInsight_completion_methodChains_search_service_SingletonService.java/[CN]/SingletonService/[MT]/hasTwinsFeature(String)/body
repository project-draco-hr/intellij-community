{
  final List<Integer> twinInfo=myTwinVariablesIndex.getTwinInfo(typeQName);
  if (twinInfo.isEmpty()) {
    return false;
  }
  int ones=0;
  for (  final int i : twinInfo) {
    if (i == 1) {
      ones++;
    }
  }
  return (twinInfo.size() - ones) * Constants.SINGLETON_MAGIC_RATIO < twinInfo.size();
}
