{
  String s1="class Foo {\n" + "  void foo(final int i,final int i2, final int i3) {\n" + "     final int x = 5;\n"+ "  }\n"+ "}";
  String s2="final '_type 'var = '_init?";
  String s3="$type$ $var$ = $init$";
  String expected="class Foo {\n" + "  void foo(int i, int i2, int i3) {\n" + "     int x = 5\n"+ "  }\n"+ "}";
  actualResult=replacer.testReplace(s1,s2,s3,options);
  assertEquals(expected,actualResult);
}
