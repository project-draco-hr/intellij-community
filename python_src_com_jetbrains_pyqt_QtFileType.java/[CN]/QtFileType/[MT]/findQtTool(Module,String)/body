{
  if (SystemInfo.isWindows) {
    if (module == null) {
      return null;
    }
    Sdk sdk=PythonSdkType.findPythonSdk(module);
    if (sdk == null) {
      return null;
    }
    String tool=findToolInPackage(toolName,module,sdk,"PyQt4");
    if (tool != null) {
      return tool;
    }
    return findToolInPackage(toolName,module,sdk,"PySide");
  }
  return null;
}
