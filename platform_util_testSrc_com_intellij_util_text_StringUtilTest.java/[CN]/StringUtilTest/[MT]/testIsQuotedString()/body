{
  assertFalse(StringUtil.isQuotedString(""));
  assertFalse(StringUtil.isQuotedString("'"));
  assertFalse(StringUtil.isQuotedString("\""));
  assertTrue(StringUtil.isQuotedString("\"\""));
  assertTrue(StringUtil.isQuotedString("''"));
  assertTrue(StringUtil.isQuotedString("'ab'"));
  assertTrue(StringUtil.isQuotedString("\"foo\""));
}
