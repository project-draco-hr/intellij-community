{
  final NodeIterator node=new HierarchyNodeIterator(el2,true,true);
  if (handler.isStrictSubtype()) {
    node.advance();
  }
  while (node.hasNext() && !handler.validate(node.current(),0,-1,matchContext)) {
    node.advance();
  }
  if (node.hasNext()) {
    handler.addResult(el2,0,-1,matchContext);
    return true;
  }
 else {
    return false;
  }
}
