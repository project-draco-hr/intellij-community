{
  BufferExposingByteArrayOutputStream out=StorageUtil.newContentIfDiffers(getDocument(loadData(true,null)),getVirtualFile());
  if (out == null) {
    return null;
  }
  File file=new File(myFile.getAbsolutePath());
  FileUtil.writeToFile(file,out.getInternalBuffer(),0,out.size());
  return file;
}
