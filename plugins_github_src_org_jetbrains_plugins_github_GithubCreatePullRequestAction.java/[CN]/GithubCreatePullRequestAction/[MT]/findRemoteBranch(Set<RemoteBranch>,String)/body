{
  if (ref == null) {
    return null;
  }
  List<String> list=StringUtil.split(ref,":");
  if (list.size() != 2) {
    return null;
  }
  for (  RemoteBranch branch : branches) {
    if (StringUtil.equalsIgnoreCase(list.get(0),branch.getUser()) && StringUtil.equals(list.get(1),branch.getBranch())) {
      return branch;
    }
  }
  return null;
}
