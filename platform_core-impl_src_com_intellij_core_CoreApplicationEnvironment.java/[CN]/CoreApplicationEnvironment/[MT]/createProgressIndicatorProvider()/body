{
  return new ProgressIndicatorProvider(){
    @Override public ProgressIndicator getProgressIndicator(){
      return new EmptyProgressIndicator();
    }
    @Override protected void doCheckCanceled() throws ProcessCanceledException {
    }
    @NotNull @Override public NonCancelableSection startNonCancelableSection(){
      return NonCancelableSection.EMPTY;
    }
  }
;
}
