{
  assertNotNull(GIST_ID);
  if (GIST == null) {
    try {
      GIST=GithubGist.create(GithubApiUtil.getGist(myGitHubSettings.getAuthData(),GIST_ID));
    }
 catch (    IOException e) {
      System.err.println(e.getMessage());
    }
  }
  assertNotNull("Gist does not exist",GIST);
  return GIST;
}
