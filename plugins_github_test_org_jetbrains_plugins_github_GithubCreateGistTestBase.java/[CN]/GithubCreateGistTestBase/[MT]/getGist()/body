{
  assertNotNull(GIST_ID);
  if (GIST == null) {
    try {
      GIST=GithubApiUtil.getGist(new GithubConnection(myGitHubSettings.getAuthData()),GIST_ID);
    }
 catch (    IOException e) {
      System.err.println(e.getMessage());
    }
  }
  assertNotNull("Gist does not exist",GIST);
  return GIST;
}
