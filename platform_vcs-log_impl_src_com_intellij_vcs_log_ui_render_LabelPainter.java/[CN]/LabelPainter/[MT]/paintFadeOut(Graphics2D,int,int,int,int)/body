{
  g2.setPaint(new GradientPaint(x,y,new Color(myBackground.getRed(),myBackground.getGreen(),myBackground.getBlue(),0),x + GRADIENT_WIDTH,y,myBackground));
  g2.fill(new Rectangle(x,y,GRADIENT_WIDTH,height));
  x+=GRADIENT_WIDTH;
  g2.setColor(myBackground);
  g2.fillRect(x,y,width - GRADIENT_WIDTH,height);
  return x;
}
