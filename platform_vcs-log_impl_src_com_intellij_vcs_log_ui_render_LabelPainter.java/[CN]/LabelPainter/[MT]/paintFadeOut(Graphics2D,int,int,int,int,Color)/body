{
  g2.setPaint(new GradientPaint(x,y,new Color(background.getRed(),background.getGreen(),background.getBlue(),0),x + GRADIENT_WIDTH,y,background));
  g2.fill(new Rectangle(x,y,GRADIENT_WIDTH,height));
  x+=GRADIENT_WIDTH;
  g2.setColor(background);
  g2.fillRect(x,y,width - GRADIENT_WIDTH,height);
  return x;
}
