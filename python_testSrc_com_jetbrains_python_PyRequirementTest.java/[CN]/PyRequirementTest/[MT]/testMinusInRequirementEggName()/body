{
  assertEquals(new PyRequirement("django-haystack",null,"git://github.com/toastdriven/django-haystack.git",true),PyRequirement.fromString("-e git://github.com/toastdriven/django-haystack.git#egg=django-haystack"));
  assertEquals(new PyRequirement("django-haystack","dev","git://github.com/toastdriven/django-haystack.git",true),PyRequirement.fromString("-e git://github.com/toastdriven/django-haystack.git#egg=django-haystack-dev"));
}
