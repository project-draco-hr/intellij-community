{
  assertEquals(new PyRequirement("Django","1.3.1"),PyRequirement.fromString("Django==1.3.1"));
  assertEquals(new PyRequirement("Django",Collections.singletonList(new PyRequirement.VersionSpec(PyRequirement.Relation.LT,"1.4"))),PyRequirement.fromString("   Django       <   1.4   "));
  assertEquals(new PyRequirement("Django",Arrays.asList(new PyRequirement.VersionSpec(PyRequirement.Relation.LT,"1.4"),new PyRequirement.VersionSpec(PyRequirement.Relation.GTE,"1.3.1"))),PyRequirement.fromString("   Django       <   1.4 ,     >= 1.3.1   "));
}
