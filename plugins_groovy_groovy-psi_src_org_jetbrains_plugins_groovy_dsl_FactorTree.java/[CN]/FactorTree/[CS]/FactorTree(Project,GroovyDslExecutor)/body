{
  myExecutor=executor;
  myProvider=new CachedValueProvider<Map>(){
    @Nullable @Override public Result<Map> compute(){
      return new Result<Map>(ContainerUtil.newConcurrentMap(),PsiModificationTracker.MODIFICATION_COUNT,ProjectRootManager.getInstance(project));
    }
  }
;
  myTopLevelCache=CachedValuesManager.getManager(project).createCachedValue(myProvider,false);
}
