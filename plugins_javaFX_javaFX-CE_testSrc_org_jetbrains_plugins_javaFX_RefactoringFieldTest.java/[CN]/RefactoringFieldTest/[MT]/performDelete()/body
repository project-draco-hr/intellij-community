{
  final PsiElement psiElement=TargetElementUtilBase.findTargetElement(myFixture.getEditor(),TargetElementUtilBase.ELEMENT_NAME_ACCEPTED | TargetElementUtilBase.REFERENCED_ELEMENT_ACCEPTED);
  assertNotNull("No element found in text:\n" + myFixture.getFile().getText(),psiElement);
  SafeDeleteHandler.invoke(getProject(),new PsiElement[]{psiElement},true);
}
