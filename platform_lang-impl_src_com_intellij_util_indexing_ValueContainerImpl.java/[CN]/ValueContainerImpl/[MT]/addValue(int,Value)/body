{
  final Object input=getInput(value);
  if (input == null) {
    attachFileSetForNewValue(value,inputId);
  }
 else {
    final IdSet idSet;
    if (input instanceof Integer) {
      idSet=new IdSet(3);
      idSet.add(((Integer)input).intValue());
      resetFileSetForValue(value,idSet);
    }
 else {
      idSet=(IdSet)input;
    }
    idSet.add(inputId);
  }
}
