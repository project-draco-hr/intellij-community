{
  final RequestFuture<T> future=removeFuture(sessionId);
  if (future != null) {
    final T handler=future.getMessageHandler();
    try {
      if (handler != null) {
        try {
          handler.sessionTerminated();
        }
 catch (        Throwable e) {
          e.printStackTrace();
        }
      }
    }
  finally {
      future.setDone();
    }
  }
}
