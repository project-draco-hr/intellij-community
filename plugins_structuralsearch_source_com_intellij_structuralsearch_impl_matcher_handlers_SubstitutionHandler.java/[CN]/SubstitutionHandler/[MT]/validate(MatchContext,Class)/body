{
  MatchResult substitution=context.getResult().findSon(name);
  if (minOccurs >= 1 && (substitution == null || StructuralSearchUtil.getProfileByFileType(context.getOptions().getFileType()).getElementContextByPsi(substitution.getMatchRef().getElement()) != elementContext)) {
    return false;
  }
 else   if (maxOccurs <= 1 && substitution != null && substitution.hasSons()) {
    return false;
  }
 else   if (maxOccurs == 0 && totalMatchedOccurs != -1) {
    return false;
  }
  return true;
}
