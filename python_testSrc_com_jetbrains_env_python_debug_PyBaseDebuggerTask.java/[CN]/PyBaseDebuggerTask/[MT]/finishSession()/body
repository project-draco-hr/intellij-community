{
  disposeDebugProcess();
  if (mySession != null) {
    new WriteAction(){
      protected void run(      Result result) throws Throwable {
        mySession.stop();
        Disposer.dispose(mySession.getConsoleView());
        mySession=null;
        myDebugProcess=null;
        myPausedSemaphore=null;
      }
    }
.execute();
  }
}
