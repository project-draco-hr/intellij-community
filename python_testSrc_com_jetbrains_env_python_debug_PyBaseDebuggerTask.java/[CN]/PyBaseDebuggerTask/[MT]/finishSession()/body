{
  disposeDebugProcess();
  if (mySession != null) {
    new WriteAction(){
      protected void run(      Result result) throws Throwable {
        mySession.stop();
      }
    }
.execute();
    waitFor(mySession.getDebugProcess().getProcessHandler());
    XDebuggerTestUtil.disposeDebugSession(mySession);
    mySession=null;
    myDebugProcess=null;
    myPausedSemaphore=null;
  }
}
