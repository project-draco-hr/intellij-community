{
  addDirs(entryPath,false);
  ZipEntry entry=new ZipEntry(entryPath);
  if (!myCompressed) {
    entry.setSize(byteOut.size());
    CRC32 crc=new CRC32();
    byteOut.updateChecksum(crc);
    entry.setCrc(crc.getValue());
  }
  myOut.putNextEntry(entry);
  byteOut.writeTo(myOut);
  myOut.closeEntry();
}
