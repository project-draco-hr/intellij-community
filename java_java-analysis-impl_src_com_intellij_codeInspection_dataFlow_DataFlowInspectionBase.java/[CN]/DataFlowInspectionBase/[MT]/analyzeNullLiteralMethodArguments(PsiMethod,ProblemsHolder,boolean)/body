{
  if (REPORT_NULLS_PASSED_TO_NOT_NULL_PARAMETER && isOnTheFly) {
    for (    PsiParameter parameter : NullParameterConstraintChecker.checkMethodParameters(method)) {
      holder.registerProblem(parameter.getNameIdentifier(),InspectionsBundle.message("dataflow.method.fails.with.null.argument"),ProblemHighlightType.GENERIC_ERROR_OR_WARNING,createNavigateToNullParameterUsagesFix(parameter));
    }
  }
}
