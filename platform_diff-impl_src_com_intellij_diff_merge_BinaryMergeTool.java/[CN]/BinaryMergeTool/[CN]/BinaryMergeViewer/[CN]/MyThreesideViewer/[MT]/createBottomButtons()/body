{
  return MergeUtil.createAcceptActionsPanel(new MergeUtil.AcceptActionProcessor(){
    @Override public boolean isEnabled(    @NotNull MergeResult result){
      return true;
    }
    @Override public boolean isVisible(    @NotNull MergeResult result){
      return result != MergeResult.RESOLVED;
    }
    @Override public void perform(    @NotNull MergeResult result){
      markConflictResolved();
      myMergeRequest.applyResult(result);
      myMergeContext.closeDialog();
    }
  }
,myPanel);
}
