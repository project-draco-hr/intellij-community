{
  List<String> result=new ArrayList<String>();
  BufferedReader in=new BufferedReader(new InputStreamReader(url.openStream(),CharsetToolkit.UTF8));
  try {
    String line;
    while ((line=in.readLine()) != null) {
      int i=line.indexOf('#');
      if (i >= 0)       line=line.substring(0,i);
      line=line.trim();
      if (!line.isEmpty()) {
        result.add(line);
      }
    }
  }
  finally {
    in.close();
  }
  return result;
}
