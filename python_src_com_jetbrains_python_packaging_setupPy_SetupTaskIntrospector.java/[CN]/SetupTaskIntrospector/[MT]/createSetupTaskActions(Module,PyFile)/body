{
  List<AnAction> result=new ArrayList<AnAction>();
  try {
    for (    SetupTask task : getTaskList(module,usesSetuptools(setupPyFile))) {
      result.add(new RunSetupTaskAction(task.name,task.name.replace("_","__") + " (" + task.description+ ")"));
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
  return result;
}
