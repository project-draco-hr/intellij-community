{
  List<AnAction> result=new ArrayList<AnAction>();
  try {
    for (    SetupTask task : getTaskList(module,usesSetuptools(setupPyFile))) {
      result.add(new RunSetupTaskAction(task.getName(),task.getName().replace("_","__") + " (" + task.getDescription()+ ")"));
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
  return result;
}
