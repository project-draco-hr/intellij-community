{
  if (getEditor1() != null && getEditor2() != null) {
    return ContainerUtil.list(getEditor1(),getEditor2());
  }
  if (getEditor1() != null) {
    return Collections.singletonList(getEditor1());
  }
  if (getEditor2() != null) {
    return Collections.singletonList(getEditor2());
  }
  return Collections.emptyList();
}
