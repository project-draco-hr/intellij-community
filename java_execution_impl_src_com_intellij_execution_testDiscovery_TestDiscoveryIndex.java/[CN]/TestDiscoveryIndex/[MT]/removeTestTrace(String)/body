{
  myLocalTestRunDataController.withTestDataHolder(new ThrowableConvertor<TestInfoHolder,Void,IOException>(){
    @Override public Void convert(    TestInfoHolder localHolder) throws IOException {
      final int testNameId=localHolder.myTestNameEnumerator.tryEnumerate(testName);
      if (testNameId != 0) {
        localHolder.doUpdateFromDiff(testNameId,null,localHolder.myTestNameToUsedClassesAndMethodMap.get(testNameId),null);
      }
      return null;
    }
  }
);
}
