{
  final String propertyName=myJavaCodeStyleManager.variableNameToPropertyName(name,VariableKind.PARAMETER);
  final ParameterSpec field=new ParameterSpec(variable,myJavaCodeStyleManager.propertyNameToVariableName(propertyName,VariableKind.FIELD),type instanceof PsiEllipsisType ? ((PsiEllipsisType)type).toArrayType() : type,setterRequired);
  fields.add(field);
}
