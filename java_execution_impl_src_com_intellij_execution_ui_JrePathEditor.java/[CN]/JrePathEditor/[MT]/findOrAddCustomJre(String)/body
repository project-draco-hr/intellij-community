{
  for (  JreComboBoxItem item : myComboBoxModel.getItems()) {
    if (item instanceof CustomJreItem && FileUtil.pathsEqual(pathOrName,((CustomJreItem)item).myPath) || item.getPresentableText().equals(pathOrName)) {
      return item;
    }
  }
  CustomJreItem item=new CustomJreItem(pathOrName);
  myComboBoxModel.add(item);
  return item;
}
