{
  if (!(o instanceof Window)) {
    throw new ClassCastException();
  }
  Window window=(Window)o;
  if (window.getTaskFile() != myTaskFile) {
    throw new ClassCastException();
  }
  if (window.getLine() == line && window.getStart() == start) {
    return 0;
  }
  if (window.getLine() == line) {
    if (window.start < start) {
      return 1;
    }
 else {
      return -1;
    }
  }
  if (window.getLine() < line) {
    return 1;
  }
  return -1;
}
