{
  Set<String> result=ContainerUtil.newHashSet(name);
  Pair<String,String> firstAndLastName=getFirstAndLastName(name);
  if (firstAndLastName != null) {
    result.add(firstAndLastName.first + " " + firstAndLastName.second);
    result.add(firstAndLastName.first + "." + firstAndLastName.second);
    result.add(firstAndLastName.first + firstAndLastName.second);
  }
  return result;
}
