{
  BufferedReader input;
  if (reader instanceof BufferedReader) {
    input=(BufferedReader)reader;
  }
 else {
    input=new BufferedReader(reader);
  }
  List<Ref> refs=new ArrayList<Ref>();
  String line;
  while ((line=input.readLine()) != null) {
    Ref ref=parse(line);
    refs.add(ref);
  }
  return refs;
}
