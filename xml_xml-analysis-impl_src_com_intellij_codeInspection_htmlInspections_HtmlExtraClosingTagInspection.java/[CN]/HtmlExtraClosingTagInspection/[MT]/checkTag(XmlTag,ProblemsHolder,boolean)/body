{
  final TextRange range=XmlTagUtil.getEndTagRange(tag);
  if (range != null && tag instanceof HtmlTag && HtmlUtil.isSingleHtmlTag(tag.getName())) {
    holder.registerProblem(tag,XmlErrorMessages.message("extra.closing.tag.for.empty.element"),ProblemHighlightType.LIKE_UNUSED_SYMBOL,range.shiftRight(-tag.getTextRange().getStartOffset()),new RemoveExtraClosingTagIntentionAction());
  }
}
