{
  myId=id;
  myDescription=description;
  myIsPublic=isPublic;
  myHtmlUrl=htmlUrl;
  myFiles=new ArrayList<GistFile>();
  for (  Map.Entry<String,GistFileRaw> rawFile : files.entrySet()) {
    myFiles.add(new GistFile(rawFile.getValue()));
  }
  myUser=user == null ? null : GithubUser.create(user);
}
