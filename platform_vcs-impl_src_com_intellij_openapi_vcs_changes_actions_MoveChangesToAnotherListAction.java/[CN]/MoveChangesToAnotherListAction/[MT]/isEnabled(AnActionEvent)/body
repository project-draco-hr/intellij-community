{
  Project project=e.getData(CommonDataKeys.PROJECT);
  if (project == null || !ProjectLevelVcsManager.getInstance(project).hasActiveVcss()) {
    return false;
  }
  return !ContainerUtil.isEmpty(e.getData(ChangesListView.UNVERSIONED_FILES_DATA_KEY)) || SelectedFilesHelper.hasChangedOrUnversionedFiles(project,e) || !ArrayUtil.isEmpty(e.getData(VcsDataKeys.CHANGES))|| !ArrayUtil.isEmpty(e.getData(CommonDataKeys.VIRTUAL_FILE_ARRAY));
}
