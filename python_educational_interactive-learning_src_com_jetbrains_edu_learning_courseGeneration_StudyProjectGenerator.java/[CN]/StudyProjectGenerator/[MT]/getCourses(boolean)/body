{
  if (myCoursesDir.exists()) {
    File cacheFile=new File(myCoursesDir,CACHE_NAME);
    if (cacheFile.exists()) {
      myCourses=getCoursesFromCache(cacheFile);
    }
  }
  if (force || myCourses.isEmpty()) {
    myCourses=EduStepicConnector.getCourses();
    flushCache();
  }
  return myCourses;
}
