{
  Project project=state.getProject();
  TaskFile taskFile=state.getTaskFile();
  AnswerPlaceholder answerPlaceholder=state.getAnswerPlaceholder();
  EduUtils.runUndoableAction(project,"Delete Answer Placeholder",new AddAction(answerPlaceholder,taskFile,state.getEditor()){
    @Override public void undo() throws UnexpectedUndoException {
      super.redo();
    }
    @Override public void redo() throws UnexpectedUndoException {
      super.undo();
    }
  }
);
}
