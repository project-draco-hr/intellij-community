{
  if (next_pointer >= 0) {
    pointer=next_pointer;
  }
 else {
    while (++pointer < size) {
      int[] index=colValuesInternal.get(pointer);
      if ((data[index[0]] & index[1]) != 0) {
        break;
      }
    }
  }
  next_pointer=-1;
  return pointer < size ? colValuesInternal.getKey(pointer) : null;
}
