{
  for (  String name : listSubFiles(path,roamingType)) {
    if (!filter.value(name)) {
      continue;
    }
    InputStream input;
    try {
      input=loadContent(path + '/' + name,roamingType);
    }
 catch (    IOException e) {
      StorageUtil.LOG.error(e);
      continue;
    }
    if (input != null && !processor.process(name,input)) {
      break;
    }
  }
}
