{
  if (myState == State.NOT_RUNNING && newState != State.RUNNING && newState != State.IGNORED || myState == State.RUNNING && newState != State.FINISHED && newState != State.FAILED && newState != State.IGNORED || myState == State.FINISHED && newState != State.FAILED || myState == State.FAILED && newState != State.FINISHED || myState == State.IGNORED && newState != State.FINISHED) {
    convertor.logProblem("Illegal state change [" + myState + " -> "+ newState+ "]: "+ toString(),false);
  }
  if (myState.ordinal() < newState.ordinal()) {
    myState=newState;
  }
}
