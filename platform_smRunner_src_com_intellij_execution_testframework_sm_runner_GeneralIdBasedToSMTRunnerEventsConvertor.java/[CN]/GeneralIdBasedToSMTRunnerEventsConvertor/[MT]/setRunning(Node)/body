{
  Node node=testNode;
  while (node != null && node.getState() == State.REGISTERED) {
    node.setState(State.RUNNING);
    myRunningNodes.add(node);
    SMTestProxy proxy=node.getProxy();
    proxy.setStarted();
    if (proxy.isSuite()) {
      fireOnSuiteStarted(proxy);
    }
 else {
      fireOnTestStarted(proxy);
    }
    node=node.getParentNode();
  }
}
