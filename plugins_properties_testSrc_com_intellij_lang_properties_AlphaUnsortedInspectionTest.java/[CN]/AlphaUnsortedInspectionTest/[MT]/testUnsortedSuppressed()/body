{
  final ExtensionPoint<AlphaUnsortedPropertiesFileInspectionSuppressor> ep=Extensions.getRootArea().getExtensionPoint(AlphaUnsortedPropertiesFileInspectionSuppressor.EP_NAME);
  final AlphaUnsortedPropertiesFileInspectionSuppressor suppressor=new AlphaUnsortedPropertiesFileInspectionSuppressor(){
    @Override public boolean suppressInspectionFor(    PropertiesFile propertiesFile){
      return propertiesFile.getName().toLowerCase().contains("suppress");
    }
  }
;
  try {
    ep.registerExtension(suppressor);
    doTest();
  }
  finally {
    ep.unregisterExtension(suppressor);
  }
}
