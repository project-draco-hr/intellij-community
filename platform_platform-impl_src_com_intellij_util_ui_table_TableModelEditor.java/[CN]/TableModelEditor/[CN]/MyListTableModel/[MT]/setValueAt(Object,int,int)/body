{
  if (rowIndex < getRowCount()) {
    @SuppressWarnings("unchecked") ColumnInfo<T,Object> column=(ColumnInfo<T,Object>)getColumnInfos()[columnIndex];
    T item=getItem(rowIndex);
    Object oldValue=column.valueOf(item);
    if (column.getColumnClass() == String.class ? !Comparing.strEqual(((String)oldValue),((String)newValue)) : !Comparing.equal(oldValue,newValue)) {
      T mutable;
      if (modifiedToOriginal.containsKey(item)) {
        mutable=item;
      }
 else {
        mutable=editor.cloneFactory.fun(item);
        modifiedToOriginal.put(mutable,item);
        items.set(rowIndex,mutable);
      }
      column.setValue(mutable,newValue);
    }
  }
}
