{
  if (myDelegates == null) {
    try {
      myDelegates=delegates;
      if (myRequestedCancelState != null) {
        for (        TaskFuture delegate : delegates) {
          delegate.cancel(myRequestedCancelState);
        }
      }
    }
  finally {
      notifyAll();
    }
    return true;
  }
  return false;
}
