{
  List<VcsLogProvider> providers=ContainerUtil.filter(Extensions.getExtensions(VcsLogManager.LOG_PROVIDER_EP,project),new Condition<VcsLogProvider>(){
    @Override public boolean value(    VcsLogProvider provider){
      return provider.getSupportedVcs().equals(GitVcs.getKey());
    }
  }
);
  assertEquals("Incorrect number of GitLogProviders",1,providers.size());
  return (GitLogProvider)providers.get(0);
}
