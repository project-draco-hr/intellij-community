{
  myPackagesCache=null;
  final List<String> args=new ArrayList<String>();
  args.add("install");
  if (url != null) {
    args.add("--extra-index-url");
    args.add(url);
  }
  for (  PyRequirement req : requirements) {
    args.add(req.toString());
  }
  if (options != null) {
    args.addAll(options);
  }
  runPythonHelper(PACKAGING_TOOL,args);
}
