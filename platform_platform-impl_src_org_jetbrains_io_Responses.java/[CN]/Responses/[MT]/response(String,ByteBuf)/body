{
  FullHttpResponse response=content == null ? new DefaultFullHttpResponse(HttpVersion.HTTP_1_1,HttpResponseStatus.OK) : new DefaultFullHttpResponse(HttpVersion.HTTP_1_1,HttpResponseStatus.OK,content);
  if (contentType != null) {
    response.headers().set(HttpHeaderNames.CONTENT_TYPE,contentType);
  }
  return response;
}
