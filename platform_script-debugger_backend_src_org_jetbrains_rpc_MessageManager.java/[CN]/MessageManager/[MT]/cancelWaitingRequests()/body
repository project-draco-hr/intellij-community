{
  ConcurrentIntObjectMap<AsyncResultCallback<SUCCESS,ERROR_DETAILS>> map=callbackMap;
  int[] keys=map.keys();
  Arrays.sort(keys);
  for (  int key : keys) {
    try {
      map.get(key).onError("Connection closed",null);
    }
 catch (    Throwable e) {
      LOG.error("Failed to reject callback on connection closed",e);
    }
  }
}
