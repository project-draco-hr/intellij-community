{
  includeMergedRevisions=false;
  List<String> parameters=prepareCommand(path,startRevision,endRevision,pegRevision,stopOnCopy,discoverChangedPaths,includeMergedRevisions,limit);
  try {
    SvnCommand command=CommandUtil.execute(myVcs,SvnTarget.fromFile(path,pegRevision),SvnCommandName.log,parameters,null);
    parseOutput(command,handler);
  }
 catch (  SVNException e) {
    throw new VcsException(e);
  }
}
