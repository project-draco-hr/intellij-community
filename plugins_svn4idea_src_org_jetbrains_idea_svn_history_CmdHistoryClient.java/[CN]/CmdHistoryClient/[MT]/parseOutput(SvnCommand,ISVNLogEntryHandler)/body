{
  try {
    LogInfo log=CommandUtil.parse(command.getOutput(),LogInfo.class);
    if (handler != null && log != null) {
      for (      LogEntry entry : log.entries) {
        handler.handleLogEntry(entry.toLogEntry());
      }
    }
  }
 catch (  JAXBException e) {
    throw new VcsException(e);
  }
}
