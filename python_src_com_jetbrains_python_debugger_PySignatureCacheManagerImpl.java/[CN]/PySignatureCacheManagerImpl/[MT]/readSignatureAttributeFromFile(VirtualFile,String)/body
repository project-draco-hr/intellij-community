{
  String content=readAttribute(file);
  if (content != null) {
    String[] lines=content.split("\n");
    for (    String sign : lines) {
      String[] parts=sign.split("\t");
      if (parts.length > 0 && parts[0].equals(name)) {
        return stringToSignature(file.getCanonicalPath(),sign);
      }
    }
  }
  return null;
}
