{
  PyStringLiteralExpression str=PsiTreeUtil.findElementOfClassAtOffset(context.getFile(),context.getStartOffset(),PyStringLiteralExpression.class,false);
  if (str != null) {
    if (str.getText().startsWith("'") && str.getText().endsWith("'") || str.getText().startsWith("\"") && str.getText().endsWith("\""))     context.setReplacementOffset(context.getStartOffset() + 1);
  }
}
