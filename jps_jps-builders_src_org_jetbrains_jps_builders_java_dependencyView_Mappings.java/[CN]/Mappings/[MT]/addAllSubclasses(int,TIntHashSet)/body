{
  if (!acc.add(root)) {
    return acc;
  }
  final TIntHashSet directSubclasses=myClassToSubclasses.get(root);
  if (directSubclasses != null) {
    directSubclasses.forEach(new TIntProcedure(){
      @Override public boolean execute(      int s){
        addAllSubclasses(s,acc);
        return true;
      }
    }
);
  }
  return acc;
}
