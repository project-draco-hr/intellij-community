{
  final Set<Pair<ClassRepr,File>> deleted=myDeletedClasses;
  if (deleted != null) {
    for (    Pair<ClassRepr,File> pair : deleted) {
      myChangedClasses.remove(pair.first.name);
    }
  }
  for (Runnable pass=myPostPasses.poll(); pass != null; pass=myPostPasses.poll()) {
    pass.run();
  }
}
