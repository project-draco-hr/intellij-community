{
  if (ourUserSkeletonsDirectory == null) {
    for (    String path : getPossibleUserSkeletonsPaths()) {
      ourUserSkeletonsDirectory=LocalFileSystem.getInstance().refreshAndFindFileByPath(path);
      if (ourUserSkeletonsDirectory != null) {
        break;
      }
    }
  }
  return ourUserSkeletonsDirectory;
}
