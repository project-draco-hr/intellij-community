{
  if (lastProcessedLine1 == -1)   return;
  int startLine1=lastProcessedLine1 + 1;
  int startLine2=lastProcessedLine2 + 1;
  int endLine1=myContextRange == -1 ? getLineCount(myDocument1) - 1 : Math.min(lastProcessedLine1 + myContextRange,getLineCount(myDocument1) - 1);
  int endLine2=myContextRange == -1 ? getLineCount(myDocument2) - 1 : Math.min(lastProcessedLine2 + myContextRange,getLineCount(myDocument2) - 1);
  appendTextMaster(startLine1,startLine2,endLine1,endLine2);
  lastProcessedLine1=-1;
  lastProcessedLine2=-1;
}
