{
  Element element=null;
  for (  Object aNode : nodes) {
    if (!XmlSerializerImpl.isIgnoredNode(aNode)) {
      element=(Element)aNode;
      break;
    }
  }
  if (element == null) {
    return context;
  }
  Object instance=ReflectionUtil.newInstance(myBeanClass);
  deserializeInto(instance,element,null);
  return instance;
}
