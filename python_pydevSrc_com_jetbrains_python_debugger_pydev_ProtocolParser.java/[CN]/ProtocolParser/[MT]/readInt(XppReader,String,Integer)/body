{
  final String value;
  try {
    value=read(reader,name);
  }
 catch (  PyDebuggerException e) {
    if (fallback != null) {
      return fallback;
    }
 else {
      throw e;
    }
  }
  try {
    return Integer.parseInt(value);
  }
 catch (  NumberFormatException e) {
    throw new PyDebuggerException("Unable to decode " + value + ": "+ e.getMessage());
  }
}
