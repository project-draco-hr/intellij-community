{
  if (!(element instanceof PsiJavaToken) && (!(element instanceof PsiJavaCodeReferenceElement) || !(element.getParent() instanceof PsiAnnotation))) {
    MatchingHandler handler=pattern.getHandlerSimple(element);
    if (handler instanceof SubstitutionHandler) {
      handler.reset();
    }
  }
  super.visitElement(element);
}
