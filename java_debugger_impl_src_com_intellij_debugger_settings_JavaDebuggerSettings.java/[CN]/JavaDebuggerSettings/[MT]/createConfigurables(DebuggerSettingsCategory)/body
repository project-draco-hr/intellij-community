{
  Getter<DebuggerSettings> settingsGetter=new Getter<DebuggerSettings>(){
    @Override public DebuggerSettings get(){
      return DebuggerSettings.getInstance();
    }
  }
;
switch (category) {
case GENERAL:
    return singletonList(SimpleConfigurable.create("reference.idesettings.debugger.launching",OptionsBundle.message("options.java.display.name"),DebuggerLaunchingConfigurable.class,settingsGetter));
case DATA_VIEWS:
  return createDataViewsConfigurable();
case STEPPING:
return singletonList(SimpleConfigurable.create("reference.idesettings.debugger.stepping",OptionsBundle.message("options.java.display.name"),DebuggerSteppingConfigurable.class,settingsGetter));
case HOTSWAP:
return singletonList(SimpleConfigurable.create("reference.idesettings.debugger.hotswap",OptionsBundle.message("options.java.display.name"),JavaHotSwapConfigurableUi.class,settingsGetter));
}
return Collections.emptyList();
}
