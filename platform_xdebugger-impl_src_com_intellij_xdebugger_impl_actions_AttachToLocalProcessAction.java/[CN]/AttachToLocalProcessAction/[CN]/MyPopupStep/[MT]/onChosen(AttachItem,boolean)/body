{
  if (finalChoice) {
    selectedValue.startDebugSession(myProject);
    return FINAL_CHOICE;
  }
  return new BaseListPopupStep<AttachItem>(XDebuggerBundle.message("xdebugger.attach.toLocal.popup.selectDebugger.title"),selectedValue.getSubItems()){
    @Override public boolean isSpeedSearchEnabled(){
      return true;
    }
    @NotNull @Override public String getTextFor(    AttachItem value){
      return value.getSelectedDebugger().getDebuggerDisplayName();
    }
    @Override public PopupStep onChosen(    AttachItem selectedValue,    boolean finalChoice){
      selectedValue.startDebugSession(myProject);
      return FINAL_CHOICE;
    }
  }
;
}
