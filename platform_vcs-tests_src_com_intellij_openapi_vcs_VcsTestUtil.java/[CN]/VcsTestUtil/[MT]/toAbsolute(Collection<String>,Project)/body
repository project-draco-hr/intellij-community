{
  return ContainerUtil.map2List(relPaths,new Function<String,String>(){
    @Override public String fun(    String s){
      try {
        return FileUtil.toSystemIndependentName((new File(project.getBasePath() + "/" + s).getCanonicalPath()));
      }
 catch (      IOException e) {
        e.printStackTrace();
        return "";
      }
    }
  }
);
}
