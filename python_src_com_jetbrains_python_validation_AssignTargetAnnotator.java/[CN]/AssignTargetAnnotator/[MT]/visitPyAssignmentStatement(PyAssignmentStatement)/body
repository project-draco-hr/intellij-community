{
  ExprVisitor visitor=new ExprVisitor(Operation.Assign);
  boolean found=false;
  for (  PyElement expr : node.iterateNames()) {
    expr.accept(visitor);
    found=true;
  }
  if (!found) {
    PsiElement lhs=node.getFirstChild();
    if (lhs instanceof PyElement) {
      lhs.accept(visitor);
    }
  }
}
