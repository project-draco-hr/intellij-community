{
  final int minusOffset=StringUtil.lastIndexOf(myBuffer,'-',myStartOffset,offset);
  if (minusOffset == -1) {
    return false;
  }
  final int eolPos=findEol(minusOffset);
  if (eolPos == -1) {
    return false;
  }
  return StringUtil.isEmptyOrSpaces(myBuffer.subSequence(minusOffset + 1,eolPos));
}
