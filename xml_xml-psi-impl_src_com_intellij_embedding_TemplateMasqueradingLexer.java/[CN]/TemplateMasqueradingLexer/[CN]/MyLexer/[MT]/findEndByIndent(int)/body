{
  while (offset < myEndOffset) {
    if (offset > myStartOffset && !StringUtil.isWhiteSpace(myBuffer.charAt(offset)) && StringUtil.isWhiteSpace(myBuffer.charAt(offset - 1))) {
      int indent=calcIndent(offset);
      if (indent != -1 && indent <= myIndent) {
        return offset;
      }
      offset=findEol(offset);
    }
 else {
      offset++;
    }
  }
  return offset;
}
