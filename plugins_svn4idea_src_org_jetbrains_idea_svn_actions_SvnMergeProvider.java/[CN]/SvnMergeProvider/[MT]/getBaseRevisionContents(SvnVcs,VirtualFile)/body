{
  ByteArrayOutputStream bos=new ByteArrayOutputStream();
  try {
    byte[] contents=SvnUtil.getFileContents(vcs,SvnTarget.fromFile(new File(file.getPath())),SVNRevision.BASE,SVNRevision.UNDEFINED);
    bos.write(contents);
  }
 catch (  VcsException e) {
    LOG.warn(e);
  }
catch (  IOException e) {
    LOG.warn(e);
  }
  return bos;
}
