{
  ByteArrayOutputStream bos=new ByteArrayOutputStream();
  try {
    byte[] contents=SvnUtil.getFileContents(vcs,file.getPath(),false,SVNRevision.BASE,SVNRevision.UNDEFINED);
    bos.write(contents);
  }
 catch (  VcsException e) {
    LOG.warn(e);
  }
catch (  IOException e) {
    LOG.warn(e);
  }
  return bos;
}
