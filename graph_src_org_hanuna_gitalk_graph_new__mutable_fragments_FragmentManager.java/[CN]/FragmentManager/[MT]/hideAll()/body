{
  int rowIndex=0;
  while (rowIndex < graph.getNodeRows().size()) {
    Node node=commitNodeInRow(rowIndex);
    if (node != null) {
      NewGraphFragment fragment=fragmentGenerator.getMaximumDownFragment(node);
      if (fragment != null) {
        hide(fragment);
      }
    }
    rowIndex++;
    if (rowIndex % 100 == 0) {
      System.out.println(rowIndex);
    }
  }
}
