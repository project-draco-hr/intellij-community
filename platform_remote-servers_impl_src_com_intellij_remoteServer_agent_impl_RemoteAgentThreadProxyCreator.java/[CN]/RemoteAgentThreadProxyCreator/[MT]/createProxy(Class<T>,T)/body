{
  ClassLoader callerClassLoader=myCallerClassLoaderProvider.getCallerClassLoader(agentInterface);
  return agentInterface.cast(Proxy.newProxyInstance(callerClassLoader,new Class[]{agentInterface},new ThreadInvocationHandler(SequentialTaskExecutor.createSequentialApplicationPoolExecutor("RemoteAgentThreadProxyCreator pool"),callerClassLoader,agentInstance,myPreWrapperCreator)));
}
