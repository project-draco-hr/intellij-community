{
  final PsiJavaCodeReferenceElement reference=(PsiJavaCodeReferenceElement)TargetElementUtil.findReference(editor);
  LOG.assertTrue(reference != null);
  String jarPath=PathUtil.getJarPathForClass(Test.class);
  final VirtualFile virtualFile=file.getVirtualFile();
  LOG.assertTrue(virtualFile != null);
  OrderEntryFix.addJarsToRootsAndImportClass(Collections.singletonList(jarPath),null,ModuleUtilCore.findModuleForFile(virtualFile,project),editor,reference,"org.testng.annotations." + reference.getReferenceName());
}
