{
  if (isNegation(condition)) {
    PyExpression negated=getNegated(condition);
    if (negated == null) {
      return "";
    }
    return negated.getText();
  }
 else {
    if (condition instanceof PyBinaryExpression)     return "not(" + condition.getText() + ")";
    return "not " + condition.getText();
  }
}
