{
  for (  PyClass c : cls.getAncestorClasses()) {
    final String name=c.getName();
    final String className=cls.getName();
    if (!OBJECT.equals(name) && !FAKE_OLD_BASE.equals(name) && className != null && !className.equals(name) && c.findMethodByName(INIT,false) != null) {
      return true;
    }
  }
  return false;
}
