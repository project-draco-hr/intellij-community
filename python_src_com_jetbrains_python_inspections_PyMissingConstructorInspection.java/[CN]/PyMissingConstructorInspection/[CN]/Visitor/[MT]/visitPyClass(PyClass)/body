{
  PsiElement[] superClasses=node.getSuperClassExpressions();
  if (superClasses.length == 0 || (superClasses.length == 1 && PyNames.OBJECT.equals(superClasses[0].getText())))   return;
  if (!superHasConstructor(node))   return;
  PyFunction initMethod=node.findMethodByName(PyNames.INIT,false);
  if (initMethod != null) {
    if (hasConstructorCall(node,initMethod))     return;
    if (superClasses.length == 1)     registerProblem(initMethod.getNameIdentifier(),"Call to constructor of super class is missed",new AddCallSuperQuickFix(node.getSuperClasses()[0],superClasses[0].getText()));
 else     registerProblem(initMethod.getNameIdentifier(),"Call to constructor of super class is missed");
  }
}
