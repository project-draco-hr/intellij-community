{
  final String className=cls.getName();
  for (  PyClass baseClass : cls.getAncestorClasses(context)) {
    if (!PyUtil.isObjectClass(baseClass) && !Comparing.equal(className,baseClass.getName()) && baseClass.findMethodByName(INIT,false,context) != null) {
      return true;
    }
  }
  return false;
}
