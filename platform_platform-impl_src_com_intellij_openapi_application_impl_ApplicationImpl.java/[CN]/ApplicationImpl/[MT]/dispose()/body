{
  fireApplicationExiting();
  ShutDownTracker.getInstance().ensureStopperThreadsFinished();
  disposeComponents();
  ourThreadExecutorsService.shutdownNow();
  super.dispose();
  Disposer.dispose(myLastDisposable);
  if (LOG.isDebugEnabled()) {
    final int[] sum={0};
    writePauses.forEach(new TIntProcedure(){
      @Override public boolean execute(      int value){
        sum[0]+=value;
        return true;
      }
    }
);
    LOG.debug("write action Statistics:\n" + "\nTotal write actions: " + writePauses.size() + "\nTotal write pauses : "+ sum[0]+ "ms"+ "\nAverage write pause: "+ sum[0] / writePauses.size() + "ms" + "\nMedian  write pause: " + ArrayUtil.averageAmongMedians(writePauses.toNativeArray(),3) + "ms");
  }
}
