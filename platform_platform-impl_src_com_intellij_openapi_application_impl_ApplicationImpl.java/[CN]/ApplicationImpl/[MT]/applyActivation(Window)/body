{
  if (!isActive()) {
    myDeactivationCancelled=true;
    myActive=true;
    myDeactivating=false;
    IdeFrame ideFrame=getIdeFrameFromWindow(window);
    if (ideFrame != null) {
      getMessageBus().syncPublisher(ApplicationActivationListener.TOPIC).applicationActivated(ideFrame);
    }
  }
  return false;
}
