{
  Status status=getStatus();
  boolean mustAcquire=!isReadAccessAllowed(status);
  if (mustAcquire) {
    assertNoPsiLock();
    if ((status.stamp=myLock.tryReadLock()) == 0)     return false;
  }
  try {
    action.run();
  }
  finally {
    if (mustAcquire) {
      endRead(status);
    }
  }
  return true;
}
