{
  return new ContentRootPanel(this,getEditHandlers()){
    @Nullable @Override protected ContentEntry getContentEntry(){
      return JavaContentEntryEditor.this.getContentEntry();
    }
    @Nullable @Override protected JComponent createRootPropertiesEditor(    ModuleSourceRootEditHandler<?> editor,    SourceFolder folder){
      return editor.createPropertiesEditor(folder,this,myCallback);
    }
  }
;
}
