{
  if (parameters.getInvocationCount() >= INVOCATIONS_THRESHOLD && ClassFilesIndexFeaturesHolder.getInstance(parameters.getPosition().getProject()).enableFeatureIfNeed(ClassFilesIndexFeature.METHOD_CHAINS_COMPLETION)) {
    super.fillCompletionVariants(parameters,result);
    if (ApplicationManager.getApplication().isUnitTestMode()) {
      result.stopHere();
    }
  }
}
