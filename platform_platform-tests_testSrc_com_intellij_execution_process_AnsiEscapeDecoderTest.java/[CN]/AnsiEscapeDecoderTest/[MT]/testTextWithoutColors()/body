{
  AnsiEscapeDecoder decoder=new AnsiEscapeDecoder();
  decoder.escapeText("",ProcessOutputTypes.STDOUT,createExpectedAcceptor(Pair.create("",ProcessOutputTypes.STDOUT)));
  decoder.escapeText("simple text",ProcessOutputTypes.STDOUT,createExpectedAcceptor(Pair.create("simple text",ProcessOutputTypes.STDOUT)));
}
