{
  AnsiEscapeDecoder decoder=new AnsiEscapeDecoder();
  decoder.escapeText("Plain\u001B[32mGreen\u001B[39mNormal\u001B[1A\u001B[2K\u001B[31mRed\u001B[39m",ProcessOutputTypes.STDOUT,createExpectedAcceptor(Pair.create("Plain",ProcessOutputTypes.STDOUT),Pair.create("Green",ColoredOutputTypeRegistry.getInstance().getOutputKey("\u001B[32m")),Pair.create("Normal",ColoredOutputTypeRegistry.getInstance().getOutputKey("\u001B[39m")),Pair.create("Red",ColoredOutputTypeRegistry.getInstance().getOutputKey("\u001B[31m"))));
}
