{
  AnsiEscapeDecoder decoder=new AnsiEscapeDecoder();
  decoder.escapeText("E\u001B[41m\u001B[37mE\u001B[0mE",ProcessOutputTypes.STDOUT,createExpectedAcceptor(ContainerUtil.newArrayList(Pair.create("E",ProcessOutputTypes.STDOUT),Pair.create("E",ColoredOutputTypeRegistry.getInstance().getOutputKey("\u001B[41;37m")),Pair.create("E",ProcessOutputTypes.STDOUT))));
}
