{
  CommandProcessor.getInstance().executeCommand(myClassUnderRefactoring.getProject(),new Runnable(){
    @Override public void run(){
      ApplicationManager.getApplication().runWriteAction(new Runnable(){
        @Override public void run(){
          refactorNoPreview();
        }
      }
);
    }
  }
,getCommandName(),null);
  myView.close();
}
