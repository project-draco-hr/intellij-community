{
  super(project,sourceClass,collectMembers(sourceClass),ExtractInterfaceHandler.REFACTORING_NAME);
  for (  MemberInfo memberInfo : myMemberInfos) {
    final PsiMember member=memberInfo.getMember();
    if (member instanceof PsiMethod && (member.hasModifierProperty(PsiModifier.STATIC) || member.hasModifierProperty(PsiModifier.PRIVATE))) {
      continue;
    }
    memberInfo.setToAbstract(true);
  }
  init();
}
