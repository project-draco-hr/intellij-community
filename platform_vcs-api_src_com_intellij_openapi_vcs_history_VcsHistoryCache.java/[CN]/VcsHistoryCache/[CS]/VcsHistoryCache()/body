{
  myLock=new Object();
  boolean preloadEnabled=Registry.is("vcs.annotations.preload");
  myHistoryCache=new SLRUMap<HistoryCacheBaseKey,CachedHistory>(preloadEnabled ? 50 : 10,preloadEnabled ? 50 : 10);
  myAnnotationCache=new SLRUMap<HistoryCacheWithRevisionKey,VcsAnnotation>(preloadEnabled ? 50 : 10,preloadEnabled ? 50 : 5);
}
