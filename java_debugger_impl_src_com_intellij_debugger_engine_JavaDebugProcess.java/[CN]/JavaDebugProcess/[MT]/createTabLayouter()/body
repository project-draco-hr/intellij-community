{
  return new XDebugTabLayouter(){
    @Override public void registerAdditionalContent(    @NotNull RunnerLayoutUi ui){
      ThreadsPanel panel=new ThreadsPanel(myJavaSession.getProject(),myStateManager);
      Content threadsContent=ui.createContent(DebuggerContentInfo.THREADS_CONTENT,panel,XDebuggerBundle.message("debugger.session.tab.threads.title"),AllIcons.Debugger.Threads,null);
      threadsContent.setCloseable(false);
      ui.addContent(threadsContent,0,PlaceInGrid.left,true);
    }
  }
;
}
