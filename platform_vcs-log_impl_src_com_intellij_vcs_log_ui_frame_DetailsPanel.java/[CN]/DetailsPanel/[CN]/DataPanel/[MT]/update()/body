{
  if (myMainText == null) {
    setText("");
  }
 else {
    setText("<html><head>" + UIUtil.getCssFontDeclaration(UIUtil.getLabelFont()) + "</head><body>"+ myMainText+ "<br/>"+ "<br/>"+ "<i>Contained in branches:</i> "+ (myBranchesText == null ? "<i>loading...</i>" : myBranchesText)+ "</body></html>");
  }
  revalidate();
  repaint();
}
