{
  if (branches.size() <= BRANCHES_LIMIT) {
    return StringUtil.join(branches,", ");
  }
 else   if (expanded) {
    return StringUtil.join(branches,", ") + " <a href=\"" + SHOW_OR_HIDE_BRANCHES+ "\"><i>Hide</i></a>";
  }
 else {
    return StringUtil.join(ContainerUtil.getFirstItems(branches,BRANCHES_LIMIT),", ") + ", ... <a href=\"" + SHOW_OR_HIDE_BRANCHES+ "\"><i>Show All</i></a>";
  }
}
