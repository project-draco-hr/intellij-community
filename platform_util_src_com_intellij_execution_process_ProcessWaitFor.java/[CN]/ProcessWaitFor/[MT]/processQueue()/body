{
  for (  Process process : ourQueue.keySet()) {
    try {
      int value=process.exitValue();
      Consumer<Integer> callback=ourQueue.remove(process);
      if (callback != null) {
        callback.consume(value);
      }
    }
 catch (    IllegalThreadStateException ignore) {
    }
catch (    RuntimeException e) {
      LOG.debug(e);
    }
  }
}
