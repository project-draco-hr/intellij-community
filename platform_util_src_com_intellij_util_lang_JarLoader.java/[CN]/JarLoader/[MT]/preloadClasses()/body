{
  try {
    ZipFile zipFile=acquireZipFile();
    try {
      JarMemoryLoader loader=JarMemoryLoader.load(zipFile,getBaseURL());
      if (loader != null) {
        myMemoryLoader=new SoftReference<JarMemoryLoader>(loader);
      }
    }
  finally {
      releaseZipFile(zipFile);
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
