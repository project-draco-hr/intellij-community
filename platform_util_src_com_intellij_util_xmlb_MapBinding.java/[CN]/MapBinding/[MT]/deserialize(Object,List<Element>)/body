{
  Map map=(Map)context;
  if (map != null) {
    map.clear();
  }
  for (  Element childNode : childNodes) {
    if (!childNode.getName().equals(getEntryAttributeName())) {
      LOG.warn("unexpected entry for serialized Map will be skipped: " + childNode);
      continue;
    }
    if (map == null) {
      map=new THashMap();
    }
    map.put(deserializeKeyOrValue(childNode,getKeyAttributeName(),context,keyBinding,keyClass),deserializeKeyOrValue(childNode,getValueAttributeName(),context,valueBinding,valueClass));
  }
  return map;
}
