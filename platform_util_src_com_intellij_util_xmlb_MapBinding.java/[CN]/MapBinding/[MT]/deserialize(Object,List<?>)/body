{
  Map map=(Map)context;
  map.clear();
  for (  Object childNode : childNodes) {
    if (XmlSerializerImpl.isIgnoredNode(childNode)) {
      continue;
    }
    Element entry=(Element)childNode;
    if (!entry.getName().equals(getEntryAttributeName())) {
      LOG.warn("unexpected entry for serialized Map will be skipped: " + entry);
      continue;
    }
    map.put(deserializeKeyOrValue(entry,getKeyAttributeName(),context,keyBinding,keyClass),deserializeKeyOrValue(entry,getValueAttributeName(),context,valueBinding,valueClass));
  }
  return map;
}
