{
  List<TestInfo> noSuites=ContainerUtil.newSmartList();
  for (  TestInfo test : myTestsWithoutSuites) {
    String url=test.getUrl();
    SuiteInfo suite=getSuite(url);
    if (suite != null) {
      suite.addTest(test);
    }
 else {
      noSuites.add(test);
    }
  }
  myTestsWithoutSuites=noSuites;
}
