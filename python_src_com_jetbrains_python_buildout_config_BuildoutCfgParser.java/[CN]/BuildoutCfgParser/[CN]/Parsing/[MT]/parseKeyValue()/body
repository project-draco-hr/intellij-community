{
  if (is(KEY_CHARACTERS)) {
    doneAdvance(mark(),KEY_CHARACTERS);
  }
  if (is(KEY_VALUE_SEPARATOR)) {
    advance();
  }
 else {
    error(": or = expected.");
  }
  PsiBuilder.Marker value=mark();
  boolean done=false;
  while (is(MULTILINE_VALUE_CHARACTERS)) {
    advance();
    done=true;
  }
  if (!done && is(VALUE_CHARACTERS)) {
    advance();
    done=true;
  }
  if (done) {
    value.done(VALUE);
  }
 else {
    error("Value expected.");
    value.drop();
  }
}
