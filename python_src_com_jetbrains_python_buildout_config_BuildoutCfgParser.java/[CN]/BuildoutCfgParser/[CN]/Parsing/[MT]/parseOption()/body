{
  PsiBuilder.Marker option=myBuilder.mark();
  if (is(KEY_CHARACTERS)) {
    doneAdvance(mark(),KEY_CHARACTERS);
  }
  if (is(KEY_VALUE_SEPARATOR)) {
    advance();
  }
 else {
    error(": or = expected.");
  }
  PsiBuilder.Marker value=mark();
  while (is(VALUE_CHARACTERS)) {
    doneAdvance(mark(),VALUE_LINE);
  }
  value.done(VALUE);
  option.done(OPTION);
}
