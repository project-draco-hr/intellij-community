{
  if (!(fileType instanceof PythonFileType))   return Result.CONTINUE;
  if (character == ':') {
    Result res=getOverTypeResult(editor,file);
    if (res == Result.STOP) {
      return res;
    }
    PyUnindentingInsertHandler.unindentAsNeeded(project,editor,file);
  }
  return Result.CONTINUE;
}
