{
  if (infos.length == 1 && infos[0] instanceof String) {
switch ((String)infos[0]) {
case DO_TEXT:
      return new DoBracesFix();
case ELSE_TEXT:
    return new ElseBracesFix();
case FOR_TEXT:
  return new ForBracesFix();
case IF_TEXT:
return new IfBracesFix();
case WHILE_TEXT:
return new WhileBracesFix();
}
}
return null;
}
