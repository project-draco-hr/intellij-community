{
  CatchAllStatement cas=new CatchAllStatement();
  cas.isFinally=this.isFinally;
  if (this.monitor != null) {
    cas.monitor=new VarExprent(DecompilerContext.getCountercontainer().getCounterAndIncrement(CounterContainer.VAR_COUNTER),VarType.VARTYPE_INT,(VarProcessor)DecompilerContext.getProperty(DecompilerContext.CURRENT_VAR_PROCESSOR));
  }
  if (!this.vars.isEmpty()) {
    vars.add(new VarExprent(DecompilerContext.getCountercontainer().getCounterAndIncrement(CounterContainer.VAR_COUNTER),new VarType(CodeConstants.TYPE_OBJECT,0,"java/lang/Throwable"),(VarProcessor)DecompilerContext.getProperty(DecompilerContext.CURRENT_VAR_PROCESSOR)));
  }
  return cas;
}
