{
  final VirtualFile file=domFileElement.getFile().getVirtualFile();
  if (!(file instanceof VirtualFileWithId))   return false;
  final String clazzName=clazz.getName();
  final int virtualFileId=((VirtualFileWithId)file).getId();
  CommonProcessors.FindFirstProcessor<String> processor=new CommonProcessors.FindFirstProcessor<String>(){
    @Override protected boolean accept(    String s){
      return clazzName.equals(s);
    }
  }
;
  StubIndex.getInstance().processAllKeys(KEY,processor,GlobalSearchScope.fileScope(domFileElement.getFile()),new IdFilter(){
    @Override public boolean containsFileId(    int id){
      return id == virtualFileId;
    }
  }
);
  return processor.isFound();
}
