{
  final PsiElement psi=node.getPsi();
  if (psi instanceof XmlAttribute && "src".equalsIgnoreCase(((XmlAttribute)psi).getName())) {
    return "data:";
  }
  if (psi instanceof XmlTag || psi instanceof XmlComment || psi instanceof XmlAttribute|| psi instanceof XmlConditionalSection)   return "...";
  if (isEntity(psi)) {
    final String value=getEntityPlaceholder(psi);
    if (value != null)     return value;
  }
  return null;
}
