{
  if (myDebugProcess != null) {
    myDebugProcess.stop();
    KillableColoredProcessHandler h=(KillableColoredProcessHandler)myDebugProcess.getProcessHandler();
    if (!h.isProcessTerminated()) {
      h.killProcess();
      if (!waitFor(h)) {
        new Throwable("Cannot stop debugger process").printStackTrace();
      }
    }
  }
}
