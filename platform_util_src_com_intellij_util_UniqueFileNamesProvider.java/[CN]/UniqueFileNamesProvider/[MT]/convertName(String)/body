{
  if (StringUtil.isEmpty(s)) {
    return "_";
  }
  StringBuilder buf=new StringBuilder();
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (Character.isJavaIdentifierPart(c) || c == ' ') {
      buf.append(c);
    }
 else {
      buf.append('_');
    }
  }
  return buf.toString();
}
