{
  if (!LanguageLevel.forElement(anchor).isAtLeast(LanguageLevel.PYTHON33)) {
    return false;
  }
  while (destination != null && destination != root) {
    if (destination.findFile(PyNames.INIT_DOT_PY) != null) {
      return false;
    }
    destination=destination.getParent();
  }
  return true;
}
