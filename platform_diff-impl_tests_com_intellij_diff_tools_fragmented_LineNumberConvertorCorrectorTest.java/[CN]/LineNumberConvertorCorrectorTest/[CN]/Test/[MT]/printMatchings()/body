{
  for (int i=0; i < myLength * 2; i++) {
    int value=myConvertor.convert1(i);
    if (value != -1)     System.out.println("L: " + i + " - "+ value);
  }
  for (int i=0; i < myLength * 2; i++) {
    int value=myConvertor.convert2(i);
    if (value != -1)     System.out.println("R: " + i + " - "+ value);
  }
}
