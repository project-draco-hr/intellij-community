{
  try {
    myMap.processKeysWithExistingMapping(new Processor<Integer>(){
      @Override public boolean process(      Integer key){
        try {
          final V value=myMap.get(key);
          return proc.execute(key,value);
        }
 catch (        IOException e) {
          throw new BuildDataCorruptedException(e);
        }
      }
    }
);
  }
 catch (  IOException e) {
    throw new BuildDataCorruptedException(e);
  }
}
