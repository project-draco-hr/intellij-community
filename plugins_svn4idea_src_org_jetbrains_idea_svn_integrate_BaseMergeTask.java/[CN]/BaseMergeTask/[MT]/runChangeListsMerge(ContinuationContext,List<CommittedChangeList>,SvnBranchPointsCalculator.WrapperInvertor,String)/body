{
  MergerFactory factory=new ChangeListsMergerFactory(lists){
    @Override public IMerger createMerger(    SvnVcs vcs,    File target,    UpdateEventHandler handler,    SVNURL currentBranchUrl,    String branchName){
      return new Merger(vcs,lists,target,handler,currentBranchUrl,branchName,false,false,true);
    }
  }
;
  context.next(new LocalChangesPromptTask(myMergeContext,myInteraction,false,lists,copyPoint),new MergeTask(myMergeContext,myInteraction,factory,title));
}
