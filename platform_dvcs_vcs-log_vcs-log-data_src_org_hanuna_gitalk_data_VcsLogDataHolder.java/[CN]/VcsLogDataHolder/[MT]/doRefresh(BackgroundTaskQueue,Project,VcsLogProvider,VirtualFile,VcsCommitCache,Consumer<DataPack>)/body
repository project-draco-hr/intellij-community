{
  queue.run(new Task.Backgroundable(project,"Loading history...",false){
    public void run(    @NotNull final ProgressIndicator indicator){
      try {
        final DataPack dataPack=DataPack.build(logProvider.readNextBlock(root),logProvider.readAllRefs(root),indicator,commitCache,logProvider,root);
        UIUtil.invokeAndWaitIfNeeded(new Runnable(){
          @Override public void run(){
            dataPackConsumer.consume(dataPack);
          }
        }
);
      }
 catch (      VcsException e) {
        throw new RuntimeException(e);
      }
    }
  }
);
}
