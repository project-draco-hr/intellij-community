{
  try {
    if (!dataBuffers.isEmpty()) {
      dataBuffers.forEachEntry(new TIntObjectProcedure<ByteBuf>(){
        @Override public boolean execute(        int a,        ByteBuf buffer){
          try {
            buffer.release();
          }
 catch (          Throwable e) {
            LOG.error(e);
          }
          return true;
        }
      }
);
      dataBuffers.clear();
    }
  }
  finally {
    super.channelInactive(context);
  }
}
