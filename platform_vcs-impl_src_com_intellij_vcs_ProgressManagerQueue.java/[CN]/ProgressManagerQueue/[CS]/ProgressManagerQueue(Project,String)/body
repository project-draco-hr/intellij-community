{
  myProject=project;
  myLock=new Object();
  myQueue=new ArrayDeque<Runnable>();
  myActive=false;
  myQueueWorker=new MyWorker();
  myProgressManager=ProgressManager.getInstance();
  myQueuePollTask=new Task.Backgroundable(project,title){
