{
  VirtualFile[] files=e.getData(CommonDataKeys.VIRTUAL_FILE_ARRAY);
  String message=files.length == 1 ? FileUtil.toSystemDependentName(files[0].getPath()) : files.length + " selected files";
  final int rc=Messages.showOkCancelDialog(e.getData(CommonDataKeys.PROJECT),getPromptText(message),"Mark as Excluded",Messages.getQuestionIcon());
  if (rc != Messages.OK) {
    return;
  }
  super.actionPerformed(e);
}
