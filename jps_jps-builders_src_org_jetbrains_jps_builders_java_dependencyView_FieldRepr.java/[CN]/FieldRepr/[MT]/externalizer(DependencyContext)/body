{
  return new DataExternalizer<FieldRepr>(){
    @Override public void save(    @NotNull final DataOutput out,    final FieldRepr value) throws IOException {
      value.save(out);
    }
    @Override public FieldRepr read(    @NotNull final DataInput in) throws IOException {
      return new FieldRepr(context,in);
    }
  }
;
}
