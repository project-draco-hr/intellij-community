{
  final LayeredLexer lexer=new LayeredLexer(new SimpleStateSteppingLexer());
  lexer.registerSelfStoppingLayer(new SimpleStateSteppingLexer(),new IElementType[]{TokenType.CODE_FRAGMENT},IElementType.EMPTY_ARRAY);
  lexer.start("1234567890123#1234567890123#1234567890123");
  assertLexerSequence(lexer,"1","2","3","4","5","6","7","8","9","0","1","2","3","#","1","2","3","4","5","6","7","8","9","0","1","2","3","#","1","2","3","4","5","6","7","8","9","0","1","2","3");
}
