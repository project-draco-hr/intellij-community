{
  if (project.isDisposed() || ApplicationManager.getApplication().isUnitTestMode()) {
    return true;
  }
  if (file != null) {
    DslErrorReporter.getInstance().invokeDslErrorPopup(e,project,file);
  }
 else {
    LOG.info("Error when executing internal GDSL " + myPath,e);
    GdslUtil.stopGdsl();
  }
  return false;
}
