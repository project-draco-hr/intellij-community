{
  if (!file.exists()) {
    LOG.info("does not exist: " + file);
    return;
  }
  try {
    file=file.getAbsoluteFile();
    File parent=file.getParentFile();
    if (parent != null) {
      doOpen(parent,file);
    }
 else {
      doOpen(file,null);
    }
  }
 catch (  Exception e) {
    LOG.warn(e);
  }
}
