{
  if (!TextAttributes.class.equals(type)) {
    return super.read(type,element);
  }
  TextAttributes attributes=new TextAttributes();
  if (element != null) {
    attributes.setAttributes(readChild(Color.class,element,FOREGROUND),readChild(Color.class,element,BACKGROUND),readChild(Color.class,element,EFFECT_COLOR),readChild(Color.class,element,ERROR_STRIPE),Effect.read(this,element),FontStyle.read(this,element));
  }
  return (T)attributes;
}
