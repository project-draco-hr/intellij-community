{
  for (  StatEdge edge : stat.getAllSuccessorEdges()) {
    if (loop.containsStatement(edge.getDestination()) && !block.containsStatement(edge.getDestination())) {
      return false;
    }
  }
  for (  Statement st : stat.getStats()) {
    if (!isExternStatement(loop,block,st)) {
      return false;
    }
  }
  return true;
}
