{
  Application app=ApplicationManager.getApplication();
  if (app.isDispatchThread()) {
    myStatusBar.updateWidget(this.ID());
  }
 else {
    app.invokeLater(new Runnable(){
      public void run(){
        myStatusBar.updateWidget(IdeaServerStatusBarComponent.this.ID());
      }
    }
);
  }
}
