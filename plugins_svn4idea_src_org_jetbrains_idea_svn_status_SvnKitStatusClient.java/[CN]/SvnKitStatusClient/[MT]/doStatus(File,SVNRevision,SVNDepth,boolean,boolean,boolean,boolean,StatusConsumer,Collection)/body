{
  try {
    return getStatusClient().doStatus(path,revision,depth,remote,reportAll,includeIgnored,collectParentExternals,new ISVNStatusHandler(){
      @Override public void handleStatus(      SVNStatus status) throws SVNException {
        handler.consume(Status.create(status));
      }
    }
,changeLists);
  }
 catch (  SVNException e) {
    throw new SvnBindException(e);
  }
}
