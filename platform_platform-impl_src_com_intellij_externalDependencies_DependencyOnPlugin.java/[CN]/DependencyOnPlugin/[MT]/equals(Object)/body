{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  DependencyOnPlugin plugin=(DependencyOnPlugin)o;
  return myPluginId.equals(plugin.myPluginId) && Comparing.equal(myMinVersion,plugin.myMinVersion) && Comparing.equal(myMaxVersion,plugin.myMaxVersion);
}
