{
  if (addMain + addRun == 0) {
    return methods;
  }
  PsiMethod[] result=new PsiMethod[methods.length + addMain + addRun];
  if (addMain == 1)   result[0]=myMainMethod;
  if (addRun == 1)   result[addMain]=myRunMethod;
  System.arraycopy(methods,0,result,addMain + addRun,methods.length);
  return result;
}
