{
  File[] files=directory.listFiles((FilenameFilter)new GlobFilenameFilter("*" + ProjectFileType.DOT_DEFAULT_EXTENSION));
  if (files != null && files.length > 0) {
    int rc=Messages.showYesNoDialog(project,VcsBundle.message("checkout.open.project.prompt",getProductNameWithArticle(),files[0].getPath()),VcsBundle.message("checkout.title"),Messages.getQuestionIcon());
    if (rc == Messages.YES) {
      ProjectUtil.openProject(files[0].getPath(),project,false);
    }
    return true;
  }
  return false;
}
