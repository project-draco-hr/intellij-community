{
  final Set<FilePath> result=ContainerUtil.newHashSet();
  File rootFile=root.getIOFile();
  myVcs.getFactory(rootFile).createStatusClient().doStatus(rootFile,SVNRevision.UNDEFINED,Depth.INFINITY,true,false,false,false,new StatusConsumer(){
    @Override public void consume(    Status status) throws SVNException {
      File file=status.getFile();
      boolean changedOnServer=isNotNone(status.getRemoteContentsStatus()) || isNotNone(status.getRemoteNodeStatus()) || isNotNone(status.getRemotePropertiesStatus());
      if (file != null && changedOnServer) {
        result.add(VcsUtil.getFilePath(file,file.isDirectory()));
      }
    }
  }
,null);
  return result;
}
