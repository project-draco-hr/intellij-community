{
  super(project,changeLists,changes,initialListSelection,capableOfExcludingChanges,highlightProblems,inclusionListener,ChangesBrowser.MyUseCase.LOCAL_CHANGES,null,Object.class);
  myRebuildListListener=rebuildListListener;
  myChangeListChooser=new ChangeListChooser(changeLists);
  myHeaderPanel.add(myChangeListChooser,BorderLayout.EAST);
  ChangeListManager.getInstance(myProject).addChangeListListener(myChangeListListener);
  setupRebuildListForActions();
  rebuildList();
}
