{
  int result=0;
  if (myShowUnversioned) {
    ChangesBrowserNode<?> node=findUnversionedFilesNode();
    if (node != null) {
      result=node instanceof ChangesBrowserManyUnversionedFilesNode ? ((ChangesBrowserManyUnversionedFilesNode)node).getUnversionedSize() : node.getAllFilesUnder().size();
    }
  }
  return result;
}
