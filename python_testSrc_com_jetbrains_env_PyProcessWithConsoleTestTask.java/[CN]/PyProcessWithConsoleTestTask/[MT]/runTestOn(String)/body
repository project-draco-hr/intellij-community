{
  longRunningThreadCreated(ApplicationManager.getApplication(),"Periodic tasks thread","ApplicationImpl pooled thread ",ProcessIOExecutorService.POOLED_THREAD_PREFIX);
  createTempSdk(sdkHome,myRequiredSdkType);
  prepare();
  final T runner=createProcessRunner();
  do {
    executeRunner(sdkHome,runner);
  }
 while (runner.shouldRunAgain());
  Disposer.dispose(runner);
}
