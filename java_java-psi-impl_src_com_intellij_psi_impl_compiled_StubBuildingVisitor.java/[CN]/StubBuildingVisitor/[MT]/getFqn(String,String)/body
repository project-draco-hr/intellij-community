{
  String fqn;
  String shortName;
  if (myShortName != null && internalName.endsWith(myShortName)) {
    shortName=myShortName;
    fqn=internalName.length() == shortName.length() ? shortName : getClassName(internalName.substring(0,internalName.length() - shortName.length() - 1)) + "." + shortName;
  }
 else {
    fqn=getClassName(internalName);
    shortName=PsiNameHelper.getShortClassName(fqn);
  }
  return fqn;
}
