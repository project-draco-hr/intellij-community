{
  myLogData=logData;
  myColorManager=colorManager;
  myDataPack=dataPack;
  setLayout(new BoxLayout(this,BoxLayout.Y_AXIS));
  setOpaque(false);
  myReferencesPanel=new ReferencesPanel(myColorManager);
  myDataPanel=new DataPanel(myLogData.getProject(),myLogData.isMultiRoot());
