{
  String result=getUserResource(url,StringUtil.notNullize(version,DEFAULT_VERSION));
  if (result == null) {
    XMLCatalogManager manager=getCatalogManager();
    if (manager != null) {
      result=manager.resolve(url);
    }
    if (result == null) {
      result=getStdResource(url,version);
      if (result == null) {
        return url;
      }
    }
  }
  return result;
}
