{
  super.update(e);
  if (e.getProject() == null) {
    e.getPresentation().setEnabledAndVisible(false);
    return;
  }
  if (getTabbedContent(getContentManager(e.getProject())) == null) {
    e.getPresentation().setEnabledAndVisible(false);
    return;
  }
  e.getPresentation().setEnabledAndVisible(true);
}
