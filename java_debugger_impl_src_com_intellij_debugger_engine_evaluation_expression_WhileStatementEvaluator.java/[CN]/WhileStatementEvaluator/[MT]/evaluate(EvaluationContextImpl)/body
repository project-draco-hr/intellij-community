{
  Object value;
  while (true) {
    value=myConditionEvaluator.evaluate(context);
    if (!(value instanceof BooleanValue)) {
      throw EvaluateExceptionUtil.BOOLEAN_EXPECTED;
    }
 else {
      if (!((BooleanValue)value).booleanValue()) {
        break;
      }
    }
    if (body(context))     break;
  }
  return value;
}
