{
  final PsiMethod method=value.getMethod();
  final String methodLabel=value.getMethodLabel();
  final String methodRender=PsiFormatUtil.formatMethod(method,PsiSubstitutor.EMPTY,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_PARAMETERS,PsiFormatUtil.SHOW_TYPE,999);
  return methodLabel != null ? methodLabel + methodRender : methodRender;
}
