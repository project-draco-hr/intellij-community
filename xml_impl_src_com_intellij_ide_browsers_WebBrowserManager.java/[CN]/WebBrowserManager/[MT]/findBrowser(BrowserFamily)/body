{
  for (  ConfigurableWebBrowser browser : browsers) {
    if (browser.isActive() && family.equals(browser.getFamily())) {
      return browser;
    }
  }
  for (  ConfigurableWebBrowser browser : browsers) {
    if (family.equals(browser.getFamily())) {
      return browser;
    }
  }
  return null;
}
