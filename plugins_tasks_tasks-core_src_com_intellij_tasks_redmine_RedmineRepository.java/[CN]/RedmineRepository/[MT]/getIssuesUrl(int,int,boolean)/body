{
  URIBuilder builder=new URIBuilder(getRestApiUrl("issues.json")).addParameter("offset",String.valueOf(offset)).addParameter("limit",String.valueOf(limit)).addParameter("status_id",withClosed ? "*" : "open").addParameter("assigned_to_id","me");
  if (isUseApiKeyAuthentication()) {
    builder.addParameter("key",myAPIKey);
  }
  return builder.build();
}
