{
  Configuration config;
  String name=_name;
  while ((config=findConfigurationByName(configurations,name)) != null) {
    int i=Messages.showYesNoDialog(project,SSRBundle.message("overwrite.message"),SSRBundle.message("overwrite.title"),AllIcons.General.QuestionDialog);
    if (i == Messages.YES) {
      configurations.remove(config);
      break;
    }
    name=showSaveTemplateAsDialog(name,project);
    if (name == null)     break;
  }
  return name;
}
