{
  PsiElement body=LambdaUtil.extractSingleExpressionFromBody(lambda.getBody());
  final PsiExpression candidate=new LambdaCanBeMethodReferenceInspection().canBeMethodReferenceProblem(body,lambda.getParameterList().getParameters(),lambda.getFunctionalInterfaceType(),lambda);
  return tryConvertToMethodReference(lambda,candidate);
}
