{
  if (collectedInfo == null)   return null;
  final String pep8Path=PythonHelpersLocator.getHelperPath("pep8.py");
  ProcessOutput output=PySdkUtil.getProcessOutput(new File(collectedInfo.interpreterPath).getParent(),new String[]{collectedInfo.interpreterPath,pep8Path,"-"},null,5000,collectedInfo.fileText.getBytes());
  if (output.getStderrLines().isEmpty()) {
    for (    String line : output.getStdoutLines()) {
      final Problem problem=parseProblem(line);
      if (problem != null) {
        collectedInfo.problems.add(problem);
      }
    }
  }
  return collectedInfo;
}
