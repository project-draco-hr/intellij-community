{
  try {
    mySocket=myServerSocket.accept();
  }
  finally {
  }
  try {
    final DebuggerReader reader=new DebuggerReader();
    ApplicationManager.getApplication().executeOnPooledThread(reader);
  }
 catch (  Exception e) {
synchronized (this) {
      mySocket.close();
    }
    throw e;
  }
  myConnected=true;
}
