{
  logFrame(frame,true);
  try {
    final byte[] packed=frame.pack();
    final OutputStream os=mySocket.getOutputStream();
    os.write(packed);
    os.flush();
  }
 catch (  SocketException se) {
    disconnect();
    fireCommunicationError();
  }
catch (  IOException e) {
    LOG.error(e);
  }
}
