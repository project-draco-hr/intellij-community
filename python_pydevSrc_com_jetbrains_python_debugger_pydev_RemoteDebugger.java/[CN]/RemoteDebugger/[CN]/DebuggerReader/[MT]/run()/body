{
  final BufferedReader reader=new BufferedReader(new InputStreamReader(myInputStream,Charset.forName("UTF-8")));
  try {
    String line;
    while ((line=reader.readLine()) != null) {
      processResponse(line);
    }
  }
 catch (  SocketException ignore) {
    fireCloseEvent();
  }
catch (  Exception e) {
    LOG.error(e);
  }
 finally {
    closeReader(reader);
  }
}
