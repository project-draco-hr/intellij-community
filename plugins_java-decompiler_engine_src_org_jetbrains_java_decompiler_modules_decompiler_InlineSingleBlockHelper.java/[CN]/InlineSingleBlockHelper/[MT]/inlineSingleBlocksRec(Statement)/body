{
  boolean res=false;
  for (  Statement st : stat.getStats()) {
    res|=inlineSingleBlocksRec(st);
  }
  if (stat.type == Statement.TYPE_SEQUENCE) {
    SequenceStatement seq=(SequenceStatement)stat;
    for (int i=1; i < seq.getStats().size(); i++) {
      if (isInlineable(seq,i)) {
        inlineBlock(seq,i);
        return true;
      }
    }
  }
  return res;
}
