{
  final PyDecoratorStub stub=getStub();
  if (stub != null) {
    return stub.getName();
  }
 else {
    ASTNode node=getNode().findChildByType(PyElementTypes.REFERENCE_EXPRESSION);
    if (node != null) {
      node=node.findChildByType(PyTokenTypes.IDENTIFIER);
      if (node != null)       return node.getText();
    }
    return null;
  }
}
