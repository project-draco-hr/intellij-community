{
  if (PsiType.CHAR.equals(type) || PsiType.BYTE.equals(type) || PsiType.SHORT.equals(type)|| PsiType.INT.equals(type)) {
    return true;
  }
  final String text=type.getCanonicalText();
  return CommonClassNames.JAVA_LANG_CHARACTER.equals(text) || CommonClassNames.JAVA_LANG_BYTE.equals(text) || CommonClassNames.JAVA_LANG_SHORT.equals(text)|| CommonClassNames.JAVA_LANG_INTEGER.equals(text);
}
