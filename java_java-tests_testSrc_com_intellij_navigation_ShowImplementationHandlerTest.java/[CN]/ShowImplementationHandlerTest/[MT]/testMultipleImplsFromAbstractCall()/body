{
  PsiFile file=myFixture.addFileToProject("Foo.java","public abstract class Hello {" + "    {" + "        Runnable r = () <caret>-> {};\n"+ "    }\n"+ "}\n"+ "\n");
  myFixture.configureFromExistingVirtualFile(file.getVirtualFile());
  final PsiElement element=TargetElementUtilBase.findTargetElement(myFixture.getEditor(),TargetElementUtilBase.getInstance().getAllAccepted());
  assertTrue(element instanceof PsiClass);
  final String qualifiedName=((PsiClass)element).getQualifiedName();
  assertEquals(CommonClassNames.JAVA_LANG_RUNNABLE,qualifiedName);
}
