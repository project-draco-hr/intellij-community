{
  myEditor=new MyTableEditor(myProject,new PyDebuggerEditorsProvider(),"numpy.array.table.view",null,new XExpressionImpl(value.toString(),PythonLanguage.getInstance(),"",EvaluationMode.CODE_FRAGMENT));
  lastValue=value;
  JComponent editorComponent=myEditor.getComponent();
  editorComponent.getInputMap(JComponent.WHEN_ANCESTOR_OF_FOCUSED_COMPONENT).put(KeyStroke.getKeyStroke(KeyEvent.VK_ENTER,0),"strokeEnter");
  editorComponent.getActionMap().put("strokeEnter",new AbstractAction(){
    @Override public void actionPerformed(    ActionEvent e){
      doOKAction(rowIndex,vColIndex);
    }
  }
);
  editorComponent.getInputMap(JComponent.WHEN_ANCESTOR_OF_FOCUSED_COMPONENT).put(KeyStroke.getKeyStroke(KeyEvent.VK_ESCAPE,0),"escapeStroke");
  editorComponent.getActionMap().put("escapeStroke",new AbstractAction(){
    @Override public void actionPerformed(    ActionEvent e){
      cancelEditing();
    }
  }
);
  return editorComponent;
}
