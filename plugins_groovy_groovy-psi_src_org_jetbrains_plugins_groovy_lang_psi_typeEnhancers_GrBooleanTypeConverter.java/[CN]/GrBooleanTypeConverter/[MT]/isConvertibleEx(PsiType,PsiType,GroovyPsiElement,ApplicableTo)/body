{
  if (!PsiType.BOOLEAN.equals(TypesUtil.unboxPrimitiveTypeWrapper(targetType)))   return null;
  if (PsiType.NULL == actualType) {
switch (currentPosition) {
case METHOD_PARAMETER:
      return null;
case EXPLICIT_CAST:
case ASSIGNMENT:
case RETURN_VALUE:
    return ConversionResult.OK;
default :
  return null;
}
}
return currentPosition == ApplicableTo.ASSIGNMENT || currentPosition == ApplicableTo.RETURN_VALUE ? ConversionResult.OK : null;
}
