{
  final PsiElement commentContainer=PsiTreeUtil.getParentOfType(target,PyAssignmentStatement.class,PyWithStatement.class,PyForPart.class);
  if (commentContainer != null) {
    final PsiComment comment=getSameLineTrailingCommentChild(commentContainer);
    if (comment != null) {
      final String text=comment.getText();
      return getTypeCommentValue(text);
    }
  }
  return null;
}
