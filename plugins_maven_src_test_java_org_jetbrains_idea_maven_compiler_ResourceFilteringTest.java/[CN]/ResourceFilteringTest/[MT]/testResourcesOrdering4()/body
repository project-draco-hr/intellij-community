{
  createProjectSubFile("resources1/a.txt","1");
  createProjectSubFile("resources2/a.txt","2");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ ""+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>resources1</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "    <resource>"+ "      <directory>resources2</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertResult("target/classes/a.txt","2");
}
