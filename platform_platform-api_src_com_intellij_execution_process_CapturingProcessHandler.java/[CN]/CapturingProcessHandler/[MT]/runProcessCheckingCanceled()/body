{
  startNotify();
  while (!waitFor(20)) {
    try {
      ProgressManager.checkCanceled();
    }
 catch (    ProcessCanceledException e) {
      throw e;
    }
  }
  if (waitFor()) {
    myOutput.setExitCode(getProcess().exitValue());
  }
 else {
    LOG.info("runProcess: exit value unavailable");
  }
  return myOutput;
}
