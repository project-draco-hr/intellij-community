{
  try {
    return doResolveProjectInfo(new ProjectResolverContext(myId,myProjectPath,mySettings,connection,myListener,myIsPreviewMode),myProjectResolverChain,myIsBuildSrcProject);
  }
 catch (  RuntimeException e) {
    LOG.info("Gradle project resolve error",e);
    throw myProjectResolverChain.getUserFriendlyError(e,myProjectPath,null);
  }
}
