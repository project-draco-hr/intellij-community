{
  super.writeSettings(node);
  if (ignoreStaticMethods) {
    node.addContent(new Element("option").setAttribute("name","ignoreStaticMethods").setAttribute("value",String.valueOf(ignoreStaticMethods)));
  }
  if (ignoreGetterCalls) {
    node.addContent(new Element("option").setAttribute("name","ignoreGetterCalls").setAttribute("value",String.valueOf(ignoreGetterCalls)));
  }
}
