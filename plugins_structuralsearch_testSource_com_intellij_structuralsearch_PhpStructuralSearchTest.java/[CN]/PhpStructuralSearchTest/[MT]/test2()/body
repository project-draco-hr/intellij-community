{
  String s="<?php\n" + "$this->_buckets = $bu;";
  doTest(s,"$a$->$b$ = $c$;",1);
  doTest(s,"$a$->_buckets = $bu;",1);
  doTest(s,"$a$->_buckets = $c$;",1);
  doTest(s,"$a$->_buckets = $bu1;",0);
  doTest(s,"$this->_buckets = $bu;",1);
  doTest(s,"$this->_buckets = $c$;",1);
  doTest(s,"$this->$b$ = $c$;",1);
  doTest(s,"$this->_buckets = $bu1;",0);
}
