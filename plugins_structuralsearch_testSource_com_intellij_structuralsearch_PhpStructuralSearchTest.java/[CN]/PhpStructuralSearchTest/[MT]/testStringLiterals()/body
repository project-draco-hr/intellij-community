{
  String s="<?php\n" + "$s1 = \'aba caba';\n" + "$s2 = 'aba';\n"+ "$s3 = \"aba caba\";\n"+ "$s4 = \"aba\"";
  doTest(s,"$v$ = '$s$'",2,2,false);
  doTest(s,"$v$ = '$s1$ $s2$'",1,1,false);
  doTest(s,"$v$ = \"$s$\"",2,2,false);
  doTest(s,"$v$ = \"$s1$ $s2$\"",1,1,false);
  doTest(s,"'$s$'",2,2,false);
  doTest(s,"'$s1$ $s2$'",1,1,false);
  doTest(s,"\"$s$\"",2,2,false);
  doTest(s,"\"$s1$ $s2$\"",1,1,false);
}
