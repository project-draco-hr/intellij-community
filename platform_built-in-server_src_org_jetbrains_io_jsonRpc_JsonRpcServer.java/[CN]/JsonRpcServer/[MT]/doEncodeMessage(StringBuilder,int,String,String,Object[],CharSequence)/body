{
  sb.append('[');
  boolean hasPrev=false;
  if (id != -1) {
    sb.append(id);
    hasPrev=true;
  }
  if (domain != null) {
    if (hasPrev) {
      sb.append(',');
    }
    sb.append('"').append(domain).append("\",\"");
    if (command == null) {
      if (rawData != null) {
        sb.append(rawData);
      }
      sb.append('"');
      return;
    }
 else {
      sb.append(command).append('"');
    }
  }
  encodeParameters(sb,params,rawData);
  sb.append(']');
}
