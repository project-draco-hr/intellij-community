{
  myActions.put(HighlightMode.BY_WORD,new SetHighlightModeAction(DiffBundle.message("diff.acton.highlight.mode.action.by.word"),HighlightMode.BY_WORD));
  myActions.put(HighlightMode.BY_LINE,new SetHighlightModeAction(DiffBundle.message("diff.acton.highlight.mode.action.by.line"),HighlightMode.BY_LINE));
  myActions.put(HighlightMode.NO_HIGHLIGHTING,new SetHighlightModeAction(DiffBundle.message("diff.acton.highlight.mode.action.no.highlighting"),HighlightMode.NO_HIGHLIGHTING));
}
