{
  if (info.statementContext) {
    int index=offset + info.startIndex;
    if (result.charAt(index) == ';' && ((result.charAt(index - 1) == '}' && !(match.getMatch() instanceof PsiDeclarationStatement)) || (match == null))) {
      result.deleteCharAt(index);
      --offset;
    }
  }
  return offset;
}
