{
  if (info.myElement == null) {
    return;
  }
  String name=((PsiParameter)info.myElement.getParent()).getName();
  name=stripTypedVariableDecoration(name);
  for (Iterator<MatchResult> i=map.get(name).getAllSons().iterator(), j=map.get(info.name).getAllSons().iterator(); i.hasNext() && j.hasNext(); ) {
    if (buf.length() > 0) {
      buf.append(',');
    }
    buf.append(j.next().getMatchImage());
    buf.append(' ');
    buf.append(i.next().getMatchImage());
  }
}
