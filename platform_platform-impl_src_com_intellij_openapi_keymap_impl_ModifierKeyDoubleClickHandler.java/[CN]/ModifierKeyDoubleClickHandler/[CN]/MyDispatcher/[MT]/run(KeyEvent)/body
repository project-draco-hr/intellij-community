{
  final ActionManager actionManager=ActionManager.getInstance();
  final AnAction action=actionManager.getAction(myActionId);
  final AnActionEvent anActionEvent=new AnActionEvent(event,DataManager.getInstance().getDataContext(IdeFocusManager.findInstance().getFocusOwner()),ActionPlaces.MAIN_MENU,action.getTemplatePresentation(),actionManager,0);
  action.actionPerformed(anActionEvent);
}
