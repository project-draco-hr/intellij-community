{
  int i=0;
  for (  TextAttributesKey rainbowKey : RainbowHighlighter.RAINBOW_COLOR_KEYS) {
    Pair<Boolean,Color> pair=myRainbowState.get(i);
    scheme.setAttributes(rainbowKey,pair.first ? new TextAttributes(pair.second,null,null,null,Font.PLAIN) : rainbowKey.getDefaultAttributes());
    ++i;
  }
}
