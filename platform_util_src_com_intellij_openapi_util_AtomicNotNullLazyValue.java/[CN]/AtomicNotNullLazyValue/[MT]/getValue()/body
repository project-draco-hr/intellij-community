{
  T value=myValue;
  if (value != null) {
    return value;
  }
synchronized (this) {
    value=myValue;
    if (value == null) {
      RecursionGuard.StackStamp stamp=ourGuard.markStack();
      value=compute();
      if (stamp.mayCacheNow()) {
        myValue=value;
      }
    }
  }
  return value;
}
