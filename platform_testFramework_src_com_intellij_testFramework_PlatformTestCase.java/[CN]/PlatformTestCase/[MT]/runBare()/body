{
  if (!shouldRunTest())   return;
  replaceIdeEventQueueSafely();
  try {
    runBareImpl();
  }
  finally {
    try {
      SwingUtilities.invokeAndWait(new Runnable(){
        @Override public void run(){
          cleanupApplicationCaches(getProject());
          resetAllFields();
        }
      }
);
    }
 catch (    Throwable e) {
    }
  }
}
