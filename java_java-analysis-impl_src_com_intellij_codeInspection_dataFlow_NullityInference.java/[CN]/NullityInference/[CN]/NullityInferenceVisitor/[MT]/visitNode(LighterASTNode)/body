{
  IElementType type=element.getTokenType();
  if (type == TokenType.ERROR_ELEMENT) {
    hasErrors=true;
  }
 else   if (type == RETURN_STATEMENT) {
    LighterASTNode value=JavaLightTreeUtil.findExpressionChild(tree,element);
    if (value == null) {
      hasErrors=true;
    }
 else {
      visitReturnedValue(value);
    }
  }
}
