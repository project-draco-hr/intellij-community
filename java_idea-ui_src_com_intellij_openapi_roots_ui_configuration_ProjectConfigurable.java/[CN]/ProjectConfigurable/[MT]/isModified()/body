{
  LanguageLevelProjectExtension extension=LanguageLevelProjectExtension.getInstance(myProject);
  if (extension.isDefault() != myLanguageLevelCombo.isDefault() || !extension.isDefault() && !extension.getLanguageLevel().equals(myLanguageLevelCombo.getSelectedLevel())) {
    return true;
  }
  final String compilerOutput=getOriginalCompilerOutputUrl();
  if (!Comparing.strEqual(FileUtil.toSystemIndependentName(VfsUtilCore.urlToPath(compilerOutput)),FileUtil.toSystemIndependentName(myProjectCompilerOutput.getText())))   return true;
  if (myProjectJdkConfigurable.isModified())   return true;
  if (!getProjectName().equals(myProject.getName()))   return true;
  return false;
}
