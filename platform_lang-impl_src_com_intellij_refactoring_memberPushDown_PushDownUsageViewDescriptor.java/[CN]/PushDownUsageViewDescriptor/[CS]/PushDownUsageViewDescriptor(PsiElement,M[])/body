{
  myMembers=memberInfos != null ? ContainerUtil.map(memberInfos,new Function<M,PsiElement>(){
    @Override public PsiElement fun(    M info){
      return info.getMember();
    }
  }
,PsiElement.EMPTY_ARRAY) : new PsiElement[]{aClass};
  myProcessedElementsHeader=RefactoringBundle.message("push.down.members.elements.header",memberInfos != null ? DescriptiveNameUtil.getDescriptiveName(aClass) : "");
}
