{
  String sourceRelativePath=SVNPathUtil.getRelativePath(myRepositoryRelativeSourcePath,SvnUtil.ensureStartSlash(repositoryRelativePath));
  SvnMergeInfoCache.MergeCheckResult result;
  if (sourceRelativePath == null) {
    result=SvnMergeInfoCache.MergeCheckResult.NOT_EXISTS;
  }
 else {
    InfoProcessor processor=new InfoProcessor(sourceRelativePath,myRepositoryRelativeSourcePath,revisionNumber);
synchronized (myLock) {
      myMergeInfoMap.getSimiliar(toKey(sourceRelativePath),processor);
    }
    result=SvnMergeInfoCache.MergeCheckResult.getInstance(processor.isMerged());
  }
  return result;
}
