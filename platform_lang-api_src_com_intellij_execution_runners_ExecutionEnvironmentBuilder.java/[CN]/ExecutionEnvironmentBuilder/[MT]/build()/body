{
  if (myRunner == null && myRunnerId == null) {
    myRunner=RunnerRegistry.getInstance().getRunner(myExecutor.getId(),myRunProfile);
  }
  ExecutionEnvironment environment=new ExecutionEnvironment(myRunProfile,myExecutor,myTarget,myProject,myRunnerSettings,myConfigurationSettings,myContentToReuse,myRunnerAndConfigurationSettings,myRunnerId,myRunner);
  if (myAssignNewId) {
    environment.assignNewExecutionId();
  }
  if (myDataContext != null) {
    environment.setDataContext(myDataContext);
  }
  return environment;
}
