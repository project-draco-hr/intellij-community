{
  int dividersTotalWidth=DIVIDER_WIDTH * 2;
  int contentsTotalWidth=Math.max(width - dividersTotalWidth,0);
  int[] contentWidths=new int[5];
  contentWidths[1]=DIVIDER_WIDTH;
  contentWidths[3]=DIVIDER_WIDTH;
  contentWidths[0]=(int)(contentsTotalWidth * proportion1);
  contentWidths[4]=(int)(contentsTotalWidth * proportion2);
  contentWidths[2]=Math.max(contentsTotalWidth - contentWidths[0] - contentWidths[4],0);
  return contentWidths;
}
