{
  StringBuilder desc=new StringBuilder();
  desc.append("<html>" + "<table>" + "<tr>"+ "<td nowrap>"+ "<table>"+ "<tr>"+ "<td nowrap>Project:</td>"+ "<td nowrap>").append(myMavenProject.getMavenId()).append("</td>" + "</tr>" + "<tr>"+ "<td nowrap>Location:</td>"+ "<td nowrap>").append(myMavenProject.getPath()).append("</td>" + "</tr>" + "</table>"+ "</td>"+ "</tr>");
