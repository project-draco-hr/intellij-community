{
  return new ObjectPattern.Capture<E>(new InitialPatternCondition(Object.class){
    @Override public boolean accepts(    @Nullable final Object o,    final ProcessingContext context){
      for (      final InitialPatternCondition pattern : initial) {
        if (!pattern.accepts(o,context))         return false;
      }
      return true;
    }
    @Override public void append(    @NotNull @NonNls final StringBuilder builder,    final String indent){
      boolean first=true;
      for (      final InitialPatternCondition pattern : initial) {
        if (!first) {
          builder.append("\n").append(indent);
        }
        first=false;
        pattern.append(builder,indent + "  ");
      }
    }
  }
);
}
