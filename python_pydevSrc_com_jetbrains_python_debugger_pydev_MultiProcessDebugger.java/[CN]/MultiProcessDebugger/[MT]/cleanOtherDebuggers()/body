{
  boolean allConnected=true;
  for (  RemoteDebugger d : myOtherDebuggers) {
    if (!d.isConnected()) {
      allConnected=false;
    }
  }
  if (!allConnected) {
    List<RemoteDebugger> newList=Lists.newArrayList();
    for (    RemoteDebugger d : myOtherDebuggers) {
      if (d.isConnected()) {
        newList.add(d);
      }
    }
    myOtherDebuggers=newList;
  }
}
