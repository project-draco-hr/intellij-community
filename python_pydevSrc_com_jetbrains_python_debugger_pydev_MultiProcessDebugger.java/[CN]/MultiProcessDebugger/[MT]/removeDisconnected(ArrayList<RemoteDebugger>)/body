{
  boolean allConnected=true;
  for (  RemoteDebugger d : debuggers) {
    if (!d.isConnected()) {
      allConnected=false;
    }
  }
  if (!allConnected) {
    List<RemoteDebugger> newList=Lists.newArrayList();
    for (    RemoteDebugger d : debuggers) {
      if (d.isConnected()) {
        newList.add(d);
      }
    }
synchronized (myOtherDebuggers) {
      myOtherDebuggers.clear();
      myOtherDebuggers.addAll(newList);
    }
  }
}
