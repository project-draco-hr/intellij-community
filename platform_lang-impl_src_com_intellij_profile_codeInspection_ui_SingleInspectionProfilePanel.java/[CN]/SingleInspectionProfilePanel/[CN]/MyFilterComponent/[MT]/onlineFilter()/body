{
  if (mySelectedProfile == null)   return;
  final String filter=getFilter();
  getExpandedNodes(mySelectedProfile).saveVisibleState(myTree);
  fillTreeData(filter,true);
  reloadModel();
  if (filter == null || filter.length() == 0) {
    restoreTreeState();
  }
 else {
    TreeUtil.expandAll(myTree);
  }
}
