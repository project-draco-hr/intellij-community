{
  File parent=root;
  List<File> parents=new ArrayList<File>();
  while (parent != null) {
    if (notUnderExcludedCache.contains(parent)) {
      return false;
    }
    if (excluded.contains(parent)) {
      return true;
    }
    parents.add(parent);
    parent=parent.getParentFile();
  }
  notUnderExcludedCache.addAll(parents);
  return false;
}
