{
  String fullString=(outerClass == null ? "" : outerClass.getName() + ".") + "this::" + psiMethod.getName();
  return LookupElementBuilder.create(psiMethod,fullString).withLookupString(psiMethod.getName()).withPresentableText(fullString).withTypeText(functionalInterfaceType.getPresentableText()).withIcon(AllIcons.Nodes.MethodReference).withAutoCompletionPolicy(AutoCompletionPolicy.NEVER_AUTOCOMPLETE);
}
