{
  constructedType=TypeConversionUtil.erasure(constructedType);
  return LookupElementBuilder.create(constructedType,constructedType.getPresentableText() + "::new").withTypeText(functionalInterfaceType.getPresentableText()).withIcon(AllIcons.Nodes.MethodReference).withInsertHandler(CONSTRUCTOR_REF_INSERT_HANDLER).withAutoCompletionPolicy(AutoCompletionPolicy.NEVER_AUTOCOMPLETE);
}
