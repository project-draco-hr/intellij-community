{
  return new AnnotationConsumer(){
    @Override public void consume(    int lineNumber,    @NotNull CommitInfo info,    @Nullable CommitInfo mergeInfo) throws SVNException {
      if (progress != null) {
        progress.checkCanceled();
      }
      result.setLineInfo(lineNumber,info,mergeInfo != null && info.getRevision() > mergeInfo.getRevision() ? mergeInfo : null);
    }
  }
;
}
