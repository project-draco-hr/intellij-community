{
  List<CommittedChangeList> lists=myInteraction.showRecentListsForSelection(myInitialChangeListsLoader.getCommittedChangeLists(),myMergeContext.getTitle(),myInitialChangeListsLoader.getHelper(),createMoreChangeListsLoader(),myInitialChangeListsLoader.isLastLoaded());
  if (!lists.isEmpty()) {
    runChangeListsMerge(context,lists,createBranchCopyPoint(),myMergeContext.getTitle());
  }
 else {
    context.cancelEverything();
  }
}
