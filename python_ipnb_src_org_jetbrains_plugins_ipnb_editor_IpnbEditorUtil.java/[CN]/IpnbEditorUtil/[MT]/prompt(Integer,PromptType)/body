{
  if (type == PromptType.In) {
    return promptNumber == null ? type + " [ ]:" : promptNumber > 0 ? String.format(type + " [%d]:",promptNumber) : type + " [*]:";
  }
 else   if (type == PromptType.Out) {
    return promptNumber == null ? type + "[ ]:" : promptNumber > 0 ? String.format(type + "[%d]:",promptNumber) : type + "[*]:";
  }
  return "";
}
