{
  InputStream stream=ProjectImportAction.class.getResourceAsStream("/org/jetbrains/plugins/gradle/tooling/internal/buildSrcInit.gradle");
  try {
    if (stream == null)     return null;
    return FileUtil.loadTextAndClose(stream);
  }
 catch (  Exception e) {
    LOG.warn("Can't use IJ gradle init script",e);
    return null;
  }
 finally {
    StreamUtil.closeStream(stream);
  }
}
