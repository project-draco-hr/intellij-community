{
  FileUtil.processFilesRecursively(directory,new Processor<File>(){
    @Override public boolean process(    File file){
      if (file.getParentFile().getName().equals(PyNames.PYCACHE) || FileUtil.getExtension(file.getName()).equals("pyc") || FileUtil.getExtension(file.getName()).equals("pyo")) {
        pycFiles.add(file);
      }
      return true;
    }
  }
);
}
