{
  FileUtil.processFilesRecursively(directory,new Processor<File>(){
    @Override public boolean process(    File file){
      if (file.getParentFile().getName().equals(PyNames.PYCACHE) || FileUtilRt.extensionEquals(file.getName(),"pyc") || FileUtilRt.extensionEquals(file.getName(),"pyo")|| file.getName().endsWith("$py.class")) {
        pycFiles.add(file);
      }
      return true;
    }
  }
);
}
