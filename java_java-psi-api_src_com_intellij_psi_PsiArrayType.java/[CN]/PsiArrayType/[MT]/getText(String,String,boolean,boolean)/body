{
  StringBuilder sb=new StringBuilder(prefix.length() + suffix.length());
  sb.append(prefix);
  if (annotated) {
    PsiAnnotation[] annotations=getAnnotations();
    if (annotations.length != 0) {
      sb.append(' ');
      PsiNameHelper.appendAnnotations(sb,annotations,qualified);
    }
  }
  sb.append(suffix);
  return sb.toString();
}
