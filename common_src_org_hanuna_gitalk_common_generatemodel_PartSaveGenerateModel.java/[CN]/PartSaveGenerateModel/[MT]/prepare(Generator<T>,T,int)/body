{
  this.size=size;
  this.generator=generator;
  listSaveT.add(new SaveT(0,firstT));
  int curIndex=intervalSave;
  T prevT=firstT;
  while (curIndex < size) {
    prevT=generator.generate(prevT,intervalSave);
    listSaveT.add(new SaveT(curIndex,prevT));
    curIndex=curIndex + intervalSave;
  }
}
