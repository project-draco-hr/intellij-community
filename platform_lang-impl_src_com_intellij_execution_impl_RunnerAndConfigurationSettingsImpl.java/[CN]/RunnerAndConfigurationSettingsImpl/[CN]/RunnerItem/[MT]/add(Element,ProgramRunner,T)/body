{
  if (runner == null) {
    if (unloadedSettings == null) {
      unloadedSettings=new SmartList<>();
    }
    unloadedSettings.add(state);
    return;
  }
  if (data != null) {
    ((JDOMExternalizable)data).readExternal(state);
  }
  settings.put(runner,data);
  loadedIds.add(runner.getRunnerId());
}
