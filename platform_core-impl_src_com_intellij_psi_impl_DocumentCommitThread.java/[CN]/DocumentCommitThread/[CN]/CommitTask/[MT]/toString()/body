{
  return "Doc: " + document + " (\""+ StringUtil.first(document.getImmutableCharSequence(),40,true).toString().replaceAll("\n"," ")+ "\")"+ (indicator.isCanceled() ? " (Canceled: " + ((UserDataHolder)indicator).getUserData(CANCEL_REASON) + ")" : "")+ " Reason: "+ reason+ (isStillValid() ? "" : "; changed: old seq=" + modificationSequence + ", new seq="+ ((DocumentEx)document).getModificationSequence());
}
