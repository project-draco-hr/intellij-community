{
  try {
    new JsonSchemaReader(project).read(new java.io.StringReader(string),true);
    return true;
  }
 catch (  IOException e) {
    LOG.info(e);
    errorConsumer.consume(e.getMessage());
    return false;
  }
catch (  Exception e) {
    LOG.info(e);
    errorConsumer.consume(e.getMessage());
    return false;
  }
}
