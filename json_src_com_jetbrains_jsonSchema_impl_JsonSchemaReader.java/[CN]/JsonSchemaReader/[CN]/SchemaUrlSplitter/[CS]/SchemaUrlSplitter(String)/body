{
  if (!ref.startsWith("#/")) {
    int idx=ref.indexOf("#/");
    if (idx == -1) {
      mySchemaId=ref.endsWith("#") ? ref.substring(0,ref.length() - 1) : ref;
      myRelativePath="";
    }
 else {
      mySchemaId=ref.substring(0,idx);
      myRelativePath=ref.substring(idx);
    }
  }
 else {
    mySchemaId=null;
    myRelativePath=ref;
  }
}
