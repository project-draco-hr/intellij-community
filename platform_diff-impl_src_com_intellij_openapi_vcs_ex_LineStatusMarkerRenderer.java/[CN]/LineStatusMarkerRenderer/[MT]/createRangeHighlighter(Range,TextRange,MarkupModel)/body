{
  TextAttributes attributes=getTextAttributes(range);
  final RangeHighlighter highlighter=markupModel.addRangeHighlighter(textRange.getStartOffset(),textRange.getEndOffset(),DiffDrawUtil.LST_LINE_MARKER_LAYER,attributes,HighlighterTargetArea.LINES_IN_RANGE);
  highlighter.setThinErrorStripeMark(true);
  highlighter.setGreedyToLeft(true);
  highlighter.setGreedyToRight(true);
  return highlighter;
}
