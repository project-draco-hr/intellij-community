{
  Map<String,String> map=new HashMap<String,String>();
  for (  String warning : warnings) {
    int ix=warning.indexOf(":");
    if (ix != -1) {
      String path=warning.substring(0,ix);
      String message=warning.substring(ix + 1).replace("\\n","\n");
      map.put(path,message);
    }
  }
  return map;
}
