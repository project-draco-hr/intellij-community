{
  super(target,operation.getEditor(),operation.getProject(),"Introduce Field",occurrences.toArray(new PsiElement[occurrences.size()]),null);
  myTarget=target;
  myOperation=operation;
  if (operation.getAvailableInitPlaces().size() > 1) {
    myPanel=new PyIntroduceFieldPanel(myProject,operation.getAvailableInitPlaces());
  }
 else {
    myPanel=null;
  }
}
