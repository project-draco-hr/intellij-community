{
  super(target,operation.getEditor(),operation.getProject(),"Introduce Field",occurrences.toArray(new PsiElement[occurrences.size()]),null);
  myTarget=target;
  if (!inConstructor(target)) {
    myPanel=new PyIntroduceFieldPanel(myProject,operation.isTestClass());
  }
 else {
    myPanel=null;
  }
}
