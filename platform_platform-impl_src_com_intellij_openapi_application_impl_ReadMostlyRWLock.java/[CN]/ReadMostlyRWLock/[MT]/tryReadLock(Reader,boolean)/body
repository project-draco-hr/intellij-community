{
  if (!writeRequested) {
    if (checkPrivileges && currentSuspension != null && !privilegedReaders.containsKey(Thread.currentThread())) {
      return false;
    }
    status.readRequested=true;
    if (!writeRequested) {
      return true;
    }
    status.readRequested=false;
  }
  return false;
}
