{
  checkWriteThreadAccess();
  writeAcquired=false;
  writeRequested=false;
  List<Reader> dead=new ArrayList<>(readers.size());
  for (  Reader reader : readers) {
    if (reader.blocked) {
      LockSupport.unpark(reader.thread);
    }
 else     if (!reader.thread.isAlive()) {
      dead.add(reader);
    }
  }
  readers.removeAll(dead);
}
