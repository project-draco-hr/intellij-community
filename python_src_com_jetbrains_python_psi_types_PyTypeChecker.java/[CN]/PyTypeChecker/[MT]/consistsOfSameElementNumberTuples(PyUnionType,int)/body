{
  for (  PyType type : unionType.getMembers()) {
    if (type instanceof PyTupleType) {
      final PyTupleType tupleType=(PyTupleType)type;
      if (!tupleType.isHomogeneous() && elementCount != tupleType.getElementCount()) {
        return false;
      }
    }
 else {
      return false;
    }
  }
  return true;
}
