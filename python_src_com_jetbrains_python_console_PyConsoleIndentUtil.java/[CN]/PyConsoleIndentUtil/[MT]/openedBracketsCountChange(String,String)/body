{
  if (pythonBrackets.keySet().contains(open)) {
    String close=pythonBrackets.get(open);
    return StringUtil.getOccurrenceCount(line,open) - StringUtil.getOccurrenceCount(line,close) + StringUtil.getOccurrenceCount(line,"\\" + close) - StringUtil.getOccurrenceCount(line,"\\" + open);
  }
 else {
    throw new UnsupportedOperationException("\"" + open + "\" not a parenthesis.");
  }
}
