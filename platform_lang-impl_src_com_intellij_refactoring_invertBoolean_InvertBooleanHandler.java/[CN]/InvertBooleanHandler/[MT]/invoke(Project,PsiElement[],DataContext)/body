{
  LOG.assertTrue(elements.length == 1);
  PsiElement element=adjustElement(elements[0],project,null);
  if (element == null) {
    CommonRefactoringUtil.showErrorHint(project,null,RefactoringBundle.getCannotRefactorMessage(RefactoringBundle.message("error.wrong.caret.position.method.or.variable.name")),REFACTORING_NAME,INVERT_BOOLEAN_HELP_ID);
    return;
  }
  new InvertBooleanDialog(element).show();
}
