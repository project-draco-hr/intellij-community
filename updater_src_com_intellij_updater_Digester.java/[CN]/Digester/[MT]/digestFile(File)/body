{
  if (!Runner.ZIP_AS_BINARY && Utils.isZipFile(file.getName())) {
    ZipFile zipFile;
    try {
      zipFile=new ZipFile(file);
    }
 catch (    IOException e) {
      Runner.printStackTrace(e);
      return digestRegularFile(file);
    }
    try {
      return doDigestZipFile(zipFile);
    }
  finally {
      zipFile.close();
    }
  }
  return digestRegularFile(file);
}
