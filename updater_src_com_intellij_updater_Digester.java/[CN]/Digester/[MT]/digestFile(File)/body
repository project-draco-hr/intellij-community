{
  if (Utils.isZipFile(file.getName())) {
    ZipFile zipFile;
    try {
      zipFile=new ZipFile(file);
    }
 catch (    IOException e) {
      Runner.infoStackTrace("Can't open file as zip file: " + file.getPath() + "\n",e);
      return doDigestRegularFile(file);
    }
    try {
      return doDigestZipFile(zipFile);
    }
  finally {
      zipFile.close();
    }
  }
  return doDigestRegularFile(file);
}
