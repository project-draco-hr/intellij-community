{
  try {
    return super.getTableCellRendererComponent(table,value,isSelected,hasFocus,row,column);
  }
  finally {
    final Module module=(Module)value;
    if (module != null) {
      setText(module.getName());
      setIcon(ModuleType.get(module).getIcon());
    }
 else {
      setText("");
      setIcon(null);
    }
  }
}
