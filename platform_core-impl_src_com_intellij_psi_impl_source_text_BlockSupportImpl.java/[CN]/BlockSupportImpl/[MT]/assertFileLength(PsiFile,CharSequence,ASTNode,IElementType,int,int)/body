{
  if (end > newFileText.length() || start > end) {
    String message="IOOBE: type=" + elementType + "; file="+ file+ "; file.class="+ file.getClass()+ "; start="+ start+ "; end="+ end+ "; length="+ node.getTextLength();
    String newTextBefore=newFileText.subSequence(0,start).toString();
    String oldTextBefore=file.getText().subSequence(0,start).toString();
    if (oldTextBefore.equals(newTextBefore)) {
      message+="; oldTextBefore==newTextBefore";
    }
    LOG.error(message,new Attachment(file.getName() + "_oldText.txt",node.getText()),new Attachment(file.getName() + "_newText.txt",node.getText()),new Attachment(file.getName() + "_oldTextBefore.txt",oldTextBefore),new Attachment(file.getName() + "_newTextBefore.txt",newTextBefore));
    return false;
  }
  return true;
}
