{
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    context.directoryName="test";
    return true;
  }
  context.directoryName=Messages.showInputDialog((Project)null,"Enter directory name for created project. Leave blank to checkout directly into \"" + context.directory.getName() + "\".","Project Directory Name",null,context.directoryName,null);
  return context.directoryName != null;
}
