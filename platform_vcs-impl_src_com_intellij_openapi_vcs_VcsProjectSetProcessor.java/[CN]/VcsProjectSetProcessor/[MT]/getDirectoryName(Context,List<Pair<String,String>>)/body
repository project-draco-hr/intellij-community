{
  for (  Pair<String,String> entry : entries) {
    String url=entry.getSecond();
    final String[] split=splitUrl(url);
    if (split[1].isEmpty()) {
      int i=url.lastIndexOf('/');
      context.directoryName=i < 0 ? "" : FileUtil.getNameWithoutExtension(url.substring(i + 1));
      break;
    }
  }
  if (ApplicationManager.getApplication().isUnitTestMode())   return true;
  context.directoryName=Messages.showInputDialog((Project)null,"Enter directory name for created project. Leave blank to checkout directly into \"" + context.directory.getName() + "\".","Project Directory Name",null,context.directoryName,null);
  return context.directoryName != null;
}
