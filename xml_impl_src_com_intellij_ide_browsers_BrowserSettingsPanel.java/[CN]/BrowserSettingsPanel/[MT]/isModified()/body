{
  GeneralSettings settings=GeneralSettings.getInstance();
  if (!Comparing.strEqual(settings.getBrowserPath(),alternativeBrowserPathField.getText()) || settings.isUseDefaultBrowser() != useSystemDefaultBrowser.isSelected() || settings.isConfirmExtractFiles() != confirmExtractFiles.isSelected()) {
    return true;
  }
  return browsersEditor.isModified(WebBrowserManager.getInstance().getList());
}
