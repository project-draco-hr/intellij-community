{
  GeneralSettings settings=GeneralSettings.getInstance();
  if (!Comparing.strEqual(settings.getBrowserPath(),alternativeBrowserPathField.getText()) || settings.isConfirmExtractFiles() != confirmExtractFiles.isSelected()) {
    return true;
  }
  WebBrowserManager browserManager=WebBrowserManager.getInstance();
  return browserManager.getDefaultBrowser() != getDefaultBrowser() || browsersEditor.isModified(browserManager.getList());
}
