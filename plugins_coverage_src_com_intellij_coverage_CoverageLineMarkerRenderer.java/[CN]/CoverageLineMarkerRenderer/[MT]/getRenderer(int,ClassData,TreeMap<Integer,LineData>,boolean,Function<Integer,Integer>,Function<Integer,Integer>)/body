{
  final LineData lineData=lines.get(lineNumber);
  if (lineData != null) {
switch (lineData.getStatus()) {
case LineCoverage.FULL:
      return new CoverageLineMarkerRenderer(CodeInsightColors.LINE_FULL_COVERAGE,qualifiedName,lines,coverageByTestApplicable,newToOldConverter,oldToNewConverter);
case LineCoverage.PARTIAL:
    return new CoverageLineMarkerRenderer(CodeInsightColors.LINE_PARTIAL_COVERAGE,qualifiedName,lines,coverageByTestApplicable,newToOldConverter,oldToNewConverter);
case LineCoverage.NONE:
  return new CoverageLineMarkerRenderer(CodeInsightColors.LINE_NONE_COVERAGE,qualifiedName,lines,coverageByTestApplicable,newToOldConverter,oldToNewConverter);
}
}
return new CoverageLineMarkerRenderer(CodeInsightColors.LINE_NONE_COVERAGE,qualifiedName,lines,coverageByTestApplicable,newToOldConverter,oldToNewConverter);
}
