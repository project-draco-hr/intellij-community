{
  try {
    if (StringUtil.isEmptyOrSpaces(executable)) {
      return false;
    }
    HgCommandResult result=getVersionOutput(executable);
    return result.getExitValue() == 0 && !result.getRawOutput().isEmpty();
  }
 catch (  Throwable e) {
    LOG.info("Error during hg executable validation: ",e);
    return false;
  }
}
