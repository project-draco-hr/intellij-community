{
  int newlength=data.length;
  if (newlength == 0) {
    newlength=1;
  }
  while (newlength <= index) {
    newlength*=2;
  }
  int[] newdata=new int[newlength];
  System.arraycopy(data,0,newdata,0,data.length);
  data=newdata;
  int[] newnext=new int[newlength];
  System.arraycopy(next,0,newnext,0,next.length);
  next=newnext;
  return newdata;
}
