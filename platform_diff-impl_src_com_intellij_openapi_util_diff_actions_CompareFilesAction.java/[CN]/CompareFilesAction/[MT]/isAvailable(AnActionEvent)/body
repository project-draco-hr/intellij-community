{
  DiffRequest request=e.getData(DIFF_REQUEST);
  if (request != null) {
    return true;
  }
  VirtualFile[] files=e.getData(CommonDataKeys.VIRTUAL_FILE_ARRAY);
  if (files == null || files.length != 2) {
    return false;
  }
  if (!files[0].isValid() || !files[1].isValid()) {
    return false;
  }
  return true;
}
