{
  remoteRepository=null;
  IcsManager.OBJECT$.getInstance().setRepositoryActive(false);
  try {
    if (fixture != null) {
      SwingUtilities.invokeAndWait(new Runnable(){
        @Override public void run(){
          try {
            fixture.tearDown();
          }
 catch (          Exception e) {
            throw new RuntimeException(e);
          }
        }
      }
);
    }
  }
  finally {
    if (ICS_DIR != null) {
      FileUtil.delete(ICS_DIR);
    }
  }
}
