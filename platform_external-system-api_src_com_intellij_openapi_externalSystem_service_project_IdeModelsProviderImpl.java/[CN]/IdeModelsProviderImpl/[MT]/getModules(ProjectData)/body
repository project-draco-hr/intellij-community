{
  final List<Module> modules=ContainerUtil.filter(getModules(),new Condition<Module>(){
    @Override public boolean value(    Module module){
      return isExternalSystemAwareModule(projectData.getOwner(),module) && StringUtil.equals(projectData.getLinkedExternalProjectPath(),getExternalRootProjectPath(module));
    }
  }
);
  return ContainerUtil.toArray(modules,new Module[modules.size()]);
}
