{
  if (ep.displayName != null || ep.key != null || ep.groupId != null) {
    T configurable=null;
    if (ep.providerClass != null) {
      configurable=ep.createConfigurable();
      if (configurable == null) {
        return null;
      }
    }
    return ep.children != null || ep.childrenEPName != null || ep.dynamic ? (T)new CompositeWrapper(ep,configurable) : (T)new ConfigurableWrapper(ep,configurable);
  }
 else {
    return ep.createConfigurable();
  }
}
