{
  return CachedValuesManager.getCachedValue(this,new CachedValueProvider<PsiMethod>(){
    @Nullable @Override public Result<PsiMethod> compute(){
      return Result.create(calcSourceMirrorMethod(),getContainingFile(),getContainingFile().getNavigationElement(),FileIndexFacade.getInstance(getProject()).getRootModificationTracker());
    }
  }
);
}
