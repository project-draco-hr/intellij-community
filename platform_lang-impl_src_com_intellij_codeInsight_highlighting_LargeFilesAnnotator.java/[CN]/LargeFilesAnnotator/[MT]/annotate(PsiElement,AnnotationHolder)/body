{
  if (element instanceof PsiFile && element.getTextLength() > PersistentFSConstants.getMaxIntellisenseFileSize()) {
    holder.createWarningAnnotation(element,"File size exceeds configured limit (" + PersistentFSConstants.getMaxIntellisenseFileSize() + "). "+ "Code insight features are not available.").setFileLevelAnnotation(true);
  }
}
