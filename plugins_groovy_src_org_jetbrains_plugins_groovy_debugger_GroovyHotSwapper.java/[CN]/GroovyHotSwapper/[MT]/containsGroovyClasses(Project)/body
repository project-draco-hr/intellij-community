{
  return CachedValuesManager.getManager(project).getCachedValue(project,new CachedValueProvider<Boolean>(){
    @Nullable @Override public Result<Boolean> compute(){
      return Result.create(FileTypeIndex.containsFileOfType(GroovyFileType.GROOVY_FILE_TYPE,GlobalSearchScope.projectScope(project)),PsiModificationTracker.JAVA_STRUCTURE_MODIFICATION_COUNT);
    }
  }
);
}
