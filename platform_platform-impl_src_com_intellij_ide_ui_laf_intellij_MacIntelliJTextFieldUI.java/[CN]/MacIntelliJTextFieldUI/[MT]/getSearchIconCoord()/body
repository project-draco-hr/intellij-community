{
  final Rectangle r=getDrawingRect();
  Icon icon=getSearchIcon(myTextField);
  return new Point(r.x + (hasText() || myTextField.hasFocus() || isSearchFieldWithHistoryPopup(myTextField) ? JBUI.scale(8) : (r.width - icon.getIconWidth()) / 2),r.y + (r.height - icon.getIconHeight()) / 2 + JBUI.scale(1));
}
