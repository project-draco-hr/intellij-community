{
  if (ref.hasAt())   return false;
  final String name=ref.getReferenceName();
  if (StringUtil.isEmpty(name))   return false;
  return Character.isUpperCase(name.charAt(0)) && !isMethodCallRef(ref) || ref.getParent() instanceof GrReferenceExpression && isPartOfFQN((GrReferenceExpression)ref.getParent());
}
