{
  if (!(expression instanceof GrReferenceExpression))   return false;
  GrReferenceExpression ref=(GrReferenceExpression)expression;
  GrExpression qualifier=ref.getQualifier();
  return "class".equals(ref.getReferenceName()) && qualifier instanceof GrReferenceExpression && ((GrReferenceExpression)qualifier).resolve() instanceof PsiClass&& !org.jetbrains.plugins.groovy.lang.psi.util.PsiUtil.isThisReference(qualifier);
}
