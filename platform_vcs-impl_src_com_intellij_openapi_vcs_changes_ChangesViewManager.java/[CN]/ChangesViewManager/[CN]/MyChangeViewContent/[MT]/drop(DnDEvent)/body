{
  Object attachedObject=event.getAttachedObject();
  if (attachedObject instanceof ShelvedChangeListDragBean) {
    FileDocumentManager.getInstance().saveAllDocuments();
    ShelvedChangeListDragBean shelvedBean=(ShelvedChangeListDragBean)attachedObject;
    ShelveChangesManager.getInstance(myProject).unshelveChangesToSeparatedChangelists(myProject,shelvedBean.getShelvedChangelists(),shelvedBean.getChanges(),shelvedBean.getBinaryFiles(),null);
  }
}
