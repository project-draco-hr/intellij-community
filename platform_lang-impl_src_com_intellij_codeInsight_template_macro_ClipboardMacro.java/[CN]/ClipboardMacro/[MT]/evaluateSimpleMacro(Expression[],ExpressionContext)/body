{
  Transferable contents=CopyPasteManager.getInstance().getContents();
  if (contents != null) {
    String result="";
    try {
      result=(String)contents.getTransferData(DataFlavor.stringFlavor);
    }
 catch (    UnsupportedFlavorException ignored) {
    }
catch (    IOException ignored) {
    }
    if (result != null) {
      return result;
    }
  }
  return "";
}
