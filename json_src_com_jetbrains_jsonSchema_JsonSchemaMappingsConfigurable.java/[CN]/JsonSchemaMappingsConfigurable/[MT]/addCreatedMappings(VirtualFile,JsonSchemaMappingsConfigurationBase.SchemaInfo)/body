{
  final JsonSchemaConfigurable configurable=new JsonSchemaConfigurable(myProject,FileUtil.toSystemDependentName(schemaFile.getPath()),info,myTreeUpdater);
  configurable.setError(myError);
  final MyNode node=new MyNode(configurable,info.isApplicationLevel());
  addNode(node,myRoot);
  selectNodeInTree(node,true);
}
