{
  if (retPrevValue) {
    final ArrayList<Value> orig=new ArrayList<Value>();
    for (    Value value : myValues) {
      final Value old=value.write(true);
      if (old != null) {
        orig.add(old);
      }
    }
    return new Gens(orig);
  }
  for (  Value value : myValues) {
    value.write(false);
  }
  return null;
}
