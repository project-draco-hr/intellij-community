{
  UnsatisfiedLinkError exception=null;
  try {
    System.loadLibrary(HRTIMER_LIB);
  }
 catch (  UnsatisfiedLinkError e) {
    if (path != null) {
      try {
        System.load(path);
      }
 catch (      UnsatisfiedLinkError ex) {
        exception=ex;
      }
    }
 else {
      exception=e;
    }
  }
  if (exception != null) {
    System.out.println("native lib '" + HRTIMER_LIB + "' not found in 'java.library.path': "+ System.getProperty("java.library.path")+ path == null ? "" : (" or in " + path));
    throw exception;
  }
}
