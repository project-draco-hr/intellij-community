{
  final java.util.List<long[]> results=new java.util.ArrayList<>();
  for (int i=0; i < 10; i++) {
    results.add(new long[3]);
  }
  for (int i=0; i < results.size(); i++) {
    final long[] longs=results.get(i);
    for (int j=0; j < 3; j++) {
      longs[j]=i * j;
    }
  }
  for (  long[] result : results) {
    for (    long l : result) {
      System.out.println(l);
    }
  }
}
