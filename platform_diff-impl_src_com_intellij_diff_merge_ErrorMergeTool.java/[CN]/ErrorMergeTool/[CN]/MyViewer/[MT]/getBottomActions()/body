{
  return MergeUtil.createBottomAction(new MergeUtil.AcceptActionProcessor(){
    @Override public boolean isVisible(    @NotNull MergeResult result){
      return result == MergeResult.CANCEL;
    }
    @Override public void perform(    @NotNull MergeResult result){
      myMergeContext.finishMerge(result);
    }
  }
);
}
