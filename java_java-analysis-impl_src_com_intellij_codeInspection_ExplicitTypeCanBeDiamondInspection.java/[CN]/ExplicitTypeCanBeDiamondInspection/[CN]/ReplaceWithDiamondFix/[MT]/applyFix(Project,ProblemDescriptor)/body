{
  final PsiElement element=descriptor.getPsiElement();
  if (!FileModificationService.getInstance().preparePsiElementForWrite(element))   return;
  final PsiNewExpression newExpression=PsiTreeUtil.getParentOfType(PsiDiamondTypeUtil.replaceExplicitWithDiamond(element),PsiNewExpression.class);
  if (newExpression != null) {
    CodeStyleManager.getInstance(project).reformat(newExpression);
  }
}
