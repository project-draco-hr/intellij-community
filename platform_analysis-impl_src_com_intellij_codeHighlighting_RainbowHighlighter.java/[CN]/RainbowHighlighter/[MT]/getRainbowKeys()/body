{
  if (RAINBOW_COLOR_KEYS.isEmpty()) {
    for (int i=0; i < RAINBOW_COLORS_DEFAULT.length; ++i) {
      RAINBOW_COLOR_KEYS.add(TextAttributesKey.createTextAttributesKey("RAINBOW_COLOR" + i,new TextAttributes(new JBColor(RAINBOW_COLORS_DEFAULT[i],RAINBOW_COLORS_DARK[i]),null,null,null,Font.PLAIN)));
    }
  }
  return RAINBOW_COLOR_KEYS;
}
