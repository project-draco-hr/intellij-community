{
  if (loaders == null)   return false;
  boolean result=false;
  if (loaders == loader) {
    result=true;
  }
 else   if (loaders instanceof Loader[]) {
    for (    Loader existing : (Loader[])loaders) {
      if (existing == loader) {
        result=true;
        break;
      }
    }
  }
  return result;
}
