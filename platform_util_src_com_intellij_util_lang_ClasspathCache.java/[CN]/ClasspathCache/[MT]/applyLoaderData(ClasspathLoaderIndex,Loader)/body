{
  myLock.writeLock().lock();
  try {
    for (    String resourceEntry : loaderData.getResourcePaths()) {
      addResourceEntry(resourceEntry,loader);
    }
    for (    String name : loaderData.getNames()) {
      addNameEntry(name,loader);
    }
  }
  finally {
    myLock.writeLock().unlock();
  }
}
