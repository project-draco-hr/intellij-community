{
  if (e instanceof MessageError) {
    ThreeState log=((MessageError)e).log;
    if (log == ThreeState.YES || (log == ThreeState.UNSURE && ApplicationManager.getApplication().isUnitTestMode())) {
      logger.error(e);
    }
  }
 else   if (!(e instanceof ProcessCanceledException)) {
    logger.error(e);
  }
}
