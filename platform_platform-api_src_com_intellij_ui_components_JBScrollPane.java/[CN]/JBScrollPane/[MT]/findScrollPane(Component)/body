{
  if (c == null)   return null;
  if (!(c instanceof JViewport)) {
    c=c.getParent();
    if (!(c instanceof JViewport))     return null;
  }
  c=c.getParent();
  if (c instanceof JLayeredPane) {
    c=c.getParent();
  }
  if (!(c instanceof JBScrollPane))   return null;
  return (JBScrollPane)c;
}
