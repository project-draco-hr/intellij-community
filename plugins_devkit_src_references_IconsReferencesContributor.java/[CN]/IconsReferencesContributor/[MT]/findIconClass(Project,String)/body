{
  final boolean isAllIcons="AllIcons".equals(className);
  final String fqnClassName=isAllIcons ? "com.intellij.icons.AllIcons" : "icons." + className;
  return JavaPsiFacade.getInstance(project).findClass(fqnClassName,isAllIcons ? GlobalSearchScope.allScope(project) : GlobalSearchScope.projectScope(project));
}
