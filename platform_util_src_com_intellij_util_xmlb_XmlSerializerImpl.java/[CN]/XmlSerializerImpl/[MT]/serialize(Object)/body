{
  try {
    Element serialized=(Element)getBinding(object.getClass()).serialize(object,null,filter);
    if (serialized == null) {
      return new Element("empty");
    }
    return serialized;
  }
 catch (  XmlSerializationException e) {
    throw e;
  }
catch (  Exception e) {
    throw new XmlSerializationException("Can't serialize instance of " + object.getClass(),e);
  }
}
