{
  StringBuilder builder=new StringBuilder();
  if (scheme != null) {
    builder.append(scheme);
    if (authority == null) {
      builder.append(':');
    }
 else {
      builder.append(URLUtil.SCHEME_SEPARATOR);
    }
    if (authority != null) {
      builder.append(authority);
    }
  }
  builder.append(getPath());
  if (parameters != null) {
    builder.append(parameters);
  }
  return builder.toString();
}
