{
switch (insn.getOpcode()) {
case IALOAD:
case LALOAD:
case FALOAD:
case DALOAD:
case AALOAD:
case BALOAD:
case CALOAD:
case SALOAD:
case PUTFIELD:
    if (nullAnalysis && value1 instanceof ParamValue) {
      deReferenced=true;
    }
  break;
default :
}
return super.binaryOperation(insn,value1,value2);
}
