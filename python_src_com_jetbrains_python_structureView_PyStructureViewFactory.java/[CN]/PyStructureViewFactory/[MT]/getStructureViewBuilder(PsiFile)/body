{
  return new TreeBasedStructureViewBuilder(){
    @NotNull public StructureViewModel createStructureViewModel(){
      return new StructureViewModelBase(psiFile,new PyStructureViewElement((PyElement)psiFile)).withSorters(Sorter.ALPHA_SORTER).withSuitableClasses(PyFunction.class,PyClass.class);
    }
  }
;
}
