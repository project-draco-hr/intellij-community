{
  PostfixTemplatesSettings settings=PostfixTemplatesSettings.getInstance();
  if (wrapping || file == null || settings == null || !settings.isPostfixTemplatesEnabled() || PsiUtilCore.getLanguageAtOffset(file,offset) != JavaLanguage.INSTANCE) {
    return false;
  }
  return StringUtil.isNotEmpty(computeTemplateKeyWithoutContextChecking(file.getText(),offset + 1));
}
