{
  addRule(new Rule(',',WrapCondition.AFTER,Rule.DEFAULT_WEIGHT));
  addRule(new Rule(' '));
  addRule(new Rule('\t'));
  addRule(new Rule(';',WrapCondition.AFTER));
  addRule(new Rule(')',WrapCondition.AFTER));
  addRule(new Rule('(',WrapCondition.BEFORE));
  addRule(new Rule('.',WrapCondition.BEFORE));
}
