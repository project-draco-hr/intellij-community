{
  try {
    final Field field=getPreparedField(myInstance.getClass().getField(name));
    if (field == null)     return null;
    return field.get(myInstance);
  }
 catch (  NoSuchFieldException e) {
    e.printStackTrace();
  }
catch (  IllegalAccessException e) {
    e.printStackTrace();
  }
  return null;
}
