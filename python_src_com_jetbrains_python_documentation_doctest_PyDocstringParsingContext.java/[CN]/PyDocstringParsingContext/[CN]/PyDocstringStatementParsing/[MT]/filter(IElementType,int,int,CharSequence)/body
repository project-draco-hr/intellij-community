{
  if (source == PyTokenTypes.DOT && CharArrayUtil.regionMatches(text,start,end,"..."))   return PyDocstringTokenTypes.DOTS;
  if (source == PyTokenTypes.GTGT && CharArrayUtil.regionMatches(text,start,end,">>>"))   return PyDocstringTokenTypes.WELCOME;
  return super.filter(source,start,end,text);
}
