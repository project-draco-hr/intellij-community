{
  AppExecutorUtil.getAppScheduledExecutorService().schedule(new Runnable(){
    public void run(){
      if (!isClosed()) {
        AppExecutorUtil.getAppScheduledExecutorService().schedule(this,NOTIFY_TEXT_DELAY,TimeUnit.MILLISECONDS);
        checkTextAvailable();
      }
    }
  }
,NOTIFY_TEXT_DELAY,TimeUnit.MILLISECONDS);
  try {
    while (!isClosed()) {
      final int c=readNextByte();
      if (c == -1) {
        break;
      }
synchronized (myBuffer) {
        myBuffer.append((char)c);
      }
      if (c == '\n') {
        checkTextAvailable();
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  close();
}
