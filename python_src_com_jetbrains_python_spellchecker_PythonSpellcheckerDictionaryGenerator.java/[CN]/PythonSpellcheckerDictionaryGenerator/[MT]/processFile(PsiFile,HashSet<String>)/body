{
  file.accept(new PyRecursiveElementVisitor(){
    @Override public void visitPyFunction(    PyFunction node){
      super.visitPyFunction(node);
      processLeafsNames(node,seenNames);
    }
    @Override public void visitPyClass(    PyClass node){
      super.visitPyClass(node);
      processLeafsNames(node,seenNames);
    }
    @Override public void visitPyTargetExpression(    PyTargetExpression node){
      super.visitPyTargetExpression(node);
      if (PsiTreeUtil.getParentOfType(node,ScopeOwner.class) instanceof PyFile) {
        processLeafsNames(node,seenNames);
      }
    }
  }
);
}
