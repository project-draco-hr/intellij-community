{
  int i=0;
  try {
    initText("");
    for (i=1; i <= ITERATIONS; i++) {
      doRandomAction();
      checkConsistency(myEditor);
    }
  }
 catch (  Throwable t) {
    String message="Failed when run with seed=" + mySeed + " in iteration "+ i;
    System.out.println(message);
    throw new RuntimeException(message,t);
  }
}
