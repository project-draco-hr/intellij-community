{
  for (  PrefixExpressionContext expressionContext : context.expressions()) {
    if (myExpressionType.isInstance(expressionContext.expression) && expressionContext.expressionRange.equals(myExpressionRange))     return expressionContext;
  }
  int index=0;
  for (  PrefixExpressionContext expressionContext : context.expressions()) {
    if (myContextIndex == index++)     return expressionContext;
  }
  return null;
}
