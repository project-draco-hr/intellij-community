{
  boolean added=false;
  for (  Locale locale : toAdd) {
    if (!myLocales.contains(locale)) {
      myLocales.add(locale);
      if (!added) {
        added=true;
      }
    }
  }
  if (added) {
    Collections.sort(myLocales,LOCALE_COMPARATOR);
    fireIntervalAdded(this,0,myLocales.size());
  }
}
