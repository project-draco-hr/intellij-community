{
  PsiElement[] arguments=node.getChildren();
  if (getLanguageLevel(node).isPy3K()) {
    for (    PsiElement element : arguments) {
      if (!((element instanceof PyParenthesizedExpression) || (element instanceof PyTupleExpression)))       getHolder().createWarningAnnotation(element,"Python versions >= 3.0 do not support this syntax. The print statement has been replaced with a print() function");
    }
  }
}
