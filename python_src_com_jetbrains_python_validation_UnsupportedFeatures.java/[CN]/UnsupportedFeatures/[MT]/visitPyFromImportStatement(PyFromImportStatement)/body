{
  PyReferenceExpression importSource=node.getImportSource();
  if (getLanguageLevel(node) == LanguageLevel.PYTHON24) {
    if (importSource == null)     getHolder().createWarningAnnotation(node,"Python version 2.4 doesn't support this syntax.");
 else {
      PsiElement prev=importSource.getPrevSibling();
      if (prev != null && prev.getNode().getElementType() == PyTokenTypes.DOT)       getHolder().createWarningAnnotation(node,"Python version 2.4 doesn't support this syntax.");
    }
  }
}
