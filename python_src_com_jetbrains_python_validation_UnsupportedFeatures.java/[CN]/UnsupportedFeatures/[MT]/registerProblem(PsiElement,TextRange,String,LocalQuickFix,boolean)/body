{
  if (range.isEmpty()) {
    return;
  }
  if (localQuickFix != null)   if (asError)   getHolder().createErrorAnnotation(range,message).registerFix(createIntention(node,message,localQuickFix));
 else   getHolder().createWarningAnnotation(range,message).registerFix(createIntention(node,message,localQuickFix));
 else   if (asError)   getHolder().createErrorAnnotation(range,message);
 else   getHolder().createWarningAnnotation(range,message);
}
