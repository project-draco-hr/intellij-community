{
  Collection<String> names=LocalVariablesUtil.calcNames(evaluationContext,myIndex);
  String nameString=StringUtil.join(names," | ");
  if (myIsParam && names.size() == 1) {
    myName=nameString;
  }
 else   if (!names.isEmpty()) {
    myName=nameString + ": " + getDefaultName();
  }
  return myValue;
}
