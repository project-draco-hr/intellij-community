{
  if (result.charAt(result.length() - 1) == '\n') {
    int index=result.length() - 2;
    while (index > 0) {
      final char c=result.charAt(index);
      if (c == '/' && result.charAt(index - 1) == '/') {
        result.insert(index - 1,"+ ");
        return;
      }
      if (c == '\n') {
        break;
      }
      index--;
    }
    result.insert(result.length() - 1,'+');
  }
 else {
    result.append('+');
  }
}
