{
  final List<PsiMethod> allMethods=JavaCompletionUtil.getAllMethods(item);
  if (allMethods != null) {
    for (    PsiMethod method : allMethods) {
      if (!method.isValid()) {
        return false;
      }
      if (!isDeprecated(method)) {
        return false;
      }
    }
    return true;
  }
  return isDeprecated(item.getPsiElement());
}
