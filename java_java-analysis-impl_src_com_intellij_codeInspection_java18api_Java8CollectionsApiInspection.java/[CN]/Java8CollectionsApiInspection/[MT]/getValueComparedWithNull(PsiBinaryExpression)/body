{
  if (!binOp.getOperationTokenType().equals(JavaTokenType.EQEQ) && !binOp.getOperationTokenType().equals(JavaTokenType.NE))   return null;
  PsiExpression left=binOp.getLOperand();
  PsiExpression right=binOp.getROperand();
  if (ExpressionUtils.isNullLiteral(right))   return left;
  if (ExpressionUtils.isNullLiteral(left))   return right;
  return null;
}
