{
  super(project,true);
  myOccurrencesCount=operation.getOccurrences().size();
  myValidator=validator;
  myProject=project;
  myExpression=operation.getInitializer();
  myHelpId=helpId;
  setUpNameComboBox(operation.getSuggestedNames());
  setTitle(caption);
  init();
  setupDialog(operation.getAvailableInitPlaces());
  updateControls();
}
