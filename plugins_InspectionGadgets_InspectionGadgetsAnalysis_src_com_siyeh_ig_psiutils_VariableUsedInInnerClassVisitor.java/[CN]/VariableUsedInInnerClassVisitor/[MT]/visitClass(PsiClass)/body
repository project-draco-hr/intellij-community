{
  if (usedInInnerClass) {
    return;
  }
  final boolean wasInInnerClass=inInnerClass;
  if (!inInnerClass) {
    inInnerClass=true;
    PsiElement child=aClass.getLBrace();
    while (child != null) {
      child.accept(this);
      child=child.getNextSibling();
    }
  }
 else {
    inInnerClass=true;
    super.visitClass(aClass);
  }
  inInnerClass=wasInInnerClass;
}
