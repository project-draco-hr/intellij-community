{
  for (  String field : principal.getName().split(",")) {
    field=field.trim();
    String[] parts=field.split("=",2);
    if (parts.length != 2) {
      continue;
    }
    String name=parts[0];
    String value=parts[1];
    String longName=FIELD_ABBREVIATIONS.get(name.toUpperCase());
    if (longName == null) {
      continue;
    }
    @SuppressWarnings("unchecked") ListTableModel<Field> model=(ListTableModel<Field>)table.getModel();
    model.addRow(new Field(String.format("%s (%s)",longName,name),value));
  }
}
