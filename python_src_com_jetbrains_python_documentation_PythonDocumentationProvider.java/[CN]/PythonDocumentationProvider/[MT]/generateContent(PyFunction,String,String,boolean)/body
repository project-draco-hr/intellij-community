{
  PyParameter[] list=element.getParameterList().getParameters();
  StringBuilder builder=new StringBuilder(offset);
  for (  PyParameter p : list) {
    if (p.getText().equals(PyNames.CANONICAL_SELF))     continue;
    builder.append(prefix);
    builder.append("param ");
    builder.append(p.getName());
    builder.append(": ");
    builder.append(offset);
  }
  if (checkReturn) {
    RaiseVisitor visitor=new RaiseVisitor();
    PyStatementList statementList=element.getStatementList();
    if (statementList != null) {
      statementList.accept(visitor);
    }
    if (visitor.myHasReturn)     builder.append(prefix).append("return:").append(offset);
    if (visitor.myHasRaise)     builder.append(prefix).append("raise:").append(offset);
  }
 else   builder.append(prefix).append("return:").append(offset);
  return builder.toString();
}
