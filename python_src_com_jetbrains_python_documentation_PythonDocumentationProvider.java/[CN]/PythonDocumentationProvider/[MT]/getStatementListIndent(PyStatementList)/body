{
  final PsiWhiteSpace whitespace=PsiTreeUtil.getPrevSiblingOfType(statementList,PsiWhiteSpace.class);
  if (whitespace != null) {
    final String text=whitespace.getText();
    final int index=text.lastIndexOf('\n');
    if (index >= 0) {
      return text.substring(index + 1);
    }
  }
  return "";
}
