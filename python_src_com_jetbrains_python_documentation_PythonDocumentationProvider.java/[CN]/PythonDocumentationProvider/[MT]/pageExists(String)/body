{
  HttpClient client=new HttpClient();
  client.setTimeout(5 * 1000);
  client.setConnectionTimeout(5 * 1000);
  HeadMethod method=new HeadMethod(url);
  try {
    int rc=client.executeMethod(method);
    if (rc == 404) {
      return false;
    }
  }
 catch (  IOException ignored) {
  }
  return true;
}
