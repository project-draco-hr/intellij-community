{
  Project project=element.getProject();
  PyDocumentationSettings documentationSettings=PyDocumentationSettings.getInstance(project);
  String result="";
  if (documentationSettings.isEpydocFormat(element.getContainingFile()))   result+=generateContent(element,offset,EPYDOC_PREFIX);
 else   if (documentationSettings.isReSTFormat(element.getContainingFile()))   result+=generateContent(element,offset,RST_PREFIX);
 else   result+=offset;
  return result;
}
