{
  Project project=element.getProject();
  PyDocumentationSettings documentationSettings=PyDocumentationSettings.getInstance(project);
  String result="";
  if (documentationSettings.isEpydocFormat())   result+=generateContent(element,offset,EPYDOC_PREFIX);
 else   if (documentationSettings.isReSTFormat())   result+=generateContent(element,offset,RST_PREFIX);
  return result;
}
