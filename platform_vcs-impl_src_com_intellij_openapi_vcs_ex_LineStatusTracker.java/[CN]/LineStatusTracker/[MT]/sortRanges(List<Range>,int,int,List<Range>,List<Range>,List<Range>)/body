{
  for (  Range range : ranges) {
    int line1=range.getLine1();
    int line2=range.getLine2();
    if (line2 < changedLine1) {
      rangesBeforeChange.add(range);
    }
 else     if (line1 > changedLine2) {
      rangesAfterChange.add(range);
    }
 else {
      changedRanges.add(range);
    }
  }
}
