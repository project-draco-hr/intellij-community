{
synchronized (myLock) {
    final Range currentRange=getRangeForLine(line);
    if (currentRange != null) {
      return getNextRange(currentRange);
    }
    for (    final Range range : myRanges) {
      if (line > range.getLine1() || line > range.getLine2()) {
        continue;
      }
      return range;
    }
    return null;
  }
}
