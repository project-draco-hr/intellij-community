{
synchronized (myLock) {
    for (    final Range range : myRanges) {
      if (range.getType() == Range.DELETED && line == range.getLine1()) {
        return range;
      }
 else       if (line >= range.getLine1() && line < range.getLine2()) {
        return range;
      }
    }
    return null;
  }
}
