{
  if (other instanceof SelfElementInfo) {
    SelfElementInfo otherInfo=(SelfElementInfo)other;
    return Comparing.equal(myVirtualFile,otherInfo.myVirtualFile) && myType == otherInfo.myType && mySyncMarkerIsValid && otherInfo.mySyncMarkerIsValid && getSyncStartOffset() == otherInfo.getSyncStartOffset() && getSyncEndOffset() == otherInfo.getSyncEndOffset();
  }
  return ApplicationManager.getApplication().runReadAction(new Computable<Boolean>(){
    @Override public Boolean compute(){
      return Comparing.equal(restoreElement(),other.restoreElement());
    }
  }
);
}
