{
  myStringPart=stringPart;
  LOG.assertTrue(expression != null || var != null || stringPart != null);
  this.project=project;
  this.editor=editor;
  this.expression=expression;
  this.occurrences=occurrences;
  this.scope=scope;
  this.var=var;
