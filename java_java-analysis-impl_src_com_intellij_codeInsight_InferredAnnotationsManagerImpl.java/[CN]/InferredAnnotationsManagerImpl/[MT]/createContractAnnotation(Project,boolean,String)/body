{
  final String attrs;
  if (!contracts.isEmpty() && pure) {
    attrs="value = " + "\"" + contracts + "\", pure = true";
  }
 else   if (pure) {
    attrs="pure = true";
  }
 else   if (!contracts.isEmpty()) {
    attrs="\"" + contracts + "\"";
  }
 else {
    return null;
  }
  return ProjectBytecodeAnalysis.getInstance(project).createContractAnnotation(attrs);
}
