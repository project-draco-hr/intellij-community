{
  if (model.isCustomScope()) {
    return Scope.SCOPE;
  }
 else   if (model.isProjectScope()) {
    return Scope.PROJECT;
  }
 else   if (model.getDirectoryName() != null) {
    return Scope.DIRECTORY;
  }
 else   if (model.getModuleName() != null) {
    return Scope.MODULE;
  }
  return Scope.PROJECT;
}
