{
  final List<T> result=new ArrayList<T>();
  type.visitMembers(new Processor<PsiElement>(){
    @Override public boolean process(    final PsiElement t){
      if (expectedMemberType.isInstance(t)) {
        @SuppressWarnings("unchecked") final T castedElement=(T)t;
        result.add(castedElement);
      }
      return true;
    }
  }
,true,context);
  return result;
}
