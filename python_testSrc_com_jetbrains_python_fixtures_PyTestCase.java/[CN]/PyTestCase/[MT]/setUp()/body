{
  super.setUp();
  initPlatformPrefix();
  IdeaTestFixtureFactory factory=IdeaTestFixtureFactory.getFixtureFactory();
  TestFixtureBuilder<IdeaProjectTestFixture> fixtureBuilder=factory.createLightFixtureBuilder(getProjectDescriptor());
