{
  if (myLastListItemLineInd != -1) {
    String l=myLines.get(myLastListItemLineInd);
    l+="</li>";
    if (finishList) {
      if (myFirstListItem.isUnordered()) {
        l+="</ul>";
      }
 else {
        l+="</ol>";
      }
      myFirstListItem=null;
    }
    myLines.set(myLastListItemLineInd,l);
    myLastListItemLineInd=-1;
  }
}
