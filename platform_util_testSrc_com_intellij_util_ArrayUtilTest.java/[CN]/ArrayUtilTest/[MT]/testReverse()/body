{
  char[] a=new char[]{};
  ArrayUtil.reverse(a);
  assertArrayEquals(new char[]{},a);
  a=new char[]{'1'};
  ArrayUtil.reverse(a);
  assertArrayEquals(new char[]{'1'},a);
  a=new char[]{'1','2','3','4'};
  ArrayUtil.reverse(a);
  assertArrayEquals(new char[]{'4','3','2','1'},a);
  a=new char[]{'1','2','3','4','5'};
  ArrayUtil.reverse(a);
  assertArrayEquals(new char[]{'5','4','3','2','1'},a);
}
