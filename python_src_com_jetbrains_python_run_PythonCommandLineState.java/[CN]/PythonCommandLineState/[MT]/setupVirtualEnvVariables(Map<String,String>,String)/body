{
  if (PythonSdkType.isVirtualEnv(sdkHome)) {
    PyVirtualEnvReader reader=new PyVirtualEnvReader(sdkHome);
    if (reader.getActivate() != null) {
      try {
        env.putAll(reader.readShellEnv());
      }
 catch (      Exception e) {
        LOG.error("Couldn't read virtualenv variables",e);
      }
    }
  }
}
