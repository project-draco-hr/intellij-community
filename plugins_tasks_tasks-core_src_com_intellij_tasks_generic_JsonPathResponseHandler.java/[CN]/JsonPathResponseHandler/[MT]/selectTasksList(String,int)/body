{
  @SuppressWarnings("unchecked") List<Object> list=(List<Object>)extractValueAndCheckType(getSelector(TASKS),response,List.class);
  if (list == null) {
    return ContainerUtil.emptyList();
  }
  return ContainerUtil.map2List(list,new Function<Object,Object>(){
    @Override public Object fun(    Object o){
      return o.toString();
    }
  }
).subList(0,Math.min(list.size(),max));
}
