{
  if (node.isObsolete())   return;
  myFrameAccessor.loadReferrers(this,new PyDebugCallback<XValueChildrenList>(){
    @Override public void ok(    XValueChildrenList value){
      if (!node.isObsolete()) {
        node.addChildren(value,true);
      }
    }
    @Override public void error(    PyDebuggerException e){
      if (!node.isObsolete()) {
        node.setErrorMessage("Unable to display children:" + e.getMessage());
      }
      LOG.warn(e);
    }
  }
);
}
