{
  StringBuilder sb=new StringBuilder();
switch (myType) {
case CREATE:
    sb.append(" created");
  break;
case ACQUIRE_BEGIN:
sb.append(" acquire started");
break;
case ACQUIRE_END:
sb.append(" acquired");
break;
case RELEASE:
sb.append(" released");
break;
default :
sb.append(" unknown command");
}
return sb.toString();
}
