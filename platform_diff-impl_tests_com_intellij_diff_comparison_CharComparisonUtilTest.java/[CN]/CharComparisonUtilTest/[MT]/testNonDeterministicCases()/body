  TestData.chars("x z"," x _ y _ z ")._______Def_("   ","-  ------ -").____Ignore_("   ","     -     ")._Def_(ins(0,0,1),ins(2,3,6),ins(3,10,1))._Ignore_(ins(2,5,1)).all();
