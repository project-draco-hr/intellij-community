{
  TestData.chars("x","")._______Def_("-","").all();
  TestData.chars("","x")._______Def_("","-").all();
  TestData.chars("x","y")._______Def_("-","-").all();
  TestData.chars("x_","")._______Def_("--","").____Ignore_("- ","").all();
  TestData.chars("","x_")._______Def_("","--").____Ignore_("","- ").all();
  TestData.chars("x_","y_")._______Def_("- ","- ").all();
  TestData.chars("_x","_y")._______Def_(" -"," -").all();
}
