{
  TestData.chars(" x ","z")._______Def_("---","-").____Ignore_(" - ","-").all();
  TestData.chars("x"," z ")._______Def_("-","---").____Ignore_("-"," - ").all();
  TestData.chars(" x","z\t")._______Def_("--","--").____Ignore_(" -","- ").all();
  TestData.chars("x ","\tz")._______Def_("--","--").____Ignore_("- "," -").all();
}
