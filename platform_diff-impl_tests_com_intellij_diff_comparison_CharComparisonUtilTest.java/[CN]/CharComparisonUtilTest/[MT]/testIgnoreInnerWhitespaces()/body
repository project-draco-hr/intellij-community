{
  TestData.chars("x z y","xmn")._______Def_(" ----"," --").____Ignore_("  ---"," --").all();
  TestData.chars("x y z ","x y m ")._______Def_("    - ","    - ").all();
  TestData.chars("x y z","x y m ")._______Def_("    -","    --").____Ignore_("    -","    - ").all();
  TestData.chars(" x y z"," m y z")._______Def_(" -    "," -    ").all();
  TestData.chars("x y z"," m y z")._______Def_("-    ","--    ").____Ignore_("-    "," -    ").all();
  TestData.chars("x y z","x m z")._______Def_("  -  ","  -  ").all();
  TestData.chars("x y z","x  z")._______Def_("  -  ","    ").all();
  TestData.chars("x  z","x m z")._______Def_("    ","  -  ").all();
  TestData.chars("x  z","x n m z")._______Def_("    ","  ---  ").all();
}
