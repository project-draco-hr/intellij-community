{
  TestData.chars(" x y z ","xyz")._______Def_("- - - -","   ").____Ignore_("       ","   ").all();
  TestData.chars("xyz"," x y z ")._______Def_("   ","- - - -").____Ignore_("   ","       ").all();
  TestData.chars("x ","x")._______Def_(" -"," ").____Ignore_("  "," ").all();
  TestData.chars("x"," x")._______Def_(" ","- ").____Ignore_(" ","  ").all();
  TestData.chars(" x ","x")._______Def_("- -"," ").____Ignore_("   "," ").all();
  TestData.chars("x"," x ")._______Def_(" ","- -").____Ignore_(" ","   ").all();
}
