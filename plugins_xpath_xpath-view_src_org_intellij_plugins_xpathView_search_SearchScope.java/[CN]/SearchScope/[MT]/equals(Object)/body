{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  SearchScope scope=(SearchScope)o;
  return myRecursive == scope.myRecursive && Comparing.equal(myCustomScope,scope.myCustomScope) && Comparing.equal(myModuleName,scope.myModuleName) && Comparing.equal(myPath,scope.myPath) && Comparing.equal(myScopeName,scope.myScopeName) && myScopeType == scope.myScopeType;
}
