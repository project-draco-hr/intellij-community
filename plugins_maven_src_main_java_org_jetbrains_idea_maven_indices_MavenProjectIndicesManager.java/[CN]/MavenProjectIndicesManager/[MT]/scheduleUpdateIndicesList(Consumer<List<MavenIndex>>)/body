{
  myUpdateQueue.queue(new Update(MavenProjectIndicesManager.this){
    public void run(){
      myProjectIndices=MavenIndicesManager.getInstance().ensureIndicesExist(myProject,getLocalRepository(),collectRemoteRepositoriesIdsAndUrls());
      if (consumer != null) {
        consumer.consume(myProjectIndices);
      }
    }
  }
);
}
