{
  String op=getNode().findChildByType(PyElementTypes.UNARY_OPS).getText();
  if (op.equals("-")) {
    final PyExpression operand=getOperand();
    if (operand != null) {
      final PyType type=context.getType(operand);
      if (PyClassType.is("int",type)) {
        return type;
      }
    }
  }
  return null;
}
