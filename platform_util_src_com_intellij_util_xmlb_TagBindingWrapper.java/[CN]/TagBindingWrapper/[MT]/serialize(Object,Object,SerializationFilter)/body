{
  Element e=new Element(myTagName);
  Content n=(Content)binding.serialize(o,e,filter);
  if (n == null) {
    return null;
  }
  String value=n.getValue();
  if (!myAttributeName.isEmpty()) {
    e.setAttribute(myAttributeName,value);
  }
 else {
    e.addContent(new Text(value));
  }
  return e;
}
