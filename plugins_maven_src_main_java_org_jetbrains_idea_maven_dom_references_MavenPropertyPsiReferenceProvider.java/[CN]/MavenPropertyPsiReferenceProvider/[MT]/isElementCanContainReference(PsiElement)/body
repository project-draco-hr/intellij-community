{
  if (element instanceof XmlTag) {
class MyProcessor implements PairProcessor<MavenPluginParamInfo.ParamInfo,MavenDomConfiguration> {
      boolean disableReferences=false;
      @Override public boolean process(      MavenPluginParamInfo.ParamInfo info,      MavenDomConfiguration configuration){
        if (Boolean.TRUE.equals(info.getParam().disableReferences)) {
          disableReferences=true;
          return false;
        }
        return true;
      }
    }
    MyProcessor p=new MyProcessor();
    MavenPluginParamInfo.processParamInfo((XmlTag)element,p);
    return !p.disableReferences;
  }
  return true;
}
