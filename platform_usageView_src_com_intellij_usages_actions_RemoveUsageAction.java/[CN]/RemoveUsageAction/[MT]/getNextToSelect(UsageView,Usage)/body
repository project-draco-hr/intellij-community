{
  List<Usage> sortedUsages=usageView.getSortedUsages();
  int curIndex=sortedUsages.indexOf(toDelete);
  int selectIndex=0;
  if (curIndex < sortedUsages.size() - 1) {
    selectIndex=curIndex + 1;
  }
 else   if (curIndex > 0) {
    selectIndex=curIndex - 1;
  }
  return sortedUsages.get(selectIndex);
}
