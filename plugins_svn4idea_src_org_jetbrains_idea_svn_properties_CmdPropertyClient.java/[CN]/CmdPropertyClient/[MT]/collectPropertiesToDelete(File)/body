{
  final PropertiesMap result=new PropertiesMap();
  list(SvnTarget.fromFile(file),null,Depth.EMPTY,new PropertyConsumer(){
    @Override public void handleProperty(    File path,    PropertyData property) throws SVNException {
      result.put(property.getName(),null);
    }
    @Override public void handleProperty(    SVNURL url,    PropertyData property) throws SVNException {
    }
    @Override public void handleProperty(    long revision,    PropertyData property) throws SVNException {
    }
  }
);
  return result;
}
