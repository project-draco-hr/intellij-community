{
  super.setUp();
  myFixture.enableInspections(new JUnit4AnnotatedMethodInJUnit3TestCaseInspection());
  myRelativePath="junit/convert_to_junit4";
  myFixture.addClass("package org.junit;" + "public class Assert {" + "  public static void assertTrue(boolean condition) {}"+ "  public static void assertFalse(boolean condition) {}"+ "}");
  myFixture.addClass("package org.junit;" + "@Retention(RetentionPolicy.RUNTIME)" + "@Target({ElementType.METHOD})"+ "public @interface Test {}");
  myFixture.addClass("package junit.framework;" + "public class Assert {" + "  public static void assertTrue(boolean condition) {}"+ "  public static void assertFalse(String message, boolean condition) {}"+ "}");
  myFixture.addClass("package junit.framework;" + "public abstract class TestCase extends Assert {}");
}
