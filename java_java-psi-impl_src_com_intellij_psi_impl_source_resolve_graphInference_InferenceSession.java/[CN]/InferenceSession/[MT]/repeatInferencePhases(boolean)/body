{
  do {
    if (!reduceConstraints()) {
      return false;
    }
  }
 while (myConstraintIdx < myConstraints.size());
  do {
    if (!reduceConstraints()) {
      return false;
    }
    if (incorporate) {
      if (!myIncorporationPhase.incorporate()) {
        return false;
      }
    }
  }
 while (incorporate && !myIncorporationPhase.isFullyIncorporated() || myConstraintIdx < myConstraints.size());
  return true;
}
