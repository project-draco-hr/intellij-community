{
  PyStatement firstStatement=null;
  if (ifStatement != null) {
    PyStatementList stList=ifStatement.getIfPart().getStatementList();
    if (stList != null) {
      if (stList.getStatements().length != 0) {
        firstStatement=stList.getStatements()[0];
      }
    }
  }
  return firstStatement;
}
