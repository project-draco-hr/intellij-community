{
  return ContainerUtil.mapNotNull(dependencies,new Function<IdeaDependency,IdeaSingleEntryLibraryDependency>(){
    @Override public IdeaSingleEntryLibraryDependency fun(    IdeaDependency dependency){
      return dependency instanceof IdeaSingleEntryLibraryDependency && scope.equals(dependency.getScope().getScope().toLowerCase()) ? (IdeaSingleEntryLibraryDependency)dependency : null;
    }
  }
);
}
