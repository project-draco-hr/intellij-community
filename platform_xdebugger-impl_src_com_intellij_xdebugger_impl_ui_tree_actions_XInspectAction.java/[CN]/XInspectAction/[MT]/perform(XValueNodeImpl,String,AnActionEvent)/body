{
  XDebugSession session=e.getData(XDebugSession.DATA_KEY);
  XDebuggerTree tree=node.getTree();
  XValue value=node.getValueContainer();
  XInspectDialog dialog=new XInspectDialog(tree.getProject(),tree.getEditorsProvider(),tree.getSourcePosition(),nodeName,value,tree.getValueMarkers(),session,true);
  dialog.show();
}
