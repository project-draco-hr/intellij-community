{
  VfsUtilCore.visitChildrenRecursively(file,new VirtualFileVisitor(){
    @Override public boolean visitFile(    @NotNull VirtualFile file){
      try {
        return operation(file);
      }
 catch (      VcsException e) {
        throw new VisitorException(e);
      }
    }
  }
,VcsException.class);
}
