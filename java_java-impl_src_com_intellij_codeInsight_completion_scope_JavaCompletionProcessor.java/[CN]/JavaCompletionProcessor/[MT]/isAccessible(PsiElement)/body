{
  if (!myOptions.checkAccess)   return true;
  if (!(element instanceof PsiMember))   return true;
  PsiMember member=(PsiMember)element;
  PsiClass accessObjectClass=member instanceof PsiClass ? null : myQualifierClass;
  return JavaPsiFacade.getInstance(element.getProject()).getResolveHelper().isAccessible(member,member.getModifierList(),myElement,accessObjectClass,myDeclarationHolder);
}
