{
  GotoActionModel model=new GotoActionModel(project,myFocusComponent,myEditor,myFile){
    @Override protected MatchMode actionMatches(    String pattern,    @NotNull AnAction anAction){
      return NameUtil.buildMatcher("*" + pattern,NameUtil.MatchingCaseSensitivity.NONE).matches(anAction.getTemplatePresentation().getText()) ? MatchMode.NAME : MatchMode.NONE;
    }
  }
;
  return new GotoActionItemProvider(model);
}
