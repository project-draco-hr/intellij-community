{
  ByteBuf buffer=context.alloc().ioBuffer(1024);
  boolean success=false;
  try {
    ByteBufOutputStream out=new ByteBufOutputStream(buffer);
    for (    String path : lockedPaths) {
      if (path != null) {
        out.writeUTF(path);
      }
    }
    out.close();
    success=true;
  }
  finally {
    if (!success) {
      buffer.release();
    }
  }
  context.writeAndFlush(buffer);
}
