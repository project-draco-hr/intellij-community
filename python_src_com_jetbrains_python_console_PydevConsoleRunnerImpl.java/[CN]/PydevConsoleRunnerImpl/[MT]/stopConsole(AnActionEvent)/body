{
  if (myPydevConsoleCommunication != null) {
    e=new AnActionEvent(e.getInputEvent(),e.getDataContext(),e.getPlace(),e.getPresentation(),e.getActionManager(),e.getModifiers());
    try {
      closeCommunication();
      Thread.sleep(300);
    }
 catch (    Exception ignored) {
    }
  }
  return e;
}
