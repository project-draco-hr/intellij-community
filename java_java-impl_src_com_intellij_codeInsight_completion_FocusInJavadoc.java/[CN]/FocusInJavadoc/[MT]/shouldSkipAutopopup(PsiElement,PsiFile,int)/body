{
  if (PsiTreeUtil.findElementOfClassAtOffset(psiFile,offset - 1,PsiDocTag.class,false) != null && PsiTreeUtil.findElementOfClassAtOffset(psiFile,offset - 1,PsiJavaCodeReferenceElement.class,false) != null) {
    return ThreeState.NO;
  }
  return super.shouldSkipAutopopup(contextElement,psiFile,offset);
}
