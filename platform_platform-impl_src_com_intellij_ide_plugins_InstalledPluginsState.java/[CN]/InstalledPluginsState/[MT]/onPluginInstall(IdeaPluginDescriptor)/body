{
  PluginId id=descriptor.getPluginId();
  boolean existing=PluginManager.isPluginInstalled(id);
synchronized (myLock) {
    myOutdatedPlugins.remove(id.getIdString());
    if (existing) {
      myUpdatedPlugins.put(id,descriptor);
    }
 else {
      myInstalledPlugins.put(id,descriptor);
    }
  }
}
