{
  assertEquals("",ProblemDescriptorUtil.clearDuplicatedWhitespaces(""));
  assertEquals("some text is here",ProblemDescriptorUtil.clearDuplicatedWhitespaces("some    text  is  here"));
  assertEquals("some text is here",ProblemDescriptorUtil.clearDuplicatedWhitespaces("some    text is here  "));
  assertEquals("some text is here",ProblemDescriptorUtil.clearDuplicatedWhitespaces("some text is here "));
  assertEquals("some text is here",ProblemDescriptorUtil.clearDuplicatedWhitespaces("some \t\t text\tis\t\t\t\t\there"));
  assertEquals("sometextishere",ProblemDescriptorUtil.clearDuplicatedWhitespaces("sometextishere"));
}
