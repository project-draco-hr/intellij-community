{
  int line1=getStartLine(side);
  int line2=getEndLine(side);
  if (line2 > oldLine1 && line1 < oldLine2) {
    for (    RangeHighlighter highlighter : myActionHighlighters) {
      highlighter.dispose();
    }
    myActionHighlighters.clear();
    myIsValid=false;
    return true;
  }
  if (oldLine2 <= getStartLine(side)) {
    shift(side,shift);
  }
  return false;
}
