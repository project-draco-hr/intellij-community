{
  LighterASTNode scope=place;
  while (scope != null) {
    if (scope.getTokenType() == CLASS || scope.getTokenType() == METHOD) {
      if (hasOwnTypeParameter(name,scope)) {
        return true;
      }
      if (isStatic(scope)) {
        break;
      }
    }
    scope=myTree.getParent(scope);
  }
  return false;
}
