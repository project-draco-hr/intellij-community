{
  LookupElementBuilder lookupBuilder=super.createLookupBuilder(item);
  if (myColorManager.isMultipleRoots()) {
    lookupBuilder=lookupBuilder.withTypeText(getTypeText(item),JBUI.scale(new ColorIcon(15,VcsLogGraphTable.getRootBackgroundColor(item.getRoot(),myColorManager))),true);
  }
  return lookupBuilder;
}
