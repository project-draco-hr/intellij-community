{
  ProgressEvent result=null;
  if (event != null) {
    if (event.getFile() == null && event.getURL() == null) {
      result=new ProgressEvent(event.getErrorMessage());
    }
 else {
      result=new ProgressEvent(event.getFile(),event.getRevision(),event.getContentsStatus(),event.getPropertiesStatus(),EventAction.from(event.getAction()),event.getErrorMessage(),event.getURL());
    }
  }
  return result;
}
