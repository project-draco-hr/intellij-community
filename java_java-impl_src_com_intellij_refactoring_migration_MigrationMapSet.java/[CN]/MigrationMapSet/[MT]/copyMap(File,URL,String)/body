{
  File targetFile=new File(dir,fileName);
  try {
    FileOutputStream outputStream=new FileOutputStream(targetFile);
    InputStream inputStream=url.openStream();
    try {
      FileUtil.copy(inputStream,outputStream);
    }
  finally {
      outputStream.close();
      inputStream.close();
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
