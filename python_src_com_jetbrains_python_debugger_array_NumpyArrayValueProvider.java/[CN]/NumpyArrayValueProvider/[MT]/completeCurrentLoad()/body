{
  setBusy(false);
  if (myPagingModel.getColumnCount() < getMaxColumn(myShape) || myPagingModel.getRowCount() < getMaxRow(myShape)) {
    String hintMessage=LOAD_SMALLER_SLICE;
    if (isNumeric()) {
      hintMessage+="\n" + DISABLE_COLOR_FOR_HUGE_ARRAY;
    }
    showInfoHint(hintMessage);
  }
}
