{
  if (thumbBounds.isEmpty() || !scrollbar.isEnabled()) {
    return;
  }
  if (isMacOverlayScrollbar()) {
    paintMacThumb(g,thumbBounds);
  }
 else {
    g.translate(thumbBounds.x,thumbBounds.y);
    paintMaxiThumb((Graphics2D)g,thumbBounds);
    g.translate(-thumbBounds.x,-thumbBounds.y);
  }
}
