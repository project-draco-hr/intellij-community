{
  Project project=e.getProject();
  if (project == null || project.isDisposed()) {
    return true;
  }
  for (  DebuggerSupport support : DebuggerSupport.getDebuggerSupports()) {
    if (isEnabled(project,e,support)) {
      perform(project,e,support);
      return true;
    }
  }
  return false;
}
