{
  final PsiIfStatement if2=(PsiIfStatement)myMatchingVisitor.getElement();
  final PsiStatement elseBranch=if1.getElseBranch();
  myMatchingVisitor.setResult(myMatchingVisitor.match(if1.getCondition(),if2.getCondition()) && compareBody(if1.getThenBranch(),if2.getThenBranch()) && (elseBranch == null || compareBody(elseBranch,if2.getElseBranch())));
}
