{
  for (  String word : filterWords) {
    final String comment=changeList.getComment();
    final String committer=changeList.getCommitterName();
    if ((comment != null && comment.toLowerCase().contains(word)) || (committer != null && committer.toLowerCase().contains(word)) || Long.toString(changeList.getNumber()).contains(word)) {
      return true;
    }
  }
  return false;
}
