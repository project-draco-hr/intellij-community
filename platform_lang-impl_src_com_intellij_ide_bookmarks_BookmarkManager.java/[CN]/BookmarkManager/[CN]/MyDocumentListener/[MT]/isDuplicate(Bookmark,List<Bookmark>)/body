{
  for (  Bookmark b : myBookmarks) {
    if (b == bookmark)     continue;
    if (!b.isValid())     continue;
    if (Comparing.equal(b.getFile(),bookmark.getFile()) && b.getLine() == bookmark.getLine()) {
      if (toRemove == null || !toRemove.contains(b)) {
        return true;
      }
    }
  }
  return false;
}
