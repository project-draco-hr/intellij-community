{
  CertificateManager.Config state=CertificateManager.getInstance().getState();
  return myAcceptAutomatically.isSelected() != state.ACCEPT_AUTOMATICALLY || myCheckHostname.isSelected() != state.CHECK_HOSTNAME || myCheckValidityPeriod.isSelected() != state.CHECK_VALIDITY || !myCertificates.equals(new HashSet<>(myTrustManager.getCertificates()));
}
