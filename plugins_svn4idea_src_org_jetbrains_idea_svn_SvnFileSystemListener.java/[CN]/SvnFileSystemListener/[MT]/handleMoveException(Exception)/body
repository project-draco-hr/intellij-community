{
  VcsException vcsException;
  if (e instanceof SVNException && SVNErrorCode.ENTRY_EXISTS.equals(((SVNException)e).getErrorMessage().getErrorCode()) || e instanceof SvnBindException && ((SvnBindException)e).contains(SVNErrorCode.ENTRY_EXISTS)) {
    vcsException=createMoveTargetExistsError(e);
  }
 else   if (e instanceof VcsException) {
    vcsException=(VcsException)e;
  }
 else {
    vcsException=new VcsException(e);
  }
  return vcsException;
}
