{
  Status srcStatus=getFileStatus(vcs,src);
  if (srcStatus == null || SvnVcs.svnStatusIsUnversioned(srcStatus) || SvnVcs.svnStatusIs(srcStatus,StatusType.STATUS_OBSTRUCTED) || SvnVcs.svnStatusIs(srcStatus,StatusType.STATUS_MISSING) || SvnVcs.svnStatusIs(srcStatus,StatusType.STATUS_EXTERNAL)) {
    return true;
  }
  return false;
}
