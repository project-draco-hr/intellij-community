{
  if (schemeToInfo.isEmpty()) {
    return;
  }
  schemeToInfo.retainEntries(new TObjectObjectProcedure<ExternalizableScheme,ExternalInfo>(){
    @Override public boolean execute(    ExternalizableScheme scheme,    ExternalInfo info){
      for (      T t : schemes) {
        if (t == scheme) {
          return true;
        }
      }
      scheduleDelete(info);
      return false;
    }
  }
);
}
