{
  myManager.doWrite(new Runnable(){
    @Override public void run(){
      if (myChildren == null) {
        myChildren=new ArrayList<RefEntity>();
      }
      myChildren.add(child);
      if (child.getOwner() == null) {
        ((RefEntityImpl)child).setOwner(RefModuleImpl.this);
      }
    }
  }
);
}
