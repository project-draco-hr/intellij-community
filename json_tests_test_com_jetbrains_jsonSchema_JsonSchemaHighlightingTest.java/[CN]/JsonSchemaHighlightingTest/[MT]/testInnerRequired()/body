{
  final String schema=schema("{\"type\": \"object\", \"properties\": {\"a\": {}, \"b\": {}}, \"required\": [\"a\"]}");
  testImpl(schema,"{\"prop\": {\"a\": 11}}");
  testImpl(schema,"{\"prop\": {\"a\": 1, \"b\": true}}");
  testImpl(schema,"{\"prop\": <warning descr=\"Missing required property 'a'\">{\"b\": \"alarm\"}</warning>}");
}
