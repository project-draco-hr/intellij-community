{
  if (visitorClass == PyArgumentListInspection.class) {
    return false;
  }
  if (visitorClass == PyDocstringInspection.class || visitorClass == PyStatementEffectInspection.class || visitorClass == PyUnboundLocalVariableInspection.class || visitorClass == PyUnnecessaryBackslashInspection.class) {
    return false;
  }
  if (visitorClass == DocStringAnnotator.class || visitorClass == ParameterListAnnotator.class || visitorClass == ReturnAnnotator.class || visitorClass == HighlightingAnnotator.class)   return false;
  return true;
}
