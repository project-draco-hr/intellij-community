{
  super(logId,NAME,VERSION,new PathsIndexer(PersistentUtil.createPersistentEnumerator(EnumeratorStringDescriptor.INSTANCE,"index-paths-ids",logId,VERSION),roots),new NullableIntKeyDescriptor(),disposableParent);
  myEmptyCommits=PersistentUtil.createPersistentHashMap(EnumeratorIntegerDescriptor.INSTANCE,"index-no-" + NAME,logId,VERSION);
  myPathsIndexer=(PathsIndexer)myIndexer;
}
