{
  super(logId,NAME,VcsLogPersistentIndex.getVersion(),new PathsIndexer(PersistentUtil.createPersistentEnumerator(EnumeratorStringDescriptor.INSTANCE,"index-paths-ids",logId,VcsLogPersistentIndex.getVersion()),roots),new NullableIntKeyDescriptor(),disposableParent);
  myEmptyCommits=PersistentUtil.createPersistentHashMap(EnumeratorIntegerDescriptor.INSTANCE,"index-no-" + NAME,logId,VcsLogPersistentIndex.getVersion());
  myPathsIndexer=(PathsIndexer)myIndexer;
}
