{
  List<Integer> result=ContainerUtil.newArrayList();
  try {
    while (path != null) {
      result.add(getPathId(path));
      VirtualFile file=path.getVirtualFile();
      if (file != null && myRoots.contains(file))       break;
      path=path.getParentPath();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return result;
}
