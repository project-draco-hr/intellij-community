{
  if (fileOrDir.isDirectory())   return true;
  if (ProjectCoreUtil.isProjectOrWorkspaceFile(fileOrDir))   return true;
  if (fileIndex.isInContent(fileOrDir) && (myIncludeTestSource || !fileIndex.isInTestSourceContent(fileOrDir)) && !isInGeneratedSources(fileOrDir,psiManager.getProject())) {
    return processFile(fileOrDir,visitor,psiManager,needReadAction);
  }
  return true;
}
