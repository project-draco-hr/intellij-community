{
  if (curr.taken != prev.taken) {
    return false;
  }
  if (!AbstractValues.equiv(curr.conf,prev.conf)) {
    return false;
  }
  if (curr.history.size() != prev.history.size()) {
    return false;
  }
  for (int i=0; i < curr.history.size(); i++) {
    if (!AbstractValues.equiv(curr.history.get(i),prev.history.get(i))) {
      return false;
    }
  }
  return true;
}
