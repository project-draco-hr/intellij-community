{
  final StringBuilder text=new StringBuilder("<html>");
  for (  String html : myCell.getHtmls()) {
    html=html.replace("\"","'");
    text.append(html);
  }
  text.append("</html>");
  final JBLabel label=new JBLabel(text.toString());
  label.setBackground(IpnbEditorUtil.getBackground());
  label.setOpaque(true);
  return label;
}
