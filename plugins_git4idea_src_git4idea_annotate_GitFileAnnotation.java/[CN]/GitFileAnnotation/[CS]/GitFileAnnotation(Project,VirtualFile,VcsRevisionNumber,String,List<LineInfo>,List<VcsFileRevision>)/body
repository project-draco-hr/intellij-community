{
  super(project);
  myProject=project;
  myFile=file;
  myVcs=ObjectUtils.assertNotNull(GitVcs.getInstance(myProject));
  myBaseRevision=revision == null ? (myVcs.getDiffProvider().getCurrentRevision(file)) : revision;
  myAnnotatedContent=annotatedContent;
  myLines=lines;
  myRevisions=revisions;
}
