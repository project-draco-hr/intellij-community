{
  if (myPassParentEnvs) {
    commandLine.withParentEnvironmentType(shellParentEnvs ? GeneralCommandLine.ParentEnvironmentType.SHELL : GeneralCommandLine.ParentEnvironmentType.SYSTEM);
  }
 else {
    commandLine.withParentEnvironmentType(GeneralCommandLine.ParentEnvironmentType.NONE);
  }
  commandLine.withEnvironment(myEnvs);
}
