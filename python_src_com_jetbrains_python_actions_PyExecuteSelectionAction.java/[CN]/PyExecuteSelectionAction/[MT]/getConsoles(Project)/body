{
  PythonConsoleToolWindow toolWindow=PythonConsoleToolWindow.getInstance(project);
  if (toolWindow != null) {
    ToolWindow tw=toolWindow.getToolWindow();
    return FluentIterable.from(Lists.newArrayList(tw.getContentManager().getContents())).transform(new Function<Content,RunContentDescriptor>(){
      @Override public RunContentDescriptor apply(      @Nullable Content input){
        return input != null ? input.getUserData(PythonConsoleToolWindow.CONTENT_DESCRIPTOR) : null;
      }
    }
).filter(Predicates.notNull()).toList();
  }
  return ExecutionHelper.findRunningConsole(project,new NotNullFunction<RunContentDescriptor,Boolean>(){
    @NotNull @Override public Boolean fun(    RunContentDescriptor dom){
      return dom.getExecutionConsole() instanceof PyCodeExecutor && isAlive(dom);
    }
  }
);
}
