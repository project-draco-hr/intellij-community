{
  PsiElement expression=descriptor.getPsiElement();
  if (expression != null && expression.isWritable() && expression instanceof PyReferenceExpression) {
    final String expressionText=expression.getText();
    if (PyNames.PRINT.equals(expressionText))     replacePrint(expression);
 else     if (PyNames.EXEC.equals(expressionText))     replaceExec(expression);
 else     expression.replace(PyElementGenerator.getInstance(project).createCallExpression(LanguageLevel.forElement(expression),expressionText));
  }
}
