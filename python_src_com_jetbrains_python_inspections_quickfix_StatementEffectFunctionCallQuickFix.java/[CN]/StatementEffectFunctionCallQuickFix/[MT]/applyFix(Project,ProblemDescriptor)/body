{
  PsiElement expression=descriptor.getPsiElement();
  if (expression != null && expression.isWritable() && expression instanceof PyReferenceExpression) {
    PyElementGenerator elementGenerator=PyElementGenerator.getInstance(project);
    if ("print".equals(expression.getText()))     replacePrint(expression,elementGenerator);
 else     expression.replace(elementGenerator.createCallExpression(LanguageLevel.forElement(expression),expression.getText()));
  }
}
