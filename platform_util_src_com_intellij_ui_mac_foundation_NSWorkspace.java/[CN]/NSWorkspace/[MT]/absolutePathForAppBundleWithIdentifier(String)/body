{
  Foundation.NSAutoreleasePool pool=new Foundation.NSAutoreleasePool();
  try {
    ID workspace=Foundation.invoke(Foundation.getObjcClass("NSWorkspace"),"sharedWorkspace");
    return Foundation.toStringViaUTF8(Foundation.invoke(workspace,"absolutePathForAppBundleWithIdentifier:",Foundation.nsString(bundleID)));
  }
  finally {
    pool.drain();
  }
}
