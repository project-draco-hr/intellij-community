{
  List<PyClass> superClasses=new ArrayList<PyClass>();
  while (true) {
    final PyClass[] classes=cl.getSuperClasses();
    if (classes.length == 0) {
      break;
    }
    superClasses.addAll(Arrays.asList(classes));
    cl=classes[0];
  }
  return superClasses.toArray(new PyClass[superClasses.size()]);
}
