{
  boolean useKeyNotation=!quoted && KEY_NOTATION_PROPERTY_NAME_PATTERN.matcher(name).matches();
  if (builder.length() != 0) {
    builder.append(useKeyNotation ? '.' : '[');
  }
  if (useKeyNotation) {
    builder.append(name);
  }
 else {
    if (quoted) {
      builder.append(name);
    }
 else {
      JsonUtil.escape(name,builder);
    }
    builder.append(']');
  }
}
