{
  StringBuilder builder=new StringBuilder(selfName);
  appendName(builder,variable.getName(),false);
  return evaluateContext.evaluate(builder.toString(),Collections.singletonMap(selfName,host),false).then(new Function<EvaluateResult,Value>(){
    @Override public Value fun(    EvaluateResult result){
      variable.setValue(result.value);
      return result.value;
    }
  }
);
}
