{
  if (!toFile.exists())   return null;
  if (toFile.isDirectory())   return null;
  ValidationResult result=validateProcessLock(toFile,action);
  if (result != null) {
    return result;
  }
  if (toFile.canRead() && toFile.canWrite() && isWritable(toFile))   return null;
  return new ValidationResult(ValidationResult.Kind.ERROR,myPath,action,ValidationResult.ACCESS_DENIED_MESSAGE,myPatch.isStrict() ? ValidationResult.Option.NONE : ValidationResult.Option.IGNORE);
}
