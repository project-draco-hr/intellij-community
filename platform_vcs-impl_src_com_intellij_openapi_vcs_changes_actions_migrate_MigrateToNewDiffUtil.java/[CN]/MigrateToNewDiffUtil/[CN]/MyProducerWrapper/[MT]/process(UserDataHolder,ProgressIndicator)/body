{
  com.intellij.openapi.diff.DiffRequest oldRequest=UIUtil.invokeAndWaitIfNeeded(new Computable<com.intellij.openapi.diff.DiffRequest>(){
    @Override public com.intellij.openapi.diff.DiffRequest compute(){
      return myChain.moveTo(myPresentable);
    }
  }
);
  if (oldRequest == null)   return new ErrorDiffRequest(this,"Can't build old-style request");
  return convertRequest(oldRequest);
}
