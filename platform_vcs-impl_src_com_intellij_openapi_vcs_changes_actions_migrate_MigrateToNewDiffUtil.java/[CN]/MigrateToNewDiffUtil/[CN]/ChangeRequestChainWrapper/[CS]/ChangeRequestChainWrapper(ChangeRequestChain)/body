{
  myChain=chain;
  myRequests=ContainerUtil.map(myChain.getAllRequests(),new Function<DiffRequestPresentable,MyProducerWrapper>(){
    @Override public MyProducerWrapper fun(    DiffRequestPresentable presentable){
      return new MyProducerWrapper(myChain,presentable);
    }
  }
);
  myIndex=chain.getAllRequests().indexOf(chain.getCurrentRequest());
}
