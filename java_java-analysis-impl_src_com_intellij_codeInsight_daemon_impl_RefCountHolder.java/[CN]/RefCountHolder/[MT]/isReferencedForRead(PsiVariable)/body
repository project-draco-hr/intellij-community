{
  List<PsiReference> array;
synchronized (myLocalRefsMap) {
    array=myLocalRefsMap.getKeysByValue(variable);
  }
  if (array == null)   return false;
  for (  PsiReference ref : array) {
    PsiElement refElement=ref.getElement();
    if (!(refElement instanceof PsiExpression)) {
      return true;
    }
    if (PsiUtil.isAccessedForReading((PsiExpression)refElement)) {
      if (refElement.getParent() instanceof PsiExpression && refElement.getParent().getParent() instanceof PsiExpressionStatement && PsiUtil.isAccessedForWriting((PsiExpression)refElement)) {
        continue;
      }
      return true;
    }
  }
  return false;
}
