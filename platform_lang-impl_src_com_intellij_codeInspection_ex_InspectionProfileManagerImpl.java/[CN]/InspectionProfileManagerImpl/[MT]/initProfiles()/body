{
  if (myProfilesAreInitialized.getAndSet(true)) {
    if (mySchemeManager.getAllSchemes().isEmpty()) {
      createDefaultProfile();
    }
    return;
  }
  if (!LOAD_PROFILES)   return;
  mySchemeManager.loadSchemes();
  if (mySchemeManager.getAllSchemes().isEmpty()) {
    createDefaultProfile();
  }
}
