{
  PsiReferenceRegistrarImpl registrar=myRegistrars.get(language);
  if (registrar == null) {
synchronized (this) {
      registrar=myRegistrars.get(language);
      if (registrar == null) {
        myRegistrars.put(language,registrar=createRegistrar(language));
      }
    }
  }
  return registrar;
}
