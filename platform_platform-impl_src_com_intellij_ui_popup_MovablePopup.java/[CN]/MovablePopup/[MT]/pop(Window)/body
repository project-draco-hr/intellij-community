{
  JRootPane root=getRootPane(owner);
  if (root != null) {
synchronized (CACHE) {
      @SuppressWarnings("unchecked") ArrayDeque<Window> cache=(ArrayDeque<Window>)root.getClientProperty(CACHE);
      if (cache != null && !cache.isEmpty()) {
        return cache.pop();
      }
    }
  }
  return null;
}
