{
  String strtype=ExprProcessor.getCastTypeName(type,false);
  if (ExprProcessor.UNDEFINED_TYPE_STRING.equals(strtype) && DecompilerContext.getOption(IFernflowerPreferences.UNDEFINED_PARAM_TYPE_OBJECT)) {
    strtype=ExprProcessor.getCastTypeName(VarType.VARTYPE_OBJECT,false);
  }
  return strtype;
}
