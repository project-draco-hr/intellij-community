{
  if (!init || throwsExceptions || paramCount > 0 || !DecompilerContext.getOption(IFernflowerPreferences.HIDE_DEFAULT_CONSTRUCTOR)) {
    return false;
  }
  int count=0;
  for (  StructMethod mt : wrapper.getClassStruct().getMethods()) {
    if ("<init>".equals(mt.getName())) {
      if (++count > 1) {
        return false;
      }
    }
  }
  return true;
}
