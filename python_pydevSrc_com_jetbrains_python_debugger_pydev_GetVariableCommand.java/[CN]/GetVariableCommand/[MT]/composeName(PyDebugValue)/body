{
  final StringBuilder sb=new StringBuilder();
  PyDebugValue p=var;
  while (p != null) {
    if (sb.length() > 0) {
      sb.insert(0,'\t');
    }
    if (p.getId() != null) {
      sb.insert(0,BY_ID).insert(0,'\t').insert(0,p.getId());
      break;
    }
 else {
      sb.insert(0,p.getTempName().replaceAll("\t",TAB_CHAR));
    }
    p=p.getParent();
  }
  return sb.toString();
}
