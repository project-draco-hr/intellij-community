{
  final RegExpLexer lexer=new RegExpLexer(EnumSet.noneOf(RegExpCapability.class));
  doTest("\\0\\123[\\123]","BAD_OCT_VALUE ('\\0')\n" + "BACKREF ('\\1')\n" + "CHARACTER ('2')\n"+ "CHARACTER ('3')\n"+ "CLASS_BEGIN ('[')\n"+ "REDUNDANT_ESCAPE ('\\1')\n"+ "CHARACTER ('2')\n"+ "CHARACTER ('3')\n"+ "CLASS_END (']')",lexer);
}
