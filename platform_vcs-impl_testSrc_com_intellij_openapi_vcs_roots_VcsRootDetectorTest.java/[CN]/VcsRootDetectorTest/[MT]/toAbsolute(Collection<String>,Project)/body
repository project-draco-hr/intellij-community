{
  return ContainerUtil.map(relPaths,new Function<String,String>(){
    @Override public String fun(    String s){
      try {
        return FileUtil.toSystemIndependentName(new File(project.getBaseDir().getPath(),s).getCanonicalPath());
      }
 catch (      IOException e) {
        fail();
        e.printStackTrace();
        return null;
      }
    }
  }
);
}
