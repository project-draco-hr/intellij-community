{
  String s=selectString(selector,context);
  if (s == null) {
    return null;
  }
  s=s.trim().toLowerCase();
  if (s.equals("true")) {
    return true;
  }
 else   if (s.equals("false")) {
    return false;
  }
  throw new Exception(String.format("Expression '%s' should match boolean value. Got '%s' instead",selector.getName(),s));
}
