{
  final PyMemberInfoStorage infoStorage=new PyMemberInfoStorage(clazz);
  for (  PyMemberInfo member : infoStorage.getClassMemberInfos(clazz)) {
    final PyElement function=member.getMember();
    member.setChecked(function.getTextOffset() >= element1.getTextOffset() && function.getTextOffset() + function.getTextLength() <= element2.getTextOffset() + element2.getTextLength());
  }
  return infoStorage;
}
