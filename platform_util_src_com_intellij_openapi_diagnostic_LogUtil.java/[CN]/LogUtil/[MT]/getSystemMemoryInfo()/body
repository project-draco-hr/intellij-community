{
  try {
    @SuppressWarnings("SpellCheckingInspection") Process process=new ProcessBuilder().command(new String[]{SystemInfo.isWindows ? "systeminfo" : SystemInfo.isMac ? "vm_stat" : "free"}).redirectErrorStream(true).start();
    return FileUtil.loadTextAndClose(process.getInputStream());
  }
 catch (  IOException e) {
    return ExceptionUtil.getThrowableText(e);
  }
}
