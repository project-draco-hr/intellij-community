{
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  importProject();
  assertEquals("1.5",CompilerConfiguration.getInstance(myProject).getBytecodeTargetLevel(getModule("m1")));
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "        <configuration>"+ "          <target>1.3</target>"+ "        </configuration>"+ "     </plugin>"+ "  </plugins>"+ "</build>");
  importProject();
  assertEquals("1.3",CompilerConfiguration.getInstance(myProject).getBytecodeTargetLevel(getModule("m1")));
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "        <configuration>"+ "          <target>1.6</target>"+ "        </configuration>"+ "     </plugin>"+ "  </plugins>"+ "</build>");
  importProject();
  assertEquals("1.6",CompilerConfiguration.getInstance(myProject).getBytecodeTargetLevel(getModule("m1")));
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  importProject();
  assertEquals("1.5",CompilerConfiguration.getInstance(myProject).getBytecodeTargetLevel(getModule("m1")));
}
