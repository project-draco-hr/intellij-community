{
  HttpClientBuilder builder=HttpClients.custom().setDefaultRequestConfig(createRequestConfig()).setSslcontext(CertificatesManager.getInstance().getSslContext()).setHostnameVerifier(SSLConnectionSocketFactory.ALLOW_ALL_HOSTNAME_VERIFIER).setDefaultCredentialsProvider(createCredentialsProvider());
  HttpRequestInterceptor interceptor=createRequestInterceptor();
  if (interceptor != null) {
    builder=builder.addInterceptorLast(interceptor);
  }
  return builder.build();
}
