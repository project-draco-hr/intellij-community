{
  for (  Object o : map.values()) {
    LogEntryPath path=(LogEntryPath)o;
    String localPath=path.getPath();
    String copyPath=path.getCopyPath();
    if ('A' == path.getType() && checkForCopyCase(logEntry,path,localPath,copyPath,myTrunkCorrector.getCurrentPath(),myBranchCorrector.getCurrentPath())) {
      throw new SVNCancelException();
    }
  }
}
