{
  final EditorColorsScheme globalScheme=EditorColorsManager.getInstance().getGlobalScheme();
  Color color=getDiffGutterColor(range,globalScheme);
  return !isDistractionFreeMode() ? color : ColorUtil.isDark(globalScheme.getDefaultBackground()) ? ColorUtil.brighter(color,1) : ColorUtil.darker(color,1);
}
