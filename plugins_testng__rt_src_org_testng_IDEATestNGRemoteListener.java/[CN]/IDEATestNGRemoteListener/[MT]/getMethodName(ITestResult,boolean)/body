{
  String methodName=result.getMethod().getMethodName();
  final Object[] parameters=result.getParameters();
  if (changeCount) {
    if (!methodName.equals(myMethodName)) {
      myInvocationCount=0;
      myMethodName=methodName;
    }
  }
  if (parameters.length > 0) {
    final List<Integer> invocationNumbers=result.getMethod().getInvocationNumbers();
    methodName+="[" + parameters[0].toString() + " ("+ INVOCATION_NUMBER+ (invocationNumbers.isEmpty() ? myInvocationCount : invocationNumbers.get(myInvocationCount))+ ")"+ "]";
    if (changeCount) {
      myInvocationCount++;
    }
  }
  return methodName;
}
