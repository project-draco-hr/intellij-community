{
  List<Integer> downNodes=new SmartList<Integer>();
  for (  int downNode : myDelegateGraph.getDownNodes(nodeIndex)) {
    if (downNode != LinearGraph.NOT_LOAD_COMMIT && nodeIsVisible(downNode))     downNodes.add(downNode);
  }
  for (  int adjNode : myDottedEdges.get(nodeIndex)) {
    if (adjNode > nodeIndex)     downNodes.add(adjNode);
  }
  return downNodes;
}
