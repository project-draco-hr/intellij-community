{
  if (myProjectHashToKey != null) {
    String hash=String.valueOf(StringHash.calc(projectKey));
    if (!myProjectHashToKey.containsKey(hash)) {
      myProjectHashToKey.put(hash,projectKey);
      try {
        if (!saveHashToKeyMap()) {
          myProjectHashToKey.remove(hash);
        }
      }
 catch (      IOException e) {
        LOG.info(e);
      }
    }
  }
}
