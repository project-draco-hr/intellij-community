{
  final GithubLoginDialog dialog=new GithubLoginDialog(project);
  dialog.setBasicOnly();
  ApplicationManager.getApplication().invokeAndWait(new Runnable(){
    @Override public void run(){
      dialog.show();
    }
  }
,indicator.getModalityState());
  if (!dialog.isOK()) {
    return null;
  }
  return dialog.getAuthData();
}
