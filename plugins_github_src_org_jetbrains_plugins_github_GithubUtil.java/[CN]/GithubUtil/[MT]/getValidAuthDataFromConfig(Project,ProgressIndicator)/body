{
  GithubAuthData auth=GithubSettings.getInstance().getAuthData();
  boolean valid=false;
  try {
    valid=checkAuthData(auth);
  }
 catch (  IOException e) {
    LOG.error("Connection error",e);
  }
  if (!valid) {
    return getValidAuthData(project,indicator);
  }
 else {
    return auth;
  }
}
