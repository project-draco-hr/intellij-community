{
  ApplicationManager.getApplication().executeOnPooledThread(new Runnable(){
    @Override public void run(){
      final UI ui=prepare(key);
      ApplicationManager.getApplication().invokeLater(new Runnable(){
        @Override public void run(){
          select(callback,key,ui);
        }
      }
,ModalityState.any());
    }
  }
);
}
