{
  try {
    if (stream != null) {
      final Reader reader=new InputStreamReader(stream,"utf-8");
      try {
        final String text=new String(FileUtil.adaptiveLoadText(reader));
        return Pair.create(parseVersion(text),text);
      }
  finally {
        reader.close();
      }
    }
  }
 catch (  IOException e) {
    LOG.info(e);
  }
  return Pair.create(Version.UNKNOWN,"");
}
