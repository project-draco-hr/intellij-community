{
  try {
    if (stream != null) {
      final Reader reader=new InputStreamReader(stream,"utf-8");
      try {
        return new String(FileUtil.adaptiveLoadText(reader));
      }
  finally {
        reader.close();
      }
    }
  }
 catch (  IOException e) {
    LOG.info(e);
  }
  return null;
}
