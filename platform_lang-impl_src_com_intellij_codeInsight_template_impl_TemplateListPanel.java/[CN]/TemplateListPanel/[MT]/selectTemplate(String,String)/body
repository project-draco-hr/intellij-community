{
  TreeUtil.traverseDepth(myTreeRoot,new TreeUtil.Traverse(){
    @Override public boolean accept(    Object node){
      Object o=((DefaultMutableTreeNode)node).getUserObject();
      if (templateKey == null && o instanceof TemplateGroup && Comparing.equal(groupName,((TemplateGroup)o).getName()) || o instanceof TemplateImpl && Comparing.equal(templateKey,((TemplateImpl)o).getKey()) && Comparing.equal(groupName,((TemplateImpl)o).getGroupName())) {
        setSelectedNode((DefaultMutableTreeNode)node);
        return false;
      }
      return true;
    }
  }
);
}
