{
  if (myDeferredOutput.size() > 1) {
    final StringBuilder buffer=new StringBuilder();
    for (    StringBuilder builder : myDeferredOutput) {
      buffer.append(builder);
    }
    return buffer.toString();
  }
  if (myDeferredOutput.size() == 1) {
    return myDeferredOutput.getFirst().substring(0);
  }
  return "";
}
