{
  String s="def int x = 0;\n" + "def y = 0;\n" + "int z = 10;\n"+ "def int x1";
  doTest(s,"def $x$ = $value$;",3,1);
  doTest(s,"def $x$",4,3);
  doTest(s,"int $x$",3,3);
  doTest(s,"def $x$ = $value$",3,1);
  doTest(s,"def $x$ = 0",2,1);
  doTest(s,"int $x$ = 0",1,1);
  doTest(s,"int $x$ = $value$",2,2);
}
