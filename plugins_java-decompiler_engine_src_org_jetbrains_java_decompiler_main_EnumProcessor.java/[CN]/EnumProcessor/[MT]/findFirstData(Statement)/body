{
  if (stat.getExprents() != null) {
    return stat;
  }
 else {
    if (stat.isLabeled()) {
      return null;
    }
switch (stat.type) {
case Statement.TYPE_SEQUENCE:
case Statement.TYPE_IF:
case Statement.TYPE_ROOT:
case Statement.TYPE_SWITCH:
case Statement.TYPE_SYNCRONIZED:
      return findFirstData(stat.getFirst());
default :
    return null;
}
}
}
