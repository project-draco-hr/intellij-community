{
  if (!isAfterEol() || yylength() < 3) {
    return tokenType;
  }
  if (startsWith(yytext(),"---")) {
    braceCount=0;
    yyBegin(YYINITIAL);
    yypushback(yylength() - 3);
    return DOCUMENT_MARKER;
  }
  if (startsWith(yytext(),"...")) {
    braceCount=0;
    yyBegin(YYINITIAL);
    yypushback(yylength() - 3);
    return DOCUMENT_END;
  }
  return tokenType;
}
