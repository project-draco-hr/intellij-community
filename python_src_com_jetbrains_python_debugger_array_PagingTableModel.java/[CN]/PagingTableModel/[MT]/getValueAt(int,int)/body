{
  Pair<Integer,Integer> key=itemToChunkKey(row,col);
  try {
    Object[][] chunk=myChunkCache.get(key);
    int r=row % CHUNK_ROW_SIZE;
    int c=col % CHUNK_COL_SIZE;
    if (r < chunk.length) {
      if (c < chunk[r].length) {
        return chunk[r][c];
      }
 else {
        return EMPTY_CELL_VALUE;
      }
    }
 else {
      return EMPTY_CELL_VALUE;
    }
  }
 catch (  ExecutionException e) {
    return EMPTY_CELL_VALUE;
  }
}
