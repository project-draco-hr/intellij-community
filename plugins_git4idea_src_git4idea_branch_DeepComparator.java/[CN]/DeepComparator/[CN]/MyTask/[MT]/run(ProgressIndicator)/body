{
  try {
    for (    Map.Entry<GitRepository,GitBranch> entry : myRepositoriesWithCurrentBranches.entrySet()) {
      GitRepository repo=entry.getKey();
      GitBranch currentBranch=entry.getValue();
      myNonPickedCommits.addAll(getNonPickedCommitsFromGit(myProject,repo.getRoot(),myProvider,currentBranch.getName(),myComparedBranch).toArray());
    }
  }
 catch (  VcsException e) {
    LOG.warn(e);
    myException=e;
  }
}
