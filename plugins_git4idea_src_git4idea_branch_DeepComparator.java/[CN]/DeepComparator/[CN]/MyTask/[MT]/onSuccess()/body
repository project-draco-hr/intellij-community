{
  if (myCancelled) {
    return;
  }
  removeHighlighting();
  if (myException != null) {
    VcsNotifier.getInstance(myProject).notifyError("Couldn't compare with branch " + myComparedBranch,myException.getMessage());
    return;
  }
  myHighlighter=new VcsLogHighlighter(){
    @NotNull @Override public VcsCommitStyle getStyle(    int commitIndex,    boolean isSelected){
      return VcsCommitStyleImpl.foreground(!myNonPickedCommits.contains(commitIndex) ? JBColor.GRAY : null);
    }
  }
;
  myUi.addHighlighter(myHighlighter);
}
