{
  InputStream inputStream=null;
  try {
    inputStream=file.getInputStream();
    consumer.consume(inputStream);
  }
 catch (  IOException e) {
    throw new SchemeImportException(e);
  }
 finally {
    if (inputStream != null) {
      try {
        inputStream.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
