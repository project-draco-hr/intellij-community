{
  final PsiClassType capture1=!PsiCapturedWildcardType.isCapture() ? thisClassType : (PsiClassType)captureToplevelWildcards(thisClassType,aClass);
  final PsiClassType capture2=!PsiCapturedWildcardType.isCapture() ? otherClassType : (PsiClassType)captureToplevelWildcards(otherClassType,bClass);
  final PsiClassType.ClassResolveResult result1=capture1.resolveGenerics();
  final PsiClassType.ClassResolveResult result2=capture2.resolveGenerics();
  return equalOnEquivalentClasses(result1.getSubstitutor(),aClass,result2.getSubstitutor(),bClass);
}
