{
  AbstractPopup popup=new AbstractPopup().init(myProject,myComponent,myPreferredFocusedComponent,myRequestFocus,myFocusable,myMovable,myDimensionServiceKey,myResizable,myTitle,myCallback,myCancelOnClickOutside,myListeners,myUseDimServiceForXYLocation,myCommandButton,myCancelButton,myCancelOnMouseOutCallback,myCancelOnWindow,myTitleIcon,myCancelKeyEnabled,myLocateByContent,myPlaceWithinScreen,myMinSize,myAlpha,null,myInStack,myModalContext,myFocusOwners,myAd,myAdAlignment,false,myKeyboardActions,mySettingsButtons,myPinCallback,myMayBeParent,myShowShadow,myCancelOnWindowDeactivation,myKeyEventHandler);
  if (myUserData != null) {
    popup.setUserData(myUserData);
  }
  Disposer.register(ApplicationManager.getApplication(),popup);
  return popup;
}
