{
synchronized (myMonitor) {
    final String rootUrl=getUrlRootForUrl(url);
    if (rootUrl == null) {
      return null;
    }
    final RootUrlInfo parentInfo=myMoreRealMapping.byUrl(rootUrl);
    if (parentInfo == null) {
      return null;
    }
    return new File(parentInfo.getIoFile(),url.substring(rootUrl.length()));
  }
}
