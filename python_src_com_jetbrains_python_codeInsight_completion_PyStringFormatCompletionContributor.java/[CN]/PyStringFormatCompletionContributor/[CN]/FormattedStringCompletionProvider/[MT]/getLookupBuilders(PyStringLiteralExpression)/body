{
  Map<String,PyStringFormatParser.SubstitutionChunk> chunks=PyStringFormatParser.getKeywordSubstitutions(PyStringFormatParser.filterSubstitutions(PyStringFormatParser.parseNewStyleFormat(literalExpression.getStringValue())));
  List<LookupElementBuilder> keys=new ArrayList<LookupElementBuilder>();
  for (  String chunk : chunks.keySet()) {
    keys.add(LookupElementBuilder.create(chunk).withTypeText("arg").withIcon(PlatformIcons.VARIABLE_ICON));
  }
  return keys;
}
