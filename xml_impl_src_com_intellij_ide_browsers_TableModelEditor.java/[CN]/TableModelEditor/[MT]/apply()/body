{
  if (!modifiedItems.isEmpty()) {
    isApplying=true;
    @SuppressWarnings("unchecked") final ColumnInfo<T,Object>[] columns=((ListTableModel)table.getModel()).getColumnInfos();
    modifiedItems.forEachEntry(new TObjectObjectProcedure<T,T>(){
      @Override public boolean execute(      T item,      T newItem){
        for (        ColumnInfo<T,Object> column : columns) {
          if (column.isCellEditable(item)) {
            column.setValue(item,column.valueOf(newItem));
          }
        }
        return true;
      }
    }
);
    isApplying=false;
    modifiedItems.clear();
  }
  return items;
}
