{
  if (!modifiedItems.isEmpty()) {
    for (    Map.Entry<T,T> entry : modifiedItems.entrySet()) {
      if (entry.getValue().equals(entry.getKey())) {
        return true;
      }
    }
  }
  if (items.size() == oldItems.size()) {
    for (int i=0, size=items.size(); i < size; i++) {
      if (items.get(i) != oldItems.get(i)) {
        return true;
      }
    }
  }
 else {
    return true;
  }
  return false;
}
