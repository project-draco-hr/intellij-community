{
  if (url.startsWith("https://")) {
    return url.substring(8);
  }
 else   if (url.startsWith("http://")) {
    return url.substring(7);
  }
 else   if (url.startsWith("git@")) {
    return url.substring(4);
  }
 else   if (url.startsWith("git://")) {
    return url.substring(6);
  }
 else {
    return url;
  }
}
