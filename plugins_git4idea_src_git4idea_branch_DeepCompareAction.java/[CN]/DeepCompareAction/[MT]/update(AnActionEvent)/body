{
  super.update(e);
  Project project=e.getData(CommonDataKeys.PROJECT);
  VcsLogUi ui=e.getData(VcsLogDataKeys.VCS_LOG_UI);
  e.getPresentation().setEnabledAndVisible(project != null && ui != null && hasGitRoots(project,VcsLogUtil.getAllVisibleRoots(ui.getDataPack().getLogProviders().keySet(),ui.getFilterUi().getFilters().getRootFilter(),ui.getFilterUi().getFilters().getStructureFilter())));
}
