{
  int index=0;
  if (key < 0) {
    index=2;
    key=-key;
  }
 else   if (key >= VarExprent.STACK_BASE) {
    index=1;
    key-=VarExprent.STACK_BASE;
  }
  ArrayList<Entry<Integer,E>> lst=lstElements.get(index);
  Entry<Integer,E> ent;
  if (key < lst.size() && (ent=lst.get(key)) != null) {
    return ent.getValue();
  }
  return null;
}
