{
  T tool=myTool;
  if (tool == null) {
    myTool=tool=(T)myEP.instantiateTool();
    if (!tool.getShortName().equals(myEP.getShortName())) {
      LOG.error("Short name not matched for " + tool.getClass() + ": getShortName() = "+ tool.getShortName()+ "; ep.shortName = "+ myEP.getShortName());
    }
  }
  return tool;
}
