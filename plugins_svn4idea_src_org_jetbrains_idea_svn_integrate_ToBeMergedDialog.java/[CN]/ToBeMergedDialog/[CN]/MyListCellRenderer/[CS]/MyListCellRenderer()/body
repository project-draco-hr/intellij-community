{
  myPanel=new JPanel(new BorderLayout());
  myCheckBox=new JCheckBox();
  myCheckBox.setEnabled(true);
  myCheckBox.setSelected(true);
  myRenderer=new CommittedChangeListRenderer(myProject,Collections.<CommittedChangeListDecorator>singletonList(new CommittedChangeListDecorator(){
    @Nullable @Override public Icon decorate(    CommittedChangeList list){
      ListMergeStatus status=myAlreadyCalculatedState ? ListMergeStatus.NOT_MERGED : ObjectUtils.notNull(myStatusMap.get(list.getNumber()),ListMergeStatus.REFRESHING);
      return status.getIcon();
    }
  }
));
}
