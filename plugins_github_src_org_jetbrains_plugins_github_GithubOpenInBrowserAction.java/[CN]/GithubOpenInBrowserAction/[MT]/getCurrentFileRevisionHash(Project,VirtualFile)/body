{
  try {
    VcsRevisionNumber revision=GitHistoryUtils.getCurrentRevision(project,VcsUtil.getFilePath(file),null);
    if (revision instanceof GitRevisionNumber) {
      return ((GitRevisionNumber)revision).getRev();
    }
    return null;
  }
 catch (  VcsException e) {
    LOG.warn(e);
    return null;
  }
}
