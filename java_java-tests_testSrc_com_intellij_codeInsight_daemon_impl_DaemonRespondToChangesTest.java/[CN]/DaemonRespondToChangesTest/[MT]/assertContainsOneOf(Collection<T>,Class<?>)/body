{
  T result=null;
  for (  T t : collection) {
    if (aClass.isInstance(t)) {
      if (result != null) {
        fail("multiple " + aClass.getName() + " objects present in collection "+ collection);
      }
 else {
        result=t;
      }
    }
  }
  assertNotNull(aClass.getName() + " object not found in collection " + collection,result);
  return result;
}
