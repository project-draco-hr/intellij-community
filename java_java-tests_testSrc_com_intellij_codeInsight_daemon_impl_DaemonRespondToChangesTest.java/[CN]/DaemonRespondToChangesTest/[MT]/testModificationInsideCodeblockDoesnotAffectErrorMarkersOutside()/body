{
  configureByFile(BASE_PATH + "ErrorMark.java");
  List<HighlightInfo> errs=highlightErrors();
  assertEquals(1,errs.size());
  assertEquals("'}' expected",errs.get(0).getDescription());
  type("//comment");
  errs=highlightErrors();
  assertEquals(1,errs.size());
  assertEquals("'}' expected",errs.get(0).getDescription());
}
