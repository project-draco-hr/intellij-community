{
  final PsiBuilder.Marker marker=builder.mark();
  if (!parseSingleExpression(builder,false)) {
    marker.drop();
    return false;
  }
  checkMatches(builder,PyTokenTypes.COLON,": expected");
  if (!parseSingleExpression(builder,false)) {
    marker.drop();
    return false;
  }
  marker.done(PyElementTypes.KEY_VALUE_EXPRESSION);
  return true;
}
