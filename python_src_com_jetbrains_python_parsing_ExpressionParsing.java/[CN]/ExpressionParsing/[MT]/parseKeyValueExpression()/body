{
  final PsiBuilder.Marker marker=myBuilder.mark();
  if (!parseSingleExpression(false)) {
    marker.drop();
    return false;
  }
  checkMatches(PyTokenTypes.COLON,message("PARSE.expected.colon"));
  if (!parseSingleExpression(false)) {
    marker.drop();
    return false;
  }
  marker.done(PyElementTypes.KEY_VALUE_EXPRESSION);
  return true;
}
