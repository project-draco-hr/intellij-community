{
  if (owner instanceof PyFunction && element instanceof PyParameter) {
    final PyFunction function=(PyFunction)owner;
    final String functionName=function.getName();
    return !PyNames.INIT.equals(functionName) && function.getContainingClass() != null && PyNames.getBuiltinMethods(LanguageLevel.forElement(function)).containsKey(functionName);
  }
  return false;
}
