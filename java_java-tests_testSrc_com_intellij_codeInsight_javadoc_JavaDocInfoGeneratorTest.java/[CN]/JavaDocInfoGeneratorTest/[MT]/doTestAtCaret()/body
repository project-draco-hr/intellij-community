{
  configureByFile("/codeInsight/javadocIG/" + getTestName(true) + ".java");
  String docInfo=JavaExternalDocumentationTest.getDocumentationText(myFile,myEditor.getCaretModel().getOffset());
  assertNotNull(docInfo);
