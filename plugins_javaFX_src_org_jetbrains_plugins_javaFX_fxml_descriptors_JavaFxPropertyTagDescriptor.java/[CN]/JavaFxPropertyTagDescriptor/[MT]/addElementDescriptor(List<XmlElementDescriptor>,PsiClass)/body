{
  if (aClass != null && !CommonClassNames.JAVA_LANG_OBJECT.equals(aClass.getQualifiedName()) && !aClass.isInterface() && !PsiUtil.isAbstractClass(aClass) && !PsiUtil.isInnerClass(aClass) && JavaFxPsiUtil.isAbleToInstantiate(aClass)) {
    descriptors.add(new JavaFxClassTagDescriptor(aClass.getName(),aClass));
  }
}
