{
  if (method == null) {
    return false;
  }
  if (method.hasModifierProperty(PsiModifier.ABSTRACT) || method.hasModifierProperty(PsiModifier.STATIC) || !method.hasModifierProperty(PsiModifier.PUBLIC)) {
    return false;
  }
  final PsiType returnType=method.getReturnType();
  if (!PsiType.VOID.equals(returnType)) {
    return false;
  }
  final PsiParameterList parameterList=method.getParameterList();
  return parameterList.getParametersCount() == 0;
}
