{
  if (project == null || ProjectLevelVcsManager.getInstance(project).getAllVcss().length == 0) {
    return Collections.emptyList();
  }
  return Collections.unmodifiableCollection(Arrays.asList(option(project,"checkbox.use.read.only.flag.for.not.edited.files","MAKE_NEW_FILES_READONLY"),option(project,"checkbox.show.cvs.server.output","SHOW_OUTPUT")));
}
