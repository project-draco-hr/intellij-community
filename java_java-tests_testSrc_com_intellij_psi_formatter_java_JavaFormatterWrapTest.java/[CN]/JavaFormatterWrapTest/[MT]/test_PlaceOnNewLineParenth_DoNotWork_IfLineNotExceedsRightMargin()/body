{
  getSettings().CALL_PARAMETERS_LPAREN_ON_NEXT_LINE=true;
  doMethodTest("run(new Runnable() {\n" + "public void run() {\n" + "}\n"+ "});","run(new Runnable() {\n" + "    public void run() {\n" + "    }\n"+ "});");
  doMethodTest("run(() -> {\n" + "int a = 2;\n" + "});","run(() -> {\n" + "    int a = 2;\n" + "});");
}
