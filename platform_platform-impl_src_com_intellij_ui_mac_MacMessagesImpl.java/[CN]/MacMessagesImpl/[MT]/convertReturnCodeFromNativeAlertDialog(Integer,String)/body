{
  int cancelCode=1;
  int code;
  if (alternateText != null) {
    cancelCode=2;
    if (returnCode == null)     returnCode=2;
switch (returnCode) {
case 1:
      code=0;
    break;
case 0:
  code=1;
break;
case -1:
default :
code=2;
break;
}
}
 else {
cancelCode=1;
if (returnCode == null) returnCode=-1;
switch (returnCode) {
case 1:
code=0;
break;
case -1:
default :
code=1;
break;
}
}
if (cancelCode == code) {
code=OPERATION_CANCELED;
}
;
return code;
}
