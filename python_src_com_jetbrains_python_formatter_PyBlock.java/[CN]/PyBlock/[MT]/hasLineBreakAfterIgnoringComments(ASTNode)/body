{
  for (ASTNode next=TreeUtil.nextLeaf(node); next != null; next=TreeUtil.nextLeaf(next)) {
    if (isWhitespace(next)) {
      if (next.textContains('\n')) {
        return true;
      }
    }
 else     if (next.getElementType() == PyTokenTypes.END_OF_LINE_COMMENT) {
      return true;
    }
 else {
      break;
    }
  }
  return false;
}
