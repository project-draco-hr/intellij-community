{
  final ASTNode nodeParent=node.getTreeParent();
  if (nodeParent == null) {
    return false;
  }
  final ASTNode nodeGrandParent=nodeParent.getTreeParent();
  if (nodeGrandParent == null) {
    return false;
  }
  return nodeParent.getElementType() == PyElementTypes.KEY_VALUE_EXPRESSION && nodeGrandParent.getElementType() == PyElementTypes.DICT_LITERAL_EXPRESSION;
}
