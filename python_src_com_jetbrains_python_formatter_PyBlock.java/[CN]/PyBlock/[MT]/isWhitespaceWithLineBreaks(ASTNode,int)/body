{
  if (isWhitespace(node)) {
    final String prevNodeText=node.getText();
    int count=0;
    for (int i=0; i < prevNodeText.length(); i++) {
      if (prevNodeText.charAt(i) == '\n') {
        count++;
        if (count == minCount) {
          return true;
        }
      }
    }
  }
  return false;
}
