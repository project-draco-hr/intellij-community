{
  if (stringValue == null) {
    final StringBuilder out=new StringBuilder();
    iterateCharacterRanges(new TextRangeConsumer(){
      public boolean process(      int startOffset,      int endOffset,      String value){
        out.append(value);
        return true;
      }
    }
);
    stringValue=out.toString();
  }
  return stringValue;
}
