{
  ConcurrentPackedBitsArray bitSet=new ConcurrentPackedBitsArray(4);
  int N=3000;
  for (int i=0; i < N; i++) {
    assertEquals(0,bitSet.get(i));
    bitSet.set(i,0xa);
    assertEquals(0xa,bitSet.get(i));
    bitSet.set(i,0x2);
    assertEquals(0x2,bitSet.get(i));
  }
  try {
    bitSet.set(1,0x10);
    fail("must throw IAE");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    bitSet.set(1,-1);
    fail("must throw IAE");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new ConcurrentPackedBitsArray(65);
    fail("must throw IAE");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new ConcurrentPackedBitsArray(0);
    fail("must throw IAE");
  }
 catch (  IllegalArgumentException e) {
  }
}
