{
  super(project);
  this.model=model;
  propertiesEditor=new RepositoryLibraryPropertiesEditor(project,model,description,new RepositoryLibraryPropertiesEditor.ModelChangeListener(){
    @Override public void onChange(    RepositoryLibraryPropertiesEditor editor){
      setOKActionEnabled(editor.isValid() && (!changesRequired || editor.hasChanges()));
    }
  }
);
  setTitle(description.getDisplayName());
  init();
}
