{
  final Element parent=attribute.getParent();
  final String attrName=attribute.getName();
  String tagName=parent.getName();
  if (tagName.equals(EnvironmentVariablesComponent.ENV) && (attrName.equals(EnvironmentVariablesComponent.NAME) || attrName.equals(EnvironmentVariablesComponent.VALUE))) {
    return true;
  }
  if (tagName.equals("configuration") && attrName.equals("name")) {
    return true;
  }
  return false;
}
