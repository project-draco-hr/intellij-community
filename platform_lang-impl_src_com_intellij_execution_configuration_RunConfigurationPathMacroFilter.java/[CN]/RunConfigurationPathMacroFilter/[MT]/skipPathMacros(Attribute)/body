{
  final Element parent=attribute.getParent();
  final String attrName=attribute.getName();
  String tagName=parent.getName();
  return tagName.equals(EnvironmentVariablesComponent.ENV) && (attrName.equals(EnvironmentVariablesComponent.NAME) || attrName.equals(EnvironmentVariablesComponent.VALUE)) || tagName.equals("option") && "MAIN_CLASS_NAME".equals(parent.getAttributeValue("name"));
}
