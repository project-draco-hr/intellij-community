{
  if (ProjectLevelVcsManager.getInstance(project).getAllVcss().length == 0) {
    return Collections.emptyList();
  }
  return Collections.unmodifiableCollection(Arrays.asList(option(project,"Mercurial: Check for incoming and outgoing changesets","isCheckIncomingOutgoing","setCheckIncomingOutgoing"),option(project,"Mercurial: Ignore whitespace differences in annotations","isWhitespacesIgnoredInAnnotations","setIgnoreWhitespacesInAnnotations")));
}
