{
  if (SystemInfo.isWindows) {
    String uid=getInstallationUIDOnWindows(propertiesComponent);
    if (uid != null) {
      return uid;
    }
  }
  String uid=propertiesComponent.getValue(INSTALLATION_UID);
  if (uid == null) {
    uid=generateUUID();
    propertiesComponent.setValue(INSTALLATION_UID,uid);
  }
  return uid;
}
