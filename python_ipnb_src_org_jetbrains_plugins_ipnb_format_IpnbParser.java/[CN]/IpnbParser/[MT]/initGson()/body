{
  final GsonBuilder builder=new GsonBuilder().setPrettyPrinting().disableHtmlEscaping().registerTypeAdapter(IpnbCellRaw.class,new RawCellAdapter()).registerTypeAdapter(IpnbFileRaw.class,new FileAdapter()).registerTypeAdapter(CellOutputRaw.class,new OutputsAdapter()).registerTypeAdapter(OutputDataRaw.class,new OutputDataAdapter()).serializeNulls();
  return builder.create();
}
