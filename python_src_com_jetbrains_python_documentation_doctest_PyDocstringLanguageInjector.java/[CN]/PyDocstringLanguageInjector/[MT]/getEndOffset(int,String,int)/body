{
  int end;
  int length=s.length();
  if (s.trim().endsWith("\"\"\"") || s.trim().endsWith("'''"))   length=length - 3;
 else   if (start + length == maxPosition && (s.trim().endsWith("\"") || s.trim().endsWith("'")))   length=length - 1;
  end=start + length;
  if (s.endsWith("\n"))   end=end - 1;
  return end;
}
