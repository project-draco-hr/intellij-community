{
  if (!MORE.equals(selectedValue))   Disposer.dispose(myMore.getDisposable());
  if (LOCAL.equals(selectedValue)) {
    createLocalSdk();
  }
 else   if (REMOTE.equals(selectedValue)) {
    createRemoteSdk();
  }
 else   if (VIRTUALENV.equals(selectedValue)) {
    createVirtualEnvSdk();
  }
 else {
    myMore.show();
  }
}
