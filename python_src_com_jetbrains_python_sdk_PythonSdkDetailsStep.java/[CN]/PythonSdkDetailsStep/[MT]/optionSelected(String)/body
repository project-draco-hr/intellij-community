{
  if (!MORE.equals(selectedValue) && myMore != null)   Disposer.dispose(myMore.getDisposable());
  if (LOCAL.equals(selectedValue)) {
    createLocalSdk();
  }
 else   if (REMOTE.equals(selectedValue)) {
    createRemoteSdk();
  }
 else   if (VIRTUALENV.equals(selectedValue)) {
    createVirtualEnvSdk();
  }
 else   if (myMore != null) {
    myMore.show();
  }
}
