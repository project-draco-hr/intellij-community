{
  final List<String> options=new ArrayList<>();
  options.add(LOCAL);
  if (PythonRemoteInterpreterManager.getInstance() != null) {
    options.add(REMOTE);
  }
  options.add(VIRTUALENV);
  if (PyCondaPackageService.getSystemCondaExecutable() != null) {
    options.add(CONDA);
  }
  if (showMore) {
    options.add(MORE);
  }
  return options;
}
