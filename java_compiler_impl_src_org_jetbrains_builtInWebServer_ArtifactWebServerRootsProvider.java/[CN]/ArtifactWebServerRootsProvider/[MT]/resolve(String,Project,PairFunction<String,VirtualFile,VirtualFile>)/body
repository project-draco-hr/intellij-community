{
  for (  Artifact artifact : ArtifactManager.getInstance(project).getArtifacts()) {
    VirtualFile root=artifact.getOutputFile();
    if (root != null) {
      VirtualFile file=root.findFileByRelativePath(path);
      if (file != null) {
        return new PathInfo(file,root);
      }
    }
  }
  return null;
}
