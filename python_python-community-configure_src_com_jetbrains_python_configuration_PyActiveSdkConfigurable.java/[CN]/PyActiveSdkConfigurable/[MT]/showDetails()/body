{
  final PythonSdkDetailsDialog moreDialog=myModule == null ? new PythonSdkDetailsDialog(myProject,myAddSdkCallback,getSettingsModifiedCallback()) : new PythonSdkDetailsDialog(myModule,myAddSdkCallback,getSettingsModifiedCallback());
  PythonSdkDetailsStep.show(myProject,myProjectSdksModel.getSdks(),moreDialog,myMainPanel,myDetailsButton.getLocationOnScreen(),myAddSdkCallback);
}
