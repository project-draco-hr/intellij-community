{
  File toFile=getFile(toDir);
  ValidationResult result=doValidateAccess(toFile,ValidationResult.Action.CREATE);
  if (result != null)   return result;
  if (toFile.exists()) {
    ValidationResult.Option[] options=myPatch.isStrict() ? new ValidationResult.Option[]{ValidationResult.Option.REPLACE} : new ValidationResult.Option[]{ValidationResult.Option.REPLACE,ValidationResult.Option.KEEP};
    return new ValidationResult(ValidationResult.Kind.CONFLICT,myPath,ValidationResult.Action.CREATE,ValidationResult.ALREADY_EXISTS_MESSAGE,options);
  }
  return null;
}
