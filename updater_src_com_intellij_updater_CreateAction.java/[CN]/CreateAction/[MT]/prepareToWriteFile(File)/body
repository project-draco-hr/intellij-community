{
  if (file.exists()) {
    try {
      Utils.delete(file);
    }
 catch (    IOException e) {
      if (Utils.isWindows() && file.exists()) {
        throw new RetryException(e);
      }
 else {
        throw e;
      }
    }
    return;
  }
  while (file != null && !file.exists()) {
    file=file.getParentFile();
  }
  if (file != null && !file.isDirectory()) {
    Utils.delete(file);
  }
}
