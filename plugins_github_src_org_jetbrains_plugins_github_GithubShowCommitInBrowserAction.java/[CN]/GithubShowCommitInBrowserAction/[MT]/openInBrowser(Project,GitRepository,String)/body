{
  String url=GithubUtil.findGithubRemoteUrl(repository);
  if (url == null) {
    GithubUtil.LOG.info(String.format("Repository is not under GitHub. Root: %s, Remotes: %s",repository.getRoot(),GitUtil.getPrintableRemotes(repository.getRemotes())));
    return;
  }
  GithubFullPath userAndRepository=GithubUrlUtil.getUserAndRepositoryFromRemoteUrl(url);
  if (userAndRepository == null) {
    GithubNotifications.showError(project,GithubOpenInBrowserAction.CANNOT_OPEN_IN_BROWSER,"Cannot extract info about repository: " + url);
    return;
  }
  String githubUrl=GithubUrlUtil.getGitHost() + '/' + userAndRepository.getUser()+ '/'+ userAndRepository.getRepository()+ "/commit/"+ revisionHash;
  BrowserUtil.launchBrowser(githubUrl);
}
