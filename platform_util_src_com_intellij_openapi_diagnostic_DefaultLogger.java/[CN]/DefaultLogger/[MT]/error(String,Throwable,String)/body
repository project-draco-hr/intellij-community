{
  System.err.println("ERROR: " + message);
  if (t != null)   t.printStackTrace();
  if (details.length > 0) {
    System.out.println("details: ");
    for (    String detail : details) {
      System.out.println(detail);
    }
  }
  AssertionError error=new AssertionError(message);
  error.initCause(t);
  throw error;
}
