{
  String newName=Messages.showInputDialog(myProject,"New name for the branch '" + myCurrentBranchName + "':","Rename Branch " + myCurrentBranchName,null,myCurrentBranchName,GitNewBranchNameValidator.newInstance(myRepositories));
  if (newName != null) {
    GitBrancher brancher=ServiceManager.getService(myProject,GitBrancher.class);
    brancher.renameBranch(myCurrentBranchName,newName,myRepositories);
    reportUsage("git.branch.rename");
  }
}
