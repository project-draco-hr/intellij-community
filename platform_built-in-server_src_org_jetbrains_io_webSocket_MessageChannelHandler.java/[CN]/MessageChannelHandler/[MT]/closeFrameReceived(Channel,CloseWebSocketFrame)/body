{
  WebSocketClient client=(WebSocketClient)channel.attr(ClientManagerKt.getCLIENT()).get();
  if (client == null) {
    super.closeFrameReceived(channel,message);
  }
 else {
    try {
      clientManager.disconnectClient(channel,client,false);
    }
  finally {
      client.disconnect(message);
    }
  }
}
