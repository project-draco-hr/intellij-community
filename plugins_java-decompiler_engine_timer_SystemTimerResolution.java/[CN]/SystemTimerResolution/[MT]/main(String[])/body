{
  for (int r=0; r < 3000; ++r)   System.currentTimeMillis();
  long time=System.currentTimeMillis(), time_prev=time;
  for (int i=0; i < 5; ++i) {
    while (time == time_prev)     time=System.currentTimeMillis();
    System.out.println("delta = " + (time - time_prev) + " ms");
    time_prev=time;
  }
}
