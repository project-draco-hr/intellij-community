{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId>maven-surefire-plugin</artifactId>"+ "      <version>2.4.3</version>"+ "    </plugin>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId>maven-surefire-plugin</artifactId>"+ "      <version>2.4.3</version>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  String goal="org.apache.maven.plugins:maven-surefire-plugin:2.4.3:test";
  assignShortcut(myProjectPom,goal,"alt shift X");
  resolvePlugins();
  assertKeymapContains(myProjectPom,goal);
}
