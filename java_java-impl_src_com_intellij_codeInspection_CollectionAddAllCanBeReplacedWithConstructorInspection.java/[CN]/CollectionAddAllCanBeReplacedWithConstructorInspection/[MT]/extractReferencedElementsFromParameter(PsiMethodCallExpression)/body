{
  final PsiExpression psiExpression=expression.getArgumentList().getExpressions()[0];
  final Collection<PsiReferenceExpression> references=new ArrayList<PsiReferenceExpression>(PsiTreeUtil.findChildrenOfType(psiExpression,PsiReferenceExpression.class));
  if (psiExpression instanceof PsiReferenceExpression) {
    references.add((PsiReferenceExpression)psiExpression);
  }
  return ContainerUtil.mapNotNull(references,new NullableFunction<PsiReferenceExpression,PsiElement>(){
    @Nullable @Override public PsiElement fun(    PsiReferenceExpression expression){
      return expression.resolve();
    }
  }
);
}
