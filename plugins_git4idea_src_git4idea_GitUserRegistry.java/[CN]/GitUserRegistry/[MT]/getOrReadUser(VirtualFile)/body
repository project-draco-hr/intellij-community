{
  VcsUser user=myUserMap.get(root);
  if (user == null) {
    try {
      user=readCurrentUser(myProject,root);
      if (user != null) {
        myUserMap.put(root,user);
      }
    }
 catch (    VcsException e) {
      LOG.warn("Could not retrieve user name in " + root,e);
    }
  }
  return user;
}
