{
  init("\n\n");
  mouse().clickAt(1,1);
  verifyCaretsAndSelections(1,1,1,1);
  executeAction("EditorRightWithSelection");
  verifyCaretsAndSelections(1,2,1,2);
  executeAction("EditorDownWithSelection");
  verifyCaretsAndSelections(1,2,1,2,2,2,1,2);
  executeAction("EditorLeftWithSelection");
  verifyCaretsAndSelections(1,1,1,1,2,1,1,1);
  executeAction("EditorLeftWithSelection");
  verifyCaretsAndSelections(1,0,0,1,2,0,0,1);
  executeAction("EditorUpWithSelection");
  verifyCaretsAndSelections(1,0,0,1);
  executeAction("EditorUpWithSelection");
  verifyCaretsAndSelections(0,0,0,1,1,0,0,1);
  executeAction("EditorRightWithSelection");
  verifyCaretsAndSelections(0,1,1,1,1,1,1,1);
  executeAction("EditorRightWithSelection");
  verifyCaretsAndSelections(0,2,1,2,1,2,1,2);
  executeAction("EditorDownWithSelection");
  verifyCaretsAndSelections(1,2,1,2);
  executeAction("EditorLeftWithSelection");
  verifyCaretsAndSelections(1,1,1,1);
}
