{
  try {
    String sessionId=login(createBuilder(null,null,null));
    if (sessionId != null) {
      settings.updateSession(sessionId);
      settings.setStatus(IdeaServerStatus.LOGGED_IN);
    }
  }
 catch (  Exception e) {
    settings.setStatus(IdeaServerStatus.CONNECTION_FAILED);
    throw e;
  }
}
