{
  try {
    List<SvnBranchItem> branches=loadBranches();
    myBunch.updateBranches(myRoot,myUrl,new InfoStorage<List<SvnBranchItem>>(branches,myInfoReliability));
  }
 catch (  VcsException e) {
    showError(e);
  }
catch (  SVNException e) {
    showError(e);
  }
 finally {
    if (myCallback != null) {
      myCallback.run();
    }
  }
}
