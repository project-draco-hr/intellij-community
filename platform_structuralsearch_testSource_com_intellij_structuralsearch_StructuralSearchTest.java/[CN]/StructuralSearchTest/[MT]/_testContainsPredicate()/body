{
  String s1="{{\n" + "  int a;\n" + "  a = 1;\n"+ "}\n"+ "{\n"+ "  int b = 1;\n"+ "  b = 1;\n"+ "}\n"+ "{\n"+ "  int c = 2;\n"+ "  c = 2;\n"+ "}}";
  assertEquals(2,findMatchesCount(s1,"{ '_a*:[contains( \"'type '_a = '_b;\" )]; }"));
  assertEquals(1,findMatchesCount(s1,"{ '_a*:[!contains( \"'_type '_a = '_b;\" )]; }"));
}
