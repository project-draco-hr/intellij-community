{
  final String name=ref.getName();
  if (name != null) {
    final PyResolveProcessor processor=new PyResolveProcessor(name,true);
    PyResolveUtil.scopeCrawlUp(processor,ref,name,null);
    for (    PsiElement element : processor.getElements()) {
      if (element != null) {
        return true;
      }
    }
  }
  return false;
}
