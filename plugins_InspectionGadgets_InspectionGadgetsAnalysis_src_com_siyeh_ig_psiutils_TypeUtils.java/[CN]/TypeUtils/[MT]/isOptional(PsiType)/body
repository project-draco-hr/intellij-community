{
  final PsiClass aClass=PsiUtil.resolveClassInClassTypeOnly(type);
  if (aClass == null) {
    return false;
  }
  final String qualifiedName=aClass.getQualifiedName();
  return CommonClassNames.JAVA_UTIL_OPTIONAL.equals(qualifiedName) || "java.util.OptionalDouble".equals(qualifiedName) || "java.util.OptionalInt".equals(qualifiedName)|| "java.util.OptionalLong".equals(qualifiedName);
}
