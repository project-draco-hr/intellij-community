{
  myRegisteredBreakpoints.put(position,breakpoint);
  if (myDebugger.isConnected()) {
    final SetBreakpointCommand command=new SetBreakpointCommand(myDebugger,breakpoint.getType().getId(),position.getFile(),position.getLine(),breakpoint.getCondition(),breakpoint.getLogExpression());
    myDebugger.execute(command);
  }
}
