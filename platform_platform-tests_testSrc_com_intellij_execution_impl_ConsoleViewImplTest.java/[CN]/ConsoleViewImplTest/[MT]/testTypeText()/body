{
  final ConsoleViewImpl console=createConsole();
  console.print("Initial",ConsoleViewContentType.NORMAL_OUTPUT);
  console.flushDeferredText();
  try {
    console.clear();
    console.print("Hi",ConsoleViewContentType.NORMAL_OUTPUT);
    type(console,'1');
    type(console,'2');
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
