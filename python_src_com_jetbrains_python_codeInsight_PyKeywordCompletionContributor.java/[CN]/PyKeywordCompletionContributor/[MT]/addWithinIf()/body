{
  extend(CompletionType.BASIC,psiElement().withLanguage(PythonLanguage.getInstance()).andNot(AFTER_QUALIFIED_REFERENCE).andOr(IN_IF,AFTER_IF),new CompletionProvider<CompletionParameters>(){
    protected void addCompletions(    @NotNull final CompletionParameters parameters,    final ProcessingContext context,    @NotNull final CompletionResultSet result){
      final @NonNls String[] space_strings={"elif"};
      addKeywords(space_strings,TailType.SPACE,parameters,result);
    }
  }
);
}
