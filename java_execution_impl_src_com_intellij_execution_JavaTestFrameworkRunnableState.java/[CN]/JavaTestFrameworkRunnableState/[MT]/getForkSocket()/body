{
  if (myForkSocket == null && (!Comparing.strEqual(getForkMode(),"none") || forkPerModule()) && getRunnerSettings() != null) {
    try {
      myForkSocket=new ServerSocket(0,0,InetAddress.getByName("127.0.0.1"));
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
  return myForkSocket;
}
