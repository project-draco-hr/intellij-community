{
  String[] newEnv=null;
  if (addEnv != null) {
    final Map<String,String> envMap=mergeEnvVariables(System.getenv(),addEnv);
    newEnv=new String[envMap.size()];
    int i=0;
    for (    Map.Entry<String,String> entry : envMap.entrySet()) {
      newEnv[i]=entry.getKey() + "=" + entry.getValue();
      i+=1;
    }
  }
  return newEnv;
}
