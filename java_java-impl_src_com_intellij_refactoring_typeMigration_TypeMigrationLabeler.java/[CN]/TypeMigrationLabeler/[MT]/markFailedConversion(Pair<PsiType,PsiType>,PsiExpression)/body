{
  LOG.assertTrue(typePair.getSecond() != null);
  final Pair<SmartPsiElementPointer<PsiExpression>,PsiType> key=Pair.create(SmartPointerManager.getInstance(expression.getProject()).createSmartPsiElementPointer(expression),typePair.getSecond());
  if (!myFailedConversions.containsKey(key)) {
    myFailedConversions.put(key,getCurrentRoot());
  }
}
