{
  final String target;
  if (myRequirementsTxt != null) {
    target="requirements.txt";
  }
 else   if (mySetupPyRequires != null || mySetupArgumentList != null) {
    target="setup.py";
  }
 else {
    target="project requirements";
  }
  return String.format("Add requirement '%s' to %s",myPackageName,target);
}
