{
  PyDecoratorList decolist=node.getDecoratorList();
  if (decolist != null) {
    PyDecorator[] decos=decolist.getDecorators();
    if (decos.length == 1) {
      PyDecorator deco=decos[0];
      String deconame=deco.getName();
      if (deco.isBuiltin() && name.equals(deconame)) {
        return true;
      }
    }
  }
  return false;
}
