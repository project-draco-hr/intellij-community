{
  if (!versionFile.exists())   return 0;
  final DataInputStream versionInput=new DataInputStream(new FileInputStream(versionFile));
  int version;
  try {
    version=DataInputOutputUtil.readINT(versionInput);
  }
  finally {
    try {
      versionInput.close();
    }
 catch (    IOException ignore) {
    }
  }
  return version;
}
