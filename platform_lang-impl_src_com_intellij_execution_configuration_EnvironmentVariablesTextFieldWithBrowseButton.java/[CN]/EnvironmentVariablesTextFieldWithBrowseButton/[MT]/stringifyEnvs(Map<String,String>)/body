{
  if (envs.isEmpty()) {
    return "";
  }
  StringBuilder buf=new StringBuilder();
  for (  Map.Entry<String,String> entry : envs.entrySet()) {
    if (buf.length() > 0) {
      buf.append(";");
    }
    buf.append(entry.getKey()).append("=").append(entry.getValue());
  }
  return buf.toString();
}
