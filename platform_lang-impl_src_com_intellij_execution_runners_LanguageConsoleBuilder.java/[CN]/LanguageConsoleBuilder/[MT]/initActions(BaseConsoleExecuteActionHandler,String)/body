{
  ConsoleExecuteAction action=myProcessHandler == null ? new ConsoleExecuteAction(myConsole,executeActionHandler) : new ProcessBackedConsoleExecuteAction(myConsole,myProcessHandler,executeActionHandler);
  action.registerCustomShortcutSet(action.getShortcutSet(),myConsole.getConsoleEditor().getComponent());
  new ConsoleHistoryController(historyType,"",myConsole,executeActionHandler.getConsoleHistoryModel()).install();
  return this;
}
