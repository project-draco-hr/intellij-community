{
  if (GraphicsEnvironment.isHeadless()) {
    showMessage("Startup Error","Unable to detect graphics environment",true);
    return false;
  }
  try {
    Toolkit.getDefaultToolkit();
  }
 catch (  Throwable t) {
    hasGraphics=false;
    showMessage("Startup Error",t);
    return false;
  }
  return true;
}
