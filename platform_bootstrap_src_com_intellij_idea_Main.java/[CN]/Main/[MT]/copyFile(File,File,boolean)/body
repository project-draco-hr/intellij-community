{
  if (move) {
    if (!original.renameTo(copy) || !FileUtilRt.delete(original)) {
      throw new IOException("Cannot create temporary file: " + copy);
    }
  }
 else {
    FileUtilRt.copy(original,copy);
    if (!copy.exists()) {
      throw new IOException("Cannot create temporary file: " + copy);
    }
  }
}
