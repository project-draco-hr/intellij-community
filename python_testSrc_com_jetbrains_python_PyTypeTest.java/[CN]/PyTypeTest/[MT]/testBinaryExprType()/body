{
  PyClassType type=(PyClassType)doTest("expr = 1 + 2");
  assertEquals("int",type.getName());
  type=(PyClassType)doTest("expr = '1' + '2'");
  assertEquals("str",type.getName());
  type=(PyClassType)doTest("expr = '%s' % ('a')");
  assertEquals("str",type.getName());
  type=(PyClassType)doTest("expr = [1] + [2]");
  assertEquals("list",type.getName());
}
