{
  PyExpression expr=parseExpr("def f():\n" + "    '''\n" + "    :rtype: int or slice\n"+ "    '''\n"+ "    raise NotImplementedError\n"+ "\n"+ "x = f()\n"+ "expr = x.start\n");
  TypeEvalContext context=TypeEvalContext.slow().withTracing();
  PyType actual=expr.getType(context);
  assertNull(actual);
}
