{
  doTest("str","a = None\n" + "if isinstance(a, str) is not False:\n" + "    expr = a\n"+ "raise TypeError('Invalid type')");
  doTest("str","a = None\n" + "if False is not isinstance(a, str):\n" + "    expr = a\n"+ "raise TypeError('Invalid type')");
  doTest("str","a = None\n" + "if not isinstance(a, str) is False:\n" + "    expr = a\n"+ "raise TypeError('Invalid type')");
  doTest("str","a = None\n" + "if not False is isinstance(a, str):\n" + "    expr = a\n"+ "raise TypeError('Invalid type')");
}
