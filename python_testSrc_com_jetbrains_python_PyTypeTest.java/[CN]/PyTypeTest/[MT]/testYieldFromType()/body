{
  PythonLanguageLevelPusher.setForcedLanguageLevel(myFixture.getProject(),LanguageLevel.PYTHON33);
  try {
    doTest("str | int | float","def subgen():\n" + "    for i in [1, 2, 3]:\n" + "        yield i\n"+ "\n"+ "def gen():\n"+ "    yield 'foo'\n"+ "    yield from subgen()\n"+ "    yield 3.14\n"+ "\n"+ "for expr in gen():\n"+ "    pass\n");
