{
  if (iftype <= IF_LE) {
    stack.push(new ConstExprent(0,true));
  }
 else   if (iftype <= IF_NONNULL) {
    stack.push(new ConstExprent(VarType.VARTYPE_NULL,null));
  }
  if (iftype == IF_VALUE) {
    condition=stack.pop();
  }
 else {
    condition=new FunctionExprent(functypes[iftype],stack);
  }
}
