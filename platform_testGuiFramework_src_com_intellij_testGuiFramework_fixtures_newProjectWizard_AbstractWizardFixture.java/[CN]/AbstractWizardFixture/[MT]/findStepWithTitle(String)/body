{
  JRootPane rootPane=target().getRootPane();
  GuiTestUtil.waitUntilFound(robot(),rootPane,new GenericTypeMatcher<JLabel>(JLabel.class){
    @Override protected boolean isMatching(    @NotNull JLabel label){
      if (!label.isShowing()) {
        return false;
      }
      return title.equals(label.getText());
    }
  }
);
  return rootPane;
}
