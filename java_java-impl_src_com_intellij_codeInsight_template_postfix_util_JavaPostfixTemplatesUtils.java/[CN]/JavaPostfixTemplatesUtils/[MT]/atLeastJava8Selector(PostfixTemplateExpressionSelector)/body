{
  return new PostfixTemplateExpressionSelector(){
    @Override public boolean hasExpression(    @NotNull PsiElement context,    @NotNull Document copyDocument,    int newOffset){
      return PsiUtil.isLanguageLevel8OrHigher(context) && selector.hasExpression(context,copyDocument,newOffset);
    }
    @NotNull @Override public List<PsiElement> getExpressions(    @NotNull PsiElement context,    @NotNull Document document,    int offset){
      return PsiUtil.isLanguageLevel8OrHigher(context) ? selector.getExpressions(context,document,offset) : Collections.<PsiElement>emptyList();
    }
    @NotNull @Override public Function<PsiElement,String> getRenderer(){
      return selector.getRenderer();
    }
  }
;
}
