{
  try {
    final Method launchApplication=Class.forName(JAVAFX_LAUNCHER).getMethod(LAUNCH_APPLICATION_METHOD_NAME,new Class[]{appClass.getClass(),parms.getClass()});
    launchApplication.invoke(null,new Object[]{appClass,parms});
    return true;
  }
 catch (  Throwable e) {
    return false;
  }
}
