{
  VirtualFile dir=createSrc();
  String url=dir.getUrl();
  ContentFolder f=entry.addExcludeFolder(dir);
  assertEquals(dir,f.getFile());
  assertEquals(url,f.getUrl());
  delete(dir);
  assertNull(f.getFile());
  assertEquals(url,f.getUrl());
  dir=createSrc();
  assertEquals(dir,f.getFile());
  assertEquals(url,f.getUrl());
}
