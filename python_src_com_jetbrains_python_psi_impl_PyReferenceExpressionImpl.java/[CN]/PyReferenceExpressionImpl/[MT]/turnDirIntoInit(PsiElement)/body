{
  if (target instanceof PsiDirectory) {
    final PsiDirectory dir=(PsiDirectory)target;
    final PsiFile file=dir.findFile(PyNames.INIT_DOT_PY);
    if (file != null) {
      file.putCopyableUserData(PyFile.KEY_IS_DIRECTORY,Boolean.TRUE);
      return file;
    }
 else     return null;
  }
 else   return target;
}
