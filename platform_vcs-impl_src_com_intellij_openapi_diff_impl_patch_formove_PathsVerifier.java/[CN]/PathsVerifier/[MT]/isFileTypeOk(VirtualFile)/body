{
  FileType fileType=file.getFileType();
  if (fileType == FileTypes.UNKNOWN) {
    fileType=FileTypeChooser.associateFileType(file.getName());
    if (fileType == null) {
      PatchApplier.showError(myProject,"Cannot apply file " + file.getPresentableName() + " from patch because its type not defined.",true);
      return false;
    }
  }
  if (fileType.isBinary()) {
    PatchApplier.showError(myProject,"Cannot apply file " + file.getPresentableName() + " from patch because it is binary.",true);
    return false;
  }
  return true;
}
