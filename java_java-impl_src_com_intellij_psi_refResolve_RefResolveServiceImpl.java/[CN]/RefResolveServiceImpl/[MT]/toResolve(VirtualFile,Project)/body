{
  if (virtualFile != null && virtualFile.isValid() && project.isInitialized() && myProjectFileIndex.isInSourceContent(virtualFile) && isSupportedFileType(virtualFile)) {
    return true;
  }
  if (virtualFile instanceof VirtualFileWithId) {
    int id=getAbsId(virtualFile);
    fileIsResolved.set(id);
  }
  return false;
}
