{
  if (expr == null)   return "";
  List<PyExpression> path=unwindQualifiers(expr);
  final PyQualifiedName qName=PyQualifiedName.fromReferenceChain(path);
  if (qName != null) {
    return qName.toString();
  }
  String name=expr.getName();
  if (name != null) {
    return name;
  }
  return "";
}
