{
  if (elt == null) {
    return null;
  }
  PsiElement parent=elt.getParent();
  while (parent != null) {
    if (parent instanceof PyClass || parent instanceof PyFunction) {
      return parent;
    }
    if (parent instanceof PsiFile) {
      break;
    }
    parent=parent.getParent();
  }
  return null;
}
