{
  PsiElement seeker=elt;
  while (seeker != null) {
    PsiElement feeler=seeker.getPrevSibling();
    if (feeler != null) {
      seeker=getInnermostChildOf(feeler);
    }
 else {
      seeker=seeker.getParent();
      if (seeker instanceof PsiFile)       return null;
    }
    if (condition.value(seeker))     return seeker;
  }
  return null;
}
