{
switch (insn.getOpcode()) {
case IALOAD:
case LALOAD:
case FALOAD:
case DALOAD:
case AALOAD:
case BALOAD:
case CALOAD:
case SALOAD:
    if (value1 instanceof ParamValue) {
      subResult=NPE;
    }
  break;
case PUTFIELD:
if (value1 instanceof ParamValue) {
  subResult=NPE;
}
if (nullableAnalysis && value2 instanceof ParamValue) {
subResult=NPE;
}
break;
default :
}
return super.binaryOperation(insn,value1,value2);
}
