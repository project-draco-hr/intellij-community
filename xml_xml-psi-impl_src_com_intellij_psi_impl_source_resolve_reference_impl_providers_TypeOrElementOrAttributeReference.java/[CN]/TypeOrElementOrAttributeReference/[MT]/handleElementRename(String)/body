{
  final String canonicalText=getCanonicalText();
  final PsiElement element=ElementManipulators.getManipulator(myElement).handleContentChange(myElement,getRangeInElement(),newElementName);
  myRange=new TextRange(myRange.getStartOffset(),myRange.getEndOffset() - (canonicalText.length() - newElementName.length()));
  return element;
}
