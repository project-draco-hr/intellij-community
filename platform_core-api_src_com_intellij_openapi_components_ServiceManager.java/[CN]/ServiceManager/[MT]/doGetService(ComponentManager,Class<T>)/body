{
  @SuppressWarnings("unchecked") T instance=(T)componentManager.getPicoContainer().getComponentInstance(serviceClass.getName());
  if (instance == null) {
    instance=componentManager.getComponent(serviceClass);
    if (instance != null) {
      LOG.warn(serviceClass.getName() + " requested as a service, but it is a component - convert it to a service or change call to " + (componentManager == ApplicationManager.getApplication() ? "ApplicationManager.getApplication().getComponent()" : "project.getComponent()"));
    }
  }
  return instance;
}
