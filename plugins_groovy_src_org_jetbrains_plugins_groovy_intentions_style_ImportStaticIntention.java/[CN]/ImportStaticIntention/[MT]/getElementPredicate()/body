{
  return new PsiElementPredicate(){
    @Override public boolean satisfiedBy(    PsiElement element){
      final PsiElement resolved=resolve(element);
      if (resolved == null)       return false;
      return resolved instanceof PsiMember && !(resolved instanceof PsiClass) && ((PsiMember)resolved).hasModifierProperty(PsiModifier.STATIC)&& ((PsiMember)resolved).getContainingClass() != null;
    }
  }
;
}
