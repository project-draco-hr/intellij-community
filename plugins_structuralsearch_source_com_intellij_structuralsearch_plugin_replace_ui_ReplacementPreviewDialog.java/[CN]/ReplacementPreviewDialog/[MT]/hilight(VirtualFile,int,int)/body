{
  removeHilighter();
  editor=FileEditorManager.getInstance(project).openTextEditor(new OpenFileDescriptor(project,file),false);
  hilighter=editor.getMarkupModel().addRangeHighlighter(start,end,HighlighterLayer.SELECTION - 100,EditorColorsManager.getInstance().getGlobalScheme().getAttributes(EditorColors.SEARCH_RESULT_ATTRIBUTES),HighlighterTargetArea.EXACT_RANGE);
}
