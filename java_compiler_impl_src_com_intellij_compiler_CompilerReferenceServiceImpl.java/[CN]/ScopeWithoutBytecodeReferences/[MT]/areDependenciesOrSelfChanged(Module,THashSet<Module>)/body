{
  if (!uniqueModules.add(module)) {
    return false;
  }
  if (myChangedModules.contains(module)) {
    return true;
  }
  for (  Module dependency : ModuleRootManager.getInstance(module).getDependencies()) {
    if (areDependenciesOrSelfChanged(dependency,uniqueModules)) {
      return true;
    }
  }
  return false;
}
