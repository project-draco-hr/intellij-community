{
  final CompilerElementInfo searchElementInfo=asCompilerElements(aClass,searchAdapter,false);
  myReadDataLock.lock();
  try {
    if (myReader == null)     return null;
    return myReader.getDirectInheritors(aClass,searchElementInfo,searchAdapter,useScope,myDirtyModulesHolder.getDirtyScope(),myProject,searchFileType);
  }
  finally {
    myReadDataLock.unlock();
  }
}
