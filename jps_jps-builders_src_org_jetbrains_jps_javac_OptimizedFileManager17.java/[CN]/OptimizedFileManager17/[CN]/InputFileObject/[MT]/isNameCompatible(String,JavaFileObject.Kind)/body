{
  cn.getClass();
  if (kind == Kind.OTHER && getKind() != kind) {
    return false;
  }
  final String n=cn + kind.extension;
  if (name.equals(n)) {
    return true;
  }
  if (name.equalsIgnoreCase(n)) {
    try {
      return n.equals(file.toPath().toRealPath(LinkOption.NOFOLLOW_LINKS).getFileName().toString());
    }
 catch (    IOException ignored) {
    }
  }
  return false;
}
