{
  if (!(ref instanceof PyImportReference)) {
    return false;
  }
  final Sdk sdk=getPythonSdk(ref);
  if (sdk == null) {
    return false;
  }
  final PythonSdkFlavor flavor=PythonSdkFlavor.getFlavor(sdk);
  if (flavor instanceof IronPythonSdkFlavor) {
    return true;
  }
  return isGtk(ref);
}
