{
  if (element instanceof PsiClass && JUnitUtil.isTestClass((PsiClass)element,false,true))   return new RunLineMarkerInfo(element,JUnitConfigurationType.getInstance().getIcon(),TOOLTIP_PROVIDER);
  if (element instanceof PsiMethod && JUnitUtil.isTestMethod(new PsiLocation<PsiMethod>((PsiMethod)element)))   return new RunLineMarkerInfo(element,JUnitConfigurationType.getInstance().getIcon(),TOOLTIP_PROVIDER);
  return null;
}
