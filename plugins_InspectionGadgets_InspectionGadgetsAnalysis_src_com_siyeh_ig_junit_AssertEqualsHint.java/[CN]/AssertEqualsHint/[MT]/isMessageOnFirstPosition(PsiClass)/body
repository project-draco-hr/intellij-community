{
  return InheritanceUtil.isInheritor(containingClass,JUnitCommonClassNames.JUNIT_FRAMEWORK_ASSERT) || InheritanceUtil.isInheritor(containingClass,JUnitCommonClassNames.ORG_JUNIT_ASSERT) || InheritanceUtil.isInheritor(containingClass,JUnitCommonClassNames.JUNIT_FRAMEWORK_TEST_CASE)|| InheritanceUtil.isInheritor(containingClass,"org.testng.AssertJUnit");
}
