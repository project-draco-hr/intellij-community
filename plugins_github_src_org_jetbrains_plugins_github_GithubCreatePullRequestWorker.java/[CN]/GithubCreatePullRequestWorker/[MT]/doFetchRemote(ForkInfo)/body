{
  if (fork.getRemoteName() == null)   return false;
  GitFetchResult result=new GitFetcher(myProject,new EmptyProgressIndicator(),false).fetch(myGitRepository.getRoot(),fork.getRemoteName(),null);
  if (!result.isSuccess()) {
    GitFetcher.displayFetchResult(myProject,result,null,result.getErrors());
    return false;
  }
  return true;
}
