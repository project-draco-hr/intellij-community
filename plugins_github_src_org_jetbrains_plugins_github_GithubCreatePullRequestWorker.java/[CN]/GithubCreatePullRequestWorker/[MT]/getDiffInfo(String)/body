{
  try {
    FutureTask<DiffInfo> future=myDiffInfos.get(branch);
    if (future == null) {
      return null;
    }
    future.run();
    return future.get();
  }
 catch (  InterruptedException e) {
    LOG.error(e);
    return null;
  }
catch (  ExecutionException e) {
    LOG.error(e);
    return null;
  }
}
