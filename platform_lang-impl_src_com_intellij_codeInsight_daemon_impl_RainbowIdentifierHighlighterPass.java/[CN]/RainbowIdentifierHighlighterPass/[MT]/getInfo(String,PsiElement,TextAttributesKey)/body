{
  if (id == null || nameKey == null || StringUtil.isEmpty(nameKey))   return null;
  if (colorKey == null)   colorKey=DefaultLanguageHighlighterColors.LOCAL_VARIABLE;
  final TextAttributes attributes=myRainbowHighlighter.getAttributes(nameKey,myEditorColorsScheme.getAttributes(colorKey));
  return HighlightInfo.newHighlightInfo(RainbowHighlighter.RAINBOW_ELEMENT).textAttributes(attributes).range(id).create();
}
