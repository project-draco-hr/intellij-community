{
  final PsiFile file=element.getContainingFile();
  boolean isAccepted=true;
  if (file instanceof PyFileImpl) {
    isAccepted=((PyFileImpl)file).isAcceptedFor(this.getClass());
  }
  return !isAccepted || isSuppressedForParent(element,PyStatement.class) || isSuppressedForParent(element,PyFunction.class)|| isSuppressedForParent(element,PyClass.class)|| isSuppressForCodeFragment(element);
}
