{
  myView.clear();
  myPackages.clearSelection();
  for (  Pair pair : filtered) {
    myView.add(pair);
  }
  Collections.sort(myView,new Comparator<Pair>(){
    @Override public int compare(    Pair pair,    Pair pair1){
      if (pair instanceof ComparablePair)       return ((ComparablePair)pair).compareTo(pair1);
      return 0;
    }
  }
);
  fireContentsChanged(this,0,myView.size());
}
