{
  init("lr");
  executeAction(IdeActions.ACTION_EDITOR_MOVE_CARET_RIGHT_WITH_SELECTION);
  executeAction(IdeActions.ACTION_EDITOR_MOVE_CARET_RIGHT_WITH_SELECTION);
  assertEquals(0,myEditor.getSelectionModel().getSelectionStart());
  assertEquals(2,myEditor.getSelectionModel().getSelectionEnd());
}
