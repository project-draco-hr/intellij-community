{
  PsiElementProcessor<PsiClass> processor=new PsiElementProcessor<PsiClass>(){
    @Override public boolean execute(    @NotNull PsiClass element){
      callback.pass(element);
      return false;
    }
  }
;
  NavigationUtil.getPsiElementPopup(scopes,PsiClassListCellRenderer.INSTANCE,"Choose class to introduce field",processor).showInBestPositionFor(editor);
}
