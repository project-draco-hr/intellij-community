{
  CommonCodeStyleSettings langSettings=settings.getCommonSettings(myLanguage);
  boolean rightMarginModified=myDefaultGeneralCheckBox.isSelected() ? langSettings.RIGHT_MARGIN >= 0 : langSettings.RIGHT_MARGIN != getFieldRightMargin(settings.getDefaultRightMargin());
  return rightMarginModified || langSettings.WRAP_ON_TYPING != getSelectedWrapOnTypingValue();
}
