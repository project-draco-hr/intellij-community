{
  CommonCodeStyleSettings langSettings=settings.getCommonSettings(myLanguage);
  boolean isRightMarginChanged=myDefaultGeneralCheckBox.isSelected() ? langSettings.RIGHT_MARGIN >= 0 : langSettings.RIGHT_MARGIN != getFieldRightMargin(settings.getDefaultRightMargin());
  return isRightMarginChanged || getWrapOnTypingIntValue() != langSettings.WRAP_ON_TYPING;
}
