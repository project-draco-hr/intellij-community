{
  if (result.length == 3 && PsiModifier.STATIC.equals(result[0].getText()) && result[1] instanceof PsiWhiteSpace && result[2] instanceof PsiBlockStatement) {
    return true;
  }
 else   if (result.length > 1 && result[0] instanceof PsiDeclarationStatement && !result[0].getText().endsWith(";")) {
    return true;
  }
  return false;
}
