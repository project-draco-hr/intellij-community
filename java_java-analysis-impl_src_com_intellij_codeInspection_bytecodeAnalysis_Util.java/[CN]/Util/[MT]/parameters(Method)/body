{
  Type[] argTypes=Type.getArgumentTypes(method.methodDesc);
  StringBuilder sb=new StringBuilder("(");
  boolean notFirst=false;
  for (  Type argType : argTypes) {
    if (notFirst) {
      sb.append(", ");
    }
 else {
      notFirst=true;
    }
    sb.append(canonical(argType.getClassName()));
  }
  sb.append(')');
  return sb.toString();
}
