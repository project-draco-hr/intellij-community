{
  InspectionToolPresentation presentation=context.getPresentation(toolWrapper);
  final RefElementNode elemNode=addNodeToParent(container,presentation,packageNode);
  if (toolWrapper instanceof LocalInspectionToolWrapper) {
    final OfflineProblemDescriptorNode child=OfflineProblemDescriptorNode.create(((OfflineProblemDescriptorContainer)container).getUserObject(),(LocalInspectionToolWrapper)toolWrapper,presentation);
    elemNode.insertByOrder(child,true);
  }
}
