{
  PsiElement module=root.isDirectory() ? psimgr.findDirectory(root) : psimgr.findFile(root);
  for (  String component : qualifiedName.getComponents()) {
    if (component == null) {
      return true;
    }
    module=resolveChild(module,component,foothold_file,false);
  }
  if (module != null) {
    results.add(module);
  }
  return true;
}
