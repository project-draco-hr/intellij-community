{
  final VirtualFile childFile=root.findChild(name + PY_SUFFIX);
  if (childFile != null) {
    result=psimgr.findFile(childFile);
    return (result == null);
  }
  final VirtualFile childDir=root.findChild(name);
  if (childDir != null) {
    result=psimgr.findDirectory(childDir);
    return (result == null);
  }
  return true;
}
