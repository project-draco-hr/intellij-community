{
  String root_name=root.getPath() + "/";
  if (myFname.startsWith(root_name)) {
    String bet=myFname.substring(root_name.length()).replace('/','.');
    int dots=0;
    for (int i=0; i < bet.length(); i+=1)     if (bet.charAt(i) == '.')     dots+=1;
    if (dots < myDots) {
      myDots=dots;
      myResult=bet;
    }
  }
  return true;
}
