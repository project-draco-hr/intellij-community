{
  if (foothold != null) {
    PsiFile file=foothold.getContainingFile();
    if (file instanceof PyFile) {
      final PyFile pyFile=(PyFile)file;
      if (pyFile.getLanguageLevel().isPy3K()) {
        return true;
      }
      return pyFile.isAbsoluteImportEnabled();
    }
  }
  return false;
}
