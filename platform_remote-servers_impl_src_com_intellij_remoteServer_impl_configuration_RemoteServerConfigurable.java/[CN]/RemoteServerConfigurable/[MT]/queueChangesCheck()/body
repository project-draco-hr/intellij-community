{
  if (myAlarm.isDisposed()) {
    return;
  }
  myAlarm.addRequest(new Runnable(){
    @Override public void run(){
      UIUtil.invokeLaterIfNeeded(new Runnable(){
        @Override public void run(){
          checkChanges();
          queueChangesCheck();
        }
      }
);
    }
  }
,CHANGES_CHECK_TIME);
}
