{
  configureByFile();
  type("par");
  int selectedIndex=0;
  myFixture.assertPreferredCompletionItems(selectedIndex,".par","parents");
  type("\b");
  assertNotNull(getLookup());
  myFixture.assertPreferredCompletionItems(selectedIndex,"parents");
  type("r");
  myFixture.assertPreferredCompletionItems(selectedIndex,".par","parents");
}
