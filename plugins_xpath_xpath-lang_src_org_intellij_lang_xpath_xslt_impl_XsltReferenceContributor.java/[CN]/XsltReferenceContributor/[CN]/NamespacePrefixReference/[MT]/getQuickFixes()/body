{
  final XmlAttributeValue valueElement=myAttribute.getValueElement();
  if (valueElement != null) {
    return new LocalQuickFix[]{new CreateNSDeclarationIntentionFix(valueElement,getCanonicalText()){
      @Override public boolean showHint(      @NotNull Editor editor){
        return false;
      }
    }
};
  }
  return LocalQuickFix.EMPTY_ARRAY;
}
