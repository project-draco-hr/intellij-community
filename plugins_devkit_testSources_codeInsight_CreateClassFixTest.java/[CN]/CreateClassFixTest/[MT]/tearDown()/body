{
  final Ref<Exception> ex=new Ref<Exception>();
  Runnable runnable=new Runnable(){
    public void run(){
      try {
        myFixture.tearDown();
        myFixture=null;
        CreateClassFixTest.super.tearDown();
      }
 catch (      Exception e) {
        ex.set(e);
      }
    }
  }
;
  invokeTestRunnable(runnable);
  final Exception exception=ex.get();
  if (exception != null) {
    throw exception;
  }
}
