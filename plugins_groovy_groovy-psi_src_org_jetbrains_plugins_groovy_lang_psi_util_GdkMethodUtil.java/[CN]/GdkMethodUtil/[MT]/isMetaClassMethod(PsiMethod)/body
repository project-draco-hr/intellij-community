{
  if (method instanceof GrGdkMethod)   method=((GrGdkMethod)method).getStaticMethod();
  PsiClass containingClass=method.getContainingClass();
  String name=method.getName();
  return "getMetaClass".equals(name) && containingClass != null && (method.getParameterList().getParametersCount() == 0 ^ GroovyCommonClassNames.DEFAULT_GROOVY_METHODS.equals(containingClass.getQualifiedName()));
}
