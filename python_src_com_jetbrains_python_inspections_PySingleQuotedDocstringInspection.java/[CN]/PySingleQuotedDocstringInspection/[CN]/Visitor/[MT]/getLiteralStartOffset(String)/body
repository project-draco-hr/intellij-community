{
  int start=0;
  char c=Character.toUpperCase(text.charAt(start));
  if (c == 'U' || c == 'B') {
    myModificator+=text.charAt(start);
    start++;
    c=Character.toUpperCase(text.charAt(start));
  }
  if (c == 'R') {
    myModificator+=text.charAt(start);
    start++;
  }
  myLength=myModificator.length();
  return start;
}
