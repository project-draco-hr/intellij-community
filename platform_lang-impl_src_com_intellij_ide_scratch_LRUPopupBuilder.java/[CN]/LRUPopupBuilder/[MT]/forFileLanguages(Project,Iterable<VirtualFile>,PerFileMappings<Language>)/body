{
  Consumer<Language> onChosen=new Consumer<Language>(){
    @Override public void consume(    Language t){
      for (      VirtualFile file : files) {
        mappings.setMapping(file,t);
      }
    }
  }
;
  return languagePopupBuilder(project,"Languages").forValues(mappings.getAvailableValues(null)).onChosen(onChosen).buildPopup();
}
