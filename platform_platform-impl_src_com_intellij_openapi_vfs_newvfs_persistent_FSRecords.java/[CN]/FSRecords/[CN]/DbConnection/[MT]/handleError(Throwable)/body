{
  if (!ourIsDisposed) {
    if (!myCorrupted && w.tryLock()) {
      w.unlock();
      createBrokenMarkerFile(e);
      myCorrupted=true;
      force();
    }
  }
  if (e instanceof Error)   throw (Error)e;
  if (e instanceof RuntimeException)   throw (RuntimeException)e;
  throw new RuntimeException(e);
}
