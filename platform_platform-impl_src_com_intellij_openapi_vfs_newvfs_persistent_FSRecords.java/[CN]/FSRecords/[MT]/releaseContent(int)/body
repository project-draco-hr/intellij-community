{
  w.lock();
  try {
    RefCountingStorage contentStorage=getContentStorage();
    if (weHaveContentHashes) {
      contentStorage.releaseRecord(contentId,false);
    }
 else {
      contentStorage.releaseRecord(contentId);
    }
  }
 catch (  Throwable e) {
    DbConnection.handleError(e);
  }
 finally {
    w.unlock();
  }
}
