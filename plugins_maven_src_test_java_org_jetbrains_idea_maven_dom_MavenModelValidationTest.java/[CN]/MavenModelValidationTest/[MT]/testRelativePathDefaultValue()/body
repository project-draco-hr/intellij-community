{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  VirtualFile modulePom=createModulePom("module1","<groupId>test</groupId>" + "<artifactId>module1</artifactId>" + "<version>1</version>"+ "<parent>"+ "<relativePath>../pom.<caret>xml</relativePath>"+ "</parent>");
  configTest(modulePom);
  PsiElement elementAtCaret=myFixture.getElementAtCaret();
  assertInstanceOf(elementAtCaret,PsiFile.class);
  assertEquals(((PsiFile)elementAtCaret).getVirtualFile(),myProjectPom);
}
