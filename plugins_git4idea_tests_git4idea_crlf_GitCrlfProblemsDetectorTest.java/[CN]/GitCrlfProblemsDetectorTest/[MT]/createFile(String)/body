{
  List<String> split=StringUtil.split(relPath,"/");
  File parent=new File(myProjectRoot.getPath());
  for (Iterator<String> it=split.iterator(); it.hasNext(); ) {
    String item=it.next();
    File file=new File(parent,item);
    if (it.hasNext()) {
      parent=file;
      parent.mkdir();
    }
 else {
      file.createNewFile();
      return file;
    }
  }
  return parent;
}
