{
  PyFunction function=PsiTreeUtil.getParentOfType(file.findElementAt(editor.getCaretModel().getOffset()),PyFunction.class);
  if (function != null) {
    final PyDocStringOwner docStringOwner=PsiTreeUtil.getParentOfType(file.findElementAt(editor.getCaretModel().getOffset()),PyDocStringOwner.class);
    if (docStringOwner != null) {
      if (docStringOwner.getDocStringExpression() != null)       return false;
    }
    if (function.getStatementList() != null && function.getStatementList().getStatements().length != 0)     return true;
  }
  return false;
}
