{
  PsiClass containingClass=method.getContainingClass();
  if (containingClass == null)   return false;
  for (  PsiMethod candidate : containingClass.findMethodsByName(method.getName(),true)) {
    if (candidate.getContainingClass() != containingClass && PsiSearchScopeUtil.isInScope(scope,candidate) && PsiSuperMethodImplUtil.isSuperMethodSmart(method,candidate)) {
      return true;
    }
  }
  return false;
}
