{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < keyComponents.size(); ++i) {
    if (i > 0) {
      sb.append(StringUtil.repeatSymbol(' ',indentAddition + 2 * i));
    }
    sb.append(keyComponents.get(i)).append(":");
    if (i + 1 < keyComponents.size()) {
      sb.append('\n');
    }
  }
  return sb.toString();
}
