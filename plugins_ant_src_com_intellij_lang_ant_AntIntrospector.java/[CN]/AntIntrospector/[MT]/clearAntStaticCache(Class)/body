{
  try {
    final Method method=helperClass.getDeclaredMethod("clearCache");
    method.invoke(null);
  }
 catch (  Throwable e) {
    try {
      Map helpersCollection=ReflectionUtil.getField(helperClass,null,null,"helpers");
      helpersCollection.clear();
    }
 catch (    Throwable _e) {
    }
  }
}
