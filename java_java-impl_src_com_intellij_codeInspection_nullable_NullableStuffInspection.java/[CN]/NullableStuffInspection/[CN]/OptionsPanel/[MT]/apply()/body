{
  REPORT_NOT_ANNOTATED_METHOD_OVERRIDES_NOTNULL=myNAMethodOverridesNN.isSelected();
  REPORT_NOTNULL_PARAMETER_OVERRIDES_NULLABLE=myBreakingOverriding.isSelected();
