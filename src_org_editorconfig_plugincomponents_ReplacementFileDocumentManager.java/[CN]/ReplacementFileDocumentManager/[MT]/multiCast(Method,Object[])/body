{
  try {
    method.invoke(bus.syncPublisher(DoneSavingTopic.DONE_SAVING),args);
  }
 catch (  InvocationTargetException e) {
    LOG.error(e.getTargetException());
  }
catch (  ClassCastException e) {
    LOG.error("Arguments: " + Arrays.toString(args),e);
  }
catch (  Exception e) {
    LOG.error(e);
  }
}
