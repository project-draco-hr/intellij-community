{
  List<PyStatement> result=Lists.newArrayList();
  for (  PyClass cls : file.getTopLevelClasses()) {
    if (isPyTestClass(cls,context)) {
      result.add(cls);
    }
  }
  for (  PyFunction cls : file.getTopLevelFunctions()) {
    if (isPyTestFunction(cls)) {
      result.add(cls);
    }
  }
  return result;
}
