{
  if (!isEditable)   return;
  if (!isFiringEvent) {
    try {
      isFiringEvent=true;
      ActionEvent event=new ActionEvent(this,ActionEvent.ACTION_PERFORMED,"colorPanelChanged");
      for (      ActionListener listener : myListeners) {
        listener.actionPerformed(event);
      }
    }
  finally {
      isFiringEvent=false;
    }
  }
}
