{
  if (tokens.size() > 1) {
    boolean seenLF=false;
    for (int i=0; i < tokens.size(); i++) {
      IElementType token=tokens.get(i);
      if (token == PyTokenTypes.LINE_BREAK) {
        seenLF=true;
      }
 else       if (token == PyTokenTypes.END_OF_LINE_COMMENT && seenLF) {
        return i;
      }
    }
  }
  return tokens.size();
}
