{
  final StringBuilder buf=new StringBuilder();
  if (group != null) {
    buf.append(group).append(':');
  }
  if (name != null) {
    buf.append(name);
  }
  if (!"jar".equals(packaging)) {
    buf.append(':').append(packaging);
  }
  if (classifier != null) {
    buf.append(':').append(classifier);
  }
  if (version != null) {
    buf.append(':').append(version);
  }
  return buf.toString();
}
