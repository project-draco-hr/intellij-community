{
  String error;
  if (lastError != null && lastError.length() != 0 && !resetPassword) {
    ApplicationManager.getApplication().invokeAndWait(new Runnable(){
      public void run(){
        showError(lastError);
      }
    }
,ModalityState.any());
    error=null;
  }
 else {
    error=lastError != null && lastError.length() == 0 ? null : lastError;
  }
  return error;
}
