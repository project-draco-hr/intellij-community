{
  PyFunction[] methods=getMethods();
  for (  PyFunction method : methods) {
    if (!processor.process(method))     return false;
  }
  if (inherited) {
    for (    PyClass ancestor : iterateAncestors()) {
      if (!ancestor.scanMethods(processor,false)) {
        return false;
      }
    }
  }
  return true;
}
