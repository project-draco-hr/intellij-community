{
  if (SystemInfo.isWindows) {
    return "windows";
  }
 else   if (SystemInfo.isMac) {
    return "mac";
  }
 else   if (SystemInfo.isLinux) {
    return "linux";
  }
 else   if (SystemInfo.isFreeBSD) {
    return "freebsd";
  }
 else   if (SystemInfo.isUnix) {
    return "unix";
  }
  return "unknown";
}
