{
  myIsLeft=false;
  myIsRight=false;
  myOperator=null;
  getInnerRight=false;
  PyExpression leftExpression=node.getLeftExpression();
  PyExpression rightExpression=node.getRightExpression();
  if (leftExpression instanceof PyBinaryExpression && rightExpression instanceof PyBinaryExpression) {
    if (node.getOperator() == PyTokenTypes.AND_KEYWORD) {
      if (isRightSimplified((PyBinaryExpression)leftExpression,(PyBinaryExpression)rightExpression) || isLeftSimplified((PyBinaryExpression)leftExpression,(PyBinaryExpression)rightExpression))       registerProblem(node,"Simplify chained comparison",new ChainedComparisonsQuickFix(myIsLeft,myIsRight,getInnerRight));
    }
  }
}
