{
  if (leftExpression.getRightExpression() instanceof PyBinaryExpression) {
    if (checkOperator((PyBinaryExpression)leftExpression.getRightExpression(),rightExpression))     return true;
  }
  if (PyTokenTypes.RELATIONAL_OPERATIONS.contains(leftExpression.getOperator())) {
    PyExpression leftRight=leftExpression.getRightExpression();
    if (leftRight != null) {
      if (leftRight.getText().equals(getLeftExpression(rightExpression).getText()))       return true;
    }
  }
  return false;
}
