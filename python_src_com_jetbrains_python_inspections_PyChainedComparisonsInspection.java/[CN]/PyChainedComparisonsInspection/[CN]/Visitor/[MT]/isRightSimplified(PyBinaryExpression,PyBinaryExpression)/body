{
  if (leftExpression.getRightExpression() instanceof PyBinaryExpression && PyTokenTypes.RELATIONAL_OPERATIONS.contains(((PyBinaryExpression)leftExpression.getRightExpression()).getOperator())) {
    if (isRightSimplified((PyBinaryExpression)leftExpression.getRightExpression(),rightExpression))     return true;
  }
  if (PyTokenTypes.RELATIONAL_OPERATIONS.contains(leftExpression.getOperator())) {
    PyExpression leftRight=leftExpression.getRightExpression();
    if (leftRight != null) {
      if (leftRight.getText().equals(getLeftExpression(rightExpression).getText())) {
        myIsLeft=false;
        myIsRight=true;
        return true;
      }
      PyExpression right=getSmallestRight(rightExpression);
      if (right != null && leftRight.getText().equals(right.getText())) {
        myIsLeft=false;
        myIsRight=false;
        return true;
      }
    }
  }
  return false;
}
