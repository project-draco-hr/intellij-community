{
  List<Hash> parents=ContainerUtil.map(record.getParentsHashes(),new Function<String,Hash>(){
    @Override public Hash fun(    String hash){
      return HashImpl.build(hash);
    }
  }
);
  return new GitCommit(project,HashImpl.build(record.getHash()),parents,record.getAuthorTimeStamp(),root,record.getSubject(),record.getAuthorName(),record.getAuthorEmail(),record.getFullMessage(),record.getCommitterName(),record.getCommitterEmail(),record.getLongTimeStamp(),record.parseChanges(project,root));
}
