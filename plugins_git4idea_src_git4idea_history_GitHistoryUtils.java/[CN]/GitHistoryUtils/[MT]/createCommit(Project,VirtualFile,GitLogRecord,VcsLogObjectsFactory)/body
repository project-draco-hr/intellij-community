{
  List<Hash> parents=ContainerUtil.map(record.getParentsHashes(),new Function<String,Hash>(){
    @Override public Hash fun(    String hash){
      return HashImpl.build(hash);
    }
  }
);
  return new GitCommit(project,HashImpl.build(record.getHash()),parents,record.getCommitTime(),root,record.getSubject(),factory.createUser(record.getAuthorName(),record.getAuthorEmail()),record.getFullMessage(),factory.createUser(record.getCommitterName(),record.getCommitterEmail()),record.getAuthorTimeStamp(),record.getStatusInfos());
}
