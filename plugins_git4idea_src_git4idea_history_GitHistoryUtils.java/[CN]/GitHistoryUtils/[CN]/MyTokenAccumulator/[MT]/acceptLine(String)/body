{
  final boolean recordStart=s.startsWith(GitLogParser.RECORD_START);
  if (recordStart) {
    s=s.substring(GitLogParser.RECORD_START.length());
  }
  if (myNotStarted) {
    myBuffer.append(s);
    myBuffer.append("\n");
    myNotStarted=false;
    return null;
  }
 else   if (recordStart) {
    final String line=myBuffer.toString();
    myBuffer.setLength(0);
    myBuffer.append(s);
    myBuffer.append("\n");
    return processResult(line);
  }
 else {
    myBuffer.append(s);
    myBuffer.append("\n");
    return null;
  }
}
