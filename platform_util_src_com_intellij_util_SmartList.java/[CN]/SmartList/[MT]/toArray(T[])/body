{
  if (mySize == 1) {
    int length=a.length;
    if (length != 0) {
      a[0]=(T)myElem;
      if (length > 1) {
        a[1]=null;
      }
      return a;
    }
  }
  if (a.length < mySize) {
    return (T[])Arrays.copyOf((E[])myElem,mySize,a.getClass());
  }
 else {
    System.arraycopy(myElem,0,a,0,mySize);
    if (a.length > mySize) {
      a[mySize]=null;
    }
    return a;
  }
}
