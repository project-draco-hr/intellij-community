{
  if (myBodyContainsForbiddenRefs)   return;
  super.visitMethodCallExpression(methodCallExpression);
  final PsiMethod psiMethod=methodCallExpression.resolveMethod();
  if (psiMethod == myMethod || functionalInterfaceMethodReferenced(psiMethod,myAnonymClass) || psiMethod != null && !methodCallExpression.getMethodExpression().isQualified() && "getClass".equals(psiMethod.getName()) && psiMethod.getParameterList().getParametersCount() == 0) {
    myBodyContainsForbiddenRefs=true;
  }
}
