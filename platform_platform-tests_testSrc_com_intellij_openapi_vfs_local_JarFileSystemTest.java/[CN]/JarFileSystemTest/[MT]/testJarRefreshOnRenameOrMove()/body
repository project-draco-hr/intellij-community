{
  File jar=IoTestUtil.createTestJar();
  assertTrue(jar.setLastModified(jar.lastModified() - 1000));
  VirtualFile vFile=LocalFileSystem.getInstance().refreshAndFindFileByIoFile(jar);
  assertNotNull(vFile);
  ApplicationManager.getApplication().getMessageBus().connect(myTestRootDisposable).subscribe(VirtualFileManager.VFS_CHANGES,new BulkFileListener.Adapter(){
    @Override public void before(    @NotNull List<? extends VFileEvent> events){
      for (      VFileEvent event : events) {
      }
    }
  }
);
  assertTrue(jar.renameTo(new File(jar.getParentFile(),jar.getName() + ".jar")));
}
