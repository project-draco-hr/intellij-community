{
  LOG.assertTrue(!(value instanceof Value),"use setValue for jdi values");
  if (hasValue(localName)) {
    throw new EvaluateRuntimeException(EvaluateExceptionUtil.createEvaluateException(DebuggerBundle.message("evaluation.error.variable.already.declared",localName)));
  }
  mySyntheticLocals.put(localName,value);
}
