{
  File outputFile=new File(PathManager.getLibPath() + "/courses.zip");
  try {
    GithubDownloadUtil.downloadAtomically(null,REPO_URL,outputFile,USER_NAME,REPOSITORY_NAME);
    if (outputFile.exists()) {
      ZipUtil.unzip(null,myDefaultCoursesBaseDir,outputFile,null,null,true);
      if (myDefaultCoursesBaseDir.exists()) {
        return true;
      }
    }
    return false;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
}
