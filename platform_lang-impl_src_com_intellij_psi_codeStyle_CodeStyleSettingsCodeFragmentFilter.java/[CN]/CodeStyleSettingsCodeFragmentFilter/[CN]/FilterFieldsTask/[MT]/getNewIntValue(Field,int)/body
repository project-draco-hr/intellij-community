{
  int newValue=oldValue;
  String fieldName=classField.getName();
  if (fieldName.contains("WRAP")) {
    newValue=oldValue == CommonCodeStyleSettings.WRAP_ALWAYS ? CommonCodeStyleSettings.DO_NOT_WRAP : CommonCodeStyleSettings.WRAP_ALWAYS;
  }
 else   if (fieldName.contains("BRACE")) {
    newValue=oldValue == CommonCodeStyleSettings.FORCE_BRACES_ALWAYS ? CommonCodeStyleSettings.DO_NOT_FORCE : CommonCodeStyleSettings.WRAP_ALWAYS;
  }
  return newValue;
}
