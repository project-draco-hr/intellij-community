{
  if (this == o)   return true;
  if (!(o instanceof ExcludesConfigurationState))   return false;
  ExcludesConfigurationState state=(ExcludesConfigurationState)o;
  return myDetectionEnabled == state.myDetectionEnabled && Comparing.haveEqualElements(myFiles,state.myFiles) && Comparing.haveEqualElements(myFrameworkTypes,state.myFrameworkTypes);
}
