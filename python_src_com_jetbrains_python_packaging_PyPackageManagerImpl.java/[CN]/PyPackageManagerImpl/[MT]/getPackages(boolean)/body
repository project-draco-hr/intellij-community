{
  if (cachedOnly) {
    if (myPackagesCache != null) {
      return myPackagesCache;
    }
    return getPackages(false);
  }
 else {
    if (myPackagesCache == null) {
      if (myExceptionCache != null) {
        throw myExceptionCache;
      }
      loadPackages();
    }
    return myPackagesCache;
  }
}
