{
synchronized (myLock) {
    if (!myDirtyPaths.isEmpty()) {
      DirtyPaths dirtyPaths=myDirtyPaths;
      myDirtyPaths=new DirtyPaths();
      for (      PluggableFileWatcher watcher : myWatchers) {
        watcher.resetChangedPaths();
      }
      return dirtyPaths;
    }
 else {
      return DirtyPaths.EMPTY;
    }
  }
}
