{
  if (textRange.getStartOffset() < parent.getStartOffset()) {
    myRangesAssert.assertInvalidRanges(textRange.getStartOffset(),parent.getStartOffset(),myModel,"child block start is less than parent block start");
  }
  if (textRange.getEndOffset() > parent.getEndOffset()) {
    myRangesAssert.assertInvalidRanges(textRange.getEndOffset(),parent.getEndOffset(),myModel,"child block end is after parent block end");
  }
}
