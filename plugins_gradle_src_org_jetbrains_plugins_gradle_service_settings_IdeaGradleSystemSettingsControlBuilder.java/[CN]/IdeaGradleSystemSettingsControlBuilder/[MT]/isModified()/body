{
  if (myServiceDirectoryPathModifiedByUser && myServiceDirectoryPathField != null && !Comparing.equal(ExternalSystemApiUtil.normalizePath(myServiceDirectoryPathField.getText()),ExternalSystemApiUtil.normalizePath(myInitialSettings.getServiceDirectoryPath()))) {
    return true;
  }
  if (myGradleVmOptionsField != null && !Comparing.equal(trimIfPossible(myGradleVmOptionsField.getText()),trimIfPossible(myInitialSettings.getGradleVmOptions()))) {
    return true;
  }
  if (myOfflineModeBox != null && myOfflineModeBox.isSelected() != myInitialSettings.isOfflineWork()) {
    return true;
  }
  return false;
}
