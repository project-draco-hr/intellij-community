{
  List<GraphEdge> result=ContainerUtil.newArrayList();
  if (nodeIndex != 0 && mySimpleNodes.get(nodeIndex - 1))   result.add(GraphEdge.createNormalEdge(nodeIndex - 1,nodeIndex,USUAL));
  for (int i=myNodeToEdgeIndex.get(nodeIndex); i < myNodeToEdgeIndex.get(nodeIndex + 1); i++) {
    int node=myLongEdges.get(i);
    if (node < 0)     result.add(GraphEdge.createEdgeWithTargetId(nodeIndex,node,GraphEdgeType.NOT_LOAD_COMMIT));
 else     result.add(GraphEdge.createNormalEdge(nodeIndex,node,USUAL));
  }
  if (mySimpleNodes.get(nodeIndex))   result.add(GraphEdge.createNormalEdge(nodeIndex,nodeIndex + 1,USUAL));
  return result;
}
