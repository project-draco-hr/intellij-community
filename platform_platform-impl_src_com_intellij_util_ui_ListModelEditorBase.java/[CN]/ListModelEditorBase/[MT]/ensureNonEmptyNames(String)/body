{
  List<T> items=getItems();
  for (int i=items.size() - 1; i >= 0; i--) {
    T item=items.get(i);
    if (StringUtil.isEmptyOrSpaces(itemEditor.getName(item))) {
      if (itemEditor.isEmpty(item)) {
        removeEmptyItem(i);
      }
 else {
        throw new ConfigurationException(errorMessage);
      }
    }
  }
}
