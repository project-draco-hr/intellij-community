{
  boolean shouldBeSelected;
  if (mySingleRepoProject) {
    shouldBeSelected=true;
  }
 else   if (repositoryManager.isSyncEnabled()) {
    shouldBeSelected=hasCommits || target.hasSomethingToPush();
  }
 else {
    shouldBeSelected=(hasCommits || target.hasSomethingToPush());
  }
  return shouldBeSelected;
}
