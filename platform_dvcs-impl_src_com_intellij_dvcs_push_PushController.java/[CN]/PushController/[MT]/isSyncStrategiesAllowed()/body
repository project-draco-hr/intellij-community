{
  return !mySingleRepoProject && ContainerUtil.and(getAffectedSupports(myProject),new Condition<PushSupport<? extends Repository,? extends PushSource,? extends PushTarget>>(){
    @Override public boolean value(    PushSupport<? extends Repository,? extends PushSource,? extends PushTarget> support){
      return support.mayChangeTargetsSync();
    }
  }
);
}
