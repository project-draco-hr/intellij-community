{
  Set<String> repos=ContainerUtil.newHashSet();
  for (  PushSupport<?,?,?> support : myPushSupports) {
    for (    Repository repo : support.getRepositoryManager().getRepositories()) {
      if (!repo.equals(except)) {
        repos.add(repo.getRoot().getName());
      }
    }
  }
  return repos;
}
