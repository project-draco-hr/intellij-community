{
  myProject=project;
  myPushSupports=getAffectedSupports();
  CheckedTreeNode rootNode=new CheckedTreeNode(null);
  mySingleRepoProject=createTreeModel(rootNode,preselectedRepositories);
  myPushLog=new PushLog(myProject,rootNode);
  myDialog=dialog;
