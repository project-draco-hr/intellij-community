{
  Stack<PsiElement> stack=new Stack<PsiElement>();
  stack.push(expression.getContainedExpression());
  while (!stack.isEmpty()) {
    PsiElement element=stack.pop();
    findProblem(element);
    if (element != null) {
      for (      PsiElement psiElement : element.getChildren()) {
        stack.push(psiElement);
      }
    }
  }
}
