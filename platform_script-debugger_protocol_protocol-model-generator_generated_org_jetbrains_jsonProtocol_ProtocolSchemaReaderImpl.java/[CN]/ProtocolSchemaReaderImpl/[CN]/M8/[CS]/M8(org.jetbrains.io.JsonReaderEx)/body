{
  reader.beginObject();
  while (reader.hasNext()) {
    CharSequence name=reader.nextNameAsCharSequence();
    if (name.equals("major")) {
      _major=readString(reader,"major");
    }
 else     if (name.equals("minor")) {
      _minor=readString(reader,"minor");
    }
 else {
      reader.skipValue();
    }
  }
  reader.endObject();
}
