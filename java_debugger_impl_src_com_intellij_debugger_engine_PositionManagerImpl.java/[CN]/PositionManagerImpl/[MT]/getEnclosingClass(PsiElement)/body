{
  if (element == null) {
    return null;
  }
  element=element.getParent();
  PsiElement previous=null;
  while (element != null) {
    if (PsiClass.class.isInstance(element) && !(previous instanceof PsiExpressionList)) {
      return (PsiClass)element;
    }
    if (element instanceof PsiFile) {
      return null;
    }
    previous=element;
    element=element.getParent();
  }
  return null;
}
