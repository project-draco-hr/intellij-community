{
  watches.clear();
  if (state != null) {
    for (    ConfigurationState expressionState : state.expressions) {
      XExpression[] expressions=ContainerUtil.mapNotNull(expressionState.myExpressionStates,new Function<WatchState,XExpression>(){
        @Override public XExpression fun(        WatchState state){
          return state.toXExpression();
        }
      }
,new XExpression[0]);
      watches.put(expressionState.myName,expressions);
    }
  }
}
