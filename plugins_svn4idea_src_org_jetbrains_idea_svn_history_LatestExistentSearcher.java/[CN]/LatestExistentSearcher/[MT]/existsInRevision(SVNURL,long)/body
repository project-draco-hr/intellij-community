{
  SVNRevision revision=SVNRevision.create(revisionNumber);
  SVNInfo info=null;
  try {
    info=myVcs.getInfo(url,revision,revision);
  }
 catch (  SVNException e) {
    if (!SVNErrorCode.RA_ILLEGAL_URL.equals(e.getErrorMessage().getErrorCode())) {
      throw e;
    }
  }
  return info != null;
}
