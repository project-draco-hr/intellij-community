{
  T t=cast(o);
  QueueEntry<T> entry=findEntry(t);
  if (entry == null)   return false;
  QueueEntry<T> prev=entry.prev;
  QueueEntry<T> next=entry.next;
  if (prev != null) {
    prev.next=next;
  }
 else {
    first=next;
  }
  if (next != null) {
    next.prev=prev;
  }
 else {
    last=prev;
  }
  set.remove(entry);
  return true;
}
