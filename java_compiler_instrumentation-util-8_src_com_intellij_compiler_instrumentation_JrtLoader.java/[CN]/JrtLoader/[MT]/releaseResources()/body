{
  myCache.clear();
  final List<Path> roots=myRoots;
  if (roots != null) {
    try {
      if (!roots.isEmpty()) {
        roots.iterator().next().getFileSystem().close();
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
 finally {
      roots.clear();
      myRoots=null;
    }
  }
}
