{
  final StringBuilder result=new StringBuilder();
  if (has(modifier,'-')) {
    result.append("<");
  }
 else   if ("s".equals(conversionChar) && !StringUtil.isEmptyOrSpaces(widthAndPrecision)) {
    result.append(">");
  }
  if (has(modifier,'+')) {
    result.append("+");
  }
 else   if (has(modifier,' '))   result.append(" ");
  if (has(modifier,'#'))   result.append("#");
  if (has(modifier,'0'))   result.append("0");
  return result.toString();
}
