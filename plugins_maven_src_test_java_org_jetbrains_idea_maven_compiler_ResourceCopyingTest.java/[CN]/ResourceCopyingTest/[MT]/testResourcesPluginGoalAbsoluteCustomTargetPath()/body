{
  createProjectSubFile("src/test/resources/dir/file.properties");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-resources-plugin</artifactId>"+ "      <version>2.6</version>"+ "      <executions>"+ "       <execution>"+ "         <id>default-testResources</id>"+ "         <phase>process-test-resources</phase>"+ "         <goals>"+ "           <goal>testResources</goal>"+ "         </goals>"+ "         <configuration>"+ "           <outputDirectory>${project.build.testOutputDirectory}/custom</outputDirectory>"+ "         </configuration>"+ "       </execution>"+ "      </executions>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  compileModules("project");
  assertCopied("target/test-classes/custom/dir/file.properties");
}
