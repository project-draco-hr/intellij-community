{
  if (AnnotationUtil.isAnnotated(owner,getNullables(),checkBases,false)) {
    return true;
  }
  return owner instanceof PsiParameter && !AnnotationUtil.isAnnotated(owner,Arrays.asList(DEFAULT_NOT_NULLS),checkBases,false) && isContainerAnnotated(owner,"javax.annotation.ParametersAreNullableByDefault");
}
