{
  if (AnnotationUtil.isAnnotated(owner,getNotNulls(),checkBases,false)) {
    return true;
  }
  return owner instanceof PsiParameter && !AnnotationUtil.isAnnotated(owner,Arrays.asList(DEFAULT_NULLABLES),checkBases,false) && isContainerAnnotated(owner,"javax.annotation.ParametersAreNonnullByDefault");
}
