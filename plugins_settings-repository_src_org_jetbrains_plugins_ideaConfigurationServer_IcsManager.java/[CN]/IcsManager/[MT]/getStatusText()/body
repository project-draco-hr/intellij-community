{
  IcsSettings settings=getInstance().getIdeaServerSettings();
switch (status) {
case CONNECTION_FAILED:
    return "Connection failed";
case LOGGED_IN:
  return "Logged in as " + settings.getLogin();
case LOGGED_OUT:
return "Logged out";
case UNAUTHORIZED:
return "Authorization failed";
case UNKNOWN:
return "Unknown status";
default :
return "Unknown";
}
}
