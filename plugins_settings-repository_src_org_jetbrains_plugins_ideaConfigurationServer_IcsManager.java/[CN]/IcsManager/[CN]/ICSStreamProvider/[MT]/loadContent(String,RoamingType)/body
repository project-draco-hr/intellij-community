{
  if (projectId == null || roamingType == RoamingType.PER_PLATFORM) {
    return repositoryManager.read(IcsUrlBuilder.buildPath(fileSpec,roamingType,projectId));
  }
 else   if (StoragePathMacros.WORKSPACE_FILE.equals(fileSpec)) {
    return repositoryManager.read(IcsUrlBuilder.buildPath(fileSpec,roamingType,projectId));
  }
 else {
    return null;
  }
}
