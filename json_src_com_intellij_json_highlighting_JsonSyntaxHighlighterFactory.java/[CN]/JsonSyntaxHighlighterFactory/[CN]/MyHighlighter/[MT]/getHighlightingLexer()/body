{
  LayeredLexer layeredLexer=new LayeredLexer(new JsonLexer());
  layeredLexer.registerSelfStoppingLayer(new StringLiteralLexer('\"',JsonElementTypes.DOUBLE_QUOTED_STRING,false,"/",false,false),new IElementType[]{JsonElementTypes.DOUBLE_QUOTED_STRING},IElementType.EMPTY_ARRAY);
  layeredLexer.registerSelfStoppingLayer(new StringLiteralLexer('\'',JsonElementTypes.SINGLE_QUOTED_STRING,false,"/",false,false),new IElementType[]{JsonElementTypes.SINGLE_QUOTED_STRING},IElementType.EMPTY_ARRAY);
  return layeredLexer;
}
