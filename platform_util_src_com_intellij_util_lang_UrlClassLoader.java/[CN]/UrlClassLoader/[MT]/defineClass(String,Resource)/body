{
  int i=name.lastIndexOf('.');
  if (i != -1) {
    String pkgName=name.substring(0,i);
    Package pkg=getPackage(pkgName);
    if (pkg == null) {
      try {
        definePackage(pkgName,res.getValue(Resource.Attribute.SPEC_TITLE),res.getValue(Resource.Attribute.SPEC_VERSION),res.getValue(Resource.Attribute.SPEC_VENDOR),res.getValue(Resource.Attribute.IMPL_TITLE),res.getValue(Resource.Attribute.IMPL_VERSION),res.getValue(Resource.Attribute.IMPL_VENDOR),null);
      }
 catch (      IllegalArgumentException e) {
      }
    }
  }
  byte[] b=res.getBytes();
  return _defineClass(name,b);
}
