{
  final int key=FileUtil.pathHashCode(outputPath);
synchronized (myDataLock) {
    final TIntHashSet state=getState(key);
    if (state != null) {
      final int value=FileUtil.pathHashCode(sourcePath);
      final boolean removed=state.remove(value);
      if (state.isEmpty()) {
        remove(key);
      }
 else {
        if (removed) {
          update(key,state);
        }
      }
      return removed;
    }
  }
  return false;
}
