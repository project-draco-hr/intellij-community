{
  String name=myModules.iterator().next().getName();
  if (myModules.size() > 1) {
    name+=" and " + (myModules.size() - 1) + " more";
    String fullName=getName();
    if (fullName.length() < name.length()) {
      name=fullName;
    }
  }
  if (containsTests()) {
    name="tests of " + name;
  }
  return name;
}
