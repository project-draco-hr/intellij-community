{
  while (true) {
    try {
      executeImpl();
      break;
    }
 catch (    VcsException e) {
      if (ourBusyExceptionProcessor.process(e)) {
        if (myCnt > 0) {
          TimeoutUtil.sleep(myTimeout * (REPEAT - myCnt + 1));
          --myCnt;
          continue;
        }
      }
      throw e;
    }
  }
}
