{
  InspectorWindow window=myComponentToInspector.get(c);
  if (window != null) {
    window.switchInfo(c);
    window.setHighlightingEnabled(true);
    window.setVisible(true);
    window.toFront();
  }
 else {
    window=new InspectorWindow(c);
    myComponentToInspector.put(c,window);
    window.pack();
    window.setVisible(true);
    window.toFront();
  }
}
