{
  int count=myExprType.getParameterCount();
  for (  PsiClassType trait : myTraitTypes) {
    count+=trait.getParameterCount();
  }
  if (count == 0)   return;
  buffer.append('<');
  for (int i=0; i < count; i++) {
    buffer.append("T").append(i).append(",");
  }
  buffer.replace(buffer.length() - 1,buffer.length(),">");
}
