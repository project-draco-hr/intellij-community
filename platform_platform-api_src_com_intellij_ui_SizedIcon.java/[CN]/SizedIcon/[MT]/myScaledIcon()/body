{
  if (myScaledDelegate != null) {
    return myScaledDelegate;
  }
  if (getScale() == 1f) {
    return myScaledDelegate=myDelegate;
  }
  if (!(myDelegate instanceof ScalableIcon)) {
    setScale(1f);
    return myScaledDelegate=myDelegate;
  }
  return myScaledDelegate=((ScalableIcon)myDelegate).scale(getScale());
}
