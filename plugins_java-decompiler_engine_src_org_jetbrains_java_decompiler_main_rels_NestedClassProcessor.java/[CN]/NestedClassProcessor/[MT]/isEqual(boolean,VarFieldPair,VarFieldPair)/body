{
  boolean eq;
  if (fObj == null || sObj == null) {
    eq=(fObj == sObj);
  }
 else {
    eq=true;
    if (fObj.fieldKey.length() == 0) {
      fObj.fieldKey=sObj.fieldKey;
    }
 else     if (sObj.fieldKey.length() == 0) {
      if (both) {
        sObj.fieldKey=fObj.fieldKey;
      }
    }
 else {
      eq=fObj.fieldKey.equals(sObj.fieldKey);
    }
  }
  return eq;
}
