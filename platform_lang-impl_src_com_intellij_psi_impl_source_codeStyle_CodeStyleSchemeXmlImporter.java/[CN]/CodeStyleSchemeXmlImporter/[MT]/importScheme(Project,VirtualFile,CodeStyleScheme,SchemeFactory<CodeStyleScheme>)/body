{
  Element rootElement=loadSchemeDom(selectedFile);
  final Pair<String,CodeStyleScheme> importPair=ImportSchemeChooserDialog.selectOrCreateTargetScheme(project,currentScheme,schemeFactory,getSchemeName(rootElement));
  if (importPair != null) {
    return readSchemeFromDom(rootElement,importPair.second);
  }
  return null;
}
