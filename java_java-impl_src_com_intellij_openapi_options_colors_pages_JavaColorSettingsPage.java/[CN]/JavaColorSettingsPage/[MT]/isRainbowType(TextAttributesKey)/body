{
  return JavaHighlightingColors.LOCAL_VARIABLE_ATTRIBUTES.equals(type) || JavaHighlightingColors.REASSIGNED_LOCAL_VARIABLE_ATTRIBUTES.equals(type) || JavaHighlightingColors.PARAMETER_ATTRIBUTES.equals(type)|| JavaHighlightingColors.REASSIGNED_PARAMETER_ATTRIBUTES.equals(type)|| JavaHighlightingColors.DOC_COMMENT_TAG_VALUE.equals(type);
}
