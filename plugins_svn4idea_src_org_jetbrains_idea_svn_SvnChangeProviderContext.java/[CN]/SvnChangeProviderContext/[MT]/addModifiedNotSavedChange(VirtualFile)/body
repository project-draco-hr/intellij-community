{
  final FilePath filePath=new FilePathImpl(file);
  final SVNInfo svnInfo=myVcs.getInfo(file);
  if (svnInfo != null) {
    final SVNStatus svnStatus=new SVNStatus();
    svnStatus.setRevision(svnInfo.getRevision());
    myChangelistBuilder.processChangeInList(createChange(SvnContentRevision.createBaseRevision(myVcs,filePath,svnInfo.getRevision()),CurrentContentRevision.create(filePath),FileStatus.MODIFIED,svnStatus),(String)null,SvnVcs.getKey());
  }
}
