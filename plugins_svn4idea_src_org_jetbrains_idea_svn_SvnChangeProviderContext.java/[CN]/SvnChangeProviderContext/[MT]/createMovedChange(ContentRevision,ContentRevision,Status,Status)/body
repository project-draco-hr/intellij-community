{
  ConflictedSvnChange change=new ConflictedSvnChange(before,after,ConflictState.mergeState(getState(copiedStatus),getState(deletedStatus)),((copiedStatus != null) && (copiedStatus.getTreeConflict() != null)) ? after.getFile() : before.getFile());
  change.setBeforeDescription(deletedStatus.getTreeConflict());
  if (copiedStatus != null) {
    change.setAfterDescription(copiedStatus.getTreeConflict());
    patchWithPropertyChange(change,copiedStatus,deletedStatus);
  }
  return change;
}
