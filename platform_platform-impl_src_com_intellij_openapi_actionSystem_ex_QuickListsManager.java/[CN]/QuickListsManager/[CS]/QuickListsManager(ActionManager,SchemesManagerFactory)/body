{
  myActionManager=actionManager;
  mySchemeManager=schemesManagerFactory.create("quicklists",new BaseSchemeProcessor<QuickList>(){
    @NotNull @Override public QuickList readScheme(    @NotNull Element element){
      return createItem(element);
    }
    @Override public Element writeScheme(    @NotNull QuickList scheme){
      Element element=new Element(LIST_TAG);
      scheme.writeExternal(element);
      return element;
    }
  }
);
}
