{
  try {
    BufferedReader br=new BufferedReader(new InputStreamReader(stream,CharsetToolkit.UTF8_CHARSET));
    try {
      String line;
      while ((line=br.readLine()) != null) {
        consumer.consume(line);
      }
    }
  finally {
      br.close();
    }
  }
 catch (  Exception e) {
    Logger.getInstance(StreamLoader.class).error(e);
  }
}
