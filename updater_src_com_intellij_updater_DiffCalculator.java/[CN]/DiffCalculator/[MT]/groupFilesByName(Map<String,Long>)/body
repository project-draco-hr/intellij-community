{
  Map<String,List<String>> result=new HashMap<>();
  for (  String path : toDelete.keySet()) {
    if (!path.endsWith("/")) {
      String name=new File(path).getName();
      List<String> paths=result.get(name);
      if (paths == null) {
        paths=new LinkedList<>();
        result.put(name,paths);
      }
      paths.add(path);
    }
  }
  return result;
}
