{
  int common=0;
  String best="";
  String[] dirs=path.split("/");
  for (  String other : paths) {
    String[] others=other.split("/");
    for (int i=0; i < dirs.length && i < others.length; i++) {
      if (dirs[dirs.length - i - 1].equals(others[others.length - i - 1])) {
        if (i + 1 > common) {
          best=other;
          common=i + 1;
        }
      }
 else {
        break;
      }
    }
  }
  return best;
}
