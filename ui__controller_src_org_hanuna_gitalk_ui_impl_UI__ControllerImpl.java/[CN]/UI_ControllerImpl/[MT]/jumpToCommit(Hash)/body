{
  Graph graph=dataPack.getGraphModel().getGraph();
  int row=-1;
  for (int i=0; i < graph.getNodeRows().size(); i++) {
    Node node=graph.getCommitNodeInRow(i);
    if (node != null && node.getCommitHash().equals(commitHash)) {
      row=i;
      break;
    }
  }
  if (row != -1) {
    events.runJumpToRow(row);
  }
}
