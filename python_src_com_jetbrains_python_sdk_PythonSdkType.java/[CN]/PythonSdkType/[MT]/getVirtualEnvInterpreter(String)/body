{
  for (  String dir : DIRS_WITH_BINARY) {
    final File binDir=new File(virtualEnvDir,dir);
    if (binDir.isDirectory()) {
      for (      String binary : BINARY_NAMES) {
        final File binFile=new File(binDir,binary);
        if (binFile.exists()) {
          return binFile.getPath();
        }
      }
    }
  }
  return null;
}
