{
  if (PySdkUtil.isRemote(sdk)) {
    final Ref<Boolean> result=Ref.create(false);
    ((PyRemoteSdkAdditionalDataBase)sdk.getSdkAdditionalData()).switchOnConnectionType(new CredentialsCase.Vagrant(){
      @Override public void process(      VagrantBasedCredentialsHolder cred){
        result.set(StringUtil.isEmpty(cred.getVagrantFolder()));
      }
    }
,new CredentialsCase.Docker(){
      @Override public void process(      DockerCredentialsHolder credentials){
      }
    }
);
    return result.get();
  }
  return false;
}
