{
  List<Sprite> chars=state.get(editor);
  if (chars == null) {
    chars=new ArrayList<Sprite>();
    state.put(editor,chars);
  }
  Sprite sprite=new Sprite(c,fontType,color,from,dx * SPEED_SCALE,dy * SPEED_SCALE);
  chars.add(sprite);
  sprite.requestRepaintInEditor(editor);
  startAnimation();
  return sprite;
}
