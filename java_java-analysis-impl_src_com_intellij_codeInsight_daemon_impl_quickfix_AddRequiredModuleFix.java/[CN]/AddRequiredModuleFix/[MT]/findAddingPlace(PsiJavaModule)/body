{
  PsiElement addingPlace=null;
  for (  PsiRequiresStatement requires : module.getRequires()) {
    addingPlace=requires;
  }
  if (addingPlace != null) {
    return addingPlace;
  }
  for (  PsiExportsStatement exports : module.getExports()) {
    addingPlace=exports;
  }
  if (addingPlace != null) {
    return addingPlace;
  }
  return getLBrace(module);
}
