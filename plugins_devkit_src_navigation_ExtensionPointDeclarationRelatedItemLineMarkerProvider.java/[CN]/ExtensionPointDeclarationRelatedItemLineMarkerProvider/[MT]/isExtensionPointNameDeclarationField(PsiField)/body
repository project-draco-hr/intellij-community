{
  if (!psiField.hasModifierProperty(PsiModifier.FINAL) || !psiField.hasModifierProperty(PsiModifier.STATIC) || psiField.hasModifierProperty(PsiModifier.ABSTRACT)) {
    return false;
  }
  if (!psiField.hasInitializer()) {
    return false;
  }
  final PsiExpression initializer=psiField.getInitializer();
  if (!(initializer instanceof PsiMethodCallExpression) && !(initializer instanceof PsiNewExpression)) {
    return false;
  }
  final PsiClass fieldClass=PsiTypesUtil.getPsiClass(psiField.getType());
  if (fieldClass == null) {
    return false;
  }
  return ExtensionPointName.class.getName().equals(fieldClass.getQualifiedName());
}
