{
  try {
    beginArguments();
    writer.name(name);
    writer.beginObject();
    for (    Map.Entry<String,String> entry : value.entrySet()) {
      writer.name(entry.getKey()).value(entry.getValue());
    }
    writer.endObject();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
