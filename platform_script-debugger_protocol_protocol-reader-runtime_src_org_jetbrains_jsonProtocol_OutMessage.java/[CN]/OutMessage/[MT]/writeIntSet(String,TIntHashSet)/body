{
  try {
    beginArguments();
    writer.name(name);
    writer.beginArray();
    value.forEach(new TIntProcedure(){
      @Override public boolean execute(      int value){
        try {
          writer.value(value);
        }
 catch (        IOException e) {
          throw new RuntimeException(e);
        }
        return true;
      }
    }
);
    writer.endArray();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
