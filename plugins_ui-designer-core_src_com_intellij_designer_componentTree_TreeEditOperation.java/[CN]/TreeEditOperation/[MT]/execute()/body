{
  if (myTarget == myContainer) {
    execute(null);
  }
 else   if (myInsertBefore) {
    execute(myTarget);
  }
 else {
    Object[] children=myContainer.getTreeChildren();
    int index=ArrayUtil.indexOf(children,myTarget) + 1;
    if (index < children.length) {
      execute((RadComponent)children[index]);
    }
 else {
      execute(null);
    }
  }
}
