{
  T[] result=myExtensionsCache;
  if (result == null) {
synchronized (this) {
      result=myExtensionsCache;
      if (result == null) {
        result=processAdapters();
        if (result == null) {
          result=(T[])Array.newInstance(getExtensionClass(),0);
        }
        myExtensionsCache=result;
      }
    }
  }
  return result.length == 0 ? result : result.clone();
}
