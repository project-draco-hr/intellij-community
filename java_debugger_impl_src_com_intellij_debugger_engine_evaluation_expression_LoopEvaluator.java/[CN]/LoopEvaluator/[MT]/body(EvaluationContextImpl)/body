{
  try {
    evaluateBody(context);
  }
 catch (  BreakException e) {
    if (Comparing.equal(e.getLabelName(),myLabelName)) {
      return true;
    }
 else {
      throw e;
    }
  }
catch (  ContinueException e) {
    if (!Comparing.equal(e.getLabelName(),myLabelName)) {
      throw e;
    }
  }
  return false;
}
