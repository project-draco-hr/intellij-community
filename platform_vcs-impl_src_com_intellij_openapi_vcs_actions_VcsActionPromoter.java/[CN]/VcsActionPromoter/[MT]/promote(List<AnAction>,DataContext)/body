{
  List<AnAction> list=new ArrayList<AnAction>(0);
  for (  AnAction action : actions) {
    if (action instanceof RollbackLineStatusAction) {
      list.add(action);
    }
    if (action instanceof ShowMessageHistoryAction) {
      list.add(action);
    }
    if (action instanceof DiffWalkerAction) {
      list.add(action);
    }
  }
  return list;
}
