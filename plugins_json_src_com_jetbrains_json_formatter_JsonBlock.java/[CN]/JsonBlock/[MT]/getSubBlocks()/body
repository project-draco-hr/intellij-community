{
  if (mySubBlocks == null) {
    mySubBlocks=ContainerUtil.mapNotNull(myNode.getChildren(null),new Function<ASTNode,Block>(){
      @Override public Block fun(      ASTNode node){
        if (isWhitespaceOrEmpty(node)) {
          return null;
        }
        return makeSubBlock(node);
      }
    }
);
  }
  return mySubBlocks;
}
