{
  Object value=myAccessor.read(o);
  if (value == null) {
    return null;
  }
  String stringValue;
  if (myConverter == null) {
    stringValue=TextBinding.convertToString(value);
  }
 else {
    stringValue=myConverter.toString(value);
  }
  return new org.jdom.Attribute(myName,stringValue);
}
