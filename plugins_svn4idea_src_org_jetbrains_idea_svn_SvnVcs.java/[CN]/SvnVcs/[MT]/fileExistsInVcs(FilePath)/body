{
  File file=path.getIOFile();
  try {
    Status status=getFactory(file).createStatusClient().doStatus(file,false);
    if (status != null) {
      if (svnStatusIs(status,StatusType.STATUS_ADDED)) {
        return status.isCopied();
      }
      return !(svnStatusIsUnversioned(status) || svnStatusIs(status,StatusType.STATUS_IGNORED) || svnStatusIs(status,StatusType.STATUS_OBSTRUCTED));
    }
  }
 catch (  SvnBindException e) {
    LOG.info(e);
  }
  return false;
}
