{
  final SVNErrorCode errorCode=e.getErrorMessage().getErrorCode();
  if (!myLogExceptions || SvnUtil.isUnversionedOrNotFound(errorCode) || SVNErrorCode.WC_UNSUPPORTED_FORMAT.equals(errorCode)|| SVNErrorCode.WC_UPGRADE_REQUIRED.equals(errorCode)) {
    LOG.debug(e);
  }
 else {
    LOG.error(e);
  }
}
