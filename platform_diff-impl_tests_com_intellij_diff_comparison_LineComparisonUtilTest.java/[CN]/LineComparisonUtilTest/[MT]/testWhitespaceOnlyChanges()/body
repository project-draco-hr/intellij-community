{
  TestData.lines("x "," x")._Def_(mod(0,0,1,1))._Trim_().all();
  TestData.lines("x \t","\t x")._Def_(mod(0,0,1,1))._Trim_().all();
  TestData.lines("x_","x ")._Def_(mod(0,0,2,1))._Trim_(del(1,1,1)).all();
  TestData.lines(" x_y ","x _ y")._Def_(mod(0,0,2,2))._Trim_().all();
  TestData.lines("x y ","x  y")._Def_(mod(0,0,1,1))._Ignore_().all();
  TestData.lines("x y_x y_x y","  x y  _x y  _x   y")._Def_(mod(0,0,3,3))._Trim_(mod(2,2,1,1))._Ignore_().all();
}
