{
  if (TypesUtil.isClassType(lType,CommonClassNames.JAVA_LANG_STRING)) {
    return Boolean.TRUE;
  }
  if (TypesUtil.unboxPrimitiveTypeWrapper(lType) == PsiType.CHAR && (TypesUtil.isClassType(rType,CommonClassNames.JAVA_LANG_STRING) || TypesUtil.isClassType(rType,GroovyCommonClassNames.GROOVY_LANG_GSTRING))) {
    return Boolean.TRUE;
  }
  return null;
}
