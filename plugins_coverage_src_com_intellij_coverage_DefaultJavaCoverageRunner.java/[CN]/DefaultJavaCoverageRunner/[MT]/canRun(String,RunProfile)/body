{
  try {
    return executorId.equals(CoverageExecutor.EXECUTOR_ID) && !(profile instanceof RunConfigurationWithSuppressedDefaultRunAction) && profile instanceof RunConfigurationBase&& Extensions.findExtension(CoverageEngine.EP_NAME,JavaCoverageEngine.class).isApplicableTo((RunConfigurationBase)profile);
  }
 catch (  Exception e) {
    return false;
  }
}
