{
  JsonCodeStyleSettings jsonSettings=settings.getCustomSettings(JsonCodeStyleSettings.class);
  CommonCodeStyleSettings commonSettings=settings.getCommonSettings(JsonLanguage.INSTANCE);
  return new SpacingBuilder(settings,JsonLanguage.INSTANCE).afterInside(COLON,JsonElementTypes.PROPERTY).spaceIf(jsonSettings.SPACE_AFTER_COLON).beforeInside(COLON,JsonElementTypes.PROPERTY).spaceIf(jsonSettings.SPACE_BEFORE_COLON).withinPair(L_BRAKET,R_BRAKET).spaceIf(commonSettings.SPACE_WITHIN_BRACKETS).withinPair(L_CURLY,R_CURLY).spaceIf(jsonSettings.SPACE_WITHIN_BRACES).after(COMMA).spaceIf(commonSettings.SPACE_AFTER_COMMA).before(COMMA).spaceIf(commonSettings.SPACE_BEFORE_COMMA);
}
