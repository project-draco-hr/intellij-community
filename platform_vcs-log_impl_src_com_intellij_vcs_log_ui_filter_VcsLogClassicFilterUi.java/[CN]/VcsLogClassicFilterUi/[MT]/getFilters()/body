{
  Pair<VcsLogTextFilter,VcsLogHashFilter> filtersFromText=getFiltersFromTextArea(myTextFilter.getText().trim());
  return new VcsLogFilterCollectionImpl(myBranchFilterComponent.getFilter(),myUserFilterComponent.getFilter(),filtersFromText.second,myDateFilterComponent.getFilter(),filtersFromText.first,myStructureFilterComponent.getFilter());
}
