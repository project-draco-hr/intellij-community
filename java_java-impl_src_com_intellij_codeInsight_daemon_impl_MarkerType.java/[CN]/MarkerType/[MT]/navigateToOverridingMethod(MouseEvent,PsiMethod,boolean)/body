{
  PsiMethod[] superMethods=composeSuperMethods(method,acceptSelf);
  if (superMethods == null)   return;
  boolean showMethodNames=!PsiUtil.allMethodsHaveSameSignature(superMethods);
  PsiElementListNavigator.openTargets(e,superMethods,DaemonBundle.message("navigation.title.super.method",method.getName()),DaemonBundle.message("navigation.findUsages.title.super.method",method.getName()),new MethodCellRenderer(showMethodNames));
}
