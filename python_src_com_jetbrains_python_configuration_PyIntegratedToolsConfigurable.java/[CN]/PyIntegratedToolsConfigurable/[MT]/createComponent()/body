{
  List<String> configurations=TestRunnerService.getInstance(myModule).getConfigurations();
  myModel=new PythonTestConfigurationsModel(configurations,TestRunnerService.getInstance(myModule).getProjectConfiguration(),myModule);
  List<String> templateConfigurations=TemplatesService.getAllTemplateLanguages();
  myTemplatesModel=new TemplatesConfigurationsModel(templateConfigurations,myTemplatesService);
  myTemplateLanguage.setModel(myTemplatesModel);
  updateConfigurations();
  initErrorValidation();
  return myMainPanel;
}
