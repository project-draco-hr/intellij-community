{
  if (myBuilder.getTokenType() == PyTokenTypes.PERC) {
    myBuilder.advanceLexer();
    if (myBuilder.getTokenType() == PyTokenTypes.IDENTIFIER) {
      myBuilder.advanceLexer();
      return true;
    }
 else {
      myBuilder.error("Identifier expected.");
      return false;
    }
  }
 else {
    return super.parseExpressionOptional();
  }
}
