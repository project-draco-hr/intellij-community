{
  List<String> parameters=new ArrayList<String>();
  parameters.add("--xml");
  SvnVcs vcs=SvnVcs.getInstance(myProject);
  File workingDirectory=new File(myWorkingCopyDir.getPath());
  CommandExecutor command=BaseSvnClient.execute(vcs,SvnTarget.fromFile(workingDirectory),workingDirectory,SvnCommandName.info,parameters,null);
  final String result=command.getOutput();
  System.out.println(result);
  Assert.assertNotNull(result);
}
