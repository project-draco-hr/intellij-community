{
  myList=createList();
  final JScrollPane pane=ScrollPaneFactory.createScrollPane(myList,ScrollPaneConstants.VERTICAL_SCROLLBAR_AS_NEEDED,ScrollPaneConstants.HORIZONTAL_SCROLLBAR_NEVER);
  return ListWithFilter.wrap(myList,pane,new Function<T,String>(){
    @Override public String fun(    T o){
      return getItemText(o);
    }
  }
);
}
