{
  initText("<selection>one\n" + "two<caret></selection>\n" + "three\n"+ "four");
  executeAction("EditorCopy");
  executeAction("EditorTextStart");
  executeAction("EditorCloneCaretBelow");
  executeAction("EditorPaste");
  checkResultByText("one\n" + "two<caret>one\n" + "one\n"+ "two<caret>two\n"+ "three\n"+ "four");
}
