{
  try {
    DebuggerReader debuggerReader=myDebuggerReader;
    if (debuggerReader != null) {
      debuggerReader.stop();
    }
  }
  finally {
synchronized (mySocketObject) {
      if (mySocket != null) {
        try {
          mySocket.close();
        }
 catch (        IOException ignored) {
        }
      }
    }
  }
}
