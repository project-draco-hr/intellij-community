{
  final PsiElement element=location.getPsiElement();
  Module module=location.getModule();
  if (module == null)   module=ModuleManager.getInstance(element.getProject()).getModules()[0];
  final Sdk sdk=PythonSdkType.findPythonSdk(module);
  return (TestRunnerService.getInstance(module).getProjectConfiguration().equals(PythonTestConfigurationsModel.PYTHONS_NOSETEST_NAME) && sdk != null);
}
