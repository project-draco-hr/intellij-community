{
  try {
    myTargetsState.save();
    try {
      myTargetStoragesOwner.close();
    }
  finally {
      myTargetStorages.clear();
    }
  }
  finally {
    try {
      closeSourceToOutputStorages();
    }
  finally {
      try {
        closeStorage(mySrcToFormMap);
      }
  finally {
        final Mappings mappings=myMappings;
        if (mappings != null) {
          try {
            mappings.close();
          }
 catch (          BuildDataCorruptedException e) {
            throw e.getCause();
          }
        }
      }
    }
  }
}
