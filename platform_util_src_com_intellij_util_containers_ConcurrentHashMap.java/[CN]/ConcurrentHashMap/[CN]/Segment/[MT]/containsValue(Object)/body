{
  if (count != 0) {
    HashEntry[] tab=table;
    for (    HashEntry tabEntry : tab) {
      for (HashEntry<K,V> e=(HashEntry<K,V>)tabEntry; e != null; e=e.next) {
        V v=e.value;
        if (v == null) {
          v=readValueUnderLock(e);
        }
        if (value.equals(v)) {
          return true;
        }
      }
    }
  }
  return false;
}
