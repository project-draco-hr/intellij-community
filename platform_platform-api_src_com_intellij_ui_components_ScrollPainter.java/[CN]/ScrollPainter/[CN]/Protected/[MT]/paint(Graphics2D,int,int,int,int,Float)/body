{
  RegionPainter<Float> painter=myFallback;
  if (myPainter != null) {
    try {
      myPainter.paint(g,x,y,width,height,value);
      return;
    }
 catch (    Throwable exception) {
      if (!SystemInfo.isWindows)       myPainter=null;
    }
  }
  if (painter != null) {
    painter.paint(g,x,y,width,height,value);
  }
}
