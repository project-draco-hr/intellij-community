{
  super(name,RESOLVE_KINDS_METHOD_PROPERTY,place);
  myIsConstructor=isConstructor;
  myThisType=thisType;
  myArgumentTypes=argumentTypes;
  myAllVariants=allVariants;
  myByShape=byShape;
