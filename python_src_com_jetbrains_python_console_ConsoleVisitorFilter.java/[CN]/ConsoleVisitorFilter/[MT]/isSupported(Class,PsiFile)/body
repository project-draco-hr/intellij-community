{
  if (PydevConsoleRunner.isInPydevConsole(file)) {
    if (visitorClass == PyUnusedLocalInspection.class || visitorClass == PyUnboundLocalVariableInspection.class || visitorClass == PyStatementEffectInspection.class || visitorClass == PySingleQuotedDocstringInspection.class || visitorClass == PyIncorrectDocstringInspection.class || visitorClass == PyMissingOrEmptyDocstringInspection.class || visitorClass == PyMandatoryEncodingInspection.class) {
      return false;
    }
    if (visitorClass == DocStringAnnotator.class) {
      return false;
    }
  }
  return true;
}
