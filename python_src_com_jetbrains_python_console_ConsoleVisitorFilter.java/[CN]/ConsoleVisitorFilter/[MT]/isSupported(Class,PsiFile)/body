{
  if (PydevConsoleRunner.isInPydevConsole(file)) {
    if (visitorClass == PyUnusedLocalInspectionVisitor.class || visitorClass == PyUnboundLocalVariableInspection.Visitor.class || visitorClass == PyStatementEffectInspection.Visitor.class || visitorClass == PySingleQuotedDocstringInspection.Visitor.class || visitorClass == PyDocstringInspection.Visitor.class) {
      return false;
    }
    if (visitorClass == DocStringAnnotator.class) {
      return false;
    }
  }
  return true;
}
