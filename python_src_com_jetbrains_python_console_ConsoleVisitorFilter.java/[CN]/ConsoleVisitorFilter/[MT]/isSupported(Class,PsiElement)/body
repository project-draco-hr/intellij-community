{
  if (PydevConsoleRunner.isInPydevConsole(element)) {
    if (visitorClass == PyUnusedLocalInspectionVisitor.class || visitorClass == PyUnboundLocalVariableInspection.Visitor.class || visitorClass == PyStatementEffectInspection.class || visitorClass == PySingleQuotedDocstringInspection.class || visitorClass == PyDocstringInspection.Visitor.class) {
      return false;
    }
    if (visitorClass == DocStringAnnotator.class)     return false;
  }
  return true;
}
