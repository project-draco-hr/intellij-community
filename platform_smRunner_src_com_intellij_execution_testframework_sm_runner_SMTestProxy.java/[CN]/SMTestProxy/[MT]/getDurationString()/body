{
switch (getMagnitudeInfo()) {
case COMPLETE_INDEX:
case PASSED_INDEX:
case FAILED_INDEX:
case ERROR_INDEX:
case IGNORED_INDEX:
case SKIPPED_INDEX:
case TERMINATED_INDEX:
case RUNNING_INDEX:
    final Long duration=getDuration();
  return duration != null ? StringUtil.formatDuration(duration.longValue()) : null;
default :
return null;
}
}
