{
  final PyUnusedLocalInspectionVisitor visitor=new PyUnusedLocalInspectionVisitor(holder,session,ignoreTupleUnpacking,ignoreLambdaParameters,ignoreLoopIterationVariables);
  final PyUnusedLocalInspectionVisitor existingVisitor=session.getUserData(KEY);
  if (existingVisitor == null) {
    session.putUserData(KEY,visitor);
  }
  return visitor;
}
