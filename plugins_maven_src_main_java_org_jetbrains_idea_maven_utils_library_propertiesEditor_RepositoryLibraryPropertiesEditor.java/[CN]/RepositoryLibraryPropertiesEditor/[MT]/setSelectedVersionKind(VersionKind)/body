{
  versionSelector.setEnabled(versionKind == VersionKind.Select);
switch (versionKind) {
case Unselected:
    versionKindSelector.setSelectedIndex(-1);
  break;
case Release:
versionKindSelector.setSelectedItem(0);
break;
case Latest:
versionKindSelector.setSelectedIndex(1);
break;
case Select:
versionKindSelector.setSelectedIndex(2);
break;
}
}
