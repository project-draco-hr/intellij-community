{
  if (myName != null) {
    addOption(element,NAME_OPTION,myName);
  }
  if (myEnabled != null) {
    addOption(element,ENABLED_OPTION,myEnabled.toString());
  }
  if (myClassName != null) {
    addOption(element,CLASSNAME_OPTION,myClassName);
  }
  if (!myShowType) {
    addOption(element,SHOWTYPE_OPTION,"false");
  }
}
