{
  String pattern="\"$str$\"";
  doTest("var s = \"hello\";",pattern,1);
  doTest("package {\n" + "public class MyClass {\n" + "    private var s:String = \"hello\";\n"+ "}\n"+ "}",pattern,1,JavaScriptSupportLoader.JAVASCRIPT,"as");
  doTest("var s = \"str1\"; var s1 = \"str2\"; var s2 = \"hello\";","\"'_str:[regex( str.* )]\"",2);
  doTest("var s = \"hello world\"; var s2 = \"hello\";","\"$s$ $z$\"",1);
}
