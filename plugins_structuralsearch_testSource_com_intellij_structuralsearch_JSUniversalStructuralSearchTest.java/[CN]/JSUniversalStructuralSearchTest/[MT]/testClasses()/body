{
  String pattern="class $name$ {}";
  doTest("package {\n" + "public class MyClass implements mx.messaging.messages.IMessage {\n" + "}\n"+ "}",pattern,1,0,JavaScriptSupportLoader.JAVASCRIPT,"as");
  doTest("package {\n" + "class MyClass implements mx.messaging.messages.IMessage {\n" + "}\n"+ "}",pattern,1,0,JavaScriptSupportLoader.JAVASCRIPT,"as");
  doTest("package {\n" + "class MyClass {\n" + "}\n"+ "}",pattern,1,1,JavaScriptSupportLoader.JAVASCRIPT,"as");
}
