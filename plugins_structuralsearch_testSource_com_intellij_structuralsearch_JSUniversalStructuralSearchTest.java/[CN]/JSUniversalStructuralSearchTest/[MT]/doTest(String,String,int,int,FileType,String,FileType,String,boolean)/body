{
  findAndCheck(source,pattern,expected,patternFileType,patternFileExtension,sourceFileType,sourceFileExtension,physicalSourceFile);
  try {
    EquivalenceDescriptorProvider.ourUseDefaultEquivalence=true;
    findAndCheck(source,pattern,expectedWithDefaultEquivalence,patternFileType,patternFileExtension,sourceFileType,sourceFileExtension,physicalSourceFile);
  }
  finally {
    EquivalenceDescriptorProvider.ourUseDefaultEquivalence=false;
  }
}
