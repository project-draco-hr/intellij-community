{
  PlatformTestCase.initPlatformLangPrefix();
  super.setUp();
  myModule=context.mock(Module.class,"myModule");
  context.checking(new Expectations(){
{
      allowing(myModule).getName();
      will(returnValue("my-module"));
    }
  }
);
  myManager=new ModuleRunConfigurationManager(myModule,new MyRunManagerImpl());
  mySettings=createSettings("my-module-run",new MyModuleBasedConfiguration("my-module-run-config",getProject(),myModule));
  final List<? extends RunnerAndConfigurationSettings> configs=ContainerUtil.newArrayList(createSettings("other-run",context.mock(RunConfiguration.class,"other-run-run-config")),createSettings("other-module-run",new MyModuleBasedConfiguration("other-module-run-config",getProject(),getModule())),mySettings);
  myConfigurations=Collections.unmodifiableCollection(configs);
}
