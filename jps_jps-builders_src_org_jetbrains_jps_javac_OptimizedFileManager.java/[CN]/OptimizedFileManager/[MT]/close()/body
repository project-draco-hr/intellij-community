{
  try {
    super.close();
  }
  finally {
    if (ourUseContentCache) {
      myContentCache.clear();
    }
    myDirectoryCache.clear();
    myByteBufferCache.clear();
    myIsFile.clear();
    if (!ourPathCacheClearProblem) {
      try {
        Paths.clearPathExistanceCache();
      }
 catch (      Throwable ignored) {
        ourPathCacheClearProblem=true;
      }
    }
  }
}
