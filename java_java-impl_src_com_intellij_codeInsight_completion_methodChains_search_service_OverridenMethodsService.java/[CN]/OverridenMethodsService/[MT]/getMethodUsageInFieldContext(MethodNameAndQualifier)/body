{
  int asField=0;
  int notField=0;
  for (  final CallingLocation callingLocation : myMethodCallingLocationIndex.getAllLocations(method)) {
    if (callingLocation.getVariableType().equals(VariableType.FIELD)) {
      asField++;
    }
 else {
      notField++;
    }
  }
  return Pair.create(asField,notField);
}
