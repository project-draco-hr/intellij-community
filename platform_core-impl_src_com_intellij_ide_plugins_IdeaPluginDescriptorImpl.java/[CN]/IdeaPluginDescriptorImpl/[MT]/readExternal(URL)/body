{
  try {
    Document document=JDOMUtil.loadDocument(url);
    readExternal(document,url,JDOMXIncluder.DEFAULT_PATH_RESOLVER);
  }
 catch (  FileNotFoundException e) {
    throw e;
  }
catch (  IOException e) {
    throw new InvalidDataException(e);
  }
catch (  JDOMException e) {
    throw new InvalidDataException(e);
  }
}
