{
  PsiElement problemElement=descriptor.getPsiElement();
  if (problemElement != null) {
    PyElement parent=PsiTreeUtil.getParentOfType(problemElement,PySequenceExpression.class,PyDictLiteralExpression.class,PyParenthesizedExpression.class,PyArgumentList.class,PyParameterList.class);
    removeBackSlash(parent);
  }
}
