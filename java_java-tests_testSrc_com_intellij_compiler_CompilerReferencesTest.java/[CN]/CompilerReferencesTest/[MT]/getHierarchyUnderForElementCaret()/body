{
  final PsiElement atCaret=myFixture.getElementAtCaret();
  assertNotNull(atCaret);
  final PsiClass classAtCaret=PsiTreeUtil.getParentOfType(atCaret,PsiClass.class,false);
  assertNotNull(classAtCaret);
  return CompilerReferenceService.getInstance(myFixture.getProject()).getDirectInheritors(classAtCaret,assertInstanceOf(classAtCaret.getUseScope(),GlobalSearchScope.class),assertInstanceOf(classAtCaret.getUseScope(),GlobalSearchScope.class),JavaBaseCompilerSearchAdapter.INSTANCE,StdFileTypes.JAVA);
}
