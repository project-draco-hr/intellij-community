{
  DequeItem<Result,Argument,RemoteTask> dequeItem=new DequeItem<Result,Argument,RemoteTask>(task,argument,resultProcessor);
  if (!busy) {
    tasks.addFirst(dequeItem);
    startNextTask();
    return;
  }
  if (force) {
    tasks.addFirst(dequeItem);
  }
 else {
    tasks.addLast(dequeItem);
  }
}
