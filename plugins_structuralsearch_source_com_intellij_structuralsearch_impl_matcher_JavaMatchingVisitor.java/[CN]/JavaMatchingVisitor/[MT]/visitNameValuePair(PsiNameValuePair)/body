{
  final PsiIdentifier nameIdentifier=pair.getNameIdentifier();
  if (nameIdentifier != null) {
    final MatchingHandler handler=myMatchingVisitor.getMatchContext().getPattern().getHandler(nameIdentifier);
    if (handler instanceof SubstitutionHandler) {
      myMatchingVisitor.setResult(((SubstitutionHandler)handler).handle(((PsiNameValuePair)myMatchingVisitor.getElement()).getNameIdentifier(),myMatchingVisitor.getMatchContext()));
    }
 else {
      myMatchingVisitor.setResult(myMatchingVisitor.match(nameIdentifier,((PsiNameValuePair)myMatchingVisitor.getElement()).getNameIdentifier()));
    }
  }
 else {
    myMatchingVisitor.setResult(true);
  }
}
