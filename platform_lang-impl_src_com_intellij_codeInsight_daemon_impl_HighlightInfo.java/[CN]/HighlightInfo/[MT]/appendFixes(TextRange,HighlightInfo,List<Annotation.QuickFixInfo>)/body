{
  if (info == null)   return;
  if (fixes != null) {
    for (    final Annotation.QuickFixInfo quickFixInfo : fixes) {
      TextRange range=fixedRange != null ? fixedRange : quickFixInfo.textRange;
      HighlightDisplayKey key=quickFixInfo.key != null ? quickFixInfo.key : HighlightDisplayKey.find(DefaultHighlightVisitorBasedInspection.AnnotatorBasedInspection.ANNOTATOR_SHORT_NAME);
      QuickFixAction.registerQuickFixAction(info,range,quickFixInfo.quickFix,key);
    }
  }
}
