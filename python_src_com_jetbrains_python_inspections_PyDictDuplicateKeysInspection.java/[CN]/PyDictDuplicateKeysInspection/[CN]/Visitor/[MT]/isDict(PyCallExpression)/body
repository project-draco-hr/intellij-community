{
  String name=expression.getCallee().getText();
  if ("dict".equals(name)) {
    PyType type=expression.getType(myTypeEvalContext);
    Application application=ApplicationManager.getApplication();
    if (application != null && application.isUnitTestMode()) {
      return true;
    }
    if (type != null) {
      if (type.isBuiltin()) {
        return true;
      }
    }
  }
  return false;
}
