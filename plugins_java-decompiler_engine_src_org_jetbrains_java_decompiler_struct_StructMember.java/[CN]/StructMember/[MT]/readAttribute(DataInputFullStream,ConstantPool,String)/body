{
  StructGeneralAttribute attribute=StructGeneralAttribute.createAttribute(name);
  if (attribute == null) {
    in.discard(in.readInt());
  }
 else {
    byte[] data=new byte[in.readInt()];
    in.readFull(data);
    attribute.setInfo(data);
    attribute.initContent(pool);
  }
  return attribute;
}
