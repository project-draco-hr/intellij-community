{
  VcsLog log=getLog(e);
  if (log == null) {
    getTemplatePresentation().setVisible(false);
    getTemplatePresentation().setEnabled(false);
  }
 else   if (log.getSelectedCommits().isEmpty()) {
    getTemplatePresentation().setVisible(true);
    getTemplatePresentation().setEnabled(false);
  }
 else {
    getTemplatePresentation().setVisible(true);
    getTemplatePresentation().setEnabled(true);
  }
}
