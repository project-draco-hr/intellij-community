{
  doTest("1 <caret>","!=a","1 != a<caret>");
  doTest("c <caret>",">=a","c >= a<caret>");
  doTest("c <caret>","<=a","c <= a<caret>");
  doTest("c <caret>","+=a","c += a<caret>");
  doTest("c <caret>","-=a","c -= a<caret>");
  doTest("c <caret>","*=a","c *= a<caret>");
  doTest("c <caret>","/=a","c /= a<caret>");
  doTest("c <caret>","&=a","c &= a<caret>");
  doTest("c <caret>","%=a","c %= a<caret>");
  doTest("c <caret>","^=a","c ^= a<caret>");
  doTest("c <caret>","|=a","c |= a<caret>");
}
