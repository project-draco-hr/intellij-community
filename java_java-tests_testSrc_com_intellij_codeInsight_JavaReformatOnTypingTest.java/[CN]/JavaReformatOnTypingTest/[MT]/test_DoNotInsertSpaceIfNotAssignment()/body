  doTest("1 <caret>","!=","1 != <caret>");
  doTest("c <caret>",">=","c >= <caret>");
  doTest("c <caret>","<=","c <= <caret>");
  doTest("c <caret>","+=","c += <caret>");
  doTest("c <caret>","-=","c -= <caret>");
  doTest("c <caret>","*=","c *= <caret>");
  doTest("c <caret>","/=","c /= <caret>");
  doTest("c <caret>","&=","c &= <caret>");
  doTest("c <caret>","%=","c %= <caret>");
  doTest("c <caret>","^=","c ^= <caret>");
  doTest("c <caret>","|=","c |= <caret>");
}
