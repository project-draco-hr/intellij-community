{
  PrefixExpressionContext expression=context.outerExpression();
  if (context.executionContext.isForceMode) {
    return new ArgumentLookupElement(expression);
  }
  if (expression.canBeStatement) {
    if (expression.expressionType == null)     return null;
    if (expression.referencedElement instanceof PsiClass)     return null;
    if (!CommonUtils.isNiceExpression(expression.expression))     return null;
    return new ArgumentLookupElement(expression);
  }
  return null;
}
