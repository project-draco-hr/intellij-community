{
  if (matchedNode == null)   return false;
  String text;
  text=getMeaningfulText(matchedNode);
  boolean result=doMatch(text,start,end,context);
  if (!result && (matchedNode instanceof PsiJavaCodeReferenceElement || matchedNode instanceof PsiClass)) {
    PsiElement element=(matchedNode instanceof PsiJavaCodeReferenceElement) ? ((PsiJavaCodeReferenceElement)matchedNode).resolve() : matchedNode;
    if (element instanceof PsiClass) {
      text=((PsiClass)element).getQualifiedName();
      if (text != null) {
        result=doMatch(text,start,end,context);
      }
    }
  }
  return result;
}
