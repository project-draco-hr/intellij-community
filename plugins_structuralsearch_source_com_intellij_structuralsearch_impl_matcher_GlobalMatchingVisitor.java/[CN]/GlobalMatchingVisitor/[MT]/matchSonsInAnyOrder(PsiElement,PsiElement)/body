{
  if (element1 == null && matchContext.getOptions().isLooseMatching()) {
    return true;
  }
  if (element1 == null || element2 == null) {
    return element1 == element2;
  }
  PsiElement e=element1.getFirstChild();
  return (e == null && matchContext.getOptions().isLooseMatching()) || matchInAnyOrder(new FilteringNodeIterator(e),new FilteringNodeIterator(element2.getFirstChild()));
}
