{
  try {
    final ExecutionEnvironment environment=new ExecutionEnvironmentBuilder(myEnvironment).setRunnerId(runner.getRunnerId()).setExecutor(executor).setRunProfile(profile).build();
    runner.execute(environment);
  }
 catch (  ExecutionException e1) {
    LOG.error(e1);
  }
 finally {
    profile.clear();
  }
}
