{
  List<T> result=ContainerUtil.newArrayList();
  List<List<String>> chunks=chunkArguments(arguments);
  for (  List<String> chunk : chunks) {
    if (ProgressManager.getInstance().getProgressIndicator() != null && ProgressManager.getInstance().getProgressIndicator().isCanceled()) {
      return result;
    }
    result.addAll(processor.fun(chunk));
  }
  return result;
}
