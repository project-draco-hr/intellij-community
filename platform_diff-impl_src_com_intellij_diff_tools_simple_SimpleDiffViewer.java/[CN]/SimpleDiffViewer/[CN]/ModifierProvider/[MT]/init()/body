{
  IdeEventQueue.getInstance().addPostprocessor(new IdeEventQueue.EventDispatcher(){
    @Override public boolean dispatch(    AWTEvent e){
      if (e instanceof KeyEvent) {
        onKeyEvent((KeyEvent)e);
      }
      return false;
    }
