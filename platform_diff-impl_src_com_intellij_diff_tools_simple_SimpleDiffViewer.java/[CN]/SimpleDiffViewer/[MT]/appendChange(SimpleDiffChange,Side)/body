{
  if (!change.isValid())   return;
  if (change.getStartLine(sourceSide) == change.getEndLine(sourceSide))   return;
  final Document document1=getEditor1().getDocument();
  final Document document2=getEditor2().getDocument();
  DiffUtil.applyModification(sourceSide.other().select(document1,document2),change.getEndLine(sourceSide.other()),change.getEndLine(sourceSide.other()),sourceSide.select(document1,document2),change.getStartLine(sourceSide),change.getEndLine(sourceSide));
  change.destroyHighlighter();
  myDiffChanges.remove(change);
}
