  assertQuickfixNotAvailable(InspectionGadgetsBundle.message("if.may.be.conditional.quickfix"),"class X {" + "  boolean b = true;" + "  void test(boolean foo) {"+ "    boolean c = complex() && foo ||/**/ !complex() && b;"+ "  }"+ "  boolean complex() {"+ "    b = !b;"+ "    return true;"+ "  }"+ "}");
