{
  WindowState state=myStateMap.get(key);
  if (state != null) {
    state.set(location,locationSet,size,sizeSet,extendedState,extendedStateSet);
    if (state.isEmpty()) {
      myStateMap.remove(key);
    }
  }
 else {
    state=new WindowState();
    state.set(location,locationSet,size,sizeSet,extendedState,extendedStateSet);
    if (!state.isEmpty()) {
      myStateMap.put(key,state);
    }
  }
}
