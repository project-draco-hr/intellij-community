{
synchronized (myStateMap) {
    myStateMap.clear();
    for (    Element child : element.getChildren()) {
      if (STATE.equals(child.getName())) {
        String key=child.getAttributeValue(KEY);
        if (key != null) {
          WindowState state=new WindowState();
          try {
            state.myLocation=new Point(Integer.parseInt(child.getAttributeValue(X)),Integer.parseInt(child.getAttributeValue(Y)));
          }
 catch (          NumberFormatException ignored) {
          }
          try {
            state.mySize=new Dimension(Integer.parseInt(child.getAttributeValue(WIDTH)),Integer.parseInt(child.getAttributeValue(HEIGHT)));
          }
 catch (          NumberFormatException ignored) {
          }
          try {
            state.myState=Integer.valueOf(child.getAttributeValue(EXTENDED));
          }
 catch (          NumberFormatException ignored) {
          }
          if (!state.isEmpty()) {
            myStateMap.put(key,state);
          }
        }
      }
    }
  }
}
