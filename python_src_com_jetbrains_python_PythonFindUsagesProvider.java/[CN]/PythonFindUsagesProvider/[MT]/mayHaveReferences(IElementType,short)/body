{
  if ((searchContext & UsageSearchContext.IN_CODE) != 0 && token == PyElementTypes.REFERENCE_EXPRESSION)   return true;
  if ((searchContext & UsageSearchContext.IN_COMMENTS) != 0 && token == PyTokenTypes.END_OF_LINE_COMMENT)   return true;
  if ((searchContext & UsageSearchContext.IN_STRINGS) != 0 && token == PyTokenTypes.STRING_LITERAL)   return true;
  return false;
}
