{
  DetailsChooser.show(myProject,myProjectSdksModel.getSdks(),new PythonSdkConfigurable(myProject).createComponent(),button.getPreferredPopupPoint(),false,new NullableConsumer<Sdk>(){
    @Override public void consume(    Sdk sdk){
      myMakeActiveAdded=false;
      addCreatedSdk(sdk,false);
    }
  }
);
}
