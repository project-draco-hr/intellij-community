{
  myResultsViewer.onCustomProgressTestsCategory("foo",1);
  assertEquals(1,myResultsViewer.getTotalTestCount());
  myResultsViewer.onCustomProgressTestStarted();
  assertEquals(1,myResultsViewer.getTotalTestCount());
  myResultsViewer.onCustomProgressTestStarted();
  assertEquals(2,myResultsViewer.getTotalTestCount());
}
