{
  Matcher matcherReportDir=REPORT_DIR_PATTERN.matcher(line);
  if (matcherReportDir.matches()) {
    final String path=matcherReportDir.group(1);
    myReportFolder=path;
    return new Result(entireLength - line.length() + matcherReportDir.start(1),entireLength - line.length() + matcherReportDir.end(1),new HyperlinkInfo(){
      @Override public void navigate(      Project project){
        File f=new File(path);
        if (f.isDirectory()) {
          ShowFilePathAction.openDirectory(f);
        }
      }
    }
);
  }
  return null;
}
