{
  try {
    RandomAccessFile f=new RandomAccessFile(path,"r");
    try {
      long length=f.length(), limit=Math.max(length - 65535,0), offset=length - 22;
      while (offset >= limit) {
        f.seek(offset);
        if (f.read() == 0x50 && f.read() == 0x4B && f.read() == 0x05 && f.read() == 0x06) {
          return true;
        }
        offset--;
      }
    }
  finally {
      f.close();
    }
  }
 catch (  IOException e) {
    LOG.warn("invalid .jar: " + path);
  }
  return false;
}
