{
  super("Group by: ",true);
  myPredefinedSeparators.add(".");
  myPredefinedSeparators.add("_");
  myPredefinedSeparators.add("/");
  String currentSeparator=getCurrentSeparator();
  if (!myPredefinedSeparators.contains(currentSeparator)) {
    myPredefinedSeparators.add(currentSeparator);
  }
  refillActionGroup();
}
