{
  path=path.replace('/','\\');
  if (TRACE_ENABLED) {
    LOG.trace("list(" + path + ")");
    long t=System.nanoTime();
    FileInfo[] children=listChildren0(path);
    t=(System.nanoTime() - t) / 1000;
    LOG.trace("  " + (children == null ? -1 : children.length) + " children, "+ t+ " mks");
    return children;
  }
 else {
    return listChildren0(path);
  }
}
