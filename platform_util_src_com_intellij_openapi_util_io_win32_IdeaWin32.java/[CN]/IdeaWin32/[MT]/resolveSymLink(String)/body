{
  path=path.replace('/','\\');
  if (TRACE_ENABLED) {
    LOG.trace("resolveSymLink(" + path + ")");
    long t=System.nanoTime();
    String result=resolveSymLink0(path);
    t=(System.nanoTime() - t) / 1000;
    LOG.trace("  " + t + " mks");
    return result;
  }
 else {
    return resolveSymLink0(path);
  }
}
