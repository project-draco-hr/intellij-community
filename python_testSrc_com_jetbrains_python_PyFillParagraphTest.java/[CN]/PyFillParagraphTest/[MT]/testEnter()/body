{
  final CommonCodeStyleSettings settings=CodeStyleSettingsManager.getInstance(myFixture.getProject()).getCurrentSettings().getCommonSettings(PythonLanguage.getInstance());
  int oldValue=settings.RIGHT_MARGIN;
  settings.RIGHT_MARGIN=80;
  try {
    doTest();
  }
  finally {
    settings.RIGHT_MARGIN=oldValue;
  }
}
