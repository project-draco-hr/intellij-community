{
  String clName=root.classStruct.qualifiedName;
  int index=clName.lastIndexOf('/');
  if (index >= 0) {
    String packageName=clName.substring(0,index);
    currentPackageSlash=packageName + '/';
    currentPackagePoint=packageName.replace('/','.');
  }
 else {
    currentPackageSlash="";
    currentPackagePoint="";
  }
}
