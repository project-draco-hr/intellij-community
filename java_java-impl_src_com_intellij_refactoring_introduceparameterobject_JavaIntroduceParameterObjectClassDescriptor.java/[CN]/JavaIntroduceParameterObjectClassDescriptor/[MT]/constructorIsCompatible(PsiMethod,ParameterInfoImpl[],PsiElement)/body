{
  final PsiParameterList parameterList=constructor.getParameterList();
  final PsiParameter[] constructorParams=parameterList.getParameters();
  if (constructorParams.length != paramsToMerge.length) {
    return false;
  }
  for (int i=0; i < constructorParams.length; i++) {
    if (!TypeConversionUtil.isAssignable(constructorParams[i].getType(),getParameterInfo(i).getTypeWrapper().getType(context,context.getManager()))) {
      return false;
    }
  }
  return true;
}
