{
  if (createInnerClass) {
    final PsiClass containingClass=method.getContainingClass();
    if (containingClass != null) {
      final String qualifiedName=containingClass.getQualifiedName();
      return qualifiedName != null ? qualifiedName : "";
    }
 else {
      return packageName;
    }
  }
  return packageName;
}
