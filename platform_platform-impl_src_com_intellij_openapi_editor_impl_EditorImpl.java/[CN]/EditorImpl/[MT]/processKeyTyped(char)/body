{
  IdeEventQueue queue=IdeEventQueue.getInstance();
  if (queue.shouldNotTypeInEditor() || ProgressManager.getInstance().hasModalProgressIndicator()) {
    return false;
  }
  FileDocumentManager manager=FileDocumentManager.getInstance();
  final VirtualFile file=manager.getFile(myDocument);
  if (file != null && !file.isValid()) {
    return false;
  }
  Graphics graphics=myEditorComponent.getGraphics();
  if (graphics != null) {
    processKeyTypedImmediately(c,graphics);
    graphics.dispose();
  }
  ActionManagerEx.getInstanceEx().fireBeforeEditorTyping(c,getDataContext());
  MacUIUtil.hideCursor();
  processKeyTypedNormally(c);
  return true;
}
