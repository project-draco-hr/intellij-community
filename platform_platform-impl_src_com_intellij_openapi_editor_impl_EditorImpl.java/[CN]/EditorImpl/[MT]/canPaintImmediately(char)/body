{
  return myDocument instanceof DocumentImpl && myHighlighter instanceof LexerEditorHighlighter && !mySelectionModel.hasSelection()&& arePositionsWithinDocument(myCaretModel.getAllCarets())&& areVisualLinesUnique(myCaretModel.getAllCarets())&& !isInplaceRenamerActive()&& !KEY_CHARS_TO_SKIP.contains(c);
}
