{
  for (  RefEntity entity : getIgnoredElements().keySet()) {
    if (Comparing.equal(entity,element)) {
      for (      RefEntity entity1 : getProblemElements().keySet()) {
        if (Comparing.equal(entity1,element)) {
          return false;
        }
      }
      return true;
    }
  }
  return false;
}
