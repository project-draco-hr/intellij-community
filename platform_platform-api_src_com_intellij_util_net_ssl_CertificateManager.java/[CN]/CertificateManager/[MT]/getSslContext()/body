{
  if (mySslContext == null) {
    SSLContext context=getSystemSslContext();
    if (Registry.is("ide.certificate.manager")) {
      try {
        context.init(null,new TrustManager[]{getTrustManager()},null);
      }
 catch (      Exception e) {
        LOG.error(e);
      }
    }
    mySslContext=context;
  }
  return mySslContext;
}
