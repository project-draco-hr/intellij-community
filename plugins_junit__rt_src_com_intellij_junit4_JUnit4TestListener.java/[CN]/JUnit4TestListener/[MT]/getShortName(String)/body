{
  if (fqName == null)   return null;
  final int idx=fqName.indexOf("[");
  if (idx == 0) {
    return fqName;
  }
  int lastPointIdx=fqName.lastIndexOf('.');
  if (idx > 0 && fqName.endsWith("]")) {
    lastPointIdx=fqName.substring(0,idx).lastIndexOf('.');
  }
  if (lastPointIdx >= 0) {
    return fqName.substring(lastPointIdx + 1);
  }
  return fqName;
}
