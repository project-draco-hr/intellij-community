{
  if (e.getSource() == mySkipButton) {
    doOKAction();
    return;
  }
  if (e.getSource() == myBackButton) {
    myIndex--;
    initCurrentStep();
    return;
  }
  if (e.getSource() == myNextButton) {
    if (myIndex >= mySteps.size() - 1) {
      doOKAction();
      return;
    }
    myIndex++;
    initCurrentStep();
  }
}
