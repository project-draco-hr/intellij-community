{
  super(manager,projectRootManager,bus);
  if (!((StartupManagerEx)startupManager).startupActivityPassed() && !ApplicationManager.getApplication().isUnitTestMode() && !manager.getProject().isDefault()) {
    Logger.getInstance("#com.intellij.psi.impl.file.impl.JavaFileManagerImpl").error("Access to psi files should be performed only after startup activity");
  }
}
