{
  return new Iterator<T>(){
    int step=0;
    T t=null;
    @Override public boolean hasNext(){
      return step < size;
    }
    @Override public T next(){
      step++;
      if (t == null) {
        t=calculator.getFirst();
      }
 else {
        t=calculator.next(t);
      }
      return t;
    }
    @Override public void remove(){
      throw new IllegalAccessError();
    }
  }
;
}
