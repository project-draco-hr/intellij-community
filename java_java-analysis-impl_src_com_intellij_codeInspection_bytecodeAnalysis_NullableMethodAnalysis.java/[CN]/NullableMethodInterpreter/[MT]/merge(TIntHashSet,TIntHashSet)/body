{
  if (set1 == null || set1.isEmpty()) {
    return set2;
  }
 else   if (set2.isEmpty()) {
    return set1;
  }
 else {
    TIntHashSet set=new TIntHashSet();
    TIntIterator iter=set1.iterator();
    while (iter.hasNext()) {
      set.add(iter.next());
    }
    iter=set2.iterator();
    while (iter.hasNext()) {
      set.add(iter.next());
    }
    return set;
  }
}
