{
  String source="import java.io.Externalizable;\n" + "import java.io.Serializable;\n" + "abstract class MyClass implements Serializable, java.util.List, Externalizable {}";
  String search="class 'TestCase implements java.util.List, 'others* {\n    'MyClassContent\n}";
  String replace="class $TestCase$ implements $others$ {\n    $MyClassContent$\n}";
  String expectedResult="import java.io.Externalizable;\n" + "import java.io.Serializable;\n" + "abstract  class MyClass implements Externalizable,Serializable {\n    \n}";
  String actualResult=replacer.testReplace(source,search,replace,options,true);
  assertEquals(expectedResult,actualResult);
}
