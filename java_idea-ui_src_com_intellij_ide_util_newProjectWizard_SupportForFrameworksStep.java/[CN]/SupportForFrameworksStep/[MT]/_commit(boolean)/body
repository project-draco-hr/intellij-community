{
  if (finishChosen && !myCommitted) {
    if (!mySupportForFrameworksPanel.validate()) {
      throw new CommitStepException(null);
    }
    if (!mySupportForFrameworksPanel.downloadLibraries(mySupportForFrameworksPanel.getMainPanel())) {
      throw new CommitStepException(null);
    }
    myCommitted=true;
  }
}
