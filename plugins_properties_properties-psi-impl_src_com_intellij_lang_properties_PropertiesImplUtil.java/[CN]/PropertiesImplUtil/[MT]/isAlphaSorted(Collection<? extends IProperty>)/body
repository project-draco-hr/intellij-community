{
  String previousKey=null;
  for (  IProperty property : properties) {
    final String key=property.getKey();
    if (key == null) {
      return false;
    }
    if (previousKey != null && String.CASE_INSENSITIVE_ORDER.compare(previousKey,key) > 0) {
      return false;
    }
    previousKey=key;
  }
  return true;
}
