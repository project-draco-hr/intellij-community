{
  final String message=RefactoringBundle.message("class.0.does.not.have.inheritors",DescriptiveNameUtil.getDescriptiveName(sourceClass)) + "\n" + RefactoringBundle.message("push.down.will.delete.members");
  final int answer=Messages.showYesNoDialog(message,conflictDialogTitle,Messages.getWarningIcon());
  if (answer != Messages.YES) {
    return NewSubClassData.EMPTY;
  }
  return null;
}
