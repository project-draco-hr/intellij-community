{
  setNameAndTooltip(getName(),null,myInfo.isUpToDate() ? "UP-TO-DATE" : null);
  setIcon(myInfo.isRunning() ? NodeProgressAnimator.getCurrentFrame() : myInfo.isFailed() ? AllIcons.RunConfigurations.TestError : myInfo.isSkipped() ? AllIcons.RunConfigurations.TestIgnored : AllIcons.RunConfigurations.TestPassed);
}
