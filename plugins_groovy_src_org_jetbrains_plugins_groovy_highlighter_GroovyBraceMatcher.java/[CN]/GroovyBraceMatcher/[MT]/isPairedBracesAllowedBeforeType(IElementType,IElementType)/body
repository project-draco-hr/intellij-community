{
  return tokenType == null || tokenType == TokenType.WHITE_SPACE || tokenType == mSEMI || tokenType == mCOMMA || tokenType == mRPAREN || tokenType == mRBRACK || tokenType == mRCURLY || tokenType == mGSTRING_BEGIN || tokenType == mREGEX_BEGIN || tokenType == mDOLLAR_SLASH_REGEX_BEGIN || TokenSets.COMMENT_SET.contains(tokenType) || tokenType.getLanguage() != GroovyFileType.GROOVY_LANGUAGE;
}
