{
  if (markupChar == 'U') {
    appendLink(markupContent);
    return;
  }
switch (markupChar) {
case 'I':
    appendTagPair(markupContent,"i");
  break;
case 'B':
appendTagPair(markupContent,"b");
break;
case 'C':
appendTagPair(markupContent,"code");
break;
default :
myResult.append(StringUtil.escapeXml(markupContent));
break;
}
}
