{
  super.buildOptions(searchOptions);
  searchOptions.add(UIUtil.createOptionLine(shortenFQN=new JCheckBox(SSRBundle.message("shorten.fully.qualified.names.checkbox"),true)));
  searchOptions.add(UIUtil.createOptionLine(formatAccordingToStyle=new JCheckBox(CodeInsightBundle.message("dialog.edit.template.checkbox.reformat.according.to.style"),true)));
  searchOptions.add(UIUtil.createOptionLine(useStaticImport=new JCheckBox(CodeInsightBundle.message("dialog.edit.template.checkbox.use.static.import"),true)));
}
