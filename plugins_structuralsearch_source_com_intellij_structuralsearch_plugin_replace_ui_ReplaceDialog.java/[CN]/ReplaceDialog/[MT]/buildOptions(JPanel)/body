{
  super.buildOptions(searchOptions);
  searchOptions.add(UIUtil.createOptionLine(shortenFQN=new JCheckBox(SSRBundle.message("shorten.fully.qualified.names.checkbox"),true)));
  searchOptions.add(UIUtil.createOptionLine(formatAccordingToStyle=new JCheckBox(SSRBundle.message("format.according.to.style.checkbox"),true)));
}
