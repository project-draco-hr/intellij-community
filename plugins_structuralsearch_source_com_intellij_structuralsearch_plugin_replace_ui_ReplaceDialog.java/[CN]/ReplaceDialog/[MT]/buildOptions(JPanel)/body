{
  super.buildOptions(searchOptions);
  searchOptions.add(UIUtil.createOptionLine(shortenFQN=new JCheckBox("Shorten fully qualified names",true)));
  shortenFQN.setMnemonic('o');
  searchOptions.add(UIUtil.createOptionLine(formatAccordingToStyle=new JCheckBox("Format according to style",true)));
  formatAccordingToStyle.setMnemonic('r');
}
