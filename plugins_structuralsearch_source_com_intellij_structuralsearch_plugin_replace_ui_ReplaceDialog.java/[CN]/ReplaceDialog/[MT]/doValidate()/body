{
  if (!super.doValidate())   return false;
  boolean result=true;
  try {
    Replacer.checkSupportedReplacementPattern(searchContext.getProject(),searchCriteriaEdit.getDocument().getText(),replaceCriteriaEdit.getDocument().getText(),getCurrentFileType());
  }
 catch (  UnsupportedPatternException ex) {
    reportMessage("unsupported.replacement.pattern.message",replaceCriteriaEdit,ex.getMessage());
    result=false;
  }
  return result;
}
