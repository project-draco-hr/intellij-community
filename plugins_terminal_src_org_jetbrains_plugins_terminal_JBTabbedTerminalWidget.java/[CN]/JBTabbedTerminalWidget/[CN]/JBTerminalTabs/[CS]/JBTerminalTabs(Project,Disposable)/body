{
  final ActionManager actionManager=ActionManager.getInstance();
  myTabs=new JBEditorTabs(project,actionManager,IdeFocusManager.getInstance(project),parent){
    @Override protected TabLabel createTabLabel(    TabInfo info){
      return new TerminalTabLabel(this,info);
    }
  }
;
  myTabs.addListener(new TabsListener.Adapter(){
    @Override public void selectionChanged(    TabInfo oldSelection,    TabInfo newSelection){
      fireChanged(new ChangeEvent(myTabs));
    }
  }
);
  myTabs.setTabDraggingEnabled(true);
}
