{
  try {
    if (!task.get() || task.isCancelled()) {
      myStopFormatting=true;
    }
  }
 catch (  InterruptedException e) {
    LOG.error("Got unexpected exception during formatting " + file,e);
  }
catch (  ExecutionException e) {
    LOG.error("Got unexpected exception during formatting " + file,e);
  }
}
