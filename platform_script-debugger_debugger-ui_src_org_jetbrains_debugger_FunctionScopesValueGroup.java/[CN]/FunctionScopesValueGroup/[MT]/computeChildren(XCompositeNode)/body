{
  node.setAlreadySorted(true);
  ObsolescentAsyncResults.consume(value.resolve(),node,new PairConsumer<FunctionValue,XCompositeNode>(){
    @Override public void consume(    FunctionValue value,    XCompositeNode node){
      Scope[] scopes=value.getScopes();
      if (scopes == null || scopes.length == 0) {
        node.addChildren(XValueChildrenList.EMPTY,true);
      }
 else {
        CallFrameView.createAndAddScopeList(node,Arrays.asList(scopes),variableContext,null);
      }
    }
  }
);
}
