{
  final PsiType fieldType=field.getType();
  if (fieldType instanceof PsiArrayType && JavaVersionService.getInstance().isAtLeast(field,JavaSdkVersion.JDK_1_5)) {
    if (isNestedArray(fieldType)) {
      buffer.append(" ");
      buffer.append("// Probably incorrect - hashCode for high dimension arrays with Arrays.hashCode");
      buffer.append("\n");
    }
    buffer.append("java.util.Arrays.hashCode(");
    buffer.append(name);
    buffer.append(")");
  }
 else {
    buffer.append(name);
    buffer.append(".hashCode()");
  }
}
