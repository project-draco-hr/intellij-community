{
  @SuppressWarnings("UndesirableClassUsage") BufferedImage image=new BufferedImage(getWidth(),getHeight(),BufferedImage.TYPE_INT_RGB);
  Graphics imageGraphics=image.getGraphics();
  imageGraphics.setColor(new Color(myFillColor.getRGB()));
  imageGraphics.fillRect(0,0,getWidth(),getHeight());
  super.paintChildren(imageGraphics);
  imageGraphics.dispose();
  UIUtil.drawImage(g,makeColorTransparent(image,myFillColor),0,0,null);
}
