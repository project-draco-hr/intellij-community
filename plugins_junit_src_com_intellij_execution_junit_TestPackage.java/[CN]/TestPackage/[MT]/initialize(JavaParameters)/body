{
  super.initialize(javaParameters);
  final JUnitConfiguration.Data data=myConfiguration.getPersistentData();
  getClassFilter(data);
  configureClasspath(javaParameters);
  try {
    createTempFiles(javaParameters);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
  createServerSocket(javaParameters);
}
