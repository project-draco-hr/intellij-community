{
  super.setupDialog(project,allSdks);
  setTitle(PyBundle.message("sdk.create.venv.dialog.title"));
  Iterables.removeIf(allSdks,new Predicate<Sdk>(){
    @Override public boolean apply(    Sdk s){
      return PythonSdkType.isInvalid(s) || PythonSdkType.isVirtualEnv(s) || RemoteSdkCredentialsHolder.isRemoteSdk(s.getHomePath())|| PythonSdkType.isCondaVirtualEnv(s);
    }
  }
);
  List<Sdk> sortedSdks=new ArrayList<Sdk>(allSdks);
  Collections.sort(sortedSdks,new PreferredSdkComparator());
  updateSdkList(allSdks,sortedSdks.get(0));
}
