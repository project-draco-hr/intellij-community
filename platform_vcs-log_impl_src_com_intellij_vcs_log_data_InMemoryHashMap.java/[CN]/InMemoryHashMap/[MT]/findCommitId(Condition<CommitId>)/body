{
  final CommitId[] result=new CommitId[]{null};
  myIntToObjectMap.forEachValue(new TObjectProcedure<CommitId>(){
    @Override public boolean execute(    CommitId commitId){
      if (condition.value(commitId)) {
        result[0]=commitId;
        return false;
      }
      return true;
    }
  }
);
  return result[0];
}
