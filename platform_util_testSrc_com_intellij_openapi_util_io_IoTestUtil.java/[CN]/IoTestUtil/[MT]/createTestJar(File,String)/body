{
  ZipOutputStream stream=new ZipOutputStream(new FileOutputStream(jarFile));
  try {
    for (int i=0; i < data.length; i+=2) {
      stream.putNextEntry(new ZipEntry(data[i]));
      stream.write(data[i + 1].getBytes(CharsetToolkit.UTF8_CHARSET));
      stream.closeEntry();
    }
  }
  finally {
    stream.close();
  }
  return jarFile;
}
