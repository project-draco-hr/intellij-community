{
  myGroupId=groupDisplayId;
  myTitle=StringUtil.notNullize(title);
  myContent=StringUtil.notNullize(content);
  myType=type;
  myListener=listener;
  myTimestamp=System.currentTimeMillis();
  myIcon=icon;
  mySubtitle=subtitle;
  LOG.assertTrue(!StringUtil.isEmptyOrSpaces(myTitle) || !StringUtil.isEmptyOrSpaces(mySubtitle) || !StringUtil.isEmptyOrSpaces(myContent),"Notification should have title: " + title + " and/or subtitle and/or content groupId: "+ myGroupId);
}
