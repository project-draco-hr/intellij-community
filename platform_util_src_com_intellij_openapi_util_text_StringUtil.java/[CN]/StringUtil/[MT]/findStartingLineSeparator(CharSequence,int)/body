{
  if (startIndex < 0 || startIndex >= text.length()) {
    return null;
  }
  char ch=text.charAt(startIndex);
  if (ch == '\r') {
    return startIndex + 1 < text.length() && text.charAt(startIndex + 1) == '\n' ? LineSeparator.CRLF : LineSeparator.CR;
  }
  return ch == '\n' ? LineSeparator.LF : null;
}
