{
  if (text != null) {
synchronized (myLock) {
      myText2Stack.put(subTask,text);
    }
    myParent.setText2(text);
  }
 else {
    String prev;
synchronized (myLock) {
      myText2Stack.remove(subTask);
      prev=myText2Stack.getLastValue();
    }
    if (prev != null) {
      myParent.setText2(prev);
    }
  }
}
