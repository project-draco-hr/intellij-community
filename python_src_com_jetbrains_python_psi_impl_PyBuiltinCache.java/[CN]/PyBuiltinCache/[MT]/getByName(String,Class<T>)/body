{
  if (myBuiltinsFile != null) {
    for (    PsiElement element : myBuiltinsFile.getChildren()) {
      if (type.isInstance(element)) {
        final T cast=type.cast(element);
        if ((name != null) && name.equals(cast.getName())) {
          return cast;
        }
      }
    }
  }
  return null;
}
