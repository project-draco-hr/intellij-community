{
  if ((stickyTag == null) && (stickyDate == null))   return;
  if (stickyTag != null) {
    myStickyDate=null;
    myStickyTag=stickyTag;
  }
 else {
    myStickyTag=null;
    try {
      myStickyDate=Entry.getLastModifiedDateFormatter().parse(stickyDate);
    }
 catch (    ParseException e) {
      LOG.error(e);
    }
  }
}
