{
  String s="function f() {\n" + "  if (a == 0) {\n" + "    doc.print(\"one\");\n"+ "    doc.print(\"two\");\n"+ "  }\n"+ "  else {\n"+ "    doc.print(\"not zero\");\n"+ "  }\n"+ "}";
  doTest(s,"if ($condition$) {\n" + "  $exp$;" + "}",0);
  doTest(s,"if ($condition$) $exp$",0);
  doTest(s,"if ($condition$)",1);
}
