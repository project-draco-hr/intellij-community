{
  if (element == null)   return null;
  T result=null;
  for (PsiElement child=element.getFirstChild(); child != null; child=child.getNextSibling()) {
    if (aClass.isInstance(child)) {
      if (result == null) {
        result=(T)child;
      }
 else {
        return null;
      }
    }
  }
  return result;
}
