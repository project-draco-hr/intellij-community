{
  JarArtifactFromModulesDialog dialog=new JarArtifactFromModulesDialog(myContext);
  if (!dialog.showAndGet()) {
    return null;
  }
  return doCreateArtifact(dialog.getSelectedModules(),dialog.getMainClassName(),dialog.getDirectoryForManifest(),dialog.isExtractLibrariesToJar(),dialog.isIncludeTests());
}
