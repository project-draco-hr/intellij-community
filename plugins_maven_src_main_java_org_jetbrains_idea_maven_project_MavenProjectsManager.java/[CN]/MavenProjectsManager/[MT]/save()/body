{
  if (myProjectsTree != null) {
    mySaveQueue.queue(new Update(MavenProjectsManager.this){
      @Override public void run(){
        try {
          myProjectsTree.save(getProjectsTreeFile());
        }
 catch (        IOException e) {
          MavenLog.LOG.info(e);
        }
      }
    }
);
  }
}
