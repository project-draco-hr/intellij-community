{
  final AsyncPromise<List<Module>> result=new AsyncPromise<List<Module>>();
  runWhenFullyOpen(new Runnable(){
    public void run(){
      myImportingQueue.queue(new Update(MavenProjectsManager.this){
        public void run(){
          result.setResult(importProjects());
        }
      }
);
    }
  }
);
  return result;
}
