{
  final YAMLFile yamlFile=(YAMLFile)PsiFileFactory.getInstance(getProject()).createFileFromText("temp." + YAMLFileType.YML.getDefaultExtension(),YAMLFileType.YML,"foo: " + text,LocalTimeCounter.currentTime(),true);
  final YAMLKeyValue topKeyValue=(YAMLKeyValue)yamlFile.getDocuments().get(0).getYAMLElements().get(0);
  getValue().replace(topKeyValue.getValue());
}
