{
  init("public class Test {\n" + "<selection>  int field;\n" + "}</selection>");
  verifySyntaxInfo(new Text(20,22),new Foreground(1),new FontFamilyName(1),new FontStyle(Font.BOLD),new FontSize(getFontSize()),new Text(22,26),new Foreground(2),new Text(26,31),new Foreground(3),new FontStyle(Font.PLAIN),new Text(31,33),new Text(33,34));
}
