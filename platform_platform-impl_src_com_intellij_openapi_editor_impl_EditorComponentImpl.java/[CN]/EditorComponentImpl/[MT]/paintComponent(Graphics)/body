{
  myApplication.editorPaintStart();
  try {
    Graphics2D gg=!EditorUtil.isRealFileEditor(myEditor) ? (Graphics2D)g : IdeBackgroundUtil.withEditorBackground(g,this);
    UIUtil.setupComposite(gg);
    UISettings.setupAntialiasing(gg);
    myEditor.paint(gg);
  }
  finally {
    myApplication.editorPaintFinish();
  }
}
