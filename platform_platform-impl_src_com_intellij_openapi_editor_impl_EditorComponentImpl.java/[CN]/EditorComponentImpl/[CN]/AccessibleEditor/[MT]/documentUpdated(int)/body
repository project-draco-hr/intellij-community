{
  final Integer offset=new Integer(index);
  if (SwingUtilities.isEventDispatchThread()) {
    firePropertyChange(ACCESSIBLE_TEXT_PROPERTY,null,offset);
  }
 else {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        firePropertyChange(ACCESSIBLE_TEXT_PROPERTY,null,offset);
      }
    }
);
  }
}
