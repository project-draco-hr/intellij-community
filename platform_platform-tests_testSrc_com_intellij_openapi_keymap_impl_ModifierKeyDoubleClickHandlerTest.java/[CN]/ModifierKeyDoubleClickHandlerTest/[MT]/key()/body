{
  IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_PRESSED,Clock.getTime(),InputEvent.SHIFT_MASK,KeyEvent.VK_BACK_SPACE,'\b'));
  IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_TYPED,Clock.getTime(),InputEvent.SHIFT_MASK,0,'\b'));
  IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_RELEASED,Clock.getTime(),InputEvent.SHIFT_MASK,KeyEvent.VK_BACK_SPACE,'\b'));
}
