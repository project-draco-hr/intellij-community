{
  for (int i=0; i < repeat; i++) {
    IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_PRESSED,Clock.getTime(),InputEvent.SHIFT_MASK,otherKey ? KeyEvent.VK_ENTER : KeyEvent.VK_BACK_SPACE,otherKey ? '\n' : '\b'));
    IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_TYPED,Clock.getTime(),InputEvent.SHIFT_MASK,0,otherKey ? '\n' : '\b'));
  }
  IdeEventQueue.getInstance().dispatchEvent(new KeyEvent(myComponent,KeyEvent.KEY_RELEASED,Clock.getTime(),InputEvent.SHIFT_MASK,otherKey ? KeyEvent.VK_ENTER : KeyEvent.VK_BACK_SPACE,otherKey ? '\n' : '\b'));
}
