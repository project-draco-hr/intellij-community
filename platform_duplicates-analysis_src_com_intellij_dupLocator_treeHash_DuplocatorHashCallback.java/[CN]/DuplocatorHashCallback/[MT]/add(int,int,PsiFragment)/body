{
  int bound;
  if (myBound >= 0) {
    bound=myBound;
  }
 else {
    final DuplocatorState duplocatorState=DuplocatorUtil.getDuplocatorState(frag);
    if (duplocatorState == null) {
      return;
    }
    bound=duplocatorState.getLowerBound();
  }
  if (cost >= bound) {
    forceAdd(hash,cost,frag);
  }
}
