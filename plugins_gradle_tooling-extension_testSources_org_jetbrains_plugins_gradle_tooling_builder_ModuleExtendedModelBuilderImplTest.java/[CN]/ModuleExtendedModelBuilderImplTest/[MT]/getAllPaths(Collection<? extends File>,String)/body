{
  List<String> list=ContainerUtil.map2List(directories,new Function<File,String>(){
    @Override public String fun(    File sourceDirectory){
      String path=FileUtil.toCanonicalPath(FileUtil.getRelativePath(new File(testDir,moduleName),sourceDirectory));
      Assert.assertNotNull(path);
      return path.substring(path.indexOf("/") + 1);
    }
  }
);
  Collections.sort(list);
  return list;
}
