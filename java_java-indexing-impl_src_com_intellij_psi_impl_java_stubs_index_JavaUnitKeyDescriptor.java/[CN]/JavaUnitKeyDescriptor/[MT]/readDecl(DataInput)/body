{
  boolean isClassDecl=in.readBoolean();
  if (isClassDecl) {
    return readClassDecl(in);
  }
 else {
    IndexTree.Decl[] decls=new IndexTree.Decl[DataInputOutputUtil.readINT(in)];
    for (int i=0; i < decls.length; i++) {
      decls[i]=readDecl(in);
    }
    return new IndexTree.MemberDecl(decls);
  }
}
