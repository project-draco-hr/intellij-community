{
  final Project project=psiClass.getProject();
  final String newVarName=chooseLongestName(JavaCodeStyleManager.getInstance(project).suggestVariableName(VariableKind.LOCAL_VARIABLE,null,null,JavaPsiFacade.getElementFactory(project).createType(psiClass)));
  return new ChainCompletionNewVariableLookupElement(psiClass,newVarName,calledMethods);
}
