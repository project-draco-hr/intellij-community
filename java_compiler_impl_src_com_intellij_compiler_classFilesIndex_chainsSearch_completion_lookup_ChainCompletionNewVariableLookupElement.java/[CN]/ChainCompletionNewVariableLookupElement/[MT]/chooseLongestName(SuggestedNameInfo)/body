{
  final String[] names=suggestedNameInfo.names;
  String longestWord=names[0];
  int maxLength=longestWord.length();
  for (int i=1; i < names.length; i++) {
    final int length=names[i].length();
    if (length > maxLength) {
      maxLength=length;
      longestWord=names[i];
    }
  }
  return longestWord;
}
