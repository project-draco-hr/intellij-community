{
  myScheme.setEditorFontSize(25);
  myScheme.setConsoleFontSize(21);
  checkState(myScheme.getFontPreferences(),Arrays.asList(FontPreferences.DEFAULT_FONT_NAME),Arrays.asList(FontPreferences.DEFAULT_FONT_NAME),FontPreferences.DEFAULT_FONT_NAME,FontPreferences.DEFAULT_FONT_NAME,25);
  assertEquals(FontPreferences.DEFAULT_FONT_NAME,myScheme.getEditorFontName());
  assertEquals(25,myScheme.getEditorFontSize());
  checkState(myScheme.getConsoleFontPreferences(),Arrays.asList(FontPreferences.DEFAULT_FONT_NAME),Arrays.asList(FontPreferences.DEFAULT_FONT_NAME),FontPreferences.DEFAULT_FONT_NAME,FontPreferences.DEFAULT_FONT_NAME,21);
  assertEquals(FontPreferences.DEFAULT_FONT_NAME,myScheme.getConsoleFontName());
  assertEquals(21,myScheme.getConsoleFontSize());
}
