{
  List<String> shortList=getNewList();
  shortList.add("1");
  assertEquals(shortList.size(),1);
  assertEquals(listToStr(shortList),"1|");
  shortList.add(null);
  assertEquals(shortList.size(),2);
  assertEquals(listToStr(shortList),"1|null|");
  shortList.remove(1);
  assertEquals(listToStr(shortList),"1|");
}
