{
  myWriteLock.lock();
  try {
    if (isBroken()) {
      return false;
    }
    myKeyStore.deleteEntry(alias);
    flushKeyStore();
    myTrustManager=initFactoryAndGetManager();
    return true;
  }
 catch (  Exception e) {
    LOG.error("Can't remove certificate for alias: " + alias,e);
    return false;
  }
 finally {
    myWriteLock.unlock();
  }
}
