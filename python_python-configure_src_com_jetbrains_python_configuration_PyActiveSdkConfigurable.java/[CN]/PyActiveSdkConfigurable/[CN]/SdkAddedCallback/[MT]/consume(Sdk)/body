{
  if (sdk == null)   return;
  final PySdkService sdkService=PySdkService.getInstance();
  sdkService.restoreSdk(sdk);
  if (!(sdk instanceof PyDetectedSdk) && myProjectSdksModel.findSdk(sdk.getName()) == null) {
    myProjectSdksModel.addSdk(sdk);
  }
  updateSdkList(false);
  setSelectedSdk(sdk);
}
