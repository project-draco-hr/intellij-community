{
  try {
    if (Registry.is("cvs.roots.refresh.uses.vfs")) {
      if (vFile.isDirectory()) {
        return directoryIsUnderCVS(vFile);
      }
      return fileIsUnderCvs(getEntryFor(vFile));
    }
 else {
      return fileIsUnderCvs(vFile);
    }
  }
 catch (  Exception e1) {
    return false;
  }
}
