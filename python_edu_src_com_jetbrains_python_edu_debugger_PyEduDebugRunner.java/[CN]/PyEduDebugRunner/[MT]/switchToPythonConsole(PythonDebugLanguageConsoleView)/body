{
  AnAction[] actions=view.createConsoleActions();
  for (  AnAction action : actions) {
    Presentation presentation=action.getTemplatePresentation();
    String text=presentation.getText();
    if (PyBundle.message("run.configuration.show.command.line.action.name").equals(text)) {
      AnActionEvent event=AnActionEvent.createFromAnAction(action,null,ActionPlaces.DEBUGGER_TOOLBAR,DataManager.getInstance().getDataContext(view));
      action.actionPerformed(event);
    }
  }
}
