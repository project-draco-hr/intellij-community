{
  myFixture.configureByFile(getTestName(true) + ".py");
  moveByText("func");
  assertFalse(isActionEnabled());
  moveByText("local");
  assertTrue(isActionEnabled());
  moveByText("method");
  assertTrue(isActionEnabled());
  moveByText("static_method");
  assertFalse(isActionEnabled());
  moveByText("class_method");
  assertFalse(isActionEnabled());
}
