{
  createStdProjectFolders();
  MavenProjectsManager.getInstance(myProject).getImportingSettings().setGeneratedSourcesFolder(MavenImportingSettings.GeneratedSourcesFolder.SUBFOLDER);
  createProjectSubFile("target/generated-sources/com/A.java","package com; class A {}");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertSources("project","src/main/java","target/generated-sources/com");
  assertResources("project","src/main/resources");
}
