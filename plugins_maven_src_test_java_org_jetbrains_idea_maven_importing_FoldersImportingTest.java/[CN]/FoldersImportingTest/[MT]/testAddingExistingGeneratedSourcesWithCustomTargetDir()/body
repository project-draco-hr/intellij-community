{
  createStdProjectFolders();
  createProjectSubDirsWithFile("targetCustom/generated-sources/src","targetCustom/generated-test-sources/test");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <directory>targetCustom</directory>"+ "</build>");
  assertSources("project","src/main/java","targetCustom/generated-sources/src");
  assertResources("project","src/main/resources");
  assertTestSources("project","src/test/java","targetCustom/generated-test-sources/test");
  assertTestResources("project","src/test/resources");
}
