{
  final PsiElement superMethod=PySuperMethodsSearch.search(function).findFirst();
  if (superMethod != null) {
    PyClass superClass=null;
    if (superMethod instanceof PyFunction) {
      superClass=((PyFunction)superMethod).getContainingClass();
    }
    return new LineMarkerInfo<PsiElement>(element,element.getTextRange().getStartOffset(),OVERRIDING_METHOD_ICON,Pass.UPDATE_ALL,superClass == null ? null : new TooltipProvider("Overrides method in " + superClass.getName()),ourSuperMethodNavigator);
  }
  return null;
}
