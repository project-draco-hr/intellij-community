{
  int counter=0;
  int index=0;
  boolean contmode=false;
  loop:   while (index < value.length()) {
switch (value.charAt(index)) {
case '*':
      if (!contmode) {
        break loop;
      }
    break;
case 'L':
case 'T':
  if (!contmode) {
    contmode=true;
  }
case '[':
case '+':
case '-':
break;
default :
if (!contmode) {
break loop;
}
break;
case '<':
counter++;
break;
case '>':
counter--;
break;
case ';':
if (counter == 0) {
break loop;
}
}
index++;
}
return value.substring(0,index + 1);
}
