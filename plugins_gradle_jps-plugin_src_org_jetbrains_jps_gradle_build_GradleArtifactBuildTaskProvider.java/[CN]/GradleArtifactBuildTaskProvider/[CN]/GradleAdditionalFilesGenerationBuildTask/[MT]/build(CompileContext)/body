{
  if (myProperties.additionalFiles != null) {
    for (    Map.Entry<String,String> entry : myProperties.additionalFiles.entrySet()) {
      try {
        File output=new File(entry.getKey());
        FileUtil.writeToFile(output,Base64.decode(entry.getValue()));
      }
 catch (      Exception e) {
        LOG.debug(e);
      }
    }
  }
}
