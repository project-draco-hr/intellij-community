{
  super.advance();
  adjustBraceLevel();
  if (DUMP_TOKENS) {
    if (getTokenType() != null) {
      System.out.print(getTokenStart() + "-" + getTokenEnd()+ ":"+ getTokenType());
      if (getTokenType() == PyTokenTypes.LINE_BREAK) {
        System.out.println("{" + myBraceLevel + "}");
      }
 else {
        System.out.print(" ");
      }
    }
  }
}
