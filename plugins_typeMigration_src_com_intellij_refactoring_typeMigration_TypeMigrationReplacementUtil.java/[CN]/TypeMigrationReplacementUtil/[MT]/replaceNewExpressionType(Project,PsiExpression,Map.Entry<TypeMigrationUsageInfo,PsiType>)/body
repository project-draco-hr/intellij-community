{
  final PsiType changeType=info.getValue();
  if (changeType != null) {
    final String image=expression.getText();
    PsiType type=expression.getType().getDeepComponentType();
    try {
      final String with=type.getPresentableText().replace("[","\\[").replace("]","\\]");
      final String replacement=image.replaceFirst(with,changeType.getDeepComponentType().getPresentableText());
      expression.replace(JavaPsiFacade.getInstance(project).getElementFactory().createExpressionFromText(replacement,expression));
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
    }
  }
}
