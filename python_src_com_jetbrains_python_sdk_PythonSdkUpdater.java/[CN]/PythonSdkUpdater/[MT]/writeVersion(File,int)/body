{
  try {
    final FileOutputStream stream=new FileOutputStream(versionFile);
    try {
      DataOutputStream dataOutputStream=new DataOutputStream(stream);
      try {
        dataOutputStream.writeInt(version);
      }
  finally {
        dataOutputStream.close();
      }
    }
  finally {
      stream.close();
    }
  }
 catch (  IOException e) {
    LOG.info(e);
  }
}
