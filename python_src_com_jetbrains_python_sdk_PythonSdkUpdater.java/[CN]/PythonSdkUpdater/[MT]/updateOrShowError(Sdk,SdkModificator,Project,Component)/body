{
  final boolean success=update(sdk,sdkModificator,project,ownerComponent);
  if (!success) {
    final String homePath=sdk.getHomePath();
    final String sdkName=homePath != null ? homePath : sdk.getName();
    Messages.showErrorDialog(project,PyBundle.message("MSG.cant.setup.sdk.$0",FileUtil.toSystemDependentName(sdkName)),PyBundle.message("MSG.title.bad.sdk"));
  }
}
