{
  try {
    final FileInputStream stream=new FileInputStream(versionFile);
    try {
      DataInputStream dataInputStream=new DataInputStream(stream);
      try {
        return dataInputStream.readInt();
      }
  finally {
        dataInputStream.close();
      }
    }
  finally {
      stream.close();
    }
  }
 catch (  IOException e) {
    return -1;
  }
}
