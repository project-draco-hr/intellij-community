{
  final List<VirtualFile> oldRoots=Arrays.asList(sdk.getRootProvider().getFiles(OrderRootType.CLASSES));
  PythonSdkAdditionalData additionalData=sdk.getSdkAdditionalData() instanceof PythonSdkAdditionalData ? (PythonSdkAdditionalData)sdk.getSdkAdditionalData() : null;
  List<String> newRoots=new ArrayList<String>();
  for (  String root : sysPath) {
    if (new File(root).exists() && !FileUtilRt.extensionEquals(root,"egg-info") && (additionalData == null || !wasOldRoot(root,additionalData.getExcludedPathFiles()))&& !wasOldRoot(root,oldRoots)) {
      newRoots.add(root);
    }
  }
  if (!newRoots.isEmpty()) {
    for (    String root : newRoots) {
      PythonSdkType.addSdkRoot(modificator,root);
    }
    return true;
  }
  return false;
}
