{
  String value=literal.getText();
  if (value.length() <= 2) {
    return;
  }
  if ((value.charAt(0) == '"' && value.charAt(value.length() - 1) == '"') || (value.charAt(0) == '\'' && value.charAt(value.length() - 1) == '\'')) {
    if (mySubstitutionPatterns == null) {
      final String[] prefixes=myGlobalVisitor.getContext().getPattern().getTypedVarPrefixes();
      mySubstitutionPatterns=createPatterns(prefixes);
    }
    for (    Pattern substitutionPattern : mySubstitutionPatterns) {
      @Nullable MatchingHandler handler=myGlobalVisitor.processPatternStringWithFragments(value,GlobalCompilingVisitor.OccurenceKind.LITERAL,substitutionPattern);
      if (handler != null) {
        literal.putUserData(CompiledPattern.HANDLER_KEY,handler);
        break;
      }
    }
  }
}
