{
  ZipInputStream stream=null;
  try {
    stream=getStream();
    ZipEntry entry;
    while ((entry=stream.getNextEntry()) != null) {
      if (condition.value(entry)) {
        return StreamUtil.readText(stream,CharsetToolkit.UTF8_CHARSET);
      }
    }
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    StreamUtil.closeStream(stream);
  }
  return null;
}
