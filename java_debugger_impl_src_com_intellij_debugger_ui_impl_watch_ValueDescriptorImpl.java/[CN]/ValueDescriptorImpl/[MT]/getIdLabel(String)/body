{
  String customLabel=null;
  if (isShowIdLabel() && myValueReady) {
    final Value value=getValue();
    Renderer lastRenderer=getLastRenderer();
    final EvaluationContextImpl evalContext=myStoredEvaluationContext;
    final String idLabel=evalContext != null && lastRenderer != null && !evalContext.getSuspendContext().isResumed() ? ((NodeRendererImpl)lastRenderer).getIdLabel(value,evalContext.getDebugProcess()) : null;
    if (idLabel != null && !label.startsWith(idLabel)) {
      return idLabel;
    }
  }
  return "";
}
