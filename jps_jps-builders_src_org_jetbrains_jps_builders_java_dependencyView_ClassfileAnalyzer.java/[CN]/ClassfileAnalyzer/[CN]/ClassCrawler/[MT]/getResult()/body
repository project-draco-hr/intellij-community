{
  ClassRepr repr=myTakeIntoAccount ? new ClassRepr(myContext,myAccess,myFileName,myName,myContext.get(mySignature),myContext.get(mySuperClass),myInterfaces,myFields,myMethods,myTargets,myRetentionPolicy,myContext.get(myOuterClassName.get()),myLocalClassFlag.get(),myAnonymousClassFlag.get(),myUsages) : null;
  if (repr != null) {
    repr.updateClassUsages(myContext,myUsages);
  }
  return Pair.create(repr,myUsages);
}
