{
  for (  VirtualFile file : files) {
    if (!myFoundFiles.contains(file)) {
      if (!myExcluded.remove(file)) {
        myAdded.add(file);
        myUserAddedToRemove.remove(file);
      }
 else {
        myFoundFiles.add(file);
        return true;
      }
    }
 else {
      myExcluded.remove(file);
    }
  }
  return false;
}
