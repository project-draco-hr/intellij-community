{
  PsiElement method=resolveClassMember(cls,PyNames.GETATTR,context);
  if (method != null) {
    return true;
  }
  method=resolveClassMember(cls,PyNames.GETATTRIBUTE,context);
  if (method != null && !PyBuiltinCache.getInstance(cls).hasInBuiltins(method)) {
    return true;
  }
  return false;
}
