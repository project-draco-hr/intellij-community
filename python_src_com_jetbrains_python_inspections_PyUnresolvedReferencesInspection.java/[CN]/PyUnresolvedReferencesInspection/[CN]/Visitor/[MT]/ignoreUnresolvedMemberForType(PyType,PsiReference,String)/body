{
  if (qtype instanceof PyNoneType || qtype instanceof PyTypeReference || (qtype instanceof PyUnionType && ((PyUnionType)qtype).isWeak())) {
    return true;
  }
  if (qtype instanceof PyImportedModuleType) {
    PyImportedModule module=((PyImportedModuleType)qtype).getImportedModule();
    if (module.resolve() == null) {
      return true;
    }
  }
  if (qtype instanceof PyClassType) {
    PyClass cls=((PyClassType)qtype).getPyClass();
    if (cls != null) {
      if (overridesGetAttr(cls)) {
        return true;
      }
      if (cls.findProperty(refText) != null) {
        return true;
      }
      if (hasUnresolvedAncestors(cls)) {
        return true;
      }
    }
  }
  if (qtype instanceof CythonBuiltinType || (qtype instanceof CythonType && reference instanceof PyOperatorReference)) {
    return true;
  }
  return false;
}
