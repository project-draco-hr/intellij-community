{
  if (!super.canMove(elements,targetContainer)) {
    return false;
  }
  if (findMovableLocalFunctionOrMethod(elements[0]) != null) {
    return true;
  }
  for (  PsiElement element : elements) {
    if (!PyMoveModuleMembersHelper.isMovableModuleMember(element)) {
      return false;
    }
  }
  return true;
}
