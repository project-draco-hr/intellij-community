{
  if (myBuilder.eof()) {
    return;
  }
  final IElementType type=myBuilder.getTokenType();
  eolSeen=eolSeen || type == EOL;
  if (type == EOL) {
    myAfterLastEolMarker=mark();
    myIndent=0;
  }
 else   if (type == INDENT) {
    myIndent=getCurrentTokenLength();
  }
 else {
    dropEolMarker();
  }
  myBuilder.advanceLexer();
}
