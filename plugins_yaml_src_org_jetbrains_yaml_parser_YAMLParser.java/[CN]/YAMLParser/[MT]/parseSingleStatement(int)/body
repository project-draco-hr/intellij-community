{
  if (eof()) {
    return;
  }
  final IElementType tokenType=getTokenType();
  if (tokenType == LBRACE) {
    parseHash();
  }
 else   if (tokenType == LBRACKET) {
    parseArray();
  }
 else   if (tokenType == SEQUENCE_MARKER) {
    parseSequence();
  }
 else   if (tokenType == SCALAR_KEY) {
    parseScalarKeyValue(indent);
  }
 else   if (SCALAR_VALUES.contains(getTokenType())) {
    parseScalarValue();
  }
 else {
    advanceLexer();
  }
}
