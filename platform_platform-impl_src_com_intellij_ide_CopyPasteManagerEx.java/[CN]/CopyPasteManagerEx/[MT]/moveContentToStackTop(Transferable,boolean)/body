{
  Transferable current=myData.isEmpty() ? null : myData.get(0);
  if (!Comparing.equal(t,current)) {
    myData.remove(t);
    myData.add(0,t);
    if (notifyOthers) {
      setSystemClipboardContent(t);
      fireContentChanged(current,t);
    }
  }
 else {
    if (notifyOthers) {
      setSystemClipboardContent(t);
    }
  }
}
