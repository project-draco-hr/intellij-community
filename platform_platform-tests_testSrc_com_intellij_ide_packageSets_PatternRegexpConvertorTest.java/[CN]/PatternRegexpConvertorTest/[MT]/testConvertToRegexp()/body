{
  assertEquals("a\\.[^\\.]*",FilePatternPackageSet.convertToRegexp("a.*",'.'));
  assertEquals("a\\.(.*\\.)?[^\\.]*",FilePatternPackageSet.convertToRegexp("a..*",'.'));
  assertEquals("a\\/[^\\/]*",FilePatternPackageSet.convertToRegexp("a/*",'/'));
  assertEquals("a\\/.*\\.css",FilePatternPackageSet.convertToRegexp("a/*.css",'/'));
  assertEquals("a\\/(.*\\/)?[^\\/]*",FilePatternPackageSet.convertToRegexp("a//*",'/'));
  assertEquals("[^\\.]*",FilePatternPackageSet.convertToRegexp("*",'.'));
}
