{
  if (myCurrentUser == null) {
    throw new IllegalStateException("User not set");
  }
  final URIBuilder url=new URIBuilder(getRestApiUrl("members","me","boards")).addParameter("filter","open").addParameter("fields",TrelloBoard.REQUIRED_FIELDS);
  return makeRequestAndDeserializeJsonResponse(url.build(),TrelloUtil.LIST_OF_BOARDS_TYPE);
}
