{
  try {
    String url=TrelloUtil.TRELLO_API_BASE_URL + "/members/me";
    return makeRequestAndDeserializeJsonResponse(url,TrelloUser.class);
  }
 catch (  Exception e) {
    LOG.warn("Error while fetching initial user info",e);
    myCurrentBoard=null;
    myCurrentList=null;
    throw e;
  }
}
