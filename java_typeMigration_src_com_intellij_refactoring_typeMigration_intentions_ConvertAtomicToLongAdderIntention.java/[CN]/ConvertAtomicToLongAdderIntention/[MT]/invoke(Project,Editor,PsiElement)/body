{
  final PsiVariable variable=getVariable(element);
  if (variable != null) {
    final PsiType longAdder=JavaPsiFacade.getElementFactory(project).createTypeFromText(LongAdderConversionRule.JAVA_UTIL_CONCURRENT_ATOMIC_LONG_ADDER,element);
    TypeMigrationVariableTypeFixProvider.runTypeMigrationOnVariable(variable,longAdder,null,false);
  }
}
