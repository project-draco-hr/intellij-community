{
  super.visitMethodCallExpression(expression);
  if (MethodCallUtils.callWithNonConstantString(expression,considerStaticFinalConstant,"java.lang.System","load","loadLibrary")) {
    registerMethodCallError(expression,"System");
  }
 else   if (MethodCallUtils.callWithNonConstantString(expression,considerStaticFinalConstant,"java.lang.Runtime","load","loadLibrary")) {
    registerMethodCallError(expression,"Runtime");
  }
}
