{
  if (EmmetOptions.getInstance().getEmmetExpandShortcut() != myTemplateExpandShortcutPanel.getSelectedChar() || super.isModified()) {
    return true;
  }
  for (  Configurable configurable : myInnerConfigurables) {
    if (configurable.isModified()) {
      return true;
    }
  }
  return false;
}
