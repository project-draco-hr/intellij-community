{
  if (name == null) {
    return false;
  }
  final Collection<String> components=name.getComponents();
  if (components.isEmpty()) {
    return false;
  }
  for (  String s : components) {
    if (!PyNames.isIdentifier(s) || PyNames.isReserved(s)) {
      return false;
    }
  }
  return true;
}
