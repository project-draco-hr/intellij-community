{
synchronized (mySocketObject) {
    try {
      if (myDebuggerReader != null) {
        myDebuggerReader.stop();
      }
    }
  finally {
      if (!myServerSocket.isClosed()) {
        try {
          myServerSocket.close();
        }
 catch (        IOException e) {
          LOG.warn("Error closing socket",e);
        }
      }
    }
  }
}
