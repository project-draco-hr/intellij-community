{
  while (!stack.isEmpty()) {
    Pair<T,Iterator<T>> top=stack.getLast();
    if (top.second.hasNext()) {
      T child=top.second.next();
      stack.addLast(Pair.create(child,children(child).iterator()));
    }
 else {
      stack.removeLast();
      return top.first;
    }
  }
  return stop();
}
