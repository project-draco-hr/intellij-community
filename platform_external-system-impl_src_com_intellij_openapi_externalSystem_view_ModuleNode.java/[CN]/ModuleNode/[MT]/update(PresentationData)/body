{
  super.update(presentation);
  presentation.setIcon(getUiAware().getProjectIcon());
  String hint=null;
  if (myIsRoot) {
    hint="root";
  }
  final String tooltip=myData.toString() + (myData.getDescription() != null ? "<br>" + myData.getDescription() : "");
  setNameAndTooltip(getName(),tooltip,hint);
}
