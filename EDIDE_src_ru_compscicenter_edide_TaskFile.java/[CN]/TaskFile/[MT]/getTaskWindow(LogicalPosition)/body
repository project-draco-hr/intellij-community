{
  int line=pos.line + 1;
  int offset=pos.column;
  int i=0;
  while (i < myTaskWindows.size() && (myTaskWindows.get(i).getLine() < line || (myTaskWindows.get(i).getLine() == line && myTaskWindows.get(i).getStartOffset() < offset))) {
    i++;
  }
  if (i == 0) {
    return null;
  }
  return myTaskWindows.get(i - 1);
}
