{
  int line=pos.line + 1;
  int offset=pos.column;
  int i=0;
  while (i < taskWindows.size() && (taskWindows.get(i).getLine() < line || (taskWindows.get(i).getLine() == line && taskWindows.get(i).getStartOffset() < offset))) {
    i++;
  }
  if (i == 0) {
    return null;
  }
  return taskWindows.get(i - 1);
}
