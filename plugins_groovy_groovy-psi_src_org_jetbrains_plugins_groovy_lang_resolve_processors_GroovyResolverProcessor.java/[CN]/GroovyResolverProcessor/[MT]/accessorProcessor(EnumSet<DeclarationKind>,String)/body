{
  return new GrScopeProcessorWithHints(name,allKinds){
    @Override public boolean execute(    @NotNull PsiElement element,    @NotNull ResolveState state){
      return !AccessorResolverProcessor.checkAccessor(element,state,GroovyResolverProcessor.this.myName,!myIsLValue) || GroovyResolverProcessor.this.execute(element,state);
    }
  }
;
}
