{
  final VirtualFile selectedFile=SchemeImportUtil.selectImportSource(importer.getSourceExtensions(),myContentPane,CodeStyleSchemesUIConfiguration.Util.getRecentImportFile());
  if (selectedFile != null) {
    CodeStyleSchemesUIConfiguration.Util.setRecentImportFile(selectedFile);
    final SchemeCreator schemeCreator=new SchemeCreator();
    final CodeStyleScheme schemeImported=importer.importScheme(myModel.getProject(),selectedFile,getSelectedScheme(),schemeCreator);
    if (schemeImported != null) {
      if (schemeCreator.isSchemeWasCreated())       myModel.fireSchemeListChanged();
 else       myModel.fireSchemeChanged(schemeImported);
      return schemeImported;
    }
  }
  return null;
}
