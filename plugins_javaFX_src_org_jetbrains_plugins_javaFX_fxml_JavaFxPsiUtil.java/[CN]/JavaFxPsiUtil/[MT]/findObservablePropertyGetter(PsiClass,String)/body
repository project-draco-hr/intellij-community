{
  if (StringUtil.isEmpty(propertyName))   return null;
  final PsiMethod[] getters=psiClass.findMethodsByName(propertyName + JavaFxCommonNames.PROPERTY_METHOD_SUFFIX,true);
  for (  PsiMethod getter : getters) {
    if (getter.hasModifierProperty(PsiModifier.PUBLIC) && !getter.hasModifierProperty(PsiModifier.STATIC) && getter.getParameterList().getParametersCount() == 0 && InheritanceUtil.isInheritor(getter.getReturnType(),JavaFxCommonNames.JAVAFX_BEANS_VALUE_OBSERVABLE_VALUE)) {
      return getter;
    }
  }
  return null;
}
