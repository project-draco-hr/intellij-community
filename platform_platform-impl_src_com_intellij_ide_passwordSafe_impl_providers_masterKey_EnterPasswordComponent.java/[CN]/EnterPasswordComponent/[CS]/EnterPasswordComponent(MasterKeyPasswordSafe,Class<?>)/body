{
  super(safe,"Enter");
  String requestorName=getRequestorTitle(requestor);
  myPromptLabel.setText("<html><br>Master password is required to unlock the password database.<br>" + "The password database will be unlocked during this session<br>" + "for all subsystems.<br>"+ "<br><b>Requested by</b>: " + requestorName + "</html>");
  UIUtil.setEnabled(myNewPasswordPanel,false,true);
  myNewPasswordPanel.setVisible(false);
}
