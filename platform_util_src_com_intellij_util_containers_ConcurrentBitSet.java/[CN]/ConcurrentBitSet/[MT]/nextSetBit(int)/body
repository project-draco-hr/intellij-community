{
  if (fromIndex < 0) {
    throw new IndexOutOfBoundsException("fromIndex < 0: " + fromIndex);
  }
  int u=wordIndex(fromIndex);
  long[] words=this.words;
  if (u >= words.length) {
    return -1;
  }
  long word=words[u] & WORD_MASK << fromIndex;
  while (true) {
    if (word != 0) {
      return u * BITS_PER_WORD + Long.numberOfTrailingZeros(word);
    }
    if (++u == words.length) {
      return -1;
    }
    word=words[u];
  }
}
