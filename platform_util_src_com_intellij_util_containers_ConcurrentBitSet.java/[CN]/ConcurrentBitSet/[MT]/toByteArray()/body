{
  long[] words=this.words;
  int n=words.length;
  if (n == 0) {
    return new byte[0];
  }
  int len=8 * (n - 1);
  for (long x=words[n - 1]; x != 0; x>>>=8) {
    len++;
  }
  byte[] bytes=new byte[len];
  ByteBuffer bb=ByteBuffer.wrap(bytes).order(ByteOrder.LITTLE_ENDIAN);
  for (int i=0; i < n - 1; i++) {
    bb.putLong(words[i]);
  }
  for (long x=words[n - 1]; x != 0; x>>>=8) {
    bb.put((byte)(x & 0xff));
  }
  return bytes;
}
