{
  if (!FileModificationService.getInstance().prepareFileForWrite(file))   return;
  final PsiExpression appendExpression=ChangeToAppendUtil.buildAppendExpression(myAssignmentExpression.getLExpression(),myAssignmentExpression.getRExpression());
  if (appendExpression == null)   return;
  myAssignmentExpression.replace(appendExpression);
}
