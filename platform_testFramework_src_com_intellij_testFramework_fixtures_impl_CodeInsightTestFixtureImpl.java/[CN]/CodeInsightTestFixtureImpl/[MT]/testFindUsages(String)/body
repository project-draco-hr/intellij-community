{
  assertInitialized();
  configureByFiles(fileNames);
  final PsiElement targetElement=TargetElementUtil.findTargetElement(getEditor(),TargetElementUtil.ELEMENT_NAME_ACCEPTED | TargetElementUtil.REFERENCED_ELEMENT_ACCEPTED);
  Assert.assertNotNull("Cannot find referenced element",targetElement);
  return findUsages(targetElement);
}
