{
  assertInitialized();
  configureByFiles(fileNames);
  final PsiElement targetElement=TargetElementUtilBase.findTargetElement(getEditor(),TargetElementUtilBase.ELEMENT_NAME_ACCEPTED | TargetElementUtilBase.REFERENCED_ELEMENT_ACCEPTED);
  Assert.assertNotNull("Cannot find referenced element",targetElement);
  return findUsages(targetElement);
}
