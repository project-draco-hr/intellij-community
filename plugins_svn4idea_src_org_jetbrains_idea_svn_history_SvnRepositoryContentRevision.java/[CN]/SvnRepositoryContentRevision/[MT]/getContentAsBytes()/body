{
  try {
    if (myFilePath.getVirtualFile() == null) {
      LocalFileSystem.getInstance().refreshAndFindFileByPath(myFilePath.getPath());
    }
    return ContentRevisionCache.getOrLoadAsBytes(myVcs.getProject(),myFilePath,getRevisionNumber(),myVcs.getKeyInstanceMethod(),ContentRevisionCache.UniqueType.REPOSITORY_CONTENT,new Throwable2Computable<byte[],VcsException,IOException>(){
      @Override public byte[] compute() throws VcsException, IOException {
        return loadContent().toByteArray();
      }
    }
);
  }
 catch (  IOException e) {
    throw new VcsException(e);
  }
}
