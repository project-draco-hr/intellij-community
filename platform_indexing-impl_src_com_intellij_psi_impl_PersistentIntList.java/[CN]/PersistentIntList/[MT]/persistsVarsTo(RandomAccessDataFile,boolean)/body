{
  return index.persistVars(new IntToIntBtree.BtreeDataStorage(){
    @Override public int persistInt(    int offset,    int value,    boolean toDisk){
      if (toDisk) {
        data.putInt(offset,value);
        return value;
      }
 else {
        return data.getInt(offset);
      }
    }
  }
,toDisk);
}
