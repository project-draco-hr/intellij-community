{
  if (modCount != startModCount)   throw new ConcurrentModificationException();
  curIndex=nextIndex;
  curElement=nextElement;
  nextElement=null;
  nextIndex=-1;
  for (int i=curIndex + 1; i < myList.size(); i++) {
    MyReference<T> reference=myList.get(i);
    T t=reference == null ? null : reference.get();
    if (t != null) {
      nextElement=t;
      nextIndex=i;
      return true;
    }
  }
  return false;
}
