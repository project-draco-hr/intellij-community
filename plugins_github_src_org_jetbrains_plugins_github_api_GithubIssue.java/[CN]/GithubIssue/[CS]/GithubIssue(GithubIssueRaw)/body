{
  myHtmlUrl=raw.htmlUrl;
  myNumber=raw.number;
  myState=raw.state;
  myTitle=raw.title;
  myBody=raw.body;
  myClosedAt=raw.closedAt;
  myCreatedAt=raw.createdAt;
  myUpdatedAt=raw.updatedAt;
  myUser=GithubUser.create(raw.user);
  myAssignee=raw.assignee == null ? null : GithubUser.create(raw.assignee);
}
