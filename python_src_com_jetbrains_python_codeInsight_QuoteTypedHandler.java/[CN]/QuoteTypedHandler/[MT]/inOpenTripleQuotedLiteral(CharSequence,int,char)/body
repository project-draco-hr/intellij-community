{
  int quote_count=0;
  int pos=offset;
  while (pos > 0) {
    char c=text.charAt(pos);
    if (c != quote)     quote_count=0;
 else {
      quote_count+=1;
      if (quote_count == 3) {
        if (isNotEscaped(text,pos - 1))         return true;
 else         quote_count=0;
      }
    }
    pos-=1;
  }
  return false;
}
