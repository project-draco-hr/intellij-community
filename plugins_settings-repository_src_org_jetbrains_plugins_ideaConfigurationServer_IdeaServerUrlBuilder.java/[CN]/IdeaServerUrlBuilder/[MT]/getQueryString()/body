{
  if (roamingType != RoamingType.GLOBAL) {
    return new NameValuePair[]{new NameValuePair("session",mySessionId),new NameValuePair("path",buildPath())};
  }
 else {
    return new NameValuePair[]{new NameValuePair("session",mySessionId),new NameValuePair("path",buildPath()),new NameValuePair("global","true")};
  }
}
