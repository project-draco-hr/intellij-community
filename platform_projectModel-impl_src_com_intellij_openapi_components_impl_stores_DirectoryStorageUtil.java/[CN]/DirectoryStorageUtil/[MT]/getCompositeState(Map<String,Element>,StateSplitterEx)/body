{
  Element state=new Element(FileStorageCoreUtil.COMPONENT);
  if (fileToState.isEmpty()) {
    return state;
  }
  for (  String fileName : fileToState.keySet()) {
    Element subState=fileToState.get(fileName);
    if (subState == null) {
      return null;
    }
    splitter.mergeStateInto(state,subState);
  }
  return state;
}
