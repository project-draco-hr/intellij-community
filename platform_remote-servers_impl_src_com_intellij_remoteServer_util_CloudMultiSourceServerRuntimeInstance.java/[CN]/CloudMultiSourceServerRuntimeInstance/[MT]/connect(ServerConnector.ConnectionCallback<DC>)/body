{
  getAgentTaskExecutor().execute(new Computable(){
    @Override public Object compute(){
      doConnect(getConfiguration(),new CloudAgentLogger(){
        @Override public void debugEx(        Exception e){
          LOG.debug(e);
        }
        @Override public void debug(        String message){
          LOG.debug(message);
        }
      }
);
      return null;
    }
  }
,new CallbackWrapper(){
    @Override public void onSuccess(    Object result){
      callback.connected(CloudMultiSourceServerRuntimeInstance.this);
    }
    @Override public void onError(    String message){
      callback.errorOccurred(message);
    }
  }
);
}
