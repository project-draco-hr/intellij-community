{
  PsiElement e=element.getNextSibling();
  PsiElement result=element;
  while (e instanceof PsiComment || (e instanceof PsiWhiteSpace && StringUtil.countChars(e.getText(),'\n') <= 1)) {
    final String text=e.getText();
    if (e instanceof PsiComment && StringUtil.isEmptyOrSpaces(StringUtil.trimStart(text.trim(),getPrefix(element))))     break;
    if (e instanceof PsiComment)     result=e;
    e=e.getNextSibling();
  }
  return result;
}
