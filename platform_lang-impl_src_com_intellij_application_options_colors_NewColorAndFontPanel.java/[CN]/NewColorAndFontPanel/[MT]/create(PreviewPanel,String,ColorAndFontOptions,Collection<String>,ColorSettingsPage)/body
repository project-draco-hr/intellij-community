{
  final SchemesPanel schemesPanel=new SchemesPanel(options);
  final OptionsPanel optionsPanel=new OptionsPanelImpl(options,schemesPanel,category,new CustomizedSwitcherPanel(options.getColorAndFontGlobalState(),previewPanel,page));
  return new NewColorAndFontPanel(schemesPanel,optionsPanel,previewPanel,category,optionList,page);
}
