{
  if (myBuilder.getTokenType() == PyTokenTypes.ASYNC_KEYWORD) {
    myBuilder.advanceLexer();
    myContext.pushScope(myContext.getScope().withAsync());
    parseSyncDeclarationAfterDecorator(endMarker);
    myContext.popScope();
  }
 else {
    parseSyncDeclarationAfterDecorator(endMarker);
  }
}
