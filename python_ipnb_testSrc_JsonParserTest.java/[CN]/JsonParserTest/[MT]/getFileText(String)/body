{
  String fullPath=PathManager.getHomePath() + "/community/python/ipnb/" + fileName;
  final BufferedReader br=new BufferedReader(new FileReader(fullPath));
  try {
    final StringBuilder sb=new StringBuilder();
    String line=br.readLine();
    while (line != null) {
      sb.append(line);
      sb.append("\n");
      line=br.readLine();
    }
    return sb.toString();
  }
  finally {
    br.close();
  }
}
