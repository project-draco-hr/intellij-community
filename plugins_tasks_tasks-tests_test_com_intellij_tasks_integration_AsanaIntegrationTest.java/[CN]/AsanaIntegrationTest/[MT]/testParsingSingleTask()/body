{
  Task task=myRepository.getActiveResponseHandler().parseIssue(SINGLE_TASK_RESPONSE);
  assertNotNull(task);
  assertTasksEqual(new TaskBuilder("7119324862208","Task #2").withRepository(myRepository).withDescription("This is task #2 description").withClosed(true).withCreated("2013-08-02T12:13:20.372Z").withUpdated("2013-08-21T16:36:36.290Z"),task);
}
