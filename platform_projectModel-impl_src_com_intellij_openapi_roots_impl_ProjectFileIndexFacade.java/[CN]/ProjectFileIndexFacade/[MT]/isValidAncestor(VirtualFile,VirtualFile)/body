{
  if (!childDir.isDirectory()) {
    childDir=childDir.getParent();
  }
  while (true) {
    if (childDir == null)     return false;
    if (childDir.equals(baseDir))     return true;
    if (!myDirectoryIndex.getInfoForFile(childDir).isInProject())     return false;
    childDir=childDir.getParent();
  }
}
