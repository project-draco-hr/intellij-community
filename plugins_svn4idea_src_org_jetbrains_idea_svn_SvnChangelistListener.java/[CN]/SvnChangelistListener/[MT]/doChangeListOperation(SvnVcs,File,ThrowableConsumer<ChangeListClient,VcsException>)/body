{
  try {
    operation.consume(vcs.getFactory(file).createChangeListClient());
  }
 catch (  SvnBindException e) {
    LOG.info(e);
    if (!e.contains(SVNErrorCode.WC_NOT_DIRECTORY) && !e.contains(SVNErrorCode.WC_NOT_FILE)) {
      throw e;
    }
  }
catch (  VcsException e) {
    LOG.info(e);
    throw e;
  }
}
