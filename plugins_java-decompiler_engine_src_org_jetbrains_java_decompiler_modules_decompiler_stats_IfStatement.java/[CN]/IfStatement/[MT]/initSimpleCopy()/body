{
  first=stats.get(0);
  List<StatEdge> lstSuccs=first.getSuccessorEdges(STATEDGE_DIRECT_ALL);
  ifedge=lstSuccs.get((iftype == IFTYPE_IF || negated) ? 0 : 1);
  if (stats.size() > 1) {
    ifstat=stats.get(1);
  }
  if (iftype == IFTYPE_IFELSE) {
    elseedge=lstSuccs.get(negated ? 1 : 0);
    elsestat=stats.get(2);
  }
}
