{
  graph.getEdgeController().createEdge(underdoneNode.getFirstUpNode(),node,underdoneNode.getBranch(),Edge.Type.USUAL);
  if (underdoneNode.getSecondUpNode() != null) {
    graph.getEdgeController().createEdge(underdoneNode.getSecondUpNode(),node,underdoneNode.getSecondEdgeBranch(),Edge.Type.USUAL);
  }
}
