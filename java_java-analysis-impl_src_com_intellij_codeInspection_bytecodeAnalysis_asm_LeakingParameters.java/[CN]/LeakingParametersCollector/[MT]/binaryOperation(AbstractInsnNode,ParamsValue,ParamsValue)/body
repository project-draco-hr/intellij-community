{
switch (insn.getOpcode()) {
case IALOAD:
case LALOAD:
case FALOAD:
case DALOAD:
case AALOAD:
case BALOAD:
case CALOAD:
case SALOAD:
case PUTFIELD:
    boolean[] params=value1.params;
  for (int i=0; i < arity; i++) {
    leaking[i]|=params[i];
  }
break;
default :
}
return super.binaryOperation(insn,value1,value2);
}
