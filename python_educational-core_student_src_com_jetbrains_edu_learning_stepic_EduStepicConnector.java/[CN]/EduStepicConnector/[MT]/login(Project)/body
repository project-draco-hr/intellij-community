{
  final StepicUser user=StudyTaskManager.getInstance(project).getUser();
  final String login=user.getEmail();
  if (StringUtil.isEmptyOrSpaces(login)) {
    return showLoginDialog();
  }
 else {
    if (login(login,user.getPassword()) == null) {
      return showLoginDialog();
    }
  }
  return true;
}
