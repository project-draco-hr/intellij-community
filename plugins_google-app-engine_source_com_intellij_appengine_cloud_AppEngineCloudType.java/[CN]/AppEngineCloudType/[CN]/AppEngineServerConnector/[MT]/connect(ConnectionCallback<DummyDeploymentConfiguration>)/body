{
  callback.connected(new ServerRuntimeInstance<DummyDeploymentConfiguration>(){
    @Override public void deploy(    @NotNull DeploymentSource source,    @NotNull DummyDeploymentConfiguration configuration,    @NotNull DeploymentOperationCallback callback){
      Artifact artifact=((ArtifactDeploymentSource)source).getArtifact();
      if (artifact == null)       return;
      AppEngineUploader uploader=AppEngineUploader.createUploader(myProject,artifact,myConfiguration);
      if (uploader != null) {
        uploader.startUploading();
      }
    }
    @Override public void undeploy(    @NotNull DeploymentSource source,    @NotNull DummyDeploymentConfiguration configuration,    @NotNull DeploymentOperationCallback callback){
    }
  }
);
}
