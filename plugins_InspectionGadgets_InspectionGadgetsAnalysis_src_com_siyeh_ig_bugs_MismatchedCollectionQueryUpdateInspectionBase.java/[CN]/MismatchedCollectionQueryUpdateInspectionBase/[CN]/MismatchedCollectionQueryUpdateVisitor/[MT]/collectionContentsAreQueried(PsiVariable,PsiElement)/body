{
  if (VariableAccessUtils.variableIsPassedAsMethodArgument(variable,context) || collectionQueryCalled(variable,context)) {
    return true;
  }
  final PsiExpression initializer=variable.getInitializer();
  if (initializer != null && !isEmptyCollectionInitializer(initializer)) {
    return true;
  }
  return collectionQueriedByAssignment(variable,context);
}
