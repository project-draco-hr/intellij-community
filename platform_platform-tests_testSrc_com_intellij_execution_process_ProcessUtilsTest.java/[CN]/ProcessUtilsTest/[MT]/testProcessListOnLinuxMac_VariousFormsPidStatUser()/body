{
  ProcessInfo[] infos=ProcessListLinux.parseOutput(true,"   PID STAT USER      COMMAND\n\n" + "     1 S    user      " + myDir + "/dir/file\n"+ "   101 Ss   user_name "+ myDir+ "/dir/file\n");
  assertOrderedEquals(infos,new ProcessInfo(1,myDir + "/dir/file","","user","S"),new ProcessInfo(101,myDir + "/dir/file","","user_name","Ss"));
}
