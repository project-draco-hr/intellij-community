{
  if (config instanceof JavaCoverageEnabledConfiguration) {
    final JavaCoverageEnabledConfiguration javaConfig=(JavaCoverageEnabledConfiguration)config;
    return createSuite(covRunner,name,coverageDataFileProvider,javaConfig.getPatterns(),new Date().getTime(),javaConfig.getSuiteToMergeWith(),javaConfig.isTrackPerTestCoverage() && !javaConfig.isSampling(),!javaConfig.isSampling(),javaConfig.isTrackTestFolders());
  }
  return null;
}
