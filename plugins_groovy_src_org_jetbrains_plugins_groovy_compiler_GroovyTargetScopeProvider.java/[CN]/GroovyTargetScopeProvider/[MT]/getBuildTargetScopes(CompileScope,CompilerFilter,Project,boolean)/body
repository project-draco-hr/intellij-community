{
  Boolean checkResourcesRebuild=baseScope.getUserData(GroovyResourceChecker.CHECKING_RESOURCES_REBUILD);
  if (checkResourcesRebuild == null)   return Collections.emptyList();
  forceBuild|=checkResourcesRebuild;
  return JBIterable.of(createTargets(project,forceBuild,JavaResourceRootType.RESOURCE,CheckResourcesTarget.PRODUCTION),createTargets(project,forceBuild,JavaResourceRootType.TEST_RESOURCE,CheckResourcesTarget.TESTS)).filter(Condition.NOT_NULL).toList();
}
