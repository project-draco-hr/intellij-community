{
  g.setColor(myColor);
  int width=getWidth();
  if (myHasBorder) {
    g.fillRect(0,0,width - 1,HEIGHT_CELL);
    UIUtil.drawLine((Graphics2D)g,width - 1,0,width - 1,HEIGHT_CELL,null,myUi.getColorManager().getRootIndicatorBorder());
  }
 else {
    g.fillRect(0,0,width,HEIGHT_CELL);
  }
  super.paintComponent(g);
}
