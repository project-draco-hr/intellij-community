{
  myPathEditor=new ComboboxWithBrowseButton(new ComboBox(100));
  myPathEditor.addActionListener(new ComponentWithBrowseButton.BrowseFolderActionListener<>("Google App Engine SDK","Specify Google App Engine Java SDK home",myPathEditor,project,FileChooserDescriptorFactory.createSingleFolderDescriptor(),TextComponentAccessor.STRING_COMBOBOX_WHOLE_TEXT));
  final JComboBox comboBox=myPathEditor.getComboBox();
  comboBox.setEditable(true);
  comboBox.removeAllItems();
  for (  AppEngineSdk sdk : AppEngineSdkManager.getInstance().getValidSdks()) {
    comboBox.addItem(FileUtil.toSystemDependentName(sdk.getSdkHomePath()));
  }
}
