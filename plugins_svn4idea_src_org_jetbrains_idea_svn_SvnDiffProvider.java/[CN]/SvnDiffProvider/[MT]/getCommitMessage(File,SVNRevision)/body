{
  String result;
  try {
    PropertyValue property=myVcs.getFactory(path).createPropertyClient().getProperty(SvnTarget.fromFile(path),COMMIT_MESSAGE,true,revision);
    result=PropertyValue.toString(property);
  }
 catch (  VcsException e) {
    LOG.info(e);
    result="";
  }
  return result;
}
