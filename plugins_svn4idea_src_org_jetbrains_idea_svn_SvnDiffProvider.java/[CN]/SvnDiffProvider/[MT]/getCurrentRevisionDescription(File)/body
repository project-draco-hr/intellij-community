{
  final Info svnInfo=myVcs.getInfo(path);
  if (svnInfo == null) {
    return null;
  }
  if (svnInfo.getCommittedRevision().equals(SVNRevision.UNDEFINED) && !svnInfo.getCopyFromRevision().equals(SVNRevision.UNDEFINED) && svnInfo.getCopyFromURL() != null) {
    SVNURL copyUrl=svnInfo.getCopyFromURL();
    String localPath=myVcs.getSvnFileUrlMapping().getLocalPath(copyUrl.toString());
    if (localPath != null) {
      return getCurrentRevisionDescription(new File(localPath));
    }
  }
  return new VcsRevisionDescriptionImpl(new SvnRevisionNumber(svnInfo.getCommittedRevision()),svnInfo.getCommittedDate(),svnInfo.getAuthor(),getCommitMessage(path,svnInfo));
}
