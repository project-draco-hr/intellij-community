{
  Map<Integer,Character> map=new HashMap<Integer,Character>();
  List<Character> characters=new ArrayList<Character>();
  characters.add('\n');
  characters.add('\n');
  characters.add('\t');
  characters.add(' ');
  characters.add(' ');
  characters.add('.');
  characters.add('<');
  characters.add('!');
  for (int i=0; i < characters.size(); i++) {
    map.put(i,characters.get(i));
  }
  return map;
}
