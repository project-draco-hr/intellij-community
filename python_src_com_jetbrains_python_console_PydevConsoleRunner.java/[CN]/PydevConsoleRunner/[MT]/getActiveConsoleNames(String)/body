{
  return FluentIterable.from(Lists.newArrayList(PythonConsoleToolWindow.getInstance(getProject()).getToolWindow().getContentManager().getContents())).transform(new Function<Content,String>(){
    @Override public String apply(    Content input){
      return input.getDisplayName();
    }
  }
).filter(new Predicate<String>(){
    @Override public boolean apply(    String input){
      return input.contains(consoleTitle);
    }
  }
).toList();
}
