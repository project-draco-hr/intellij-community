{
  ProjectFileIndex projectFileIndex=ProjectFileIndex.SERVICE.getInstance(project);
  if (isLibraryElement) {
    List<OrderEntry> orders=projectFileIndex.getOrderEntriesForFile(file);
    for (    OrderEntry orderEntry : orders) {
      if (orderEntry instanceof ModuleJdkOrderEntry || orderEntry instanceof JdkOrderEntry || orderEntry instanceof LibraryOrderEntry) {
        return true;
      }
    }
    return false;
  }
 else {
    return projectFileIndex.isInContent(file);
  }
}
