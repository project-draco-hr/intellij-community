{
  if (myIsReturnedVal) {
    return AllIcons.Debugger.WatchLastReturnValue;
  }
 else   if (!myContainer) {
    return AllIcons.Debugger.Db_primitive;
  }
 else   if ("list".equals(myType) || "tuple".equals(myType)) {
    return AllIcons.Debugger.Db_array;
  }
 else {
    return AllIcons.Debugger.Value;
  }
}
