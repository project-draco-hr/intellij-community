{
  Element parentNode=new Element("TemplateSettings");
  Element element=new Element(DEFAULT_SHORTCUT);
  element.setAttribute(SHORTCUT,myDefaultShortcutChar == TAB_CHAR ? TAB : myDefaultShortcutChar == ENTER_CHAR ? ENTER : myDefaultShortcutChar == CUSTOM_CHAR ? CUSTOM : SPACE);
  parentNode.addContent(element);
  return parentNode;
}
