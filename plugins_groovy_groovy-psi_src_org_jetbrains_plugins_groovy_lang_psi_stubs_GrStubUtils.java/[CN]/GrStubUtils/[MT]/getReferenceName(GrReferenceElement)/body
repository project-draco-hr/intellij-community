{
  final String referenceName=element.getReferenceName();
  if (referenceName == null)   return null;
  final String mappedFqn=getAliasMapping(element.getContainingFile()).get(referenceName);
  final String fullText=element instanceof GrReferenceElementImpl ? ((GrReferenceElementImpl)element).getTextSkipWhiteSpaceAndComments() : element.getText();
  return mappedFqn == null ? fullText : fullText.replace(referenceName,mappedFqn);
}
