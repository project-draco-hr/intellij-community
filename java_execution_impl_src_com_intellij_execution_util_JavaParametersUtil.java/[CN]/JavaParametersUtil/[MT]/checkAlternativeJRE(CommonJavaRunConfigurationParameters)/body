{
  if (configuration.isAlternativeJrePathEnabled()) {
    String alternativeJrePath=configuration.getAlternativeJrePath();
    if (StringUtil.isEmpty(alternativeJrePath) || ProjectJdkTable.getInstance().findJdk(alternativeJrePath) == null && !JavaSdk.checkForJre(alternativeJrePath)) {
      throw new RuntimeConfigurationWarning(ExecutionBundle.message("jre.path.is.not.valid.jre.home.error.mesage",alternativeJrePath));
    }
  }
}
