{
  if (configuration.isAlternativeJrePathEnabled()) {
    final String alternativeJrePath=configuration.getAlternativeJrePath();
    if (alternativeJrePath == null || alternativeJrePath.length() == 0 || ProjectJdkTable.getInstance().findJdk(alternativeJrePath) == null && !JavaSdk.checkForJre(alternativeJrePath)) {
      throw new RuntimeConfigurationWarning(ExecutionBundle.message("jre.path.is.not.valid.jre.home.error.mesage",alternativeJrePath));
    }
  }
}
