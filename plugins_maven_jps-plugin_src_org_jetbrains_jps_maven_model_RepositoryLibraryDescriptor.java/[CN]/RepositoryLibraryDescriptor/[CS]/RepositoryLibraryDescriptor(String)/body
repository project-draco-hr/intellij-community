{
  myMavenId=mavenId;
  if (mavenId == null) {
    myGroupId=myArtifactId=myVersion=null;
  }
 else {
    String[] parts=mavenId.split(":");
    myGroupId=parts.length > 0 ? parts[0] : null;
    myArtifactId=parts.length > 1 ? parts[1] : null;
    myVersion=parts.length > 2 ? parts[2] : null;
  }
}
