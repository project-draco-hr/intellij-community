{
  myBuilder.getUi().queueUpdate(connection).doWhenDone(new Runnable(){
    @Override public void run(){
      myBuilder.select(ServersTreeStructure.DeploymentLogNode.class,new TreeVisitor<ServersTreeStructure.DeploymentLogNode>(){
        @Override public boolean visit(        @NotNull ServersTreeStructure.DeploymentLogNode node){
          AbstractTreeNode parent=node.getParent();
          return parent instanceof ServersTreeStructure.DeploymentNodeImpl && isDeploymentNodeMatch((ServersTreeStructure.DeploymentNodeImpl)parent,connection,deploymentName) && node.getValue().second.equals(logName);
        }
      }
,null,false);
    }
  }
);
}
