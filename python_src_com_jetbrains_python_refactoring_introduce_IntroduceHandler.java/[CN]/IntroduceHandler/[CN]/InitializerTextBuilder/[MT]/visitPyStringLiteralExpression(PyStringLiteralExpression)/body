{
  ASTNode child=node.getNode().getFirstChildNode();
  while (child != null) {
    String text=child.getText();
    if (child.getElementType() == TokenType.WHITE_SPACE) {
      if (text.contains("\n")) {
        if (!text.contains("\\")) {
          myResult.append("\\");
        }
        myResult.append(text);
      }
    }
 else {
      myResult.append(text);
    }
    child=child.getTreeNext();
  }
}
