{
  int replaceLen=replace.length();
  int withLen=with.length();
  int matchPos=0;
  for (int i=0; i < this.count; i++) {
    if (this.value[i] == replace.charAt(matchPos)) {
      matchPos++;
      if (matchPos == replaceLen) {
        this.replace(i - (replaceLen - 1),i + 1,with);
        matchPos=0;
        i=i - (replaceLen - withLen);
      }
      continue;
    }
 else {
      matchPos=0;
    }
  }
  return this;
}
