{
  String realm=errText;
  final int idx1=realm.indexOf('\'');
  if (idx1 == -1) {
    throw new SvnBindException("Can not detect authentication realm name: " + errText);
  }
  final int idx2=realm.indexOf('\'',idx1 + 1);
  if (idx2 == -1) {
    throw new SvnBindException("Can not detect authentication realm name: " + errText);
  }
  realm=realm.substring(idx1 + 1,idx2);
  return realm;
}
