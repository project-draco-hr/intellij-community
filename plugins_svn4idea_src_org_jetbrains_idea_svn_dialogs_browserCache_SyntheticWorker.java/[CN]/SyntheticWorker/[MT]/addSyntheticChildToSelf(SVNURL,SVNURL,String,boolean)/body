{
  final String currentUrlAsString=myUrl.toString();
  final List<DirectoryEntry> children=myCache.getChildren(currentUrlAsString);
  if (children == null) {
    return;
  }
  children.add(createSyntheticEntry(newUrl,repositoryUrl,name,isDir));
  Collections.sort(children,new Comparator<DirectoryEntry>(){
    public int compare(    final DirectoryEntry o1,    final DirectoryEntry o2){
      final boolean dirStatus=NodeKind.DIR.equals(o1.getKind()) ^ NodeKind.DIR.equals(o1.getKind());
      if (dirStatus) {
        return NodeKind.DIR.equals(o1.getKind()) ? -1 : 1;
      }
      return o1.toString().compareTo(o2.toString());
    }
  }
);
  myCache.put(currentUrlAsString,children);
}
