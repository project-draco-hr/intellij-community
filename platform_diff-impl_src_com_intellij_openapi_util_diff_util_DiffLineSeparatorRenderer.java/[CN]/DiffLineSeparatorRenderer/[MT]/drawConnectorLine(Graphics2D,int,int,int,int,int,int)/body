{
  int y1=(start1 + end1) / 2;
  int y2=(start2 + end2) / 2;
  DiffDrawUtil.drawCurveTrapezium(g,x1,x2,y1 - TornLine.ourOuterRadius,y1 + TornLine.ourOuterRadius,y2 - TornLine.ourOuterRadius,y2 + TornLine.ourOuterRadius,null,getOuterColor());
  DiffDrawUtil.drawCurveTrapezium(g,x1,x2,y1 - TornLine.ourInnerRadius,y1 + TornLine.ourInnerRadius,y2 - TornLine.ourInnerRadius,y2 + TornLine.ourInnerRadius,null,getInnerColor());
}
