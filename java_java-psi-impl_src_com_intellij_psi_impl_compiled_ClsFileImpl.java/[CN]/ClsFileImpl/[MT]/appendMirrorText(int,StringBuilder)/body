{
  buffer.append(BANNER);
  PsiJavaModule module=getModuleDeclaration();
  if (module != null) {
    appendText(module,0,buffer);
  }
 else {
    appendText(getPackageStatement(),0,buffer,"\n\n");
    PsiClass[] classes=getClasses();
    if (classes.length > 0) {
      appendText(classes[0],0,buffer);
    }
  }
}
