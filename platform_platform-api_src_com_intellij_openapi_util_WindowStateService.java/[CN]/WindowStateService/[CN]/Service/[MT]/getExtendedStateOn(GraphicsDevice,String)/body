{
  Integer extendedState=getExtendedStateImpl(DimensionService.getKey(screen,key));
  if (extendedState != null) {
    return extendedState;
  }
  extendedState=getExtendedStateImpl(key);
  if (extendedState != null) {
    return extendedState;
  }
  return getDefaultExtendedStateOn(screen,key);
}
