{
  ApplicationManager.getApplication().executeOnPooledThread(new Runnable(){
    public void run(){
      try {
        final List<PyDebugValue> values=myDebugProcess.loadFrame();
        node.addChildren(values,true);
      }
 catch (      PyDebuggerException e) {
        LOG.error(e);
      }
    }
  }
);
}
