{
  if (path != null) {
    return path.startsWith(SSH_PREFIX) || path.startsWith(RemoteConnectionCredentialsWrapper.VAGRANT_PREFIX) || path.startsWith(RemoteConnectionCredentialsWrapper.SFTP_DEPLOYMENT_PREFIX)|| path.startsWith(RemoteConnectionCredentialsWrapper.DOCKER_PREFIX);
  }
 else {
    return false;
  }
}
