{
  final String typeNames=StringUtil.join(Extensions.getExtensions(ServerType.EP_NAME),new Function<ServerType,String>(){
    @Override public String fun(    ServerType type){
      return type.getPresentableName();
    }
  }
,", ");
  if (typeNames.length() > 0) {
    return CloudBundle.getText("clouds.configure.empty.selection.string",typeNames);
  }
  return null;
}
