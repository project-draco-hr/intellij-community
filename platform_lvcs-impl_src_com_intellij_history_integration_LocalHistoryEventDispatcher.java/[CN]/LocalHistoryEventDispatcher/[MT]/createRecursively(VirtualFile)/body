{
  if (isVersioned(f)) {
    myVcs.created(f.getPath(),f.isDirectory());
  }
  if (f.isDirectory()) {
    Iterable<VirtualFile> children=myGateway.isVersioned(f,true) ? IdeaGateway.loadAndIterateChildren(f) : IdeaGateway.iterateDBChildren(f);
    for (    VirtualFile each : children) {
      createRecursively(each);
    }
  }
}
