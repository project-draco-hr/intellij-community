{
  boolean result=false;
  if (changes != null) {
    result=ContainerUtil.or(changes,new Condition<Change>(){
      @Override public boolean value(      Change change){
        return isUnderSvn(change.getBeforeRevision()) || isUnderSvn(change.getAfterRevision());
      }
    }
);
  }
  return result;
}
