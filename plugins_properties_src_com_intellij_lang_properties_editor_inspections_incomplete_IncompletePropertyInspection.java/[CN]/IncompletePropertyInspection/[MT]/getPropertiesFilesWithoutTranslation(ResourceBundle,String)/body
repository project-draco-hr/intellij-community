{
  return ContainerUtil.filter(resourceBundle.getPropertiesFiles(),new Condition<PropertiesFile>(){
    @Override public boolean value(    PropertiesFile propertiesFile){
      return propertiesFile.findPropertyByKey(key) == null && !getIgnoredSuffixes().contains(PropertiesUtil.getSuffix(propertiesFile));
    }
  }
);
}
