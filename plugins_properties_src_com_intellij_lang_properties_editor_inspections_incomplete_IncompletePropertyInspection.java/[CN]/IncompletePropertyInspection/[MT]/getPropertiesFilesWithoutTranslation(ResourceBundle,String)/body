{
  final PropertiesFile defaultPropertiesFile=resourceBundle.getDefaultPropertiesFile();
  return ContainerUtil.filter(resourceBundle.getPropertiesFiles(),new Condition<PropertiesFile>(){
    @Override public boolean value(    PropertiesFile propertiesFile){
      return !defaultPropertiesFile.equals(propertiesFile) && propertiesFile.findPropertyByKey(key) == null && !getIgnoredSuffixes().contains(PropertiesUtil.getSuffix(propertiesFile));
    }
  }
);
}
