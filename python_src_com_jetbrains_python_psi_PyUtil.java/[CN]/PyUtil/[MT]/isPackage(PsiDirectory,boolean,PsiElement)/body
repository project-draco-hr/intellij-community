{
  if (directory.findFile(PyNames.INIT_DOT_PY) != null) {
    return true;
  }
  final LanguageLevel level=anchor != null ? LanguageLevel.forElement(anchor) : getLanguageLevelForVirtualFile(directory.getProject(),directory.getVirtualFile());
  if (level.isAtLeast(LanguageLevel.PYTHON33)) {
    return true;
  }
  return checkSetupToolsPackages && isSetuptoolsNamespacePackage(directory);
}
