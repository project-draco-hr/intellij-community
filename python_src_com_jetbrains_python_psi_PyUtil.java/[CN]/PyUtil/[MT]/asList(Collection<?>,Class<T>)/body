{
  if ((expression == null) || expression.isEmpty()) {
    return Collections.emptyList();
  }
  final List<T> result=new ArrayList<T>();
  for (  final Object element : expression) {
    final T toAdd=as(element,elementClass);
    if (toAdd != null) {
      result.add(toAdd);
    }
  }
  return result;
}
