{
  if (elt == null)   return "null!";
  ASTNode node=elt.getNode();
  if (node == null) {
    return "null";
  }
 else {
    String s=node.getText();
    int cut_pos;
    if (cutAtEOL) {
      cut_pos=s.indexOf('\n');
    }
 else {
      cut_pos=-1;
    }
    if (cut_pos < 0)     cut_pos=s.length();
    return s.substring(0,Math.min(cut_pos,s.length()));
  }
}
