{
  PyDecoratorList decolist=node.getDecoratorList();
  if (decolist != null) {
    PyDecorator[] decos=decolist.getDecorators();
    if (decos.length > 1) {
      for (int i=decos.length - 1; i >= 1; i-=1) {
        PyDecorator deco=decos[i];
        String deconame=deco.getName();
        if (("classmethod".equals(deconame) || "staticmethod".equals(deconame)) && deco.isBuiltin()) {
          registerProblem(decos[i - 1],PyBundle.message("INSP.decorator.receives.unexpected.builtin"),ProblemHighlightType.GENERIC_ERROR_OR_WARNING,null);
        }
      }
    }
  }
}
