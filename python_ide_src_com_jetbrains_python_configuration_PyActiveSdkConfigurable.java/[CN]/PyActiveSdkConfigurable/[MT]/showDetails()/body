{
  final PythonSdkDetailsDialog moreDialog=myModule == null ? new PythonSdkDetailsDialog(myProject,myAddSdkCallback) : new PythonSdkDetailsDialog(myModule,myAddSdkCallback);
  PythonSdkDetailsStep.show(myProject,myProjectSdksModel.getSdks(),moreDialog,myMainPanel,myDetailsButton.getLocationOnScreen(),myAddSdkCallback);
}
