{
  PsiModifierListOwner element=getElement(editor,file);
  if (element == null || (!ApplicationManager.getApplication().isUnitTestMode() && element.getManager().isInProject(element)) || AnnotationUtil.findAnnotation(element,Nls.class.getName()) != null)   return false;
  myText="Annotate capitalization type";
  return true;
}
