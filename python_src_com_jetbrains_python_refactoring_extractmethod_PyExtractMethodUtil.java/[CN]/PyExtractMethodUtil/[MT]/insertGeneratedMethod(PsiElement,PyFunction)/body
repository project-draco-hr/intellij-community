{
  final Pair<PsiElement,TextRange> data=anchor.getUserData(PyPsiUtils.SELECTION_BREAKS_AST_NODE);
  if (data != null) {
    anchor=data.first;
  }
  final PsiElement statement=PyPsiUtils.getStatement(anchor);
  statement.getParent().addBefore(generatedMethod,statement);
  return generatedMethod;
}
