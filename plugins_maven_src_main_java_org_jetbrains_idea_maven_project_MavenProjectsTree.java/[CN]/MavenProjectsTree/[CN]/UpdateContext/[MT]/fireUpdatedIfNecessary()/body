{
  if (updatedProjectsWithChanges.isEmpty() && deletedProjects.isEmpty())   return;
  List<MavenProject> mavenProjects=deletedProjects.isEmpty() ? Collections.emptyList() : new ArrayList<>(deletedProjects);
  List<Pair<MavenProject,MavenProjectChanges>> updated=updatedProjectsWithChanges.isEmpty() ? Collections.emptyList() : MavenUtil.mapToList(updatedProjectsWithChanges);
  fireProjectsUpdated(updated,mavenProjects);
}
