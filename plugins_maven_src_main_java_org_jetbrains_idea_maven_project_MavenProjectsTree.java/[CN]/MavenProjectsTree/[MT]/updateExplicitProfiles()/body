{
  Collection<String> available=getAvailableProfiles();
synchronized (myStateLock) {
    updateExplicitProfiles(myExplicitProfiles.getEnabledProfiles(),myTemporarilyRemovedExplicitProfiles.getEnabledProfiles(),available);
    updateExplicitProfiles(myExplicitProfiles.getDisabledProfiles(),myTemporarilyRemovedExplicitProfiles.getDisabledProfiles(),available);
  }
}
