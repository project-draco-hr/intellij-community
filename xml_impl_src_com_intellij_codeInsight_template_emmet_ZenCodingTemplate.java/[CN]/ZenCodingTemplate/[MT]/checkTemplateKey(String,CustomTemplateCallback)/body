{
  ZenCodingGenerator generator=findApplicableDefaultGenerator(callback.getContext(),true);
  if (generator == null) {
    LOG.error("Emmet is disabled for context for file " + callback.getFileType().getName(),AttachmentFactory.createAttachment(callback.getEditor().getDocument()));
    return false;
  }
  return checkTemplateKey(inputString,callback,generator);
}
