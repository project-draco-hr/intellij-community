{
  return FluentIterable.from(Lists.newArrayList(getToolWindow().getContentManager().getContents())).transform(new Function<Content,RunContentDescriptor>(){
    @Override public RunContentDescriptor apply(    @Nullable Content input){
      return input != null ? input.getUserData(CONTENT_DESCRIPTOR) : null;
    }
  }
).filter(Predicates.notNull()).toList();
}
