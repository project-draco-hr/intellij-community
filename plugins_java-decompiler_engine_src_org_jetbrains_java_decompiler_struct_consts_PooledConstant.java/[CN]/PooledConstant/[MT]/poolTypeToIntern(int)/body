{
switch (type) {
case CONSTANT_Integer:
    return INT;
case CONSTANT_Float:
  return FLOAT;
case CONSTANT_Long:
return LONG;
case CONSTANT_Double:
return DOUBLE;
case CONSTANT_String:
case CONSTANT_Class:
return REFERENCE;
default :
throw new RuntimeException("Huh?? What are you trying to load?");
}
}
