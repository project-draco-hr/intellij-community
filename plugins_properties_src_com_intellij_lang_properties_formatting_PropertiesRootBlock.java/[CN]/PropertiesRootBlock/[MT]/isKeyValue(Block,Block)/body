{
  if (!(maybeKey instanceof PropertyBlock) || !PropertiesTokenTypes.KEY_CHARACTERS.equals(((PropertyBlock)maybeKey).getNode().getElementType())) {
    return false;
  }
  if (!(maybeValue instanceof PropertyBlock) || !PropertiesTokenTypes.VALUE_CHARACTERS.equals(((PropertyBlock)maybeValue).getNode().getElementType())) {
    return false;
  }
  return true;
}
