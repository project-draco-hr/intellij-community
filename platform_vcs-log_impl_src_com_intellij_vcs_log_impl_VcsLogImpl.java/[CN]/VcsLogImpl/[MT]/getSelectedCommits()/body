{
  List<Hash> hashes=ContainerUtil.newArrayList();
  JBTable table=myLogManager.getLogUi().getTable();
  for (  int row : table.getSelectedRows()) {
    Hash hash=((AbstractVcsLogTableModel)table.getModel()).getHashAtRow(row);
    if (hash != null) {
      hashes.add(hash);
    }
  }
  return hashes;
}
