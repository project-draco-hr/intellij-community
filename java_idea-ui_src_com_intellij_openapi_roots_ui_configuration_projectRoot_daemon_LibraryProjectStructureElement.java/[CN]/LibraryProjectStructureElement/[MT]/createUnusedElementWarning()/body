{
  final List<ConfigurationErrorQuickFix> fixes=Arrays.asList(new AddLibraryToDependenciesFix(),new RemoveLibraryFix(),new RemoveAllUnusedLibrariesFix());
  final String name=StringUtil.escapeXml(myLibrary.getName());
  String libraryName=Registry.is("ide.new.project.settings") ? "<a href='http://library/" + name + "'>"+ name+ "</a>" : "'" + name + "'";
  return new ProjectStructureProblemDescription("Library " + libraryName + " is not used",null,createPlace(),ProjectStructureProblemType.unused("unused-library"),ProjectStructureProblemDescription.ProblemLevel.PROJECT,fixes,false);
}
