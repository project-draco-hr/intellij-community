{
  int result=0;
  for (  DomFileDescription description : getAllFileDescriptions()) {
    if (forStubs) {
      if (description.hasStubs()) {
        result+=description.getStubVersion();
        result+=description.getRootTagName().hashCode();
      }
    }
 else {
      result+=description.getVersion();
      result+=description.getRootTagName().hashCode();
    }
  }
  return result;
}
