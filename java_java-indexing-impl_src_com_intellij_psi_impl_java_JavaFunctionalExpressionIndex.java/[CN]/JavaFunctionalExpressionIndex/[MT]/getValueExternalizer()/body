{
  return new DataExternalizer<List<FunExprOccurrence>>(){
    @Override public void save(    @NotNull DataOutput out,    List<FunExprOccurrence> value) throws IOException {
      DataInputOutputUtil.writeINT(out,value.size());
      for (      FunExprOccurrence info : value) {
        info.serialize(out);
      }
    }
    @Override public List<FunExprOccurrence> read(    @NotNull DataInput in) throws IOException {
      int length=DataInputOutputUtil.readINT(in);
      List<FunExprOccurrence> list=new SmartList<FunExprOccurrence>();
      for (int i=0; i < length; i++) {
        list.add(FunExprOccurrence.deserialize(in));
      }
      return list;
    }
  }
;
}
