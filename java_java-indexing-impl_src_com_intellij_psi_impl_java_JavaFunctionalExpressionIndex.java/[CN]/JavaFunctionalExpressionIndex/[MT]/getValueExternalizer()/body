{
  return new DataExternalizer<TIntArrayList>(){
    @Override public void save(    @NotNull DataOutput out,    TIntArrayList value) throws IOException {
      DataInputOutputUtil.writeINT(out,value.size());
      for (int i=0; i < value.size(); i++) {
        DataInputOutputUtil.writeINT(out,value.get(i));
      }
    }
    @Override public TIntArrayList read(    @NotNull DataInput in) throws IOException {
      int length=DataInputOutputUtil.readINT(in);
      TIntArrayList list=new TIntArrayList(length);
      for (int i=0; i < length; i++) {
        list.add(DataInputOutputUtil.readINT(in));
      }
      return list;
    }
  }
;
}
