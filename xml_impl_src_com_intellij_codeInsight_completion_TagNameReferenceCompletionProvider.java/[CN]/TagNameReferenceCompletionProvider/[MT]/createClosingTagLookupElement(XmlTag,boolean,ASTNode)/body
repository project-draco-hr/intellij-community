{
  LookupElementBuilder builder=LookupElementBuilder.create(includePrefix || !nameElement.getText().contains(":") ? tag.getName() : tag.getLocalName());
  return LookupElementDecorator.withInsertHandler(TailTypeDecorator.withTail(AutoCompletionPolicy.GIVE_CHANCE_TO_OVERWRITE.applyPolicy(builder),TailType.createSimpleTailType('>')),XmlClosingTagInsertHandler.INSTANCE);
}
