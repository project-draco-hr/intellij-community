{
  if (count == 1)   return ContainerUtil.list(color1);
  if (count == 2)   return ContainerUtil.list(color1,color2);
  List<Color> result=new ArrayList<>();
  for (int i=0; i < count; i++) {
    float ratio=(float)i / (count - 1);
    result.add(new Color(ratio(color1.getRed(),color2.getRed(),ratio),ratio(color1.getGreen(),color2.getGreen(),ratio),ratio(color1.getBlue(),color2.getBlue(),ratio)));
  }
  return result;
}
