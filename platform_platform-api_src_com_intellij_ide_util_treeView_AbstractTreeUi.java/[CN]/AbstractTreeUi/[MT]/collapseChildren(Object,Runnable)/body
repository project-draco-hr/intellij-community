{
  runDone(new TreeRunnable("AbstractTreeUi.collapseChildren"){
    @Override public void perform(){
      final DefaultMutableTreeNode node=getNodeForElement(element,false);
      if (node != null) {
        getTree().collapsePath(new TreePath(node.getPath()));
        runDone(onDone);
      }
    }
  }
);
}
