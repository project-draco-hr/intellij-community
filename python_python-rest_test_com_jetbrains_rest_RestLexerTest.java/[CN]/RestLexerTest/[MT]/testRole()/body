  doTest(" role :roole1:`some text` :notparsed: text\n" + ":list field:\n" + " :second field:\n","[ , LINE]","[role, LINE]","[ , LINE]","[:roole1:, FIELD]","[`some text`, INTERPRETED]","[ , LINE]","[:, LINE]","[notparsed, LINE]","[:, LINE]","[ , LINE]","[text, LINE]","[\n, WHITESPACE]","[:list field:, FIELD]","[\n, WHITESPACE]","[ , LINE]","[:second field:, FIELD]","[\n, WHITESPACE]");
