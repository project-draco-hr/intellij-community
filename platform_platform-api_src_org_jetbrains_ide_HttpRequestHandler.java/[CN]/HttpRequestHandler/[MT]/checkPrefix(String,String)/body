{
  if (uri.length() > prefix.length() && uri.charAt(0) == '/' && uri.regionMatches(true,1,prefix,0,prefix.length())) {
    if ((uri.length() - prefix.length()) == 1) {
      return true;
    }
 else {
      char c=uri.charAt(prefix.length() + 1);
      return c == '/' || c == '?';
    }
  }
  return false;
}
