{
  ToolbarDecorator decorator=ToolbarDecorator.createDecorator(myTree).addExtraAction(new SelectAllButton()).addExtraAction(new UnselectAllButton()).setToolbarPosition(ActionToolbarPosition.BOTTOM).setToolbarBorder(IdeBorderFactory.createEmptyBorder());
  contentPanel.add(decorator.createPanel());
  return mainPanel;
}
