{
  if (StringUtil.isEmptyOrSpaces(browserPath)) {
    showError(browser == null ? IdeBundle.message("error.please.specify.path.to.web.browser",CommonBundle.settingsActionPath()) : browser.getBrowserNotFoundMessage(),browser,project,IdeBundle.message("title.browser.not.found"));
    return false;
  }
  return doLaunch(url,BrowserUtil.getOpenBrowserCommand(browserPath,false),browser,project,additionalParameters);
}
