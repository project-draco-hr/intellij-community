{
  HttpClientBuilder builder=HttpClients.custom();
  builder.setDefaultRequestConfig(createRequestConfig(auth)).setDefaultConnectionConfig(createConnectionConfig(auth)).setDefaultHeaders(createHeaders(auth)).setSslcontext(CertificateManager.getInstance().getSslContext()).setHostnameVerifier((X509HostnameVerifier)CertificateManager.HOSTNAME_VERIFIER);
  setupCredentialsProvider(builder,auth);
  return builder.build();
}
