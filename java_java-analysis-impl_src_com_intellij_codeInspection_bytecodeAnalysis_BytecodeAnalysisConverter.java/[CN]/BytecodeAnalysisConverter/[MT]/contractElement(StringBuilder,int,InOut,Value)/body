{
  for (int i=0; i < arity; i++) {
    Value currentValue=Value.Top;
    if (i == inOut.paramIndex) {
      currentValue=inOut.inValue;
    }
    if (i > 0) {
      sb.append(',');
    }
    sb.append(contractValueString(currentValue));
  }
  sb.append("->");
  sb.append(contractValueString(value));
  return sb.toString();
}
