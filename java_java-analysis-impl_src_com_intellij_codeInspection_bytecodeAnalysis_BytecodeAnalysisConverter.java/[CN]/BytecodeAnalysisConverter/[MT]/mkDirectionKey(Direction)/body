{
  if (dir == Out) {
    return 0;
  }
 else   if (dir == NullableOut) {
    return 1;
  }
 else   if (dir == Pure) {
    return 2;
  }
 else   if (dir instanceof In) {
    In in=(In)dir;
    return 3 + 8 * in.paramId() + in.nullityMask;
  }
 else {
    InOut inOut=(InOut)dir;
    return 3 + 8 * inOut.paramId() + 2 + inOut.valueId();
  }
}
