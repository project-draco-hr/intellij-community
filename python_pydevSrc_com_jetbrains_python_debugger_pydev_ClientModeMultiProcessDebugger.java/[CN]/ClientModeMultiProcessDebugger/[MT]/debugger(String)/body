{
  ProcessDebugger debugger=myThreadRegistry.getDebugger(threadId);
  if (debugger != null) {
    return debugger;
  }
 else {
    for (    ProcessDebugger d : myOtherDebuggers) {
      for (      PyThreadInfo thread : d.getThreads()) {
        if (threadId.equals(thread.getId())) {
          return d;
        }
      }
    }
    return myMainDebugger;
  }
}
