{
  checkAccess();
  myContext.setFromElement(foothold);
  if (PydevConsoleRunner.isInPydevConsole(foothold) || PyUtil.isInScratchFile(foothold)) {
    withAllModules();
    Sdk sdk=PydevConsoleRunner.getConsoleSdk(foothold);
    if (sdk != null) {
      myContext.setSdk(sdk);
    }
  }
  return this;
}
