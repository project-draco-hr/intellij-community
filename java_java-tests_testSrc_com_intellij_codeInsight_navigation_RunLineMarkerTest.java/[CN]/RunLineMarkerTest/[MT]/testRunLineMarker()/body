{
  myFixture.configureByText("MainTest.java","public class MainTest {\n" + "    public static void <caret>foo(String[] args) {\n" + "    }\n "+ "    public static void main(String[] args) {\n"+ "    }\n"+ "}");
  assertEquals(0,myFixture.findGuttersAtCaret().size());
  assertEquals(2,myFixture.findAllGutters().size());
}
