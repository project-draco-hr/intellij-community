{
  PsiElement seeker;
  for (seeker=getFirstChild(); seeker != null; seeker=seeker.getNextSibling()) {
    ASTNode node=seeker.getNode();
    if (node != null && node.getElementType() == PyTokenTypes.LPAR)     break;
  }
  while (seeker != null) {
    seeker=seeker.getNextSibling();
    if (seeker != null) {
      ASTNode node=seeker.getNode();
      if (node != null && node.getElementType() == PyTokenTypes.LPAR)       return true;
    }
  }
  return false;
}
