{
  for (  Project project : ProjectManager.getInstance().getOpenProjects()) {
    IdeFrameImpl frame=WindowManagerEx.getInstanceEx().getFrame(project);
    if (frame != null) {
      frame.updateView();
    }
  }
  IdeFrameImpl frame=WindowManagerEx.getInstanceEx().getFrame(null);
  if (frame != null) {
    frame.updateView();
  }
}
