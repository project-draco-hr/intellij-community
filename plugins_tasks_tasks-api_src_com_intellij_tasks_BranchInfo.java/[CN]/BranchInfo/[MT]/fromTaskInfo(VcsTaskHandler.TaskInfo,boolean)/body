{
  return ContainerUtil.map(taskInfo.getRepositories(),new Function<String,BranchInfo>(){
    @Override public BranchInfo fun(    String s){
      BranchInfo info=new BranchInfo();
      info.name=taskInfo.getName();
      info.repository=s;
      info.original=original;
      return info;
    }
  }
);
}
