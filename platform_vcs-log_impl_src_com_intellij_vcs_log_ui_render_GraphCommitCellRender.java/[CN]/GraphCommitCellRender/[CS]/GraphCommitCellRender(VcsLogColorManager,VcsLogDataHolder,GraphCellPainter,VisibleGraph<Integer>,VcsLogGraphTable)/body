{
  myDataHolder=dataHolder;
  myPainter=painter;
  myVisibleGraph=graph;
  myGraphTable=table;
  myRefPainter=new RefPainter(colorManager,false){
    @Override protected int getRowHeight(){
      return myGraphTable.getRowHeight();
    }
  }
;
