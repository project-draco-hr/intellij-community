{
  myProjectOrNull=context.getProject();
  myBuilder=builder;
  myContext=context;
  if (includeArtifacts) {
    myArchetypes=new MavenArchetypesStep(builder,this);
    myArchetypesPanel.add(myArchetypes.getMainPanel(),BorderLayout.CENTER);
  }
 else {
    myArchetypes=null;
  }
  initComponents();
  loadSettings();
}
