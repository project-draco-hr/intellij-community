{
  final PyExpression expr=node.getExpression();
  PyType returnType;
  returnType=expr == null ? PyNoneType.INSTANCE : myContext.getType(expr);
  if (!myHasReturns) {
    myResult=returnType;
    myHasReturns=true;
  }
 else {
    if (myResult == null) {
      myResult=returnType;
    }
 else {
      if (returnType != null) {
        myResult=PyUnionType.union(myResult,returnType);
      }
    }
  }
}
