{
  final PyExpression expr=node.getExpression();
  PyType returnType;
  returnType=expr == null ? PyNoneType.INSTANCE : expr.getType(TypeEvalContext.slow());
  if (!myHasReturns) {
    myResult=returnType;
    myHasReturns=true;
  }
 else {
    myResult=PyUnionType.union(myResult,returnType);
  }
}
