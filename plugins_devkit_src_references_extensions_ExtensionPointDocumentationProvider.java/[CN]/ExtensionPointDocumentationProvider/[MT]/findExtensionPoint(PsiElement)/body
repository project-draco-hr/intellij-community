{
  if (element == null)   return null;
  if (element.getLanguage() == XMLLanguage.INSTANCE && (element instanceof XmlTag || element instanceof XmlToken && ((XmlToken)element).getTokenType() == XmlTokenType.XML_NAME) && DescriptorUtil.isPluginXml(element.getContainingFile())) {
    final DomElement domElement=DomUtil.getDomElement(element);
    if (domElement instanceof Extension) {
      return ((Extension)domElement).getExtensionPoint();
    }
  }
  return null;
}
