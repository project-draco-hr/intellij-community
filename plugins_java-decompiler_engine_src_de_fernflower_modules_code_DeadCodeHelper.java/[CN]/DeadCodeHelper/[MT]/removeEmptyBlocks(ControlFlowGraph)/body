{
  List<BasicBlock> blocks=graph.getBlocks();
  boolean cont;
  do {
    cont=false;
    for (int i=blocks.size() - 1; i >= 0; i--) {
      BasicBlock block=(BasicBlock)blocks.get(i);
      if (removeEmptyBlock(graph,block,false)) {
        cont=true;
        break;
      }
    }
  }
 while (cont);
}
