{
  if (myRawData != null)   return myRawData;
  ByteArrayOutputStream bOut=new ByteArrayOutputStream();
  ObjectOutputStream oOut=new ObjectOutputStream(bOut);
  try {
    oOut.writeObject(myData);
    final byte[] bytes=bOut.toByteArray();
    myRawData=bytes;
    return bytes;
  }
  finally {
    oOut.close();
  }
}
