{
  long count=0, size=0;
  Enumeration<? extends ZipEntry> en=archive.entries();
  while (en.hasMoreElements()) {
    ZipEntry entr=(ZipEntry)en.nextElement();
    if (!entr.isDirectory() && entr.getName().endsWith(".class")) {
      count++;
      size+=entr.getSize();
    }
  }
  return new long[]{count,size};
}
