{
  boolean ok=false;
  if (stat != null && stat.containsStatementStrict(source)) {
    List<StatEdge> lst=stat.getAllSuccessorEdges();
    ok=lst.isEmpty();
    if (!ok) {
      StatEdge edge=lst.get(0);
      ok=(edge.closure == closure && edge.getType() == StatEdge.TYPE_BREAK);
    }
  }
  return ok;
}
