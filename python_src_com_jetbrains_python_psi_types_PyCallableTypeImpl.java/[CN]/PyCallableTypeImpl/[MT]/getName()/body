{
  return String.format("(%s) -> %s",myParameters != null ? StringUtil.join(myParameters,new Function<Pair<String,PyType>,String>(){
    @Override public String fun(    Pair<String,PyType> param){
      if (param != null) {
        final StringBuilder builder=new StringBuilder();
        final String name=param.getFirst();
        final PyType type=param.getSecond();
        if (name != null) {
          builder.append(name);
          if (type != null) {
            builder.append(": ");
          }
        }
        builder.append(type != null ? type.getName() : PyNames.UNKNOWN_TYPE);
        return builder.toString();
      }
      return PyNames.UNKNOWN_TYPE;
    }
  }
,", ") : "...",myReturnType != null ? myReturnType.getName() : PyNames.UNKNOWN_TYPE);
}
