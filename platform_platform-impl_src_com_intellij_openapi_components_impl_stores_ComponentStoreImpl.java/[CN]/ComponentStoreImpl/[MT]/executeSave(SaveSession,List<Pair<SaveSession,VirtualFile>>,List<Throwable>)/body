{
  try {
    session.save();
  }
 catch (  ReadOnlyModificationException e) {
    LOG.warn(e);
    readonlyFiles.add(Pair.create(e.getSession() == null ? session : e.getSession(),e.getFile()));
  }
catch (  Exception e) {
    if (errors == null) {
      errors=new SmartList<Throwable>();
    }
    errors.add(e);
  }
  return errors;
}
