{
  if (columnIndex == 0) {
    Boolean select=(Boolean)aValue;
    Commit commit=orderedRefCommit.get(rowIndex);
    if (select) {
      checkedCommits.add(commit);
    }
 else {
      checkedCommits.remove(commit);
      if (checkedCommits.size() == 0) {
        checkedCommits.add(orderedRefCommit.get(0));
      }
    }
  }
}
