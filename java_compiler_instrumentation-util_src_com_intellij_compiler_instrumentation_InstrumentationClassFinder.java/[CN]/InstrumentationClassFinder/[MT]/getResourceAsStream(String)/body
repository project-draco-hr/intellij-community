{
  InputStream is=null;
  Resource resource=myPlatformClasspath.getResource(resourceName);
  if (resource != null) {
    is=resource.getInputStream();
  }
  if (is == null) {
    resource=myClasspath.getResource(resourceName);
    if (resource != null) {
      is=resource.getInputStream();
    }
  }
  return is;
}
