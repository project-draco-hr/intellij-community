{
  StringBuilder desc=new StringBuilder();
  final ProjectData projectData=getData();
  desc.append("<html>" + "<table>" + "<tr>"+ "<td nowrap>"+ "<table>"+ "<tr>"+ "<td nowrap>Project:</td>"+ "<td nowrap>").append(getName()).append("</td>" + "</tr>").append(projectData != null ? "<tr>" + "<td nowrap>Location:</td>" + "<td nowrap>"+ projectData.getLinkedExternalProjectPath() : "").append("</td>" + "</tr>" + "</table>"+ "</td>"+ "</tr>");
  appendProblems(desc);
  desc.append("</table></html>");
  return desc.toString();
}
