{
  for (int i=0; i < nextAvailableIndex; i++) {
    PointerReference ref=references[i];
    if (ref == null)     continue;
    SmartPsiElementPointerImpl pointer=ref.get();
    if (pointer == null) {
      removeReference(ref);
      continue;
    }
    if (!processor.process(pointer)) {
      return false;
    }
  }
  return true;
}
