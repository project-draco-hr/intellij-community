{
  final PsiElement method=PsiTreeUtil.getParentOfType(thenBranch,PsiMethod.class,PsiLambdaExpression.class);
  if (method == null) {
    return null;
  }
  final PsiType methodType=method instanceof PsiMethod ? ((PsiMethod)method).getReturnType() : LambdaUtil.getFunctionalInterfaceReturnType((PsiLambdaExpression)method);
  final String conditional=getConditionalText(condition,thenReturnValue,elseReturnValue,methodType);
  if (conditional == null) {
    return null;
  }
  return conditional;
}
