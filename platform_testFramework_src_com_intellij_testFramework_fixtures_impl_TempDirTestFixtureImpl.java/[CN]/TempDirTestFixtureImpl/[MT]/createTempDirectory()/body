{
  try {
    if (myTempDir == null) {
      File tempHome=getTempHome();
      myTempDir=tempHome == null ? FileUtil.createTempDirectory("unitTest",null,false) : FileUtil.createTempDirectory(tempHome,"unitTest",null,false);
    }
    return myTempDir;
  }
 catch (  IOException e) {
    throw new RuntimeException("Cannot create temp dir",e);
  }
}
