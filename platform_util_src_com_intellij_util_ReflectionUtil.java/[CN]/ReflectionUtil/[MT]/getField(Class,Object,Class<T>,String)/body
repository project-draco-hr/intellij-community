{
  try {
    final Field field=findAssignableField(objectClass,fieldType,fieldName);
    @SuppressWarnings("unchecked") T t=(T)field.get(object);
    return t;
  }
 catch (  NoSuchFieldException e) {
    LOG.debug(e);
    return null;
  }
catch (  IllegalAccessException e) {
    LOG.debug(e);
    return null;
  }
}
