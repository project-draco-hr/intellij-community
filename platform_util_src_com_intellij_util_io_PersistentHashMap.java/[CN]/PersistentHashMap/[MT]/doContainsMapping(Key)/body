{
  myEnumerator.lockStorage();
  try {
    myAppendCache.remove(key);
    final int id=tryEnumerate(key);
    if (id == PersistentEnumerator.NULL_ID) {
      return false;
    }
    if (myIntMapping)     return true;
    return readValueId(id) != NULL_ADDR;
  }
  finally {
    myEnumerator.unlockStorage();
  }
}
