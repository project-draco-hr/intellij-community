{
  try {
    final FileEditor fileEditor=FileEditorManagerEx.getInstanceEx(project).getSplitters().getCurrentWindow().getSelectedEditor().getSelectedEditorWithProvider().getFirst();
    if (fileEditor instanceof StudyEditor) {
      return (StudyEditor)fileEditor;
    }
  }
 catch (  Exception e) {
    return null;
  }
  return null;
}
