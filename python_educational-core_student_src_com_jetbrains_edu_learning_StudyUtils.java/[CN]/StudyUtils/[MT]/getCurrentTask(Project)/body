{
  VirtualFile[] files=FileEditorManager.getInstance(project).getSelectedFiles();
  TaskFile taskFile=null;
  for (  VirtualFile file : files) {
    taskFile=getTaskFile(project,file);
    if (taskFile != null) {
      break;
    }
  }
  if (taskFile != null) {
    return taskFile.getTask();
  }
  return null;
}
