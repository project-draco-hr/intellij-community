{
  super.visitIdentifier(identifier);
  final JavaSdkVersion version=JavaVersionService.getInstance().getJavaSdkVersion(identifier);
  if (version != null && version.isAtLeast(JavaSdkVersion.JDK_1_8)) {
    if ("_".equals(identifier.getText())) {
      myHolder.registerProblem(identifier,JavaErrorMessages.message("underscore.identifier"),version.isAtLeast(JavaSdkVersion.JDK_1_9) ? ProblemHighlightType.ERROR : ProblemHighlightType.GENERIC_ERROR_OR_WARNING);
    }
  }
}
