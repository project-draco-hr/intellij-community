{
  final PsiElement parent=PsiTreeUtil.skipParentsOfType(referenceExpression,PsiParenthesizedExpression.class);
  if (!(parent instanceof PsiAssignmentExpression)) {
    return false;
  }
  final PsiAssignmentExpression assignmentExpression=(PsiAssignmentExpression)parent;
  if (JavaTokenType.EQ != assignmentExpression.getOperationTokenType() || !PsiTreeUtil.isAncestor(assignmentExpression.getLExpression(),referenceExpression,false)) {
    return false;
  }
  final PsiExpression qualifier=referenceExpression.getQualifierExpression();
  if (qualifier == null) {
    return true;
  }
 else   if (!(qualifier instanceof PsiThisExpression)) {
    return false;
  }
  final PsiThisExpression thisExpression=(PsiThisExpression)qualifier;
  return thisExpression.getQualifier() == null;
}
