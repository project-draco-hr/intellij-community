{
  int oldLastId=myLastId;
  JComponent component=myEditor.getContentComponent();
  component.dispatchEvent(new MouseEvent(component,myLastId=MouseEvent.MOUSE_RELEASED,System.currentTimeMillis(),getModifiers(),myX,myY,clickCount,false,myButton));
  if (oldLastId == MouseEvent.MOUSE_PRESSED) {
    component.dispatchEvent(new MouseEvent(component,myLastId=MouseEvent.MOUSE_CLICKED,System.currentTimeMillis(),getModifiers(),myX,myY,clickCount,false,myButton));
  }
  return this;
}
