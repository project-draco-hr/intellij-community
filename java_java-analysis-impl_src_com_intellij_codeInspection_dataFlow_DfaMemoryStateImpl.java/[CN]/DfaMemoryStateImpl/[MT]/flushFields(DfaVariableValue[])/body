{
  for (  EqClass aClass : myEqClasses) {
    if (aClass != null) {
      for (      DfaVariableValue value : aClass.getVariables()) {
        if (value.isFlushableByCalls()) {
          doFlush(value,true);
        }
      }
    }
  }
  for (  DfaVariableValue value : new ArrayList<DfaVariableValue>(getChangedVariable())) {
    if (value.isFlushableByCalls()) {
      doFlush(value,true);
    }
  }
}
