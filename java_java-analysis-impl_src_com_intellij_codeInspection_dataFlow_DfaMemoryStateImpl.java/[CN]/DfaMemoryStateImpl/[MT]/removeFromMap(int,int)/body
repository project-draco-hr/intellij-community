{
  if (id < 0)   return;
  id=unwrap(myFactory.getValue(id)).getID();
  int[] classes=myIdToEqClassesIndices.get(id);
  if (classes != null) {
    int i=ArrayUtil.indexOf(classes,index);
    if (i != -1) {
      classes=ArrayUtil.remove(classes,i);
      myIdToEqClassesIndices.put(id,classes);
    }
  }
}
