{
  final StringBuilder s=new StringBuilder("{");
  forEachEntry(new TIntObjectProcedure<int[]>(){
    @Override public boolean execute(    int id,    int[] set){
      DfaValue value=myFactory.getValue(id);
      s.append(value + " -> " + Arrays.toString(set)+ ", ");
      return true;
    }
  }
);
  s.append("}");
  return s.toString();
}
