{
  return new PyVariableLocator(){
    @Override public String getThreadId(){
      return threadId;
    }
    @Override public String getPyDBLocation(){
      if (var.getId() == null) {
        return threadId + "\t" + frameId+ "\tFRAME\t"+ var.getName();
      }
      return getThreadId() + "\t" + var.getId()+ "\tBY_ID";
    }
  }
;
}
