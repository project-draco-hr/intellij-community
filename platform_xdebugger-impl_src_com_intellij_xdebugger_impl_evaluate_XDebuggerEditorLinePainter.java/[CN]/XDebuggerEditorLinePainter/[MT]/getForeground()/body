{
  return new JBColor(new NotNullProducer<Color>(){
    @SuppressWarnings("UseJBColor") @NotNull @Override public Color produce(){
      return isDarkEditor() ? Registry.getColor("ide.debugger.inline.dark.fg.color",new Color(0x3d8065)) : Registry.getColor("ide.debugger.inline.fg.color",new Color(0x3d8065));
    }
  }
);
}
