{
  String s="<?php\n" + "if ($a == 1) {\n" + "  echo \"aba\";"+ "  echo \"caba\";"+ "}\n"+ "if ($b == 1) {\n"+ "  echo \"aba\";\n"+ "}\n"+ "if ($c == 1) {}";
  doTest(s,"if ($cond$) {\n" + " '_T*" + "}",3,3);
  doTest(s,"if ($cond$) {\n" + " '_T+" + "}",2,2);
  doTest(s,"if ($cond$) {\n" + " $st$" + "}",1,1);
}
