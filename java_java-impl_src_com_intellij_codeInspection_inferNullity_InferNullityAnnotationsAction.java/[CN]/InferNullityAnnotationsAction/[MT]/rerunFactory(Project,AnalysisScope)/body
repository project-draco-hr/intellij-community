{
  return new Factory<UsageSearcher>(){
    @Override public UsageSearcher create(){
      return new UsageInfoSearcherAdapter(){
        @Override protected UsageInfo[] findUsages(){
          return InferNullityAnnotationsAction.this.findUsages(project,scope);
        }
        @Override public void generate(        @NotNull Processor<Usage> processor){
          processUsages(processor,project);
        }
      }
;
    }
  }
;
}
