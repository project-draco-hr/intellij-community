{
  for (PsiStatement statement=getPrevNonEmptyStatement(returnStatement,null); statement != null; statement=getPrevNonEmptyStatement(statement,null)) {
    if (statement instanceof PsiExpressionStatement) {
      PsiExpressionStatement expressionStatement=(PsiExpressionStatement)statement;
      PsiExpression expression=expressionStatement.getExpression();
      if (expression instanceof PsiAssignmentExpression) {
        PsiAssignmentExpression assignmentExpression=(PsiAssignmentExpression)expression;
        if (isVariableUsed(flow,assignmentExpression.getLExpression(),variable) && isVariableUsed(flow,assignmentExpression.getRExpression(),variable)) {
          return true;
        }
      }
    }
  }
  return false;
}
