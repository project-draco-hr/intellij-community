{
  PsiComment comment=null;
  for (PsiElement child=element.getLastChild(); child != null; child=child.getPrevSibling()) {
    if (child instanceof PsiComment) {
      comment=(PsiComment)child;
      break;
    }
  }
  if (comment != null) {
    element.replace(comment);
  }
 else {
    element.delete();
  }
}
