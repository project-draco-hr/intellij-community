{
  StringBuilder res=new StringBuilder();
  int i=0;
  for (  Module module : modules) {
    res.append('\'').append(module.getName()).append("'\n");
    if (++i > 20)     break;
  }
  if (i > 20) {
    res.append("\n ... and other ").append(modules.size() - 20).append(" modules");
  }
  return res.toString();
}
