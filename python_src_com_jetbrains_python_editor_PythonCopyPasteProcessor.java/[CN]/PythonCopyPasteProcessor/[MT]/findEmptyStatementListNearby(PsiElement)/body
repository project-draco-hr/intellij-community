{
  PyStatementList statementList=ObjectUtils.chooseNotNull(as(whitespace.getNextSibling(),PyStatementList.class),as(whitespace.getPrevSibling(),PyStatementList.class));
  if (statementList == null) {
    final PsiElement prevLeaf=getPrevNonCommentLeaf(whitespace);
    if (prevLeaf instanceof PsiErrorElement) {
      statementList=as(prevLeaf.getParent(),PyStatementList.class);
    }
  }
  return statementList;
}
