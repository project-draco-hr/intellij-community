{
  serverRuntime.getTaskExecutor().submit(new Runnable(){
    @Override public void run(){
      try {
        result.set(CloudRuntimeTask.this.run(serverRuntime));
        mySuccess.set(true);
      }
 catch (      ServerRuntimeException e) {
        runtimeErrorOccurred(e.getMessage());
      }
 finally {
        semaphore.up();
      }
    }
  }
);
}
