{
  if (node.getContainedExpression() instanceof PyReferenceExpression || node.getContainedExpression() instanceof PyStringLiteralExpression || node.getContainedExpression() instanceof PyNumericLiteralExpression) {
    registerProblem(node,"Remove redundant parentheses",ProblemHighlightType.INFORMATION,null,new RedundantParenthesesQuickFix());
  }
 else   if (node.getParent() instanceof PyIfPart || node.getParent() instanceof PyWhilePart || node.getParent() instanceof PyReturnStatement) {
    String eol=System.getProperty("line.separator");
    if (!node.getText().contains(eol)) {
      registerProblem(node,"Remove redundant parentheses",ProblemHighlightType.INFORMATION,null,new RedundantParenthesesQuickFix());
    }
  }
}
