{
  if (myWriter == null) {
    try {
      if (mySocket == null) {
        mySocket=new Socket(InetAddress.getLoopbackAddress(),myPortNumber);
      }
      myWriter=new PrintWriter(new BufferedWriter(new OutputStreamWriter(mySocket.getOutputStream())));
    }
 catch (    IOException e) {
      return;
    }
  }
  myWriter.println(s);
  myWriter.flush();
}
