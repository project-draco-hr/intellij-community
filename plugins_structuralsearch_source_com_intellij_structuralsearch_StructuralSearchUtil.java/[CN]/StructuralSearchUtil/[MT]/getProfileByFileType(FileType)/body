{
  for (  StructuralSearchProfile profile : myRegisteredProfiles) {
    if (ArrayUtil.contains(fileType,profile.getFileTypes())) {
      return profile;
    }
  }
  for (  StructuralSearchProfile profile : StructuralSearchProfile.EP_NAME.getExtensions()) {
    if (ArrayUtil.contains(fileType,profile.getFileTypes())) {
      return profile;
    }
  }
  return null;
}
