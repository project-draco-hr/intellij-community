{
  List<PsiFile> files=ContainerUtil.newArrayList();
  for (  Change change : changes) {
    VirtualFile vFile=change.getVirtualFile();
    if (vFile != null) {
      PsiFile file=PsiManager.getInstance(project).findFile(vFile);
      if (file != null)       files.add(file);
    }
  }
  return files;
}
