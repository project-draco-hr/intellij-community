{
  ExecutionManager.getInstance(project).startRunProfile(new RunProfileStarter(){
    @Override public RunContentDescriptor execute(    @NotNull Project project,    @NotNull Executor executor,    @NotNull RunProfileState state,    @Nullable RunContentDescriptor contentToReuse,    @NotNull ExecutionEnvironment environment) throws ExecutionException {
      return postProcess(environment,doExecute(project,state,contentToReuse,environment),callback);
    }
  }
,state,environment);
}
