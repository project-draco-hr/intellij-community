{
  MavenArchetype selectedArchetype=getSelectedArchetype();
  if (((myBuilder.getArchetype() == null) != (selectedArchetype == null))) {
    myBuilder.setArchetype(selectedArchetype);
    skipUpdateUI=true;
    try {
      if (myStep != null) {
        myStep.fireStateChanged();
      }
    }
  finally {
      skipUpdateUI=false;
    }
  }
}
