{
  String as_clause;
  if (asName == null) {
    as_clause="";
  }
 else {
    as_clause=" as " + asName;
  }
  final PyImportStatement importNodeToInsert=PyElementGenerator.getInstance(file.getProject()).createImportStatementFromText("import " + name + as_clause);
  try {
    file.addBefore(importNodeToInsert,getInsertPosition(file,name,priority));
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
