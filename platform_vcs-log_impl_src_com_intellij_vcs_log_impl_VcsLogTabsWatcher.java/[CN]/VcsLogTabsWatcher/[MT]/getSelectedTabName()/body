{
  if (myToolWindowManager instanceof ToolWindowManagerImpl && ((ToolWindowManagerImpl)myToolWindowManager).isToolWindowRegistered(TOOLWINDOW_ID) && myToolWindow != null && myToolWindow.isVisible()) {
    Content content=myToolWindow.getContentManager().getSelectedContent();
    if (content != null) {
      return content.getTabName();
    }
  }
  return null;
}
