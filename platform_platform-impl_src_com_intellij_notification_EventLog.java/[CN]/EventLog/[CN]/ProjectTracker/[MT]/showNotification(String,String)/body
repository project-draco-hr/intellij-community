{
  ToolWindow eventLog=getEventLog(myProject);
  if (eventLog != null) {
    activate(eventLog,groupId,new Runnable(){
      @Override public void run(){
        EventLogConsole console=getConsole(groupId);
        if (console != null) {
          console.showNotification(id);
        }
      }
    }
);
  }
}
