{
  if (myPanel == null) {
    final SvnChangeList list=getList();
    if (list == null) {
      myPanel=new JPanel();
    }
 else {
      ChangeListViewerDialog dialog=new ChangeListViewerDialog(myProject,list);
      Disposer.register(myProject,dialog.getDisposable());
      myPanel=dialog.createCenterPanel();
    }
  }
  return myPanel;
}
