{
  Collection<GitRepository> toRefresh=ContainerUtil.newHashSet(ContainerUtil.filter(successfulWithTypes.keySet(),new Condition<GitRepository>(){
    @Override public boolean value(    GitRepository repository){
      return successfulWithTypes.get(repository) != SuccessType.UP_TO_DATE;
    }
  }
));
  toRefresh.add(latestProcessed);
  return toRefresh;
}
