{
  String message=GitUntrackedFilesHelper.createUntrackedFilesOverwrittenDescription("rebase",true) + mentionRetryAndAbort(somethingWasRebased,successful) + mentionSkippedCommits()+ GitRebaseUtils.mentionLocalChangesRemainingInStash(mySaver);
