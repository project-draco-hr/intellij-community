{
  String description="Rebase failed with error" + mention(currentRepository) + ": "+ error+ "<br/>"+ mentionRetryAndAbort(somethingWasRebased,successful)+ mentionSkippedCommits()+ GitRebaseUtils.mentionLocalChangesRemainingInStash(mySaver);
  myNotifier.notifyError("Rebase Failed",description,new RebaseNotificationListener(currentRepository,somethingWasRebased,successful));
}
