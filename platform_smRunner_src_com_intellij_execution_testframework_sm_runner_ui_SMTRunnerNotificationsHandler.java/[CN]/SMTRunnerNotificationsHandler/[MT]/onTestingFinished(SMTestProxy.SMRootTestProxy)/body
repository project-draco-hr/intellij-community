{
  if (testsRoot.isEmptySuite() && !testsRoot.isInterrupted() && myConsoleProperties instanceof SMTRunnerConsoleProperties&& ((SMTRunnerConsoleProperties)myConsoleProperties).fixEmptySuite()) {
    return;
  }
  TestsUIUtil.notifyByBalloon(myConsoleProperties.getProject(),myStarted,testsRoot,myConsoleProperties,null);
}
