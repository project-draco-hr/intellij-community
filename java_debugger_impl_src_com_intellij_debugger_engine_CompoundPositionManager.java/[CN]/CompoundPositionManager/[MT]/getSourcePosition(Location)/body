{
  for (  PositionManager positionManager : myPositionManagers) {
    try {
      return positionManager.getSourcePosition(location);
    }
 catch (    NoDataException ignored) {
    }
catch (    Exception e) {
      LOG.error(e);
    }
catch (    AssertionError e) {
      LOG.error(e);
    }
  }
  return null;
}
