{
  try {
  }
 catch (  Exception e) {
    String message=e.getMessage();
    if (message != null && message.length() > 1024) {
      Exception truncated=new RuntimeException(message.substring(0,1024) + "...");
      truncated.setStackTrace(e.getStackTrace());
      e=truncated;
    }
    System.out.println(e);
  }
}
