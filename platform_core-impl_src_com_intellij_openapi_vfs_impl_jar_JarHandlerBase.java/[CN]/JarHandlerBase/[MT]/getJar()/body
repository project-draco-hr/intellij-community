{
  JarFile jar=myJarFile.get();
  if (jar == null) {
synchronized (myLock) {
      if (myCorruptedJar) {
        return null;
      }
      jar=myJarFile.get();
      if (jar == null) {
        try {
          jar=createJarFile();
          myJarFile.set(jar);
        }
 catch (        IOException e) {
          myCorruptedJar=true;
          LOG.warn(e.getMessage() + ": " + myBasePath,e);
          return null;
        }
      }
    }
  }
  return jar;
}
