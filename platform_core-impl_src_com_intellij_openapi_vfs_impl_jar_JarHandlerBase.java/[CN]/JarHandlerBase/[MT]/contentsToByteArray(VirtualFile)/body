{
  JarFile jar=getJar();
  if (jar != null) {
    JarFile.JarEntry entry=jar.getEntry(getRelativePath(file));
    if (entry != null) {
      InputStream stream=jar.getInputStream(entry);
      if (stream != null) {
        try {
          return FileUtil.loadBytes(stream,(int)entry.getSize());
        }
  finally {
          stream.close();
        }
      }
    }
  }
  return ArrayUtil.EMPTY_BYTE_ARRAY;
}
