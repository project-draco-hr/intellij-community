{
  if (e.getProject() == null) {
    e.getPresentation().setVisible(true);
    e.getPresentation().setEnabled(false);
    return;
  }
  if (getDescriptor(e.getDataContext()) == null) {
    e.getPresentation().setVisible(true);
    e.getPresentation().setEnabled(false);
    return;
  }
  e.getPresentation().setEnabledAndVisible(true);
}
