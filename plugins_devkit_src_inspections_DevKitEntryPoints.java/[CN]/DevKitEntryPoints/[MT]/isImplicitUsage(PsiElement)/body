{
  if (element instanceof PsiClass) {
    final PsiClass psiClass=(PsiClass)element;
    if (psiClass.isEnum() || psiClass.isAnnotationType() || psiClass.hasModifierProperty(PsiModifier.PRIVATE)) {
      return false;
    }
    final PsiClass domClass=JavaPsiFacade.getInstance(element.getProject()).findClass("com.intellij.util.xml.DomElement",element.getResolveScope());
    if (domClass != null && psiClass.isInheritor(domClass,true)) {
      return true;
    }
  }
  return false;
}
