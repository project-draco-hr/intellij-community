{
  final Throwable[] ex={null};
  boolean success=JobLauncher.getInstance().invokeConcurrentlyUnderProgress(Arrays.asList(new Object[8]),ProgressManager.getInstance().getProgressIndicator(),true,new Processor<Object>(){
    @Override public boolean process(    Object o){
      try {
        doAsyncRefreshTest();
      }
 catch (      Throwable t) {
        ex[0]=t;
      }
      return true;
    }
  }
);
  if (ex[0] != null)   throw ex[0];
  if (!success)   System.out.println("!success");
}
