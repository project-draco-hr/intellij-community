{
  Notification result=createDefaultNotification();
  final Version version=getConfiguredClientVersion();
  if (version != null) {
    try {
      result=validateVersion(version);
      if (result == null) {
        result=validateLocale();
      }
    }
 catch (    Throwable e) {
      LOG.info(e);
    }
  }
  return result;
}
