{
  List<Runnable> result=new ArrayList<>();
  for (  String s : l < caret > ist) {
    Runnable r=new Runnable(){
      @Override public void run(){
        String str=s;
        if (str.isEmpty())         str="none";
        System.out.println(str);
      }
    }
;
    result.add(r);
  }
  return result;
}
