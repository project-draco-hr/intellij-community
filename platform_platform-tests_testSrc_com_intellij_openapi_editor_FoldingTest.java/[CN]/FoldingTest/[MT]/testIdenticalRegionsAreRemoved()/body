{
  addFoldRegion(0,5,"...");
  addFoldRegion(0,4,"...");
  assertNumberOfValidFoldRegions(2);
  WriteCommandAction.runWriteCommandAction(getProject(),new Runnable(){
    @Override public void run(){
      myEditor.getDocument().deleteString(4,5);
    }
  }
);
  assertNumberOfValidFoldRegions(1);
}
