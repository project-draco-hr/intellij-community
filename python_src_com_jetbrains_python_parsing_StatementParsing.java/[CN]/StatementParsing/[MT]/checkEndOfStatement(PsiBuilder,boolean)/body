{
  if (builder.getTokenType() == PyTokenTypes.STATEMENT_BREAK) {
    builder.advanceLexer();
  }
 else   if (builder.getTokenType() == PyTokenTypes.SEMICOLON) {
    if (!inSuite) {
      builder.advanceLexer();
      if (builder.getTokenType() == PyTokenTypes.STATEMENT_BREAK) {
        builder.advanceLexer();
      }
    }
  }
 else   if (builder.eof()) {
    return;
  }
 else {
    builder.error("end of statement expected");
  }
}
