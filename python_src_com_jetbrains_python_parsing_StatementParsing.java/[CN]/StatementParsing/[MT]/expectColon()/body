{
  if (myBuilder.getTokenType() == PyTokenTypes.COLON) {
    myBuilder.advanceLexer();
    return true;
  }
  final PsiBuilder.Marker marker=myBuilder.mark();
  if (myBuilder.getTokenType() == PyTokenTypes.STATEMENT_BREAK) {
    myBuilder.advanceLexer();
  }
  marker.error("Colon expected");
  return false;
}
