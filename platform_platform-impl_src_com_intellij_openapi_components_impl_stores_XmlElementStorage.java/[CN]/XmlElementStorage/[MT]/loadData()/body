{
  StorageData result=createStorageData();
  Element element;
  if (myStreamProvider != null && myStreamProvider.isEnabled()) {
    try {
      element=loadDataFromStreamProvider();
      if (element != null) {
        loadState(result,element);
      }
    }
 catch (    Exception e) {
      LOG.error(e);
      element=null;
    }
  }
 else {
    element=loadLocalData();
  }
  if (element != null) {
    loadState(result,element);
  }
  return result;
}
