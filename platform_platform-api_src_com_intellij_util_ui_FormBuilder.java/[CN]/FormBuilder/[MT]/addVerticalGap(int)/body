{
  if (height == -1) {
    myPanel.add(new JLabel(),new GridBagConstraints(0,myLineCount++,2,1,0,1,CENTER,NONE,new Insets(0,0,0,0),0,0));
    return this;
  }
  return addLabeledComponent((JLabel)null,new Box.Filler(new Dimension(0,height),new Dimension(0,height),new Dimension(Short.MAX_VALUE,height)));
}
