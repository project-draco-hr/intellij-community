{
  if (GroovyConfigUtils.getInstance().isVersionAtLeast(myPlace,GroovyConfigUtils.GROOVY2_2_2) && InheritanceUtil.isInheritor(argType,GroovyCommonClassNames.GROOVY_LANG_CLOSURE)) {
    PsiType converted=handleConversionOfSAMType(paramType,(PsiClassType)argType);
    if (converted != null) {
      return converted;
    }
  }
  if (!TypesUtil.isAssignable(TypeConversionUtil.erasure(paramType),argType,myPlace) && TypesUtil.isAssignableByMethodCallConversion(paramType,argType,myPlace)) {
    return paramType;
  }
  return argType;
}
