{
  int pos=refName.indexOf('.');
  String domainName;
  String shortName;
  if (pos == -1) {
    domainName=scopeDomainName;
    shortName=refName;
  }
 else {
    domainName=refName.substring(0,pos);
    shortName=refName.substring(pos + 1);
  }
  return typeMap.resolve(domainName,shortName,direction);
}
