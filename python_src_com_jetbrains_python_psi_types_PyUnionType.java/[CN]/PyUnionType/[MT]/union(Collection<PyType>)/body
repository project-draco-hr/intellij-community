{
  final int n=members.size();
  if (n == 0) {
    return null;
  }
 else   if (n == 1) {
    return members.iterator().next();
  }
 else {
    final Iterator<PyType> it=members.iterator();
    PyType res=unit(it.next());
    while (it.hasNext()) {
      res=union(res,it.next());
    }
    return res;
  }
}
