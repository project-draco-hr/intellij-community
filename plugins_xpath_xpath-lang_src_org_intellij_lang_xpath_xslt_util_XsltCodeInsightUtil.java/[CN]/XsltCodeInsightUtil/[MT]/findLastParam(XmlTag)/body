{
  final ArrayList<XmlTag> list=new ArrayList<>();
  final PsiElementProcessor.CollectFilteredElements<XmlTag> processor=new PsiElementProcessor.CollectFilteredElements<>(XSLT_PARAM_FILTER,list);
  templateTag.processElements(processor,templateTag);
  return list.size() > 0 ? list.get(list.size() - 1) : null;
}
