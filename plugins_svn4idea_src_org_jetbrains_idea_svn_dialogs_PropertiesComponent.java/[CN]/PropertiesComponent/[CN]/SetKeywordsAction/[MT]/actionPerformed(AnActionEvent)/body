{
  Project project=CommonDataKeys.PROJECT.getData(e.getDataContext());
  SVNPropertyData propValue=null;
  try {
    propValue=myVcs.getFactory(myFile).createPropertyClient().getProperty(SvnTarget.fromFile(myFile),SVNProperty.KEYWORDS,false,SVNRevision.WORKING);
  }
 catch (  VcsException e1) {
  }
  SetKeywordsDialog dialog=new SetKeywordsDialog(project,propValue != null ? SVNPropertyValue.getPropertyAsString(propValue.getValue()) : null);
  dialog.show();
  if (dialog.isOK()) {
    setProperty(SvnPropertyKeys.SVN_KEYWORDS,dialog.getKeywords(),false,false);
  }
  updateFileView(false);
}
