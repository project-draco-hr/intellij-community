{
  InputFilter wrapper=new InputFilter(){
    boolean isBroken;
    @Nullable @Override public List<Pair<String,ConsoleViewContentType>> applyFilter(    String text,    ConsoleViewContentType contentType){
      if (!isBroken) {
        try {
          return filter.applyFilter(text,contentType);
        }
 catch (        Throwable e) {
          isBroken=true;
          LOG.error(e);
        }
      }
      return null;
    }
  }
;
  myFilters.add(Pair.create(wrapper,DumbService.isDumbAware(filter)));
}
