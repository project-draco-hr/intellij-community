{
  final String name=keymap.getName();
  if (getDefaultKeymapName().equals(name)) {
    return "Default";
  }
  if (KeymapManager.DEFAULT_IDEA_KEYMAP.equals(name)) {
    return "IntelliJ IDEA Classic";
  }
  if ("Mac OS X".equals(name)) {
    return "IntelliJ IDEA Classic (OS X)";
  }
  return name;
}
