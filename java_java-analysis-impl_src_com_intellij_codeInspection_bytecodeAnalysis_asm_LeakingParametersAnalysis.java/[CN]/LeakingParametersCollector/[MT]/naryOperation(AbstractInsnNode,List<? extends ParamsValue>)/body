{
switch (insn.getOpcode()) {
case INVOKESTATIC:
case INVOKESPECIAL:
case INVOKEVIRTUAL:
case INVOKEINTERFACE:
    for (    ParamsValue value : values) {
      boolean[] params=value.params;
      for (int i=0; i < arity; i++) {
        leaking[i]|=params[i];
      }
    }
  break;
default :
}
return super.naryOperation(insn,values);
}
