{
  try {
    return getIssues(query);
  }
 catch (  GithubAuthenticationException e) {
    throw new Exception(e.getMessage(),e);
  }
catch (  GithubStatusCodeException e) {
    throw new Exception(e.getMessage(),e);
  }
catch (  GithubJsonException e) {
    throw new Exception("Bad response format",e);
  }
}
