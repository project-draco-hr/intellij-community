{
  CommonCodeStyleSettings.IndentOptions options=CodeStyleSettingsManager.getInstance(getProject()).getCurrentSettings().getIndentOptions(JavaFileType.INSTANCE);
  int indent=options.INDENT_SIZE;
  options.INDENT_SIZE*=2;
  try {
    doTest("Bounds");
  }
  finally {
    options.INDENT_SIZE=indent;
  }
}
