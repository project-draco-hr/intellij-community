{
  int line1=getStartLine(side);
  int line2=getEndLine(side);
  if (line2 <= oldLine1)   return false;
  if (line1 >= oldLine2) {
    myLineStartShifts[side.getIndex()]+=shift;
    myLineEndShifts[side.getIndex()]+=shift;
    return false;
  }
  if (line1 <= oldLine1 && line2 >= oldLine2) {
    myLineEndShifts[side.getIndex()]+=shift;
    return false;
  }
  return true;
}
