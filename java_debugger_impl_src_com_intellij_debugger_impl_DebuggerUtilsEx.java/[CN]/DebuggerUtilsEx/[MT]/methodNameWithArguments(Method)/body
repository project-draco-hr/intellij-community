{
  StringBuilder res=new StringBuilder();
  res.append(m.name()).append("(");
  res.append(StringUtil.join(m.argumentTypeNames(),new Function<String,String>(){
    @Override public String fun(    String s){
      return getSimpleName(s);
    }
  }
,", "));
  res.append(")");
  return res.toString();
}
