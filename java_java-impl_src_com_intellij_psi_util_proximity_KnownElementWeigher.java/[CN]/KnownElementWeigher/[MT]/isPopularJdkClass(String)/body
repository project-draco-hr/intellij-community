{
  return qname.startsWith(CommonClassNames.JAVA_LANG_STRING) || qname.equals(System.class.getName()) || qname.equals(CommonClassNames.JAVA_LANG_EXCEPTION)|| qname.equals(CommonClassNames.JAVA_LANG_THROWABLE)|| qname.equals(CommonClassNames.JAVA_LANG_RUNTIME_EXCEPTION)|| qname.equals(CommonClassNames.JAVA_LANG_RUNNABLE)|| qname.equals(CommonClassNames.JAVA_LANG_CLASS);
}
