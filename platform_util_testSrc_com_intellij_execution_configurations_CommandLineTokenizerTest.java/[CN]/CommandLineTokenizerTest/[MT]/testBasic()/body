{
  assertTokens("a b","a","b");
  assertTokens("\"a b\"","a b");
  assertTokens("a\" b\"","a b");
  assertTokens("\"a b","a b");
  assertTokens("a b\"","a","b");
  assertTokens("a b\\\"","a","b\"");
  assertTokens("a b\" c","a","b c");
  assertTokens("\"a b\" c \"d e\"","a b","c","d e");
  assertTokens("\"-Dquote=\\\\\"","-Dquote=\\\\");
}
