  doTest(InspectionGadgetsBundle.message("arrays.as.list.with.one.argument.quickfix"),"import java.util.*;" + "class X {{\n" + "  List<Map<String, String>> list = Arrays.<Map<String, String>>/**/asList(new HashMap<String, String>());"+ "}}","import java.util.*;" + "class X {{\n" + "  List<Map<String, String>> list = Collections.<Map<String, String>>singletonList(new HashMap<String, String>());"+ "}}");
