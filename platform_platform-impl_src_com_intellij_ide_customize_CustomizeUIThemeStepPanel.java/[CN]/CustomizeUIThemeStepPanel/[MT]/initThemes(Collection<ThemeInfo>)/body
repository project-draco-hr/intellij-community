{
  if (SystemInfo.isMac) {
    result.add(LafManagerImpl.useIntelliJInsteadOfAqua() ? INTELLIJ : AQUA);
    result.add(DARCULA);
  }
 else   if (SystemInfo.isWindows) {
    result.add(INTELLIJ);
    result.add(DARCULA);
  }
 else {
    result.add(INTELLIJ);
    result.add(DARCULA);
    result.add(GTK);
  }
}
