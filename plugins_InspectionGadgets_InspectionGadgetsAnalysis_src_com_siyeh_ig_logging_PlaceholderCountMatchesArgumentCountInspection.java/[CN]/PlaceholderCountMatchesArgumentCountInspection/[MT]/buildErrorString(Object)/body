{
  final Integer argumentCount=(Integer)infos[0];
  final Integer placeholderCount=(Integer)infos[1];
  final Object value=infos[2];
  if (argumentCount.intValue() > placeholderCount.intValue()) {
    return InspectionGadgetsBundle.message("placeholder.count.matches.argument.count.more.problem.descriptor",argumentCount,placeholderCount,value);
  }
 else {
    return InspectionGadgetsBundle.message("placeholder.count.matches.argument.count.fewer.problem.descriptor",argumentCount,placeholderCount,value);
  }
}
