{
  this();
  first=head;
  stats.addWithKey(head,head.id);
  this.handler=handler;
  stats.addWithKey(handler,handler.id);
  List<StatEdge> lstSuccs=head.getSuccessorEdges(STATEDGE_DIRECT_ALL);
  if (!lstSuccs.isEmpty()) {
    StatEdge edge=lstSuccs.get(0);
    if (edge.getType() == StatEdge.TYPE_REGULAR) {
      post=edge.getDestination();
    }
  }
  vars.add(new VarExprent(DecompilerContext.getCountercontainer().getCounterAndIncrement(CounterContainer.VAR_COUNTER),new VarType(CodeConstants.TYPE_OBJECT,0,"java/lang/Throwable"),(VarProcessor)DecompilerContext.getProperty(DecompilerContext.CURRENT_VAR_PROCESSOR)));
}
