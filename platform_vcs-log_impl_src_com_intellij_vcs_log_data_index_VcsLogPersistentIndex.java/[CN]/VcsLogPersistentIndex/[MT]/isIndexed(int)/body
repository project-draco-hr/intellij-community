{
  try {
    return myMessagesIndex.get(commit) != null && (myUserIndex == null || myUserIndex.isIndexed(commit)) && (myPathsIndex == null || myPathsIndex.isIndexed(commit)) && (myTrigramIndex == null || myTrigramIndex.isIndexed(commit));
  }
 catch (  IOException e) {
    myFatalErrorsConsumer.consume(this,e);
  }
  return false;
}
