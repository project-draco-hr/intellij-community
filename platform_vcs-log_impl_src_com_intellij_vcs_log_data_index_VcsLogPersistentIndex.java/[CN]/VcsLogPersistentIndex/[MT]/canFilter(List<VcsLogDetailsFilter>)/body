{
  if (filters.isEmpty())   return false;
  for (  VcsLogDetailsFilter filter : filters) {
    if (!((filter instanceof VcsLogTextFilter && myTrigramIndex != null) || (filter instanceof VcsLogUserFilterImpl && myUserIndex != null) || (filter instanceof VcsLogStructureFilter && myPathsIndex != null))) {
      return false;
    }
  }
  return true;
}
