{
  if (filters.isEmpty())   return false;
  for (  VcsLogDetailsFilter filter : filters) {
    if (filter instanceof VcsLogTextFilter && myTrigramIndex != null && !VcsLogUtil.isRegexp(((VcsLogTextFilter)filter).getText()) || filter instanceof VcsLogUserFilterImpl && myUserIndex != null || filter instanceof VcsLogStructureFilter && myPathsIndex != null) {
      continue;
    }
    return false;
  }
  return true;
}
