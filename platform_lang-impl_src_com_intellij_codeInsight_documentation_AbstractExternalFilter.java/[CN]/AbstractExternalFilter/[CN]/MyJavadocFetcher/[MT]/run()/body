{
  try {
    if (url == null) {
      return;
    }
    try {
      getReaderByUrl(url,new ThrowableConsumer<Reader,IOException>(){
        @Override public void consume(        Reader reader) throws IOException {
          myBuilder.buildFromStream(url,reader,data);
        }
      }
);
    }
 catch (    ProcessCanceledException ignored) {
    }
catch (    IOException e) {
      myException=e;
    }
  }
  finally {
    ourFree=true;
  }
}
