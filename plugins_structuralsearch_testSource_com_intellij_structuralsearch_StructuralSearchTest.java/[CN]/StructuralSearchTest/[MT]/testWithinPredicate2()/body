{
  String s3="class C {\n" + "  void aaa() {\n" + "        LOG.debug(true);\n"+ "        LOG.debug(true);\n"+ "        LOG.debug(true);\n"+ "        LOG.debug(true);\n"+ "        LOG.debug(true);\n"+ "        if (true) {\n"+ "            LOG.debug(true);\n"+ "        }\n"+ "        if (true) LOG.debug(true);\n"+ "    }"+ "}";
  String s4="LOG.debug('_params*:[!within( \"if('_a) { 'st*; }\" )]);";
  assertEquals(5,findMatchesCount(s3,s4));
}
