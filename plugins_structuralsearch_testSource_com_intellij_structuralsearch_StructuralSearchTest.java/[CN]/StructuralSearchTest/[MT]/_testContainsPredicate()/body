{
  String s1="{{\n" + "  int a;\n" + "  a = 1;\n"+ "}\n"+ "{\n"+ "  int b = 1;\n"+ "  b = 1;\n"+ "}\n"+ "{\n"+ "  int c = 2;\n"+ "  c = 2;\n"+ "}}";
  String s2="{\n" + "  '_a*:[contains( \"'type $a$ = $b$;\" )];\n" + "}";
  String s2_2="{\n" + "  '_a*:[!contains( \"$type$ $a$ = $b$;\" )];\n" + "}";
  assertEquals(2,findMatchesCount(s1,s2));
  assertEquals(1,findMatchesCount(s1,s2_2));
}
