{
  String s1="import java.util.*;\n" + "class A {\n" + "  void b() {\n"+ "    Map map = new HashMap();"+ "    class AppPreferences {}\n"+ "    String key = \"key\";\n"+ "    AppPreferences value = new AppPreferences();\n"+ "    map.put(key, value );\n"+ "    map.put(value, value );\n"+ "    map.put(\"key\", value );\n"+ "    map.put(\"key\", new AppPreferences());\n"+ "  }\n"+ "}";
  String s2="'_map:[exprtype( *java\\.util\\.Map )].put('_key:[ exprtype( *Object ) ], '_value:[ exprtype( *AppPreferences ) ]);";
  assertEquals("expr type with object",4,findMatchesCount(s1,s2,true));
}
