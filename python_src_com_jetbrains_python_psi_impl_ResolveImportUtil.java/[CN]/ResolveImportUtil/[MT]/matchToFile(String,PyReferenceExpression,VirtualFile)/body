{
  if (name.equals(root_file.getName())) {
    VirtualFile initpy=root_file.findChild(INIT_PY);
    if (initpy != null) {
      PsiFile initfile=importRef.getManager().findFile(initpy);
      if (initfile != null) {
        initfile.putCopyableUserData(PyFile.KEY_IS_DIRECTORY,Boolean.TRUE);
        return initfile;
      }
    }
  }
  return null;
}
