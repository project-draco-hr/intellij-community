{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "    <plugins>"+ "        <plugin>"+ "            <artifactId>maven-jar-plugin</artifactId>"+ "            <version>2.4</version>"+ "            <configuration>"+ "                <archive>"+ "                    <manifestEntries>"+ "                        <Dependencies>some.package</Dependencies>"+ "                        <otherEntry>other entry value </otherEntry>"+ "                    </manifestEntries>"+ "                </archive>"+ "            </configuration>"+ "        </plugin>"+ "    </plugins>"+ "</build>");
  compileModules("project");
  assertUnorderedLinesWithFile(getProjectPath() + "/target/MANIFEST.MF","Manifest-Version: 1.0\n" + "otherEntry: other entry value\n" + "Dependencies: some.package\n"+ "Build-Jdk: " + extractJdkVersion(getModule("project")) + "\n"+ "Built-By: "+ System.getProperty("user.name")+ "\n"+ "Created-By: "+ ApplicationNamesInfo.getInstance().getFullProductName());
}
