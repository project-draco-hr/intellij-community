{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "    <plugins>"+ "        <plugin>"+ "            <artifactId>maven-jar-plugin</artifactId>"+ "            <version>2.4</version>"+ "            <configuration>"+ "                <archive>"+ "                    <manifestSections>"+ "                        <manifestSection>"+ "                            <name>org/test/Some.class</name>"+ "                            <manifestEntries>"+ "                                <Java-Bean>true</Java-Bean>"+ "                            </manifestEntries>"+ "                        </manifestSection>"+ "                        <manifestSection>"+ "                            <name>org/test/SomeOther.class</name>"+ "                            <manifestEntries>"+ "                                <Java-Bean>true</Java-Bean>"+ "                            </manifestEntries>"+ "                        </manifestSection>"+ "                    </manifestSections>"+ "                </archive>"+ "            </configuration>"+ "        </plugin>"+ "    </plugins>"+ "</build>");
  compileModules("project");
  assertSameLinesWithFile(getProjectPath() + "/target/MANIFEST.MF","Manifest-Version: 1.0\n" + "Build-Jdk: " + extractJdkVersion(getModule("project")) + "\n"+ "Built-By: "+ System.getProperty("user.name")+ "\n"+ "Created-By: IntelliJ IDEA\n"+ "\n"+ "Name: org/test/SomeOther.class\n"+ "Java-Bean: true\n"+ "\n"+ "Name: org/test/Some.class\n"+ "Java-Bean: true");
}
