{
  out.append(m.getName()).append('(');
  boolean firstArg=true;
  Iterator<String> namesIt=paramNames.iterator();
  for (  Type arg : m.getGenericParameterTypes()) {
    if (firstArg) {
      firstArg=false;
    }
 else {
      out.comma();
    }
    Util.writeJavaTypeName(arg,out);
    out.append(' ').append(namesIt.next());
  }
  out.append(')');
}
