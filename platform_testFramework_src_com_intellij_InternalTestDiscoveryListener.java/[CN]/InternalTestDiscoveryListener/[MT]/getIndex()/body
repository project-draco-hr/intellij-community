{
  if (myDiscoveryIndex == null) {
    final Project project=ProjectManager.getInstance().getDefaultProject();
    try {
      myDiscoveryIndexClass=Class.forName("com.intellij.execution.testDiscovery.TestDiscoveryIndex");
      myDiscoveryIndex=myDiscoveryIndexClass.getConstructor(Project.class,String.class).newInstance(project,myTracesDirectory);
    }
 catch (    Throwable e) {
      e.printStackTrace();
    }
  }
  return myDiscoveryIndex;
}
