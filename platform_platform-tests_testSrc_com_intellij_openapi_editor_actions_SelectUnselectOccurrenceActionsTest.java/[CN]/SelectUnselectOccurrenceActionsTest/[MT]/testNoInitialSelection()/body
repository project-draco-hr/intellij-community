{
  init("some t<caret>ext\n" + "some texts\n" + "another text here");
  executeAction();
  checkResult("some <selection>t<caret>ext</selection>\n" + "some texts\n" + "another text here");
  executeAction();
  checkResult("some <selection>t<caret>ext</selection>\n" + "some texts\n" + "another <selection>t<caret>ext</selection> here");
  assertEquals(0,hintCount);
}
