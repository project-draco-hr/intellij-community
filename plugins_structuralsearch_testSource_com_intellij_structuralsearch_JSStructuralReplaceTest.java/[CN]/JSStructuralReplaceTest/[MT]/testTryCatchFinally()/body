{
  String s="try {\n" + "  doc.doSomeWork();\n" + "}\n"+ "catch (err) {\n"+ "  doc.doCatch();"+ "}\n";
  String what1="try {$exp$;} catch(err) {$exp1$;}";
  String by1="try {\n" + "$exp$;\n" + "}\n"+ "finally {\n"+ "$exp1$;\n"+ "}";
  String expected1="try {\n" + "doc.doSomeWork();\n" + "}\n"+ "finally {\n"+ "doc.doCatch();\n"+ "}\n";
  actualResult=replacer.testReplace(s,what1,by1,options,false,true);
  assertEquals(expected1,actualResult);
}
