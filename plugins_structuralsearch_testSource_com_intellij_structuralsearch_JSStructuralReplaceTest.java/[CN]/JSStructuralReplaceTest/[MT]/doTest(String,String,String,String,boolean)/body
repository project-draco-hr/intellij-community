{
  doTest(s,what,by,expected,JAVASCRIPT,null,JAVASCRIPT,null);
  if (wrapAsSourceWithFunction) {
    s="class A { function f() {" + s + "} }";
    expected="class A { function f() {" + expected + "} }";
  }
