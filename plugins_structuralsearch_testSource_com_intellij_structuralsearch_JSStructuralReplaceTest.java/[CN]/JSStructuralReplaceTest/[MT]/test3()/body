{
  String s="function f() {\n" + "    document.appendChild(null, \"hello\");\n" + "    document.appendChild(\"ehlo\", \"ehlo\");\n"+ "}";
  String what="document.appendChild($arg1$,$arg2$);";
  String by="document.appendChild();";
  String expected="function f() {\n" + "    document.appendChild();\n" + "    document.appendChild();\n"+ "}";
  actualResult=replacer.testReplace(s,what,by,options,false,true);
  assertEquals(expected,actualResult);
}
