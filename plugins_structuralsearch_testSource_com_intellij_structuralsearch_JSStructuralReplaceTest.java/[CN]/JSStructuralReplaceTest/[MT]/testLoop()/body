{
  String s="for (var i = 0; i < n ; i++) {\n" + "  doc.print(i);\n" + "}";
  String what1="for (var $i$ = $start$; $i$ < $end$; $i$++)\n" + "  $exp$;\n";
  String by1="var $i$ = $start$;\n" + "while ($i$ < $end$) {\n" + "  $exp$;\n"+ "  $i$++;\n"+ "}";
  String expected1="var i = 0;\n" + "while (i < n) {\n" + "  doc.print(i);\n"+ "  i++;\n"+ "}";
  doTest(s,what1,by1,expected1);
}
