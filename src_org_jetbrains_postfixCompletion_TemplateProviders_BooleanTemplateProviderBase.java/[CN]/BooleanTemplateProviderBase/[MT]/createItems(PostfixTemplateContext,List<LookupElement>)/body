{
  for (  PrefixExpressionContext expression : context.expressions()) {
    if (isBooleanExpression(expression)) {
      if (createBooleanItems(expression,consumer))       return;
    }
  }
  if (context.executionContext.isForceMode) {
    for (    PrefixExpressionContext expression : context.expressions()) {
      if (createBooleanItems(expression,consumer))       return;
    }
  }
}
