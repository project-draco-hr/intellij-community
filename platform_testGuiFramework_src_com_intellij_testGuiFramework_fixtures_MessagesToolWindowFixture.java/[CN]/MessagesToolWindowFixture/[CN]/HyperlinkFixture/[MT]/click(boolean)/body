{
  if (synchronous) {
    execute(new GuiTask(){
      @Override protected void executeInEDT(){
        new Runnable(){
          @Override public void run(){
            doClick();
          }
        }
.run();
      }
    }
);
  }
 else {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        doClick();
      }
    }
);
  }
}
