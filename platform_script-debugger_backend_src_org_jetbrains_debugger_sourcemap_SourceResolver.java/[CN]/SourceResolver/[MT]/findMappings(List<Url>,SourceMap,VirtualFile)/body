{
  for (  Url sourceUrl : sourceUrls) {
    int index=canonicalizedSourcesMap.get(sourceUrl.trimParameters());
    if (index != -1) {
      return sourceMap.sourceIndexToMappings[index];
    }
  }
  if (sourceFile != null) {
    MappingList mappings=findByFile(sourceMap,sourceFile);
    if (mappings != null) {
      return mappings;
    }
  }
  return null;
}
