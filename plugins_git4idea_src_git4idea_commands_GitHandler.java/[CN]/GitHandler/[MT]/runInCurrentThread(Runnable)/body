{
  final GitVcs vcs=GitVcs.getInstance(myProject);
  if (vcs == null) {
    return;
  }
  if (WRITE == myCommand.lockingPolicy()) {
    vcs.getCommandLock().writeLock().lock();
  }
  try {
    start();
    if (isStarted()) {
      if (postStartAction != null) {
        postStartAction.run();
      }
      waitFor();
    }
  }
  finally {
    if (WRITE == myCommand.lockingPolicy()) {
      vcs.getCommandLock().writeLock().unlock();
    }
    logTime();
  }
}
