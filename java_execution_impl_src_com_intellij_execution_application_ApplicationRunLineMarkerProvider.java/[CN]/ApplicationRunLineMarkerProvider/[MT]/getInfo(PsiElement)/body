{
  if (isIdentifier(e)) {
    PsiElement element=e.getParent();
    if (element instanceof PsiClass && PsiMethodUtil.findMainInClass((PsiClass)element) != null)     return new Info(ApplicationConfigurationType.getInstance().getIcon(),null,ExecutorAction.getActions(0));
    if (element instanceof PsiMethod && "main".equals(((PsiMethod)element).getName()) && PsiMethodUtil.isMainMethod((PsiMethod)element))     return new Info(ApplicationConfigurationType.getInstance().getIcon(),null,ExecutorAction.getActions(0));
  }
  return null;
}
