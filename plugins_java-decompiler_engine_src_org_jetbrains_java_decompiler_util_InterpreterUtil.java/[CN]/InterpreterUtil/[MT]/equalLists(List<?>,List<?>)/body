{
  if (first == null) {
    return second == null;
  }
 else   if (second == null) {
    return first == null;
  }
  if (first.size() == second.size()) {
    for (int i=0; i < first.size(); i++) {
      if (!equalObjects(first.get(i),second.get(i))) {
        return false;
      }
    }
    return true;
  }
  return false;
}
