{
  if (first == null || second == null) {
    return equalObjects(first,second);
  }
 else {
    if (first.length != second.length) {
      return false;
    }
    for (int i=0; i < first.length; i++) {
      if (!equalObjects(first[i],second[i])) {
        return false;
      }
    }
  }
  return true;
}
