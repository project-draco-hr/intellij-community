{
  DirectoryStorageData oldData=myStorageData;
  DirectoryStorageData newData=loadState();
  myStorageData=newData;
  if (oldData == null) {
    componentNames.addAll(newData.getComponentNames());
  }
 else {
    componentNames.addAll(oldData.getComponentNames());
    componentNames.addAll(newData.getComponentNames());
  }
}
