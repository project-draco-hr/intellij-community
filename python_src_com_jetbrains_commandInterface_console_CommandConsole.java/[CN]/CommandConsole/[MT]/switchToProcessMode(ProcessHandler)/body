{
  ApplicationManager.getApplication().invokeAndWait(new Runnable(){
    @Override public void run(){
      resetAction(LanguageConsoleBuilder.registerExecuteAction(CommandConsole.this,new ProcessModeConsumer(processHandler),getTitle(),getTitle(),null));
      setLanguage(PlainTextLanguage.INSTANCE);
      setPrompt("");
    }
  }
,ModalityState.NON_MODAL);
}
