{
  CharSequence whiteSpace=super.adjustWhiteSpaceIfNecessary(whiteSpaceText,startElement,startOffset,endOffset,codeStyleSettings);
  if (whiteSpace.length() > 0 && whiteSpace.charAt(0) == '\n' && !StringUtil.contains(whiteSpace,0,whiteSpace.length(),'\\') && PythonEnterHandler.needInsertBackslash(startElement.getContainingFile(),startOffset,false)) {
    return addBackslashPrefix(whiteSpace,codeStyleSettings);
  }
  return whiteSpace;
}
