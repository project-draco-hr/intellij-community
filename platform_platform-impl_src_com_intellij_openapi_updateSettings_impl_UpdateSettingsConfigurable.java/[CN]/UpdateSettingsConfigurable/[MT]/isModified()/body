{
  if (myPanel == null) {
    return false;
  }
  if (mySettings.isCheckNeeded() != myPanel.myCheckForUpdates.isSelected() || mySettings.isSecureConnection() != myPanel.myUseSecureConnection.isSelected()) {
    return true;
  }
  Object channel=myPanel.myUpdateChannels.getSelectedItem();
  return channel != null && !channel.equals(ChannelStatus.fromCode(mySettings.getUpdateChannelType()));
}
