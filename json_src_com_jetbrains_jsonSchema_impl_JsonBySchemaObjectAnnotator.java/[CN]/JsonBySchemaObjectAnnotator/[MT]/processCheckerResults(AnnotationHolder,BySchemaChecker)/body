{
  if (!checker.isCorrect()) {
    for (    Map.Entry<PsiElement,String> entry : checker.getErrors().entrySet()) {
      if (checkIfAlreadyProcessed(holder,entry.getKey()))       continue;
      holder.createWarningAnnotation(entry.getKey(),entry.getValue());
    }
    return true;
  }
  return false;
}
