{
  StringWriter out=new StringWriter();
  out.write("hg push http://<my repo URL>\n" + "pushing to http://<my repo URL>\n" + "searching for changes\n"+ "remote: kiln: successfully pushed one changeset");
  HgCommandResult testResult=new HgCommandResult(out,new StringWriter(),0);
  assertTrue(HgPusher.getNumberOfPushedCommits(testResult) == 1);
}
