{
  myLock.lock();
  try {
    indicator.setText(TITLE);
    visitorService.visit(new Consumer<File>(){
      @Override public void consume(      final File file){
        try {
          doIndexing(file,indicator);
        }
 catch (        ProcessCanceledException e0) {
          throw e0;
        }
catch (        RuntimeException e) {
          LOG.error(e);
        }
      }
    }
);
  }
  finally {
    myLock.unlock();
  }
}
