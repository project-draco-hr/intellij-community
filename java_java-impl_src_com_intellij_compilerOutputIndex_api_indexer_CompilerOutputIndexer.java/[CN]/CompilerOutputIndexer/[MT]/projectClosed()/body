{
  if (myInitialized.get()) {
    for (    final CompilerOutputBaseIndex index : getAllIndexes()) {
      index.closeIfInitialized();
    }
    try {
      myFileTimestampsIndex.close();
      myFileEnumerator.close();
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
