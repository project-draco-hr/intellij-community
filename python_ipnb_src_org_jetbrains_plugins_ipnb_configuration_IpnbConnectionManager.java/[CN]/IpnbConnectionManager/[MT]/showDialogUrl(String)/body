{
  final String url=Messages.showInputDialog("IPython Notebook URL:","Start IPython Notebook",null,initialUrl,new InputValidator(){
    @Override public boolean checkInput(    String inputString){
      try {
        final URI uri=new URI(inputString);
        if (uri.getPort() == -1 || StringUtil.isEmptyOrSpaces(uri.getHost())) {
          return false;
        }
      }
 catch (      URISyntaxException e) {
        return false;
      }
      return !inputString.isEmpty();
    }
    @Override public boolean canClose(    String inputString){
      return true;
    }
  }
);
  return url == null ? null : StringUtil.trimEnd(url,"/");
}
