{
  for (  IpnbConnection connection : myKernels.values()) {
    connection.shutdown();
    try {
      connection.close();
    }
 catch (    IOException e) {
      LOG.error(e);
    }
catch (    InterruptedException e) {
      LOG.error(e);
    }
  }
  myKernels.clear();
  if (myProcessHandler != null) {
    myProcessHandler.killProcess();
  }
}
