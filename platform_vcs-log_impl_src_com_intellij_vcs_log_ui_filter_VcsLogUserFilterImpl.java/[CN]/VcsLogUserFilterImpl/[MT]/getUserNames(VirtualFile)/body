{
  Set<String> result=ContainerUtil.newHashSet();
  for (  String user : myUsers) {
    if (ME.equals(user)) {
      VcsUser vcsUser=myData.get(root);
      if (vcsUser != null) {
        result.addAll(getVariants(vcsUser.getName()));
      }
    }
 else {
      result.addAll(getVariants(user));
    }
  }
  return result;
}
