{
  Pair<String,String> firstAndLastName=VcsUserUtil.getFirstAndLastName(name);
  if (firstAndLastName != null) {
    return Arrays.asList(firstAndLastName.first + " " + firstAndLastName.second,firstAndLastName.first + "." + firstAndLastName.second,firstAndLastName.first + firstAndLastName.second);
  }
  return Collections.singletonList(name);
}
