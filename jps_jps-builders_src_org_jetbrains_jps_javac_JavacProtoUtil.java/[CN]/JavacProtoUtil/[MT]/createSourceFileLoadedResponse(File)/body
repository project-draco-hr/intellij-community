{
  final JavacRemoteProto.Message.Response.OutputObject outObjMsg=JavacRemoteProto.Message.Response.OutputObject.newBuilder().setKind(convertKind(JavaFileObject.Kind.SOURCE)).setFilePath(FileUtil.toSystemIndependentName(srcFile.getPath())).build();
  final JavacRemoteProto.Message.Response.Builder builder=JavacRemoteProto.Message.Response.newBuilder();
  builder.setResponseType(JavacRemoteProto.Message.Response.Type.SRC_FILE_LOADED).setOutputObject(outObjMsg);
  return builder.build();
}
