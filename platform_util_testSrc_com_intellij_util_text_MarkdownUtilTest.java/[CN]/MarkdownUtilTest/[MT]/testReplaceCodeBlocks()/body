{
  List<String> markdown=Arrays.asList(" Create the app:","","    $ npm install -g express","    $ express /tmp/foo && cd /tmp/foo");
  MarkdownUtil.replaceCodeBlock(markdown);
  Assert.assertEquals(Arrays.asList(" Create the app:","","<pre><code>$ npm install -g express","$ express /tmp/foo && cd /tmp/foo</code></pre>"),markdown);
}
