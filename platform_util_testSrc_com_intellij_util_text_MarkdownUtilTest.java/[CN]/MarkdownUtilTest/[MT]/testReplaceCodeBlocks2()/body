{
  List<String> markdown=Arrays.asList("   text","    code block","```"," code block too","```","simple text","    $ code","\t$ code continues","code done");
  MarkdownUtil.replaceCodeBlock(markdown);
  Assert.assertEquals(Arrays.asList("   text","<pre><code>code block</code></pre>","<pre><code>"," code block too","</code></pre>","simple text","<pre><code>$ code","$ code continues</code></pre>","code done"),markdown);
}
