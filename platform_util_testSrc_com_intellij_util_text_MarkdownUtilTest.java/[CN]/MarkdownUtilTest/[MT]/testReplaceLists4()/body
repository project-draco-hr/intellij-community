{
  List<String> markdown=Arrays.asList("+ one two","three",""," four","+ five","six","","seven","","+ eight");
  MarkdownUtil.generateLists(markdown);
  Assert.assertEquals(Arrays.asList("<ul><li>one two","three","","four</li>","<li>five","six</li></ul>","","seven","","<ul><li>eight</li></ul>"),markdown);
}
