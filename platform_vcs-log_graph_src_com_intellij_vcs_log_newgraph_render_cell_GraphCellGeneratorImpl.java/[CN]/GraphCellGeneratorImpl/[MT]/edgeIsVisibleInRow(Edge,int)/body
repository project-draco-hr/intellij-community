{
  int edgeSize=edge.getDownNodeVisibleIndex() - edge.getUpNodeVisibleIndex();
  if (edgeSize < getLongEdge()) {
    return true;
  }
 else {
    return visibleRowIndex - edge.getUpNodeVisibleIndex() <= getLongEdgeArrow() || edge.getDownNodeVisibleIndex() - visibleRowIndex <= getLongEdgeArrow();
  }
}
