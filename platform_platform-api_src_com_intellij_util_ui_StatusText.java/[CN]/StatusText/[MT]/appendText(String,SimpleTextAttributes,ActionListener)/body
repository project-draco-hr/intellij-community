{
  if (myIsDefaultText) {
    clear();
    myIsDefaultText=false;
  }
  myText+=text;
  myComponent.append(text,attrs);
  myClickListeners.add(listener);
  if (listener != null) {
    myHasActiveClickListeners=true;
  }
  if (myOwner != null && isStatusVisible())   myOwner.repaint();
  return this;
}
