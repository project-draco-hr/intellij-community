{
  PsiExpression previousExpression=null;
  for (int i=0; i < expressions.length; i++) {
    PsiExpression expression=expressions[i];
    if (previousExpression != null && (isZero(expression) || i == 1 && EquivalenceChecker.expressionsAreEquivalent(previousExpression,expression))) {
      return true;
    }
    previousExpression=expression;
  }
  return false;
}
