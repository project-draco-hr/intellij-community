{
  final LoggingHandler loggingHandler=myLogManager.addAdditionalLog(pipeName);
  loggingHandler.attachToProcess(new ProcessHandler(){
    @Override protected void destroyProcessImpl(){
    }
    @Override protected void detachProcessImpl(){
    }
    @Override public boolean detachIsDefault(){
      return false;
    }
    @Nullable @Override public OutputStream getProcessInput(){
      return consoleInput;
    }
  }
);
  return new LogListener(){
    @Override public void lineLogged(    String line){
      loggingHandler.print(line);
    }
  }
;
}
