{
  ListModel model=myProjectTypeList.getModel();
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < model.getSize(); i++) {
    if (builder.length() > 0) {
      builder.append(", ");
    }
    builder.append(((TemplatesGroup)model.getElementAt(i)).getName());
  }
  return builder.toString();
}
