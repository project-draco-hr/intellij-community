{
  if (mySettingsStep != null) {
    if (!mySettingsStep.validate())     return false;
  }
  ModuleWizardStep step=getCustomStep();
  if (step != null && !step.validate()) {
    return false;
  }
  if (isFrameworksMode() && !myFrameworksPanel.validate()) {
    return false;
  }
  return super.validate();
}
