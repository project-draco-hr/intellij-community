{
  if (!myCommitted && myTabs.getSelectedInfo() == myFrameworksTab) {
    boolean ok=myFrameworksPanel.downloadLibraries();
    if (!ok) {
      int answer=Messages.showYesNoDialog(getComponent(),ProjectBundle.message("warning.message.some.required.libraries.wasn.t.downloaded"),CommonBundle.getWarningTitle(),Messages.getWarningIcon());
      if (answer != Messages.YES) {
        throw new CommitStepException(null);
      }
    }
    myCommitted=true;
  }
}
