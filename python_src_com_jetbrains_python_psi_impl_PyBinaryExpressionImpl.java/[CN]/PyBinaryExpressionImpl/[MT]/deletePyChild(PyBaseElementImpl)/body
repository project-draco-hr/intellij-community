{
  PyExpression left=getLeftExpression();
  PyExpression right=getRightExpression();
  if (left == element) {
    replace(right);
  }
 else   if (right == element) {
    replace(left);
  }
 else {
    throw new IncorrectOperationException("Element " + element + " is neither left expression or right expression");
  }
}
