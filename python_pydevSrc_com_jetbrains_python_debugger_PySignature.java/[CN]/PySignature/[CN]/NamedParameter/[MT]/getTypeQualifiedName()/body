{
  if (myTypes.size() == 1) {
    return myTypes.get(0);
  }
 else {
    StringBuilder sb=new StringBuilder();
    boolean notFirst=false;
    for (    String type : myTypes) {
      if (notFirst) {
        sb.append(" or ");
      }
      sb.append(type);
      notFirst=true;
    }
    return sb.toString();
  }
}
