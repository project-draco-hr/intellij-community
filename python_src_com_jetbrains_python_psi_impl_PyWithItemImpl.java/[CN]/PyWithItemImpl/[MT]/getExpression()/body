{
  ASTNode[] children=getNode().getChildren(null);
  for (  ASTNode child : children) {
    final PsiElement e=child.getPsi();
    if (e instanceof PyExpression) {
      return (PyExpression)e;
    }
 else     if (child.getElementType() == PyTokenTypes.AS_KEYWORD) {
      break;
    }
  }
  return null;
}
