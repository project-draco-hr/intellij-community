{
  createTempSdk(sdkHome);
  before();
  final Semaphore s=new Semaphore();
  s.down();
  myOutput=new StringBuilder();
  runProcess(sdkHome,s);
  try {
    testing();
    Assert.assertTrue(s.waitFor(60000));
    XDebuggerTestUtil.waitForSwing();
    after();
  }
  finally {
    disposeProcess(myProcessHandler);
  }
}
