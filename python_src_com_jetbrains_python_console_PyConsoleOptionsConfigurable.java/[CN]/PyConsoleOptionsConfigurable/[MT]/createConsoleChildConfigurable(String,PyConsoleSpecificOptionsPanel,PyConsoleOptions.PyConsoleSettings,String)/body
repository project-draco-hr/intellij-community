{
  return new SearchableConfigurable(){
    @NotNull @Override public String getId(){
      return "PyConsoleConfigurable." + name;
    }
    @Override public Runnable enableSearch(    String option){
      return null;
    }
    @Nls @Override public String getDisplayName(){
      return name;
    }
    @Override public String getHelpTopic(){
      return helpReference;
    }
    @Override public JComponent createComponent(){
      return panel.createPanel(settings);
    }
    @Override public boolean isModified(){
      return panel.isModified();
    }
    @Override public void apply() throws ConfigurationException {
      panel.apply();
    }
    @Override public void reset(){
      panel.reset();
    }
    @Override public void disposeUIResources(){
    }
  }
;
}
