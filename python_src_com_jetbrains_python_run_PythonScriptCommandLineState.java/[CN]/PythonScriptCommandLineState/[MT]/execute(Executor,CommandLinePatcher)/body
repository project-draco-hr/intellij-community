{
  if (myConfig.showCommandLineAfterwards()) {
    PydevConsoleRunner runner=new PydevConsoleRunner(myConfig.getProject(),myConfig.getSdk(),PyConsoleType.PYTHON,myConfig.getWorkingDirectory(),myConfig.getEnvs());
    runner.run();
    return new DefaultExecutionResult(runner.getConsoleView(),runner.getProcessHandler());
  }
 else {
    return super.execute(executor,patchers);
  }
}
