{
  while (!suspended && !ended) {
    if (tasks.isEmpty()) {
      ended=true;
      break;
    }
    final Runnable task=tasks.removeFirst();
    try {
      task.run();
    }
 catch (    ProcessCanceledException e) {
      tasks.clear();
      ended=true;
      throw e;
    }
catch (    Throwable th) {
      LOG.error(th);
    }
  }
  if (ended)   clearSchedule();
}
