{
  CharSequence text=document.getCharsSequence();
  int start=document.getLineStartOffset(startLine);
  int end=document.getLineStartOffset(endLine);
  for (int i=start; i < end; i++) {
    char c=text.charAt(i);
    if (c != ' ' && c != '\t' && c != '\n') {
      return true;
    }
  }
  return false;
}
