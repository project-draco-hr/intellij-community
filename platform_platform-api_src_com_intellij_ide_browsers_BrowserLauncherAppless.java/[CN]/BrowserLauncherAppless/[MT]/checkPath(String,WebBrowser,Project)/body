{
  if (!StringUtil.isEmptyOrSpaces(browserPath)) {
    return true;
  }
  doShowError(browser == null ? IdeBundle.message("error.please.specify.path.to.web.browser",CommonBundle.settingsActionPath()) : browser.getBrowserNotFoundMessage(),browser,project,IdeBundle.message("title.browser.not.found"));
  return false;
}
