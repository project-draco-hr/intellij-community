{
  if (myNameForRendering == null) {
    myNameForRendering=first instanceof VirtualFilePathWrapper && ((VirtualFilePathWrapper)first).enforcePresentableName() ? ((VirtualFilePathWrapper)first).getPresentablePath() : UISettings.getInstance().SHOW_DIRECTORY_FOR_NON_UNIQUE_FILENAMES ? UniqueVFilePathBuilder.getInstance().getUniqueVirtualFilePath(myProject,first) : first.getName();
  }
  return myNameForRendering;
}
