{
  final PyCallerFunctionTreeStructure callerStructure=new PyCallerFunctionTreeStructure(myFixture.getProject(),function,HierarchyBrowserBaseEx.SCOPE_PROJECT);
  assertSameLinesWithFile(getVerificationCallerFilePath(),dump(callerStructure,null));
  final PyCalleeFunctionTreeStructure calleeStructure=new PyCalleeFunctionTreeStructure(myFixture.getProject(),function,HierarchyBrowserBaseEx.SCOPE_PROJECT);
  assertSameLinesWithFile(getVerificationCalleeFilePath(),dump(calleeStructure,null));
}
