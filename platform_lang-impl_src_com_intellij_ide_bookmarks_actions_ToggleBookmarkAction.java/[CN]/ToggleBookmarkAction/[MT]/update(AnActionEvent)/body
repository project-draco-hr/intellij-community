{
  Project project=event.getProject();
  DataContext dataContext=event.getDataContext();
  event.getPresentation().setEnabled(project != null && (ToolWindowManager.getInstance(project).isEditorComponentActive() && CommonDataKeys.EDITOR.getData(dataContext) != null || CommonDataKeys.VIRTUAL_FILE.getData(dataContext) != null));
  event.getPresentation().setText(IdeBundle.message("action.bookmark.toggle"));
}
