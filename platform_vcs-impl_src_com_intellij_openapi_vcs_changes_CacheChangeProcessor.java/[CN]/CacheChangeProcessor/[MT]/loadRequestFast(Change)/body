{
  if (change == null)   return NoDiffRequest.INSTANCE;
  Pair<Change,DiffRequest> pair=myRequestCache.get(change);
  if (pair != null) {
    Change oldChange=pair.first;
    if (Comparing.equal(oldChange.getBeforeRevision(),change.getBeforeRevision()) && Comparing.equal(oldChange.getAfterRevision(),change.getAfterRevision())) {
      return pair.second;
    }
  }
  return null;
}
