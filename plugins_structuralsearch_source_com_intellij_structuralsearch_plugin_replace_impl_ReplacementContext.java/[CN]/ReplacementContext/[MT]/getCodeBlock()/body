{
  if (codeBlock == null) {
    PsiCodeBlock search;
    search=(PsiCodeBlock)MatcherImplUtil.createTreeFromText(options.getMatchOptions().getSearchPattern(),MatcherImplUtil.TreeContext.Block,options.getMatchOptions().getFileType(),project)[0].getParent();
    codeBlock=search;
  }
  return codeBlock;
}
