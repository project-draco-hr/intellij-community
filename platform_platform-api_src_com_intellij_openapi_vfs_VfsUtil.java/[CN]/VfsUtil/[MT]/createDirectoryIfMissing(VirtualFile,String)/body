{
  if (parent == null) {
    return createDirectoryIfMissing(relativePath);
  }
  for (  String each : StringUtil.split(relativePath,"/")) {
    VirtualFile child=parent.findChild(each);
    if (child == null) {
      child=parent.createChildDirectory(LocalFileSystem.getInstance(),each);
    }
    parent=child;
  }
  return parent;
}
