{
  super();
  setLayout(new GridBagLayout());
  int row=0;
  GridBagConstraints c=new GridBagConstraints();
  c.fill=GridBagConstraints.HORIZONTAL;
  c.anchor=GridBagConstraints.PAGE_START;
  c.gridx=0;
  for (  String cell : cells) {
    JPanel panel=cell.startsWith("#") ? new CodePanel(project,"In[1]:",cell) : new MarkdownPanel(project,cell);
    c.gridy=row;
    row++;
    add(panel,c);
  }
  c.weighty=1;
  add(new JPanel(),c);
}
