{
  for (  String spec : specs) {
    String target=getTarget(spec,sourceBranchName);
    if (target == null) {
      LOG.info("Push spec [" + spec + "] in "+ repository.getRoot()+ " is invalid or doesn't match source branch "+ sourceBranchName);
    }
 else {
      return target;
    }
  }
  return null;
}
