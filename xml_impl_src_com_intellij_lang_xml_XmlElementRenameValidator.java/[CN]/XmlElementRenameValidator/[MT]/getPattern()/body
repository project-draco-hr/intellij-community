{
  return or(XmlPatterns.xmlTag().withMetaData(or(StandardPatterns.instanceOf(XmlElementDescriptor.class),StandardPatterns.instanceOf(XmlAttributeDescriptor.class))),psiElement(XmlElementDecl.class),psiElement(XmlAttributeDecl.class),XmlPatterns.xmlTag().withDescriptor(or(StandardPatterns.instanceOf(XmlElementDescriptor.class),StandardPatterns.instanceOf(XmlAttributeDescriptor.class))));
}
