{
  SourceValue result=super.newOperation(insn);
switch (insn.getOpcode()) {
case ICONST_0:
case ICONST_1:
    if (!isReference) {
      return result;
    }
  return new SourceValue(result.getSize());
case ACONST_NULL:
case LDC:
case NEW:
if (isReference) {
  return result;
}
return new SourceValue(result.getSize());
default :
return new SourceValue(result.getSize());
}
}
