{
  int count=comboBox.getItemCount();
  if (newItems.size() != count) {
    return true;
  }
  for (int i=0; i < count; i++) {
    Object oldItem=comboBox.getItemAt(i);
    T newItem=newItems.get(i);
    if (!ComparatorUtil.equalsNullable(oldItem,newItem)) {
      return true;
    }
  }
  return false;
}
