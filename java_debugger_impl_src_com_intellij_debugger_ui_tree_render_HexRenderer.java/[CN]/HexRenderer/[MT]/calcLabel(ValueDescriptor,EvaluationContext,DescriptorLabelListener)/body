{
  Value value=valueDescriptor.getValue();
  StringBuilder buf=new StringBuilder();
  if (value == null) {
    return "null";
  }
 else   if (value instanceof CharValue) {
    PrimitiveRenderer.appendCharValue((CharValue)value,buf);
    buf.append(' ');
    appendHexValue((PrimitiveValue)value,buf);
    return buf.toString();
  }
 else {
    appendHexValue((PrimitiveValue)value,buf);
    return buf.toString();
  }
}
