{
  assertEquals(new PyRequirement("Orange-Bioinformatics","2.5+local.version"),PyRequirement.fromString("Orange-Bioinformatics==2.5+local-version"));
  assertEquals(new PyRequirement("Orange-Bioinformatics","2.5+local.version"),PyRequirement.fromString("Orange-Bioinformatics==2.5+local_version"));
}
