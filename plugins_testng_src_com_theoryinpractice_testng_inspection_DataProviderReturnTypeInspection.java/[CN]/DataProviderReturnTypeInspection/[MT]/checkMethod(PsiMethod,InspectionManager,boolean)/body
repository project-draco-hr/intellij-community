{
  final String dataProviderFqn=DataProvider.class.getName();
  final PsiAnnotation annotation=AnnotationUtil.findAnnotation(method,dataProviderFqn);
  if (annotation != null) {
    final PsiType returnType=method.getReturnType();
    if (returnType != null && !isSuitableReturnType(returnType)) {
      final PsiTypeElement returnTypeElement=method.getReturnTypeElement();
      LOG.assertTrue(returnTypeElement != null);
      return new ProblemDescriptor[]{manager.createProblemDescriptor(returnTypeElement,"Data provider must return Object[][] or Iterator<Object[]>",true,createFixes(method),ProblemHighlightType.ERROR)};
    }
  }
  return null;
}
