{
  super.visitVariable(variable);
  if (!PsiType.BOOLEAN.equals(variable.getType())) {
    return;
  }
  final String name=variable.getName();
  for (  final String negativeName : NEGATIVE_NAMES) {
    if (name.startsWith(negativeName) && (name.length() == negativeName.length() || Character.isUpperCase(name.charAt(negativeName.length())))) {
      registerVariableError(variable,variable);
      break;
    }
  }
}
