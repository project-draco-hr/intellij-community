{
  Subroutine oldSubroutine=subroutines[insn];
  boolean changes=false;
  if (!wasQueued[insn]) {
    wasQueued[insn]=true;
    changes=true;
  }
  if (oldSubroutine != null && subroutineBeforeJSR != null) {
    changes|=oldSubroutine.merge(subroutineBeforeJSR);
  }
  if (changes && !queued[insn]) {
    queued[insn]=true;
    queue[top++]=insn;
  }
}
