{
  setState(State.Loading);
  Task task=new Task.Backgroundable(project,ProjectBundle.message("maven.loading.library.version.hint",repositoryLibraryDescription.getDisplayName()),false){
    public void run(    @NotNull ProgressIndicator indicator){
      try {
        List<String> versions=RepositoryAttachHandler.retrieveVersions(project,properties.getGroupId(),properties.getArtifactId(),repositoryLibraryDescription.getRemoteRepositories());
        versionsLoaded(versions);
      }
 catch (      Exception e) {
        versionsFailedToLoad();
      }
    }
  }
;
  ProgressManager.getInstance().run(task);
}
