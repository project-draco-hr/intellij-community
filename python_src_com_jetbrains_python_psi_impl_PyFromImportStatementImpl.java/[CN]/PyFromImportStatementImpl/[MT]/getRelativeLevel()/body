{
  int result=0;
  PsiElement import_kwd=findChildByType(PyTokenTypes.IMPORT_KEYWORD);
  if (import_kwd != null) {
    ASTNode seeker=import_kwd.getNode();
    while (seeker != null) {
      if (seeker.getElementType() == PyTokenTypes.DOT)       result+=1;
      seeker=seeker.getTreePrev();
    }
  }
  return result;
}
