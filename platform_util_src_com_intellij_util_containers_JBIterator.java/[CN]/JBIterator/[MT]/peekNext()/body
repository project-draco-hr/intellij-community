{
  if (myNext != NONE)   return;
  Object o=nextImpl();
  if (myNext == STOP)   return;
  Op op=myFirstOp.nextOp;
  while (op != null) {
    o=op.apply(o);
    if (myNext == SKIP) {
      myNext=NONE;
      o=nextImpl();
      op=myFirstOp;
    }
    if (myNext == STOP)     return;
    op=op.nextOp;
  }
  myNext=o;
}
