{
  if (isAnnotationsJarInPath(module))   return true;
  if (module == null)   return false;
  final LocateLibraryDialog dialog=new LocateLibraryDialog(module,PathManager.getLibPath(),"annotations.jar",QuickFixBundle.message("add.library.annotations.description"));
  if (dialog.showAndGet()) {
    new WriteCommandAction(module.getProject()){
      @Override protected void run(      final Result result) throws Throwable {
        addJarToRoots(dialog.getResultingLibraryPath(),module,null);
      }
    }
.execute();
    return true;
  }
  return false;
}
