{
  @NonNls String sample="NoClassDefFoundException";
  assertOrderedEquals(NameUtil.buildMatcher("ncldfou*ion",NameUtil.MatchingCaseSensitivity.NONE).matchingFragments(sample),TextRange.from(0,1),TextRange.from(2,2),TextRange.from(7,1),TextRange.from(10,3),TextRange.from(21,3));
  sample="doGet(HttpServletRequest, HttpServletResponse):void";
  assertOrderedEquals(NameUtil.buildMatcher("d*st",NameUtil.MatchingCaseSensitivity.NONE).matchingFragments(sample),TextRange.from(0,1),TextRange.from(22,2));
  assertOrderedEquals(NameUtil.buildMatcher("doge*st",NameUtil.MatchingCaseSensitivity.NONE).matchingFragments(sample),TextRange.from(0,4),TextRange.from(22,2));
  sample="_test";
  assertOrderedEquals(NameUtil.buildMatcher("_",NameUtil.MatchingCaseSensitivity.NONE).matchingFragments(sample),TextRange.from(0,1));
  assertOrderedEquals(NameUtil.buildMatcher("_t",NameUtil.MatchingCaseSensitivity.NONE).matchingFragments(sample),TextRange.from(0,2));
}
