{
  final boolean isStatic=method.hasModifierProperty(PsiModifier.STATIC);
  return isStatic && method.getParameterList().getParametersCount() == 2 && InheritanceUtil.isInheritor(method.getParameterList().getParameters()[0].getType(),JavaFxCommonNames.JAVAFX_SCENE_NODE) || !isStatic && !method.hasModifierProperty(PsiModifier.PUBLIC) && AnnotationUtil.isAnnotated(method,JavaFxCommonNames.JAVAFX_FXML_ANNOTATION,false);
}
