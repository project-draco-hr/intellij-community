{
  final boolean isStatic=method.hasModifierProperty(PsiModifier.STATIC);
  return isStatic && method.getParameterList().getParametersCount() == 2 || !isStatic && !method.hasModifierProperty(PsiModifier.PUBLIC) && AnnotationUtil.isAnnotated(method,JavaFxCommonNames.JAVAFX_FXML_ANNOTATION,false);
}
