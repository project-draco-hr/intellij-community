{
  if (codeBlock == null) {
    PsiCodeBlock search;
    search=(PsiCodeBlock)MatcherImplUtil.createTreeFromText(myContext.getOptions().getMatchOptions().getSearchPattern(),PatternTreeContext.Block,myContext.getOptions().getMatchOptions().getFileType(),myContext.getProject())[0].getParent();
    codeBlock=search;
  }
  return codeBlock;
}
