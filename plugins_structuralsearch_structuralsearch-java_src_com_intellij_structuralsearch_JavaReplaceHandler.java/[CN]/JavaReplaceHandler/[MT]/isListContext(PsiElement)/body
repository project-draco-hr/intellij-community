{
  boolean listContext=false;
  final PsiElement parent=el.getParent();
  if (parent instanceof PsiParameterList || parent instanceof PsiExpressionList || parent instanceof PsiCodeBlock|| parent instanceof PsiClass|| parent instanceof XmlText|| (parent instanceof PsiIfStatement && (((PsiIfStatement)parent).getThenBranch() == el || ((PsiIfStatement)parent).getElseBranch() == el))|| (parent instanceof PsiLoopStatement && ((PsiLoopStatement)parent).getBody() == el)) {
    listContext=true;
  }
  return listContext;
}
