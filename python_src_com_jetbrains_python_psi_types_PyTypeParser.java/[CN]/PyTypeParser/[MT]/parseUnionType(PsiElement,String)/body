{
  final List<String> elements=StringUtil.split(type," or ");
  PyType result=null;
  for (  String element : elements) {
    PyType elementType=getTypeByName(anchor,element);
    if (elementType != null) {
      if (result == null) {
        result=elementType;
      }
 else {
        result=PyUnionType.union(result,elementType);
      }
    }
  }
  return result;
}
