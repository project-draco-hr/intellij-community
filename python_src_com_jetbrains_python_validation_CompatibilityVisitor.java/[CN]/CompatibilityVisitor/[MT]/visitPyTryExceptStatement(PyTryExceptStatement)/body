{
  super.visitPyTryExceptStatement(node);
  if (PydevConsoleRunner.isInPydevConsole(node))   return;
  if (myVersionsToProcess.contains(LanguageLevel.PYTHON24)) {
    PyExceptPart[] excepts=node.getExceptParts();
    PyFinallyPart finallyPart=node.getFinallyPart();
    if (excepts.length != 0 && finallyPart != null)     registerProblem(node,"Python version 2.4 doesn't support this syntax. You could use a finally block to ensure " + "that code is always executed, or one or more except blocks to catch specific exceptions.");
  }
}
