{
  super.visitPyPrintStatement(node);
  if (shouldBeCompatibleWithPy3()) {
    boolean hasProblem=false;
    PsiElement[] arguments=node.getChildren();
    for (    PsiElement element : arguments) {
      if (!((element instanceof PyParenthesizedExpression) || (element instanceof PyTupleExpression))) {
        hasProblem=true;
        break;
      }
    }
    if (hasProblem || arguments.length == 0)     registerProblem(node,"Python version >= 3.0 do not support this syntax. The print statement has been replaced with a print() function",new CompatibilityPrintCallQuickFix());
  }
}
