{
  if (attribute.getName().equals("value")) {
    Element parent=attribute.getParent();
    if (parent != null && "option".equals(parent.getName()) && "ADDITIONAL_OPTIONS_STRING".equals(parent.getAttributeValue("name"))) {
      Element grandParent=parent.getParentElement();
      return grandParent != null && grandParent.getName().equals("component") && grandParent.getAttributeValue("name").equals("JavacSettings");
    }
  }
  return false;
}
