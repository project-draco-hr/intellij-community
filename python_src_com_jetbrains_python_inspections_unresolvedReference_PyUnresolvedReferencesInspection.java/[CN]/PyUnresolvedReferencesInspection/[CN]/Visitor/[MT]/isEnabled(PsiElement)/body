{
  if (myIsEnabled == null) {
    final boolean isPyCharm=PlatformUtils.isPyCharm();
    if (PySkeletonRefresher.isGeneratingSkeletons()) {
      myIsEnabled=false;
    }
 else     if (isPyCharm) {
      myIsEnabled=PythonSdkType.getSdk(anchor) != null || PyUtil.isInScratchFile(anchor);
    }
 else {
      myIsEnabled=true;
    }
  }
  return myIsEnabled;
}
