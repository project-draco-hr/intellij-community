{
  myProjectOrNull=context.getProject();
  myBuilder=builder;
  myContext=context;
  myParentProjectForm=new GradleParentProjectForm(myProjectOrNull,new NullableConsumer<ProjectData>(){
    @Override public void consume(    @Nullable ProjectData data){
      myParent=data;
      updateComponents();
    }
  }
);
  initComponents();
  loadSettings();
}
