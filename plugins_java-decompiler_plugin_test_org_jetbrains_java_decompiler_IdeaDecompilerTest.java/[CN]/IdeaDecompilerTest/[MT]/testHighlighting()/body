{
  VirtualFile file=getTestFile("Navigation.class");
  myFixture.openFileInEditor(file);
  IdentifierHighlighterPassFactory.doWithHighlightingEnabled(new Runnable(){
    public void run(){
      myFixture.getEditor().getCaretModel().moveToOffset(offset(11,14));
      assertEquals(2,myFixture.doHighlighting().size());
      myFixture.getEditor().getCaretModel().moveToOffset(offset(15,21));
      assertEquals(2,myFixture.doHighlighting().size());
      myFixture.getEditor().getCaretModel().moveToOffset(offset(16,28));
      assertEquals(2,myFixture.doHighlighting().size());
      myFixture.getEditor().getCaretModel().moveToOffset(offset(19,24));
      assertEquals(2,myFixture.doHighlighting().size());
    }
  }
);
}
