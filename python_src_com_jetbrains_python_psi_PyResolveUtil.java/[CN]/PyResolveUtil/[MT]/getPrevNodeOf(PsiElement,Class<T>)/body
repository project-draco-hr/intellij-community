{
  PsiElement seeker=elt;
  while (seeker != null) {
    PsiElement feeler=seeker.getPrevSibling();
    if (feeler != null) {
      seeker=getInnermostChildOf(feeler);
    }
 else {
      seeker=seeker.getParent();
      if (seeker instanceof PyFile)       return null;
    }
    if (cls.isInstance(seeker))     return (T)seeker;
  }
  return null;
}
