{
  final TreeSet<String> set=new TreeSet<>(Arrays.asList(myValues));
  for (  T value : values) {
    final String converted=convertor.convert(value);
    if (converted != null) {
      set.add(converted);
    }
  }
  myValues=ArrayUtil.toStringArray(set);
  fireContentsChanged(this,0,myValues.length);
}
