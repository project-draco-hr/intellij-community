{
  DebuggerManagerThreadImpl.assertIsManagerThread();
  try {
    StackFrameProxyImpl frame=myThreadProxy.frame(0);
    if (frame != null) {
      myTopFrame=new JavaStackFrame(frame,myTracker);
    }
  }
 catch (  EvaluateException e) {
    LOG.info(e);
  }
 finally {
    myTopFrameReady=true;
  }
}
