{
  return new IpnbFilePanel(project,this,vFile,new CellSelectionListener(){
    @Override public void selectionChanged(    @NotNull IpnbPanel ipnbPanel,    boolean byMouse){
      if (myCellTypeCombo == null)       return;
      updateCellTypeCombo(ipnbPanel);
      if (byMouse)       return;
      updateScrollPosition(ipnbPanel);
    }
  }
);
}
