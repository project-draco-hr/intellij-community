{
  T result=myValue;
  if (result != null)   return result;
synchronized (this) {
    result=myValue;
    if (result == null) {
      final RecursionGuard.StackStamp stamp=ourGuard.markStack();
      result=myComputable.compute();
      if (stamp.mayCacheNow()) {
        myValue=result;
        myComputable=null;
      }
    }
  }
  return result;
}
