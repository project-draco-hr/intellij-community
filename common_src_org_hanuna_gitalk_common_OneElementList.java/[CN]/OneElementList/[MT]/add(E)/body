{
  if (elementOrList == null) {
    elementOrList=elementToObj(e);
    return true;
  }
  List<E> list=getList();
  if (list == null) {
    list=new ArrayList<E>(2);
    list.add(objToElement(elementOrList));
    list.add(e);
    elementOrList=list;
  }
 else {
    list.add(e);
  }
  return true;
}
