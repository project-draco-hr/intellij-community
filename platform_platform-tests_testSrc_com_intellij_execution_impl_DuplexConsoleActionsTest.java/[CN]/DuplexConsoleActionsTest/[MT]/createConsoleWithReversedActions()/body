{
  Project project=getProject();
  ConsoleViewImpl console=new ConsoleViewImpl(project,GlobalSearchScope.allScope(project),false,false){
    @NotNull @Override public AnAction[] createConsoleActions(){
      return ContainerUtil.reverse(Arrays.asList(super.createConsoleActions())).toArray(new AnAction[0]);
    }
  }
;
  console.getComponent();
  ProcessHandler processHandler=new ConsoleViewImplTest.MyProcessHandler();
  processHandler.startNotify();
  console.attachToProcess(processHandler);
  return console;
}
