{
  if (matchedNode instanceof PsiIdentifier) {
    matchedNode=matchedNode.getParent();
  }
  if (matchedNode instanceof PsiReferenceExpression && (!(matchedNode.getParent() instanceof PsiMethodCallExpression) && (!(matchedNode.getParent() instanceof PsiAssignmentExpression) || ((PsiAssignmentExpression)matchedNode.getParent()).getLExpression() != matchedNode)) && MatchUtils.getReferencedElement(matchedNode) instanceof PsiVariable) {
    return true;
  }
  return false;
}
