{
  if (!PsiEquivalenceUtil.areElementsEquivalent(thenExpression,elseExpression)) {
    thenExpression.replace(JavaPsiFacade.getElementFactory(project).createExpressionFromText(conditionalExpression.getCondition().getText() + "?" + thenExpression.getText()+ ":"+ elseExpression.getText(),thenExpression));
    return true;
  }
  return false;
}
