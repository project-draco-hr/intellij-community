{
  final EditorEx editor=(EditorEx)getConsoleEditor();
  editor.getMarkupModel().processRangeHighlightersOverlappingWith(0,editor.getDocument().getTextLength(),new Processor<RangeHighlighterEx>(){
    @Override public boolean process(    RangeHighlighterEx rangeHighlighter){
      if (id.equals(NOTIFICATION_ID.get(rangeHighlighter))) {
        editor.getCaretModel().moveToOffset(rangeHighlighter.getStartOffset());
        editor.getScrollingModel().scrollToCaret(ScrollType.CENTER_UP);
        return false;
      }
      return true;
    }
  }
);
}
