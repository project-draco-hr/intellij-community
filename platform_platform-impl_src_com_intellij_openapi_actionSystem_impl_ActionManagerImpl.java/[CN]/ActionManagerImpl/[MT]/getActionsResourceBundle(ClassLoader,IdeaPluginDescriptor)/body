{
  @NonNls final String resBundleName=plugin != null && !"com.intellij".equals(plugin.getPluginId().getIdString()) ? plugin.getResourceBundleBaseName() : ACTIONS_BUNDLE;
  ResourceBundle bundle=null;
  if (resBundleName != null) {
    bundle=AbstractBundle.getResourceBundle(resBundleName,loader);
  }
  return bundle;
}
