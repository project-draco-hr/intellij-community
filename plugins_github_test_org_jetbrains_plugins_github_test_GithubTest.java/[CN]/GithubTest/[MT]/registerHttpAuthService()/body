{
  GitHttpAuthTestService myHttpAuthService=(GitHttpAuthTestService)ServiceManager.getService(GitHttpAuthService.class);
  myHttpAuthService.register(new GitHttpAuthenticator(){
    @NotNull @Override public String askPassword(    @NotNull String url){
      return myPassword;
    }
    @NotNull @Override public String askUsername(    @NotNull String url){
      return myLogin1;
    }
    @Override public void saveAuthData(){
    }
    @Override public void forgetPassword(){
    }
    @Override public boolean wasCancelled(){
      return false;
    }
  }
);
}
