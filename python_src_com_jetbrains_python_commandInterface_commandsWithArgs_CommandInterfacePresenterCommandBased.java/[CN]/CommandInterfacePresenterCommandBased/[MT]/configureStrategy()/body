{
  if (myView.getText().isEmpty()) {
    myStrategy=new NoCommandStrategy(this);
    return;
  }
  final String[] parts=getTextAsParts();
  if (parts.length > 0) {
    final Command command=myCommands.get(parts[0]);
    if (command != null) {
      myStrategy=new InCommandStrategy(command,this);
      return;
    }
  }
  myStrategy=new NoCommandStrategy(this);
}
