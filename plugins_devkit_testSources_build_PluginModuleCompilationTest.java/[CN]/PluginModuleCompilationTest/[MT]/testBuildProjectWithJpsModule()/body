{
  Module module=setupPluginProjectWithJpsModule();
  rebuild();
  prepareForDeployment(module);
  File outputFile=new File(getProjectBasePath() + "/pluginProject.zip");
  assertTrue(outputFile + " not found",outputFile.exists());
  fs().archive("pluginProject.zip").dir("pluginProject").dir("lib").archive("pluginProject.jar").dir("META-INF").file("plugin.xml").file("MANIFEST.MF").end().dir("xxx").file("MyAction.class").end().end().dir("jps").archive("jps-plugin.jar").file("Builder.class").build().assertFileEqual(outputFile);
}
