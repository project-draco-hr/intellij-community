{
  entry.accept(new TestEntryVisitor(){
    @Override public void visitTest(    @NotNull SingleTestEntry test){
      run(test.getUrl());
    }
    @Override public void visitSuite(    @NotNull SuiteEntry suite){
      run(suite.getSuiteUrl());
    }
    @Override public void visitRunConfiguration(    @NotNull RunConfigurationEntry configuration){
      run(configuration.getRunSettings());
    }
  }
);
}
