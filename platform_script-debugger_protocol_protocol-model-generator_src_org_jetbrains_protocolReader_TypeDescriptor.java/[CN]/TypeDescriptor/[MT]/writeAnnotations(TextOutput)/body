{
  if (optional || asRawString) {
    out.append("@org.jetbrains.jsonProtocol.JsonField(");
    if (optional) {
      out.append("optional=true");
    }
    if (asRawString) {
      if (optional) {
        out.append(", ");
      }
      out.append("allowAnyPrimitiveValue=true");
    }
    out.append(")").newLine();
  }
  if (isNullable()) {
    out.append("@org.jetbrains.jsonProtocol.JsonNullable").newLine();
  }
}
