{
  if (bytes instanceof ArrayReference) {
    List<Value> values=((ArrayReference)bytes).getValues();
    byte[] res=new byte[values.size()];
    int idx=0;
    for (    Value value : values) {
      if (value instanceof ByteValue) {
        res[idx++]=((ByteValue)value).value();
      }
 else {
        return null;
      }
    }
    return res;
  }
  return null;
}
