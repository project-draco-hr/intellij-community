{
  final ResourceBundle[] resourceBundles=ResourceBundle.ARRAY_DATA_KEY.getData(dataContext);
  if (resourceBundles != null && resourceBundles.length != 0) {
    final Project project=CommonDataKeys.PROJECT.getData(dataContext);
    LOG.assertTrue(project != null);
    new SafeDeleteHandler().invoke(project,map2Array(flatten(map(resourceBundles,MAPPER_RES_BUNDLE_TO_FILES)),PsiElement.class,MAPPER_FILE_AS_PSI_ELEMENT),dataContext);
  }
}
