{
  ProcessInfo[] infos=ProcessListLinux.parseOutput(true,"   PID STAT USER    COMMAND\n\n" + "     1 S    user    " + myDir + "/dir/file\n"+ "     2 Z    user    "+ myDir+ "/dir/file\n"+ "     3 SZ   user    "+ myDir+ "/dir/file\n");
  assertOrderedEquals(infos,new ProcessInfo(1,myDir + "/dir/file","","user","S"));
}
