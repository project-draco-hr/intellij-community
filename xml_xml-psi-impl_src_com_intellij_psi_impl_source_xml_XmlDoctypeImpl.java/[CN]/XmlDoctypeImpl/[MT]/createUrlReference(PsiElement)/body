{
  return new URLReference(XmlDoctypeImpl.this){
    @Override @NotNull public Object[] getVariants(){
      return findChildByRoleAsPsiElement(XmlChildRole.XML_DOCTYPE_PUBLIC) != null ? super.getVariants() : EMPTY_ARRAY;
    }
    @Override @NotNull public String getCanonicalText(){
      return extractValue(dtdUrlElement);
    }
    @Override public TextRange getRangeInElement(){
      return TextRange.from(dtdUrlElement.getTextRange().getStartOffset() - getTextRange().getStartOffset() + 1,Math.max(dtdUrlElement.getTextRange().getLength() - 2,0));
    }
  }
;
}
