{
  for (  PyParameter param : function.getParameterList().getParameters()) {
    PyNamedParameter namedParameter=param.getAsNamed();
    if (namedParameter != null) {
      if (namedParameter.getAnnotation() != null) {
        return true;
      }
    }
  }
  if (function.getAnnotation() != null) {
    return true;
  }
  return false;
}
