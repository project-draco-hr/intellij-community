{
  if (element instanceof PsiFunctionalExpression) {
    final PsiType samType=element instanceof PsiLambdaExpression ? ((PsiLambdaExpression)element).getFunctionalInterfaceType() : ((PsiMethodReferenceExpression)element).getFunctionalInterfaceType();
    return getFunctionalInterfaceMethod(samType);
  }
  return null;
}
