{
  List<Library> suitableLibraries=new ArrayList<Library>();
  for (  Library library : myLibrariesContainer.getAllLibraries()) {
    if (myLibraryDescription instanceof OldCustomLibraryDescription && ((OldCustomLibraryDescription)myLibraryDescription).isSuitableLibrary(library,myLibrariesContainer) || LibraryPresentationManager.getInstance().isLibraryOfKind(library,myLibrariesContainer,myLibraryDescription.getSuitableLibraryKinds())) {
      suitableLibraries.add(library);
    }
  }
  return suitableLibraries;
}
