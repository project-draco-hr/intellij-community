{
  new SwitchGenerator<String>(){
    @Override protected String getSwitchedValue(    ReportingPlace place){
      return index < place.args.length ? place.args[index] : null;
    }
    @Override protected void generateCaseBody(    ReportingPlace place){
      String value=getSwitchedValue(place);
      if (value != null) {
        mv.visitInsn(DUP);
        pushIntConstant(mv,index);
        mv.visitLdcInsn(value);
        mv.visitInsn(AASTORE);
      }
    }
  }
.generateSwitch(mv);
}
