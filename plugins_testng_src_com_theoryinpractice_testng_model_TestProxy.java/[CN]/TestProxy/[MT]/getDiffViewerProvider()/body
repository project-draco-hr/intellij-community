{
  if (myHyperlink == null) {
    for (    TestProxy proxy : getChildren()) {
      if (!proxy.isDefect())       continue;
      final DiffHyperlink provider=proxy.getDiffViewerProvider();
      if (provider != null) {
        return provider;
      }
    }
    return null;
  }
  return myHyperlink;
}
