{
  if (!(element instanceof LiveTemplateElement)) {
    return null;
  }
  TemplateImpl template=((LiveTemplateElement)element).getTemplate();
  String description=template.getDescription();
  if (StringUtil.isEmpty(description)) {
    description=template.getTemplateText();
  }
  return "<h2>" + template.getKey() + "</h2>"+ "<pre>"+ StringUtil.escapeXml(description)+ "</pre>";
}
