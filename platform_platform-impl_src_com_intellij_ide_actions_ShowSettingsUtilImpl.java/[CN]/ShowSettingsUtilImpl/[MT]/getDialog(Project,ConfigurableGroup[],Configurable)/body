{
  return Registry.is("ide.perProjectModality") ? new OptionsEditorDialog(getProject(project),filterEmptyGroups(groups),toSelect,true) : Registry.is("ide.new.preferences") ? new PreferencesDialog(getProject(project),filterEmptyGroups(groups)) : new OptionsEditorDialog(getProject(project),filterEmptyGroups(groups),toSelect);
}
