{
  if (Registry.is("ide.new.settings.dialog")) {
    if (!withIdeSettings) {
      project=getProject(project);
    }
    return new ConfigurableGroup[]{ConfigurableExtensionPointUtil.getConfigurableGroup(project,withIdeSettings)};
  }
  ConfigurableGroup[] groups=!withIdeSettings ? new ConfigurableGroup[]{new ProjectConfigurablesGroup(getProject(project))} : (project == null) ? new ConfigurableGroup[]{new IdeConfigurablesGroup()} : new ConfigurableGroup[]{new ProjectConfigurablesGroup(project),new IdeConfigurablesGroup()};
  return groups;
}
