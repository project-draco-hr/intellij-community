{
  if (baseDir == null) {
    return false;
  }
  for (  VirtualFile dir : new VirtualFile[]{baseDir,baseDir.findChild("community")}) {
    if (dir == null || !dir.isDirectory()) {
      continue;
    }
    if (dir.findChild("idea.iml") != null || dir.findChild("community-main.iml") != null) {
      return true;
    }
  }
  return false;
}
