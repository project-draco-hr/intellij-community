{
  GlobalInspectionTool tool=getTool();
  JobDescriptor[] additionalJobs=ObjectUtils.notNull(tool.getAdditionalJobs(context),JobDescriptor.EMPTY_ARRAY);
  StdJobDescriptors stdJobDescriptors=context.getStdJobDescriptors();
  if (tool.isGraphNeeded()) {
    additionalJobs=additionalJobs.length == 0 ? stdJobDescriptors.BUILD_GRAPH_ONLY : ArrayUtil.append(additionalJobs,stdJobDescriptors.BUILD_GRAPH);
  }
  if (tool instanceof GlobalSimpleInspectionTool) {
    additionalJobs=additionalJobs.length == 0 ? stdJobDescriptors.LOCAL_ANALYSIS_ARRAY : ArrayUtil.contains(stdJobDescriptors.LOCAL_ANALYSIS,additionalJobs) ? additionalJobs : ArrayUtil.append(additionalJobs,stdJobDescriptors.LOCAL_ANALYSIS);
  }
  return additionalJobs;
}
