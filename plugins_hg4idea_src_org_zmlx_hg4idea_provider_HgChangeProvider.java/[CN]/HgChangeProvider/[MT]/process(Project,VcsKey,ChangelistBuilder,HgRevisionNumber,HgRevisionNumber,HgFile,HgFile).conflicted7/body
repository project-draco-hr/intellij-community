{
  if (beforeFile.getFile().exists()) {
    processChange(null,HgCurrentContentRevision.create(afterFile,currentNumber),FileStatus.ADDED,builder,vcsKey);
  }
 else {
    processChange(HgContentRevision.create(project,beforeFile,parentRevision),HgCurrentContentRevision.create(afterFile,currentNumber),RENAMED,builder,vcsKey);
  }
}
