{
  PsiType type=variable.getType();
  String mapOp=null;
  if (type.equals(PsiType.INT)) {
    mapOp="mapToInt";
  }
 else   if (type.equals(PsiType.LONG)) {
    mapOp="mapToLong";
  }
 else   if (type.equals(PsiType.DOUBLE)) {
    mapOp="mapToDouble";
  }
  return mapOp == null ? "" : "." + mapOp + "("+ variable.getName()+ " -> "+ variable.getName()+ ")";
}
