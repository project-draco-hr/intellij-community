{
  StdArrangementSettings s=new StdRulePriorityAwareSettings(myGroupingRulesPanel.getRules(),myMatchingRulesPanel.getRules());
  return !Comparing.equal(getSettings(settings),s) || myForceArrangementPanel != null && settings.getCommonSettings(myLanguage).FORCE_REARRANGE_MODE != myForceArrangementPanel.getRearrangeMode();
}
